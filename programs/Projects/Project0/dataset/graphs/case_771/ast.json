  {
    "name": "sequence_flow",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SequenceFlow771 >",
      "lamports: u64"
    ],
    "attributes": [],
    "body": [
      "let flow_bump = * ctx . bumps . get (\"flow\") . unwrap () ;",
      "let audit_bump = * ctx . bumps . get (\"audit\") . unwrap () ;",
      "let ix = system_program :: Transfer { from : ctx . accounts . payer . to_account_info () , to : ctx . accounts . receiver . to_account_info () , } ;",
      "system_program :: transfer (CpiContext :: new (ctx . accounts . sys_prog . to_account_info () , ix) , lamports ,) ? ;",
      "let clk = Clock :: get () ? ;",
      "let rent_bal = ctx . accounts . rent . minimum_balance (0) ;",
      "let flow = & mut ctx . accounts . flow ;",
      "flow . bump = flow_bump ;",
      "flow . total = flow . total . checked_add (lamports) . unwrap () ;",
      "flow . last_slot = clk . slot ;",
      "let audit = & mut ctx . accounts . audit ;",
      "audit . bump = audit_bump ;",
      "audit . collected_rent = rent_bal ;",
      "msg ! (\"Case 771: flow_bump={} total={} slot={} rent={}\" , flow_bump , flow . total , flow . last_slot , audit . collected_rent) ;",
      "Ok (())"
    ]
  },
