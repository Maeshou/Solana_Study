    "attributes": [],
    "body": [
      "require_keys_neq ! (ctx . accounts . acc1 . key () , ctx . accounts . acc2 . key () , ErrorCode :: DuplicateAccount) ;",
      "let diff = if ctx . accounts . acc1 . amount > ctx . accounts . acc2 . amount { ctx . accounts . acc1 . amount - ctx . accounts . acc2 . amount } else { ctx . accounts . acc2 . amount - ctx . accounts . acc1 . amount } ;",
      "ctx . accounts . acc1 . amount = diff ;",
      "ctx . accounts . acc2 . amount = diff ;",
      "msg ! (\"Case 500: diff value {}\" , diff) ;",
      "Ok (())"
    ]
  },
