  {
    "name": "save_swatch",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SaveSwatch >",
      "rgb: [u8 ; 3]",
      "tag: [u8 ; 6]",
      "bump: u8"
    ],
    "attributes": [],
    "body": [
      "let lum = (rgb [0] as u16 * 30 + rgb [1] as u16 * 59 + rgb [2] as u16 * 11) as u16 ;",
      "let mut t = tag ;",
      "for i in 0 .. t . len ()",
      "{",
      "if ! t [i] . is_ascii_alphanumeric ()",
      "{",
      "t [i] = b'-' ;",
      "}",
      "}",
      "let seeds = [& ctx . accounts . artist . key () . to_bytes () [..] , & t [..]] ;",
      "let addr = Pubkey :: create_program_address (& seeds , & ctx . program_id , & [bump]) . map_err (| _ | error ! (PaletteErr :: Pda)) ? ;",
      "if addr != ctx . accounts . swatch_cell . key ()",
      "{",
      "return Err (error ! (PaletteErr :: Pda)) ;",
      "}",
      "let s = & mut ctx . accounts . swatch ;",
      "s . artist = ctx . accounts . artist . key () ;",
      "s . rgb = rgb ;",
      "s . tag = t ;",
      "s . luma = lum ;",
      "Ok (())"
    ]
  },
