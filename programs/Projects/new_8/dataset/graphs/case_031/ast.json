[
  {
    "name": "setup",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Setup >",
      "step: u32"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . station ;",
      "s . admin = ctx . accounts . admin . key () ;",
      "s . step = step ;",
      "s . energy = 2 ;",
      "s . queue = 0 ;",
      "if s . step < 2",
      "{",
      "s . step = 2 ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "craft",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Craft >",
      "power: u16",
      "user_bump: u8"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . station ;",
      "let seeds = & [b\"tool_bin\" , ctx . accounts . admin . key . as_ref () , & [user_bump] ,] ;",
      "let tb = Pubkey :: create_program_address (seeds , ctx . program_id) . map_err (| _ | error ! (ForgeErr :: Seed)) ? ;",
      "if tb != ctx . accounts . tool_bin . key ()",
      "{",
      "return Err (error ! (ForgeErr :: ToolRoute)) ;",
      "}",
      "let mut loops = power as u32 ;",
      "if loops > 8",
      "{",
      "loops = 8 ;",
      "}",
      "let mut acc = 3u32 ;",
      "while acc < 50",
      "{",
      "s . energy = s . energy . saturating_add (1) ;",
      "acc = acc . saturating_add (loops) ;",
      "}",
      "if s . queue < 5",
      "{",
      "s . queue = s . queue . saturating_add (2) ;",
      "}",
      "Ok (())"
    ]
  },
