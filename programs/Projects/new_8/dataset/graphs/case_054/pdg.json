      },
      {
        "id": 27,
        "label": "if qty % (steps as u64) != 1 { coupon . audit = coupon . audit . saturating_add ((qty as u32 % 11) + 2) ; }"
      },
      {
        "id": 28,
        "label": "coupon . audit = coupon . audit . saturating_add ((qty as u32 % 11) + 2) ;"
      },
      {
        "id": 29,
        "label": "steps = steps . saturating_add (2) ;"
      },
      {
        "id": 30,
        "label": "if qty > 500 { let drift = ((qty % 17) as u32) + 4 ; coupon . base = coupon . base . saturating_add (drift) ; }"
      },
      {
        "id": 31,
        "label": "let drift = ((qty % 17) as u32) + 4 ;"
      },
      {
        "id": 32,
        "label": "coupon . base = coupon . base . saturating_add (drift) ;"
