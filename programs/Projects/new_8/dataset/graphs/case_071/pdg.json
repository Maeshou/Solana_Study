        "label": "t . manager = ctx . accounts . manager . key () ;"
      },
      {
        "id": 3,
        "label": "t . bump_main = * ctx . bumps . get (\"treasury\") . ok_or (error ! (E :: MissingBump)) ? ;"
      },
      {
        "id": 4,
        "label": "t . energy = base % 900 + 50 ;"
      },
      {
        "id": 5,
        "label": "t . rank = 1 ;"
      },
      {
        "id": 6,
        "label": "if t . energy & 1 == 1 { t . rank = t . rank . saturating_add (7) ; t . energy = t . energy . rotate_left (1) . wrapping_add (13) ; }"
      },
      {
        "id": 7,
        "label": "t . rank = t . rank . saturating_add (7) ;"
      },
