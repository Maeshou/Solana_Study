        "id": 8,
        "label": "t . energy = t . energy . rotate_left (1) . wrapping_add (13) ;"
      },
      {
        "id": 9,
        "label": "for i in 0 .. 5 { t . energy = t . energy . wrapping_add ((i as u64) * 17 + 3) ; t . rank = t . rank . saturating_add ((t . energy % 23) as u32 + 5) ; }"
      },
      {
        "id": 10,
        "label": "t . energy = t . energy . wrapping_add ((i as u64) * 17 + 3) ;"
      },
      {
        "id": 11,
        "label": "t . rank = t . rank . saturating_add ((t . energy % 23) as u32 + 5) ;"
      },
      {
        "id": 12,
        "label": "if t . rank & 1 == 0 { t . energy = t . energy . wrapping_mul (2) . wrapping_add (19) ; t . rank = t . rank . saturating_add (6) ; }"
      },
      {
        "id": 13,
        "label": "t . energy = t . energy . wrapping_mul (2) . wrapping_add (19) ;"
      },
      {
        "id": 14,
        "label": "t . rank = t . rank . saturating_add (6) ;"
      },
      {
        "id": 15,
        "label": "let mut k = 0u8 ;"
      },
      {
        "id": 16,
        "label": "while k < 3 { t . energy = t . energy . rotate_right (2) . wrapping_add ((k as u64) * 11 + 9) ; t . rank = t . rank . saturating_add (4 + k as u32) ; k = k . saturating_add (1) ; }"
      },
      {
        "id": 17,
        "label": "t . energy = t . energy . rotate_right (2) . wrapping_add ((k as u64) * 11 + 9) ;"
      },
      {
        "id": 18,
        "label": "t . rank = t . rank . saturating_add (4 + k as u32) ;"
      },
      {
        "id": 19,
        "label": "k = k . saturating_add (1) ;"
      },
      {
        "id": 20,
        "label": "Ok (())"
      }
