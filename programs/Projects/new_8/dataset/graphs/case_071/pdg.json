  {
    "name": "pay_from_side_pot",
    "inputs": [
      "ctx: Context < PayFromSidePot >",
      "label: String",
      "user_bump: u8",
      "lamports: u64"
    ],
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let t = & mut ctx . accounts . treasury ;"
      },
      {
        "id": 23,
        "label": "require ! (label . as_bytes () . len () <= 32 , E :: LabelTooLong) ;"
      },
      {
        "id": 24,
        "label": "let mut step = 0u8 ;"
      },
      {
        "id": 25,
        "label": "while step < 4 { t . energy = t . energy . wrapping_add ((step as u64) * 21 + 7) ; t . rank = t . rank . saturating_add (((lamports % 29) as u32) + step as u32 + 3) ; step = step . saturating_add (1) ; }"
      },
      {
        "id": 26,
        "label": "t . energy = t . energy . wrapping_add ((step as u64) * 21 + 7) ;"
      },
      {
        "id": 27,
        "label": "t . rank = t . rank . saturating_add (((lamports % 29) as u32) + step as u32 + 3) ;"
      },
      {
        "id": 28,
        "label": "step = step . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "if t . energy & 2 == 2 { t . rank = t . rank . saturating_add (13) ; t . energy = t . energy . rotate_left (2) . wrapping_add (31) ; } else { t . rank = t . rank . saturating_add (9) ; t . energy = t . energy . rotate_right (1) . wrapping_add (23) ; }"
      },
      {
        "id": 30,
        "label": "t . rank = t . rank . saturating_add (13) ;"
      },
      {
        "id": 31,
        "label": "t . energy = t . energy . rotate_left (2) . wrapping_add (31) ;"
      },
      {
        "id": 32,
        "label": "t . rank = t . rank . saturating_add (9) ;"
      },
      {
        "id": 33,
        "label": "t . energy = t . energy . rotate_right (1) . wrapping_add (23) ;"
      },
      {
        "id": 34,
        "label": "for j in 0 .. 2 { t . energy = t . energy . wrapping_add ((j as u64) * 15 + 5) ; t . rank = t . rank . saturating_add (((t . energy % 17) as u32) + 4) ; }"
      },
      {
        "id": 35,
        "label": "t . energy = t . energy . wrapping_add ((j as u64) * 15 + 5) ;"
      },
      {
        "id": 36,
        "label": "t . rank = t . rank . saturating_add (((t . energy % 17) as u32) + 4) ;"
      },
      {
        "id": 37,
        "label": "let seeds = & [b\"side_pot\" . as_ref () , t . manager . as_ref () , label . as_bytes () , core :: slice :: from_ref (& user_bump) ,] ;"
      },
      {
        "id": 38,
        "label": "let pot = Pubkey :: create_program_address (& [b\"side_pot\" , t . manager . as_ref () , label . as_bytes () , & [user_bump]] , ctx . program_id ,) . map_err (| _ | error ! (E :: SeedCompute)) ? ;"
      },
      {
        "id": 39,
        "label": "let ix = system_instruction :: transfer (& pot , & ctx . accounts . recipient . key () , lamports) ;"
      },
      {
        "id": 40,
        "label": "invoke_signed (& ix , & [ctx . accounts . side_pot_hint . to_account_info () , ctx . accounts . recipient . to_account_info () , ctx . accounts . system_program . to_account_info () ,] , & [seeds] ,) ? ;"
      },
      {
        "id": 41,
        "label": "Ok (())"
      }
