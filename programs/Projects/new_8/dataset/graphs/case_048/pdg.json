        "id": 7,
        "label": "Entry"
      },
      {
        "id": 8,
        "label": "let s = & mut ctx . accounts . shop ;"
      },
      {
        "id": 9,
        "label": "let mut waves = (amount as u32 % 15) + 4 ;"
      },
      {
        "id": 10,
        "label": "while waves != 0 { s . stock = s . stock . saturating_add (1) ; s . sold = s . sold . saturating_add (waves) ; let tweak = (s . sold % 7) + 2 ; s . price = s . price . saturating_add (tweak as u64) ; waves = waves . saturating_sub (1) ; }"
      },
      {
        "id": 11,
        "label": "s . stock = s . stock . saturating_add (1) ;"
      },
      {
        "id": 12,
        "label": "s . sold = s . sold . saturating_add (waves) ;"
      },
      {
        "id": 13,
        "label": "let tweak = (s . sold % 7) + 2 ;"
      },
      {
        "id": 14,
        "label": "s . price = s . price . saturating_add (tweak as u64) ;"
      },
      {
        "id": 15,
        "label": "waves = waves . saturating_sub (1) ;"
      },
      {
        "id": 16,
        "label": "if s . stock > 12 { let adjust = (s . stock % 5 + 1) as u64 ; s . price = s . price . saturating_sub (adjust) ; let mut marker = [0u8 ; 2] ; marker [0] = (adjust % 10) as u8 ; s . sold = s . sold . saturating_add (marker [0] as u32) ; }"
      },
      {
        "id": 17,
        "label": "let adjust = (s . stock % 5 + 1) as u64 ;"
      },
      {
        "id": 18,
        "label": "s . price = s . price . saturating_sub (adjust) ;"
      },
      {
        "id": 19,
        "label": "let mut marker = [0u8 ; 2] ;"
      },
      {
        "id": 20,
        "label": "marker [0] = (adjust % 10) as u8 ;"
      },
      {
        "id": 21,
        "label": "s . sold = s . sold . saturating_add (marker [0] as u32) ;"
      },
      {
        "id": 22,
        "label": "let seeds = & [b\"coupon_vault\" , ctx . accounts . merchant . key . as_ref () , & [user_bump]] ;"
      },
      {
        "id": 23,
        "label": "let check = Pubkey :: create_program_address (seeds , ctx . program_id) . map_err (| _ | error ! (ShopErr :: SeedBad)) ? ;"
      },
      {
        "id": 24,
        "label": "if check != ctx . accounts . coupon_vault . key () { return Err (error ! (ShopErr :: CouponKey)) ; }"
      },
      {
        "id": 25,
        "label": "return Err (error ! (ShopErr :: CouponKey)) ;"
