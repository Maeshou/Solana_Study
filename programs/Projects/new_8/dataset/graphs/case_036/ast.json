[
  {
    "name": "init_workshop",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitWorkshop >",
      "capacity: u16"
    ],
    "attributes": [],
    "body": [
      "let ws = & mut ctx . accounts . workshop ;",
      "ws . curator = ctx . accounts . curator . key () ;",
      "ws . capacity = capacity % 64 + 8 ;",
      "ws . queue_len = capacity as u32 / 2 + 3 ;",
      "ws . tokens_issued = 11 ;",
      "if ws . queue_len < 2",
      "{",
      "ws . queue_len = 2 ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "print_token",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PrintToken >",
      "series: u16",
      "user_bump: u8"
    ],
    "attributes": [],
    "body": [
      "let ws = & mut ctx . accounts . workshop ;",
      "let seeds = & [b\"print_sink\" , ctx . accounts . curator . key . as_ref () , & [user_bump]] ;",
      "let expect = Pubkey :: create_program_address (seeds , ctx . program_id) . map_err (| _ | error ! (CraftErr :: SeedBad)) ? ;",
      "if expect != ctx . accounts . print_sink . key ()",
      "{",
      "return Err (error ! (CraftErr :: SinkMismatch)) ;",
      "}",
      "let mut cycles = series as u32 % 17 + 5 ;",
      "let mut wave = 2u32 ;",
      "while wave < cycles",
      "{",
      "ws . tokens_issued = ws . tokens_issued . saturating_add (wave) ;",
      "if ws . tokens_issued % 3 != 1",
      "{",
      "ws . queue_len = ws . queue_len . saturating_add (1) ;",
      "} wave = wave . saturating_add (4) ;",
      "}",
      "Ok (())"
    ]
  },
