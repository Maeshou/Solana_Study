  {
    "name": "initialize_license",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeLicense >"
    ],
    "attributes": [
      "# [doc = \" ライセンスを初期化\"]"
    ],
    "body": [
      "let lic = & mut ctx . accounts . license ;",
      "lic . issuer = ctx . accounts . issuer . key () ;",
      "lic . total_issued = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "issue_key",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < IssueKey >",
      "user: Pubkey"
    ],
    "attributes": [
      "# [doc = \" キーを発行\"]"
    ],
    "body": [
      "let lic = & mut ctx . accounts . license ;",
      "let key = & mut ctx . accounts . key_account ;",
      "key . user = user ;",
      "key . license_id = lic . key () ;",
      "key . activated = false ;",
      "lic . total_issued = lic . total_issued . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "activate_key",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ActivateKey >"
    ],
    "attributes": [
      "# [doc = \" キーを有効化\"]"
    ],
    "body": [
      "let key = & mut ctx . accounts . key_account ;",
      "key . activated = true ;",
      "Ok (())"
    ]
  }
]