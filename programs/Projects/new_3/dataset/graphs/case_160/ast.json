  {
    "name": "initialize_warehouse",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeWarehouse >",
      "capacity: u64"
    ],
    "attributes": [
      "# [doc = \" 倉庫を初期化\"]"
    ],
    "body": [
      "let wh = & mut ctx . accounts . warehouse ;",
      "wh . manager = ctx . accounts . manager . key () ;",
      "wh . capacity = capacity ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_stock",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddStock >",
      "item_id: u64",
      "qty: u64"
    ],
    "attributes": [
      "# [doc = \" 在庫を追加\"]"
    ],
    "body": [
      "let wh = & mut ctx . accounts . warehouse ;",
      "let se = & mut ctx . accounts . stock_entry ;",
      "se . item_id = item_id ;",
      "se . warehouse = wh . key () ;",
      "se . quantity = qty ;",
      "wh . capacity = wh . capacity . checked_sub (qty) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "remove_stock",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RemoveStock >",
      "qty: u64"
    ],
    "attributes": [
      "# [doc = \" 在庫を出庫\"]"
    ],
    "body": [
      "let wh = & mut ctx . accounts . warehouse ;",
      "let se = & mut ctx . accounts . stock_entry ;",
      "se . quantity = se . quantity . checked_sub (qty) . unwrap () ;",
      "wh . capacity = wh . capacity . checked_add (qty) . unwrap () ;",
      "Ok (())"
    ]
  }
]