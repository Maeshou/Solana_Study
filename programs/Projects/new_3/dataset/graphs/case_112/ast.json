[
  {
    "name": "list_for_rent",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ListForRent >",
      "price: u64"
    ],
    "attributes": [
      "# [doc = \" リスト登録時のみ has_one = owner で所有者チェックを行うが…\"]"
    ],
    "body": [
      "let listing = & mut ctx . accounts . listing ;",
      "listing . price = price ;",
      "listing . active = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "unlist",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Unlist >"
    ],
    "attributes": [
      "# [doc = \" has_one = owner がないため、誰でも任意のリストを取り下げ可能\"]"
    ],
    "body": [
      "let listing = & mut ctx . accounts . listing ;",
      "listing . active = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_price",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdatePrice >",
      "new_price: u64"
    ],
    "attributes": [
      "# [doc = \" 価格変更にも照合チェックがなく、誰でも任意のリスト価格を更新可能\"]"
    ],
    "body": [
      "let listing = & mut ctx . accounts . listing ;",
      "listing . price = new_price ;",
      "Ok (())"
    ]
  },
