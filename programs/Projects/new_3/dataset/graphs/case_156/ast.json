  {
    "name": "EndRental",
    "node_type": "struct",
    "fields": [
      {
        "name": "rent_contract",
        "attribute": "# [doc = \" RentContract.owner == provider.key() は検証される\"] # [account (mut , has_one = owner)]",
        "field_type": "Account < 'info , RentContract >"
      },
      {
        "name": "nft_asset",
