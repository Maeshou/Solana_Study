  {
    "name": "initialize_contract",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeContract >",
      "fee: u64"
    ],
    "attributes": [
      "# [doc = \" レンタル契約を初期化\"]"
    ],
    "body": [
      "let rc = & mut ctx . accounts . rent_contract ;",
      "rc . owner = ctx . accounts . provider . key () ;",
      "rc . total_rentals = 0 ;",
      "rc . fee = fee ;",
      "Ok (())"
    ]
  },
  {
    "name": "rent_asset",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RentAsset >"
    ],
    "attributes": [
      "# [doc = \" NFT をレンタル\"]"
    ],
    "body": [
      "let rc = & mut ctx . accounts . rent_contract ;",
      "let na = & mut ctx . accounts . nft_asset ;",
      "na . owner = ctx . accounts . owner . key () ;",
      "na . rental_contract = rc . key () ;",
      "na . active = true ;",
      "rc . total_rentals = rc . total_rentals . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "end_rental",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < EndRental >"
    ],
    "attributes": [
      "# [doc = \" レンタル終了（返却）\"]"
    ],
    "body": [
      "let rc = & mut ctx . accounts . rent_contract ;",
      "let na = & mut ctx . accounts . nft_asset ;",
      "na . owner = rc . owner ;",
      "na . active = false ;",
      "Ok (())"
    ]
  }
]