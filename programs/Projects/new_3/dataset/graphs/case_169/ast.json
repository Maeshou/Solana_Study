  {
    "name": "create_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateProfile >",
      "username: String"
    ],
    "attributes": [
      "# [doc = \" プロフィールを作成\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . profile ;",
      "p . owner = ctx . accounts . owner . key () ;",
      "p . username = username ;",
      "p . bio = String :: from (\"Welcome!\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "record_change",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RecordChange >",
      "note: String"
    ],
    "attributes": [
      "# [doc = \" 変更履歴を記録\"]"
    ],
    "body": [
      "let rec = & mut ctx . accounts . record ;",
      "rec . editor = ctx . accounts . owner . key () ;",
      "rec . profile = ctx . accounts . profile . key () ;",
      "rec . note = note ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_bio",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateBio >",
      "addition: String"
    ],
    "attributes": [
      "# [doc = \" プロフィールの自己紹介文（bio）を更新\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . profile ;",
      "let rec = & mut ctx . accounts . record ;",
      "p . bio . reserve (addition . len ()) ;",
      "p . bio . push_str (\" \") ;",
      "p . bio . push_str (& addition) ;",
      "rec . note = format ! (\"Appended to bio: {}\" , addition) ;",
      "Ok (())"
    ]
  }
]