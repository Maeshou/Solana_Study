  {
    "name": "create_posting",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreatePosting >",
      "title: String"
    ],
    "attributes": [
      "# [doc = \" 求人を作成\"]"
    ],
    "body": [
      "let jp = & mut ctx . accounts . posting ;",
      "jp . organizer = ctx . accounts . organizer . key () ;",
      "jp . title = title ;",
      "Ok (())"
    ]
  },
  {
    "name": "submit_application",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SubmitApplication >",
      "resume: String"
    ],
    "attributes": [
      "# [doc = \" 応募を提出\"]"
    ],
    "body": [
      "let jp = & mut ctx . accounts . posting ;",
      "let ar = & mut ctx . accounts . record ;",
      "ar . candidate = ctx . accounts . candidate . key () ;",
      "ar . posting = jp . key () ;",
      "ar . resume = resume ;",
      "jp . applicants . push (ar . candidate) ;",
      "Ok (())"
    ]
  },
  {
    "name": "withdraw_applications",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < WithdrawApplications >"
    ],
    "attributes": [
      "# [doc = \" 応募を取り下げ（一覧から除外）\"]"
    ],
    "body": [
      "let jp = & mut ctx . accounts . posting ;",
      "let ar = & ctx . accounts . record ;",
      "jp . applicants . retain (| & pk | pk != ar . candidate) ;",
      "Ok (())"
    ]
  }
