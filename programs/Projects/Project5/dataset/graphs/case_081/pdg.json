      {
        "id": 23,
        "label": "let t = & mut ctx . accounts . taster ;"
      },
      {
        "id": 24,
        "label": "for i in 0 .. cycles { let tri = ((i % 10) as i32 - 5) . abs () as u32 + 1 ; a . heat = ((a . heat as u64 * 7 + (tri + 2) as u64 * 3) / 10) as u32 ; b . heat = b . heat . saturating_add (tri / 2 + 1) ; t . ticks = t . ticks . saturating_add (1) ; t . check ^= ((a . heat ^ b . heat) as u64) << (i % 8) ; }"
      },
      {
        "id": 25,
        "label": "let tri = ((i % 10) as i32 - 5) . abs () as u32 + 1 ;"
      },
      {
        "id": 26,
        "label": "a . heat = ((a . heat as u64 * 7 + (tri + 2) as u64 * 3) / 10) as u32 ;"
      },
      {
        "id": 27,
        "label": "b . heat = b . heat . saturating_add (tri / 2 + 1) ;"
      },
      {
        "id": 28,
        "label": "t . ticks = t . ticks . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "t . check ^= ((a . heat ^ b . heat) as u64) << (i % 8) ;"
      },
      {
        "id": 30,
        "label": "let avg = (a . heat + b . heat) / 2 ;"
      },
      {
        "id": 31,
        "label": "if avg > h . target { h . state = Rest ; a . spout ^= 1 ; b . spout = b . spout . saturating_add (1) ; t . spout = t . spout . saturating_add (1) ; msg ! (\"rest: spout tweak & taster move\") ; } else { h . state = Steep ; a . heat = a . heat . saturating_add (7) ; b . heat = b . heat / 2 + 9 ; t . check ^= 0x0F0F_F0F0 ; msg ! (\"steep: adjust heat & check flip\") ; }"
      },
      {
        "id": 32,
        "label": "h . state = Rest ;"
      },
      {
        "id": 33,
        "label": "a . spout ^= 1 ;"
      },
      {
        "id": 34,
        "label": "b . spout = b . spout . saturating_add (1) ;"
      },
      {
        "id": 35,
        "label": "t . spout = t . spout . saturating_add (1) ;"
      },
      {
        "id": 36,
