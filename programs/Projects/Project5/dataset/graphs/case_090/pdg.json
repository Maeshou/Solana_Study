        "id": 9,
        "label": "a . season = Dormant ;"
      },
      {
        "id": 10,
        "label": "let prod = (water as u32) . saturating_mul (1000) . max (1) ;"
      },
      {
        "id": 11,
        "label": "let mut x = prod as u64 ;"
      },
      {
        "id": 12,
        "label": "let mut y = (x + 1) / 2 ;"
      },
      {
        "id": 13,
        "label": "while y < x { x = y ; y = (x + prod as u64 / x) / 2 ; }"
      },
      {
        "id": 14,
        "label": "x = y ;"
      },
      {
        "id": 15,
        "label": "y = (x + prod as u64 / x) / 2 ;"
      },
      {
        "id": 16,
        "label": "let base = x as u32 ;"
      },
      {
        "id": 17,
        "label": "let t1 = & mut ctx . accounts . tree_a ;"
      },
      {
        "id": 18,
        "label": "t1 . atelier = a . key () ;"
      },
      {
        "id": 19,
        "label": "t1 . twig = (water as u8) & 7 ;"
      },
      {
        "id": 20,
        "label": "t1 . vigor = base + 7 ;"
      },
      {
        "id": 21,
        "label": "let t2 = & mut ctx . accounts . tree_b ;"
      },
      {
        "id": 22,
        "label": "t2 . atelier = a . key () ;"
      },
      {
        "id": 23,
        "label": "t2 . twig = ((water >> 2) as u8) & 7 ;"
      },
      {
        "id": 24,
        "label": "t2 . vigor = base . rotate_left (3) + 11 ;"
      },
      {
        "id": 25,
        "label": "j . atelier = a . key () ;"
      },
      {
        "id": 26,
        "label": "Ok (())"
      }
