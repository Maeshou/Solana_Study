      {
        "id": 4,
        "label": "let mut x = isbn_like ;"
      },
      {
        "id": 5,
        "label": "while x > 0 { let d = (x % 10) as i32 ; ds = ds + d as u32 ; alt = alt + sign * d ; sign = - sign ; x /= 10 ; }"
      },
      {
        "id": 6,
        "label": "let d = (x % 10) as i32 ;"
      },
      {
        "id": 7,
        "label": "ds = ds + d as u32 ;"
      },
      {
        "id": 8,
        "label": "alt = alt + sign * d ;"
      },
      {
        "id": 9,
        "label": "sign = - sign ;"
      },
      {
        "id": 10,
        "label": "x /= 10 ;"
      },
      {
        "id": 11,
        "label": "let b = & mut ctx . accounts . bindery ;"
      },
      {
        "id": 12,
        "label": "b . owner = ctx . accounts . binder . key () ;"
      },
      {
        "id": 13,
        "label": "b . budget = ds . saturating_mul (10) + alt . unsigned_abs () ;"
      },
      {
        "id": 14,
        "label": "b . phase = Prep ;"
      },
      {
        "id": 15,
        "label": "let s = & mut ctx . accounts . stack_a ;"
      },
      {
        "id": 16,
        "label": "s . bindery = b . key () ;"
      },
      {
        "id": 17,
        "label": "s . tray = (ds as u8) & 7 ;"
      },
      {
        "id": 18,
        "label": "s . leaves = ds . saturating_mul (3) + 50 ;"
      },
      {
        "id": 19,
        "label": "let l = & mut ctx . accounts . ledger ;"
      },
      {
        "id": 20,
        "label": "l . bindery = b . key () ;"
      },
      {
        "id": 21,
        "label": "l . tray = 9 ;"
      },
      {
        "id": 22,
        "label": "l . count = 0 ;"
      },
      {
        "id": 23,
        "label": "l . sign = (alt . unsigned_abs () as u64) << 8 | ds as u64 ;"
      },
      {
        "id": 24,
        "label": "let t = & mut ctx . accounts . stack_b ;"
      },
      {
        "id": 25,
        "label": "t . bindery = b . key () ;"
      },
      {
        "id": 26,
        "label": "t . tray = ((ds >> 3) as u8) & 7 ;"
      },
      {
        "id": 27,
        "label": "t . leaves = (ds ^ (alt . unsigned_abs ())) + 60 ;"
      },
      {
        "id": 28,
        "label": "Ok (())"
      }
