        "label": "ctx . accounts . vote_data . count = 0 ;"
      },
      {
        "id": 20,
        "label": "ctx . accounts . settings . active = false ;"
      },
      {
        "id": 21,
        "label": "ctx . accounts . electorate_data . total_voters -= sum ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"Below threshold\") ;"
