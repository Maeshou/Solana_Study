        "label": "if sum > ctx . accounts . settings . threshold as u64 { ctx . accounts . electorate_data . total_voters += sum ; msg ! (\"Threshold exceeded\") ; ctx . accounts . vote_data . count = 0 ; ctx . accounts . settings . active = false ; } else { ctx . accounts . electorate_data . total_voters -= sum ; msg ! (\"Below threshold\") ; ctx . accounts . vote_data . count = sum as u32 ; ctx . accounts . settings . active = true ; }"
      },
      {
        "id": 17,
        "label": "ctx . accounts . electorate_data . total_voters += sum ;"
      },
      {
