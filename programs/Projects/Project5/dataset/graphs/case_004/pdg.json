        "label": "Entry"
      },
      {
        "id": 11,
        "label": "require ! (ctx . accounts . electorate_data . key () != ctx . accounts . vote_data . key () , ProgramError :: InvalidArgument) ;"
      },
      {
        "id": 12,
        "label": "let mut count = ctx . accounts . vote_data . count ;"
      },
      {
