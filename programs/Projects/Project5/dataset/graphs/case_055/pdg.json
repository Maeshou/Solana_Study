  {
    "name": "incubate",
    "inputs": [
      "ctx: Context < Incubate >",
      "days: u32"
    ],
    "nodes": [
      {
        "id": 9,
        "label": "Entry"
      },
      {
        "id": 10,
        "label": "let s = & mut ctx . accounts ;"
      },
      {
        "id": 11,
        "label": "for d in 0 .. days { let inc = 5 + (d % 4) ; s . egg_a . energy = s . egg_a . energy . saturating_add (inc * 3) ; s . egg_b . energy = s . egg_b . energy . saturating_add (inc * 2 + 1) ; s . journal . stamps = s . journal . stamps . wrapping_add (1) ; }"
      },
      {
        "id": 12,
        "label": "let inc = 5 + (d % 4) ;"
      },
      {
        "id": 13,
        "label": "s . egg_a . energy = s . egg_a . energy . saturating_add (inc * 3) ;"
      },
      {
        "id": 14,
        "label": "s . egg_b . energy = s . egg_b . energy . saturating_add (inc * 2 + 1) ;"
      },
      {
        "id": 15,
        "label": "s . journal . stamps = s . journal . stamps . wrapping_add (1) ;"
      },
      {
        "id": 16,
        "label": "let heat = (s . egg_a . energy + s . egg_b . energy) / 2 ;"
      },
      {
        "id": 17,
        "label": "if heat > s . hatchery . target_temp { s . hatchery . mode = Rest ; s . journal . alerts = s . journal . alerts . wrapping_add (2) ; s . egg_a . energy = s . egg_a . energy / 2 + 9 ; s . egg_b . energy = s . egg_b . energy / 2 + 7 ; msg ! (\"rest: alerts+2, energies damped\") ; } else { s . hatchery . mode = Active ; s . journal . notes = s . journal . notes . wrapping_add (3) ; s . egg_a . energy = s . egg_a . energy + 13 ; s . egg_b . energy = s . egg_b . energy + 11 ; msg ! (\"active: notes+3, energy boosts\") ; }"
      },
      {
        "id": 18,
        "label": "s . hatchery . mode = Rest ;"
      },
      {
        "id": 19,
        "label": "s . journal . alerts = s . journal . alerts . wrapping_add (2) ;"
      },
      {
        "id": 20,
        "label": "s . egg_a . energy = s . egg_a . energy / 2 + 9 ;"
      },
      {
        "id": 21,
        "label": "s . egg_b . energy = s . egg_b . energy / 2 + 7 ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"rest: alerts+2, energies damped\") ;"
      },
      {
        "id": 23,
        "label": "s . hatchery . mode = Active ;"
      },
      {
        "id": 24,
        "label": "s . journal . notes = s . journal . notes . wrapping_add (3) ;"
      },
      {
        "id": 25,
        "label": "s . egg_a . energy = s . egg_a . energy + 13 ;"
      },
      {
        "id": 26,
        "label": "s . egg_b . energy = s . egg_b . energy + 11 ;"
      },
      {
        "id": 27,
        "label": "msg ! (\"active: notes+3, energy boosts\") ;"
      },
      {
        "id": 28,
        "label": "Ok (())"
      }
