      {
        "id": 20,
        "label": "let st = & mut ctx . accounts . stage ;"
      },
      {
        "id": 21,
        "label": "let a = & mut ctx . accounts . chair_a ;"
      },
      {
        "id": 22,
        "label": "let b = & mut ctx . accounts . chair_b ;"
      },
      {
        "id": 23,
