  {
    "name": "record",
    "inputs": [
      "ctx: Context < RecordWave >",
      "loops: u32"
    ],
    "nodes": [
      {
        "id": 20,
        "label": "Entry"
      },
      {
        "id": 21,
        "label": "let st = & mut ctx . accounts . station ;"
      },
      {
        "id": 22,
        "label": "let a = & mut ctx . accounts . sensor_a ;"
      },
      {
        "id": 23,
        "label": "let b = & mut ctx . accounts . sensor_b ;"
      },
      {
        "id": 24,
        "label": "let tp = & mut ctx . accounts . tape ;"
      },
      {
        "id": 25,
        "label": "for i in 0 .. loops { let z = ((a . amplitude ^ b . amplitude) as u64) . wrapping_mul (1469598103934665603) ; a . amplitude = a . amplitude . checked_add (((z & 63) as u32) + 3) . unwrap_or (u32 :: MAX) ; b . amplitude = b . amplitude . saturating_add ((((z >> 6) & 63) as u32) + 5) ; tp . ticks = tp . ticks . saturating_add (1) ; tp . hash ^= z . rotate_left ((i % 19) as u32) ; }"
      },
      {
        "id": 26,
        "label": "let z = ((a . amplitude ^ b . amplitude) as u64) . wrapping_mul (1469598103934665603) ;"
      },
      {
        "id": 27,
        "label": "a . amplitude = a . amplitude . checked_add (((z & 63) as u32) + 3) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 28,
        "label": "b . amplitude = b . amplitude . saturating_add ((((z >> 6) & 63) as u32) + 5) ;"
      },
      {
        "id": 29,
        "label": "tp . ticks = tp . ticks . saturating_add (1) ;"
      },
      {
        "id": 30,
        "label": "tp . hash ^= z . rotate_left ((i % 19) as u32) ;"
      },
      {
        "id": 31,
        "label": "let sum = a . amplitude + b . amplitude ;"
      },
      {
        "id": 32,
        "label": "if sum > st . threshold { st . mode = Alarm ; a . channel ^= 1 ; b . channel = b . channel . saturating_add (1) ; tp . channel = tp . channel . saturating_add (1) ; msg ! (\"alarm: channel tweaks & tape move\") ; } else { st . mode = Alert ; a . amplitude = a . amplitude . saturating_add (9) ; b . amplitude = b . amplitude / 2 + 11 ; tp . hash ^= 0x0F0F_F0F0 ; msg ! (\"alert: amp adjust & hash flip\") ; }"
      },
      {
        "id": 33,
        "label": "st . mode = Alarm ;"
      },
      {
        "id": 34,
        "label": "a . channel ^= 1 ;"
      },
      {
        "id": 35,
        "label": "b . channel = b . channel . saturating_add (1) ;"
      },
      {
        "id": 36,
        "label": "tp . channel = tp . channel . saturating_add (1) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"alarm: channel tweaks & tape move\") ;"
      },
      {
        "id": 38,
        "label": "st . mode = Alert ;"
      },
      {
        "id": 39,
        "label": "a . amplitude = a . amplitude . saturating_add (9) ;"
      },
      {
        "id": 40,
        "label": "b . amplitude = b . amplitude / 2 + 11 ;"
      },
      {
        "id": 41,
        "label": "tp . hash ^= 0x0F0F_F0F0 ;"
      },
      {
        "id": 42,
        "label": "msg ! (\"alert: amp adjust & hash flip\") ;"
      },
      {
        "id": 43,
        "label": "Ok (())"
      }
