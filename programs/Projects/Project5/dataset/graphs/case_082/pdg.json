      },
      {
        "id": 8,
        "label": "let r = x . max (y) + y . min (x) / 2 ;"
      },
      {
        "id": 9,
        "label": "a . obs = o . key () ;"
      },
      {
        "id": 10,
        "label": "a . slot = (x & 7) as u8 ;"
      },
      {
        "id": 11,
        "label": "a . focus = r + 7 ;"
      },
      {
        "id": 12,
        "label": "b . obs = o . key () ;"
      },
      {
        "id": 13,
        "label": "b . slot = (y & 7) as u8 ;"
      },
      {
        "id": 14,
        "label": "b . focus = r . rotate_left (3) + 9 ;"
      },
      {
        "id": 15,
        "label": "lg . obs = o . key () ;"
      },
      {
        "id": 16,
        "label": "lg . slot = 9 ;"
      },
      {
        "id": 17,
        "label": "lg . count = 0 ;"
      },
      {
        "id": 18,
