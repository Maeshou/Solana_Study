  {
    "name": "record",
    "inputs": [
      "ctx: Context < Record >",
      "frames: u32"
    ],
    "nodes": [
      {
        "id": 12,
        "label": "Entry"
      },
      {
        "id": 13,
        "label": "let e = & mut ctx . accounts ;"
      },
      {
        "id": 14,
        "label": "for k in 0 .. frames { e . log . count = e . log . count . saturating_add (1) ; let cnt = e . log . count . max (1) ; let sample = (e . mic . level as u64) + (k as u64 * 5) ; let delta = sample as i128 - e . log . mean as i128 ; e . log . mean = (e . log . mean as i128 + delta / cnt as i128) . max (0) as u64 ; let step = if e . amp . gain < 100 { 3 } else { 1 } ; e . amp . gain = e . amp . gain . saturating_add (step) ; }"
      },
      {
        "id": 15,
        "label": "e . log . count = e . log . count . saturating_add (1) ;"
      },
      {
        "id": 16,
        "label": "let cnt = e . log . count . max (1) ;"
      },
      {
        "id": 17,
        "label": "let sample = (e . mic . level as u64) + (k as u64 * 5) ;"
      },
      {
        "id": 18,
        "label": "let delta = sample as i128 - e . log . mean as i128 ;"
      },
      {
        "id": 19,
        "label": "e . log . mean = (e . log . mean as i128 + delta / cnt as i128) . max (0) as u64 ;"
      },
      {
        "id": 20,
        "label": "let step = if e . amp . gain < 100 { 3 } else { 1 } ;"
      },
      {
        "id": 21,
        "label": "3"
      },
      {
        "id": 22,
        "label": "1"
      },
      {
        "id": 23,
        "label": "e . amp . gain = e . amp . gain . saturating_add (step) ;"
      },
      {
        "id": 24,
        "label": "if e . log . mean > e . room . bound as u64 { e . room . state = Cooling ; e . log . flags = e . log . flags . saturating_add (2) ; e . amp . gain = e . amp . gain / 2 + 6 ; e . mic . level = e . mic . level / 2 + 4 ; msg ! (\"cooling: flags+2, damp mic/amp\") ; } else { e . room . state = Recording ; e . log . count = e . log . count . saturating_add (3) ; e . mic . level = e . mic . level . saturating_add (7) ; e . amp . gain = e . amp . gain . saturating_add (5) ; msg ! (\"recording: count+3, mic+7, amp+5\") ; }"
      },
      {
        "id": 25,
        "label": "e . room . state = Cooling ;"
      },
      {
        "id": 26,
        "label": "e . log . flags = e . log . flags . saturating_add (2) ;"
      },
      {
        "id": 27,
        "label": "e . amp . gain = e . amp . gain / 2 + 6 ;"
      },
      {
        "id": 28,
        "label": "e . mic . level = e . mic . level / 2 + 4 ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"cooling: flags+2, damp mic/amp\") ;"
      },
      {
        "id": 30,
        "label": "e . room . state = Recording ;"
      },
      {
        "id": 31,
        "label": "e . log . count = e . log . count . saturating_add (3) ;"
      },
      {
        "id": 32,
        "label": "e . mic . level = e . mic . level . saturating_add (7) ;"
      },
      {
        "id": 33,
        "label": "e . amp . gain = e . amp . gain . saturating_add (5) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"recording: count+3, mic+7, amp+5\") ;"
      },
      {
        "id": 35,
        "label": "Ok (())"
      }
