      },
      {
        "id": 16,
        "label": "let sum = a . relic_a . score as u64 + a . relic_b . score as u64 ;"
      },
      {
        "id": 17,
        "label": "if sum > a . museum . cap as u64 { a . museum . state = Seal ; a . log . flags = a . log . flags . wrapping_add (2) ; a . relic_a . score = a . relic_a . score / 2 + 11 ; a . relic_b . score = a . relic_b . score / 2 + 13 ; msg ! (\"sealed: flags+2, both scores halved+adj\") ; } else { a . museum . state = Open ; a . log . checks = a . log . checks . wrapping_add (1) ; a . relic_a . score = a . relic_a . score . rotate_left (1) ; a . relic_b . score = a . relic_b . score . rotate_right (1) ; msg ! (\"open: checks+1, rotate scores\") ; }"
      },
      {
        "id": 18,
        "label": "a . museum . state = Seal ;"
      },
      {
        "id": 19,
        "label": "a . log . flags = a . log . flags . wrapping_add (2) ;"
      },
      {
        "id": 20,
        "label": "a . relic_a . score = a . relic_a . score / 2 + 11 ;"
      },
      {
        "id": 21,
        "label": "a . relic_b . score = a . relic_b . score / 2 + 13 ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"sealed: flags+2, both scores halved+adj\") ;"
      },
      {
        "id": 23,
        "label": "a . museum . state = Open ;"
      },
