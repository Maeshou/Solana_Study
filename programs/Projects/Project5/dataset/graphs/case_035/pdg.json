  {
    "name": "irrigate",
    "inputs": [
      "ctx: Context < Irrigate >",
      "days: u32",
      "rate_q16: u32"
    ],
    "nodes": [
      {
        "id": 6,
        "label": "Entry"
      },
      {
        "id": 7,
        "label": "let o = & mut ctx . accounts ;"
      },
      {
        "id": 8,
        "label": "let rate = rate_q16 . max (1) ;"
      },
      {
        "id": 9,
        "label": "for _ in 0 .. days { let g = (u128 :: from (o . tree . growth_q16) * u128 :: from (rate)) >> 16 ; o . tree . growth_q16 = (u128 :: from (o . tree . growth_q16) + g) . min (u128 :: from (u64 :: MAX)) as u64 ; o . tree . color = o . tree . color . rotate_left (3) ^ (o . tree . color >> 5) ; }"
      },
      {
        "id": 10,
        "label": "let g = (u128 :: from (o . tree . growth_q16) * u128 :: from (rate)) >> 16 ;"
      },
      {
        "id": 11,
        "label": "o . tree . growth_q16 = (u128 :: from (o . tree . growth_q16) + g) . min (u128 :: from (u64 :: MAX)) as u64 ;"
      },
      {
        "id": 12,
        "label": "o . tree . color = o . tree . color . rotate_left (3) ^ (o . tree . color >> 5) ;"
      },
      {
        "id": 13,
        "label": "let growth = (o . tree . growth_q16 >> 16) as u32 ;"
      },
      {
        "id": 14,
        "label": "if growth > o . cfg . cap { o . cfg . state = OrchardState :: Prune ; o . journal . cuts = o . journal . cuts . wrapping_add (2) ; o . tree . growth_q16 = u64 :: from (o . cfg . cap) << 16 ; msg ! (\"prune: clip growth to cap, cuts+2\") ; } else { o . cfg . state = OrchardState :: Bloom ; o . journal . notes = o . journal . notes . wrapping_add (1) ; o . tree . color ^= 0x0F0F ; msg ! (\"bloom: notes+1, color xor\") ; }"
      },
      {
        "id": 15,
        "label": "o . cfg . state = OrchardState :: Prune ;"
      },
      {
        "id": 16,
        "label": "o . journal . cuts = o . journal . cuts . wrapping_add (2) ;"
      },
      {
        "id": 17,
        "label": "o . tree . growth_q16 = u64 :: from (o . cfg . cap) << 16 ;"
      },
      {
        "id": 18,
        "label": "msg ! (\"prune: clip growth to cap, cuts+2\") ;"
      },
      {
        "id": 19,
        "label": "o . cfg . state = OrchardState :: Bloom ;"
      },
      {
        "id": 20,
        "label": "o . journal . notes = o . journal . notes . wrapping_add (1) ;"
      },
      {
        "id": 21,
