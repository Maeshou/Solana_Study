    "nodes": [
      {
        "id": 6,
        "label": "Entry"
      },
      {
        "id": 7,
        "label": "let f = & mut ctx . accounts ;"
      },
      {
        "id": 8,
        "label": "assert_ne ! (f . cfg . key () , f . log . key () , \"cfg/log must differ\") ;"
      },
      {
        "id": 9,
        "label": "for _ in 0 .. ticks { let next = (u128 :: from (f . field . a) + u128 :: from (f . field . b)) & ((1u128 << 40) - 1) ; f . field . a = f . field . b ; f . field . b = next as u64 ; let x = f . field . b as u32 ; let v = x ^ (x >> 1) ^ (x >> 2) ; f . log . events = f . log . events . wrapping_add ((v & 0xFF) as u32) ; }"
      },
      {
        "id": 10,
        "label": "let next = (u128 :: from (f . field . a) + u128 :: from (f . field . b)) & ((1u128 << 40) - 1) ;"
      },
      {
        "id": 11,
        "label": "f . field . a = f . field . b ;"
      },
      {
        "id": 12,
        "label": "f . field . b = next as u64 ;"
      },
      {
        "id": 13,
        "label": "let x = f . field . b as u32 ;"
      },
      {
        "id": 14,
        "label": "let v = x ^ (x >> 1) ^ (x >> 2) ;"
      },
      {
        "id": 15,
        "label": "f . log . events = f . log . events . wrapping_add ((v & 0xFF) as u32) ;"
      },
      {
        "id": 16,
        "label": "if f . field . b > f . cfg . cap { f . cfg . state = FarmState :: Cap ; f . field . b = f . cfg . cap ; f . log . badges = f . log . badges . wrapping_add (2) ; msg ! (\"cap reached: clip b to cap, badges+2\") ; } else { f . cfg . state = FarmState :: Grow ; f . field . a = f . field . a + (f . field . b / 3) ; f . log . events = f . log . events . wrapping_add (1) ; msg ! (\"grow: a += b/3, events+1\") ; }"
      },
      {
        "id": 17,
        "label": "f . cfg . state = FarmState :: Cap ;"
      },
      {
        "id": 18,
        "label": "f . field . b = f . cfg . cap ;"
      },
      {
        "id": 19,
        "label": "f . log . badges = f . log . badges . wrapping_add (2) ;"
      },
      {
        "id": 20,
        "label": "msg ! (\"cap reached: clip b to cap, badges+2\") ;"
      },
      {
        "id": 21,
        "label": "f . cfg . state = FarmState :: Grow ;"
      },
      {
        "id": 22,
        "label": "f . field . a = f . field . a + (f . field . b / 3) ;"
      },
      {
        "id": 23,
        "label": "f . log . events = f . log . events . wrapping_add (1) ;"
      },
      {
        "id": 24,
        "label": "msg ! (\"grow: a += b/3, events+1\") ;"
      },
      {
        "id": 25,
        "label": "Ok (())"
      }
