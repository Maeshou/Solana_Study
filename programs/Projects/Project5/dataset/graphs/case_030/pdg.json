  {
    "name": "mix",
    "inputs": [
      "ctx: Context < Mix >",
      "ticks: u32"
    ],
    "nodes": [
      {
        "id": 6,
        "label": "Entry"
      },
      {
        "id": 7,
        "label": "let a = & mut ctx . accounts ;"
      },
      {
        "id": 8,
        "label": "assert_ne ! (a . bus . key () , a . track_a . key () , \"bus/track_a must differ\") ;"
      },
      {
        "id": 9,
        "label": "assert_ne ! (a . bus . key () , a . track_b . key () , \"bus/track_b must differ\") ;"
      },
      {
        "id": 10,
        "label": "for _ in 0 .. ticks { a . track_a . gain = a . track_a . gain . saturating_add (5) ; a . track_b . gain = a . track_b . gain . saturating_add (7) ; a . bus . sum = a . bus . sum . saturating_add (9) ; }"
      },
      {
        "id": 11,
        "label": "a . track_a . gain = a . track_a . gain . saturating_add (5) ;"
      },
      {
        "id": 12,
        "label": "a . track_b . gain = a . track_b . gain . saturating_add (7) ;"
      },
      {
        "id": 13,
        "label": "a . bus . sum = a . bus . sum . saturating_add (9) ;"
      },
      {
        "id": 14,
        "label": "if a . bus . sum > a . studio . ceiling as u64 { a . studio . mode = MixMode :: Master ; a . bus . peaks = a . bus . peaks . saturating_add (3) ; a . track_b . flags = a . track_b . flags . saturating_add (2) ; msg ! (\"ceiling exceeded; mastering, peaks+3 flagsB+2\") ; } else { a . studio . mode = MixMode :: Live ; a . bus . peaks = a . bus . peaks . saturating_add (1) ; a . track_a . flags = a . track_a . flags . saturating_add (1) ; msg ! (\"live mixing; peaks+1 flagsA+1\") ; }"
      },
      {
        "id": 15,
        "label": "a . studio . mode = MixMode :: Master ;"
      },
      {
        "id": 16,
        "label": "a . bus . peaks = a . bus . peaks . saturating_add (3) ;"
      },
      {
        "id": 17,
        "label": "a . track_b . flags = a . track_b . flags . saturating_add (2) ;"
      },
      {
        "id": 18,
        "label": "msg ! (\"ceiling exceeded; mastering, peaks+3 flagsB+2\") ;"
      },
      {
        "id": 19,
        "label": "a . studio . mode = MixMode :: Live ;"
      },
      {
        "id": 20,
        "label": "a . bus . peaks = a . bus . peaks . saturating_add (1) ;"
      },
      {
        "id": 21,
        "label": "a . track_a . flags = a . track_a . flags . saturating_add (1) ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"live mixing; peaks+1 flagsA+1\") ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
    ],
