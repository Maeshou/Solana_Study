      },
      {
        "id": 9,
        "label": "v1 . plant = p . key () ;"
      },
      {
        "id": 10,
        "label": "v1 . gate = (minerals & 7) as u8 ;"
      },
      {
        "id": 11,
        "label": "v1 . flow = ((r * 300) / 97) as u32 + 20 ;"
      },
      {
        "id": 12,
        "label": "v2 . plant = p . key () ;"
      },
      {
        "id": 13,
        "label": "v2 . gate = ((minerals >> 2) & 7) as u8 ;"
      },
      {
        "id": 14,
        "label": "v2 . flow = ((r * 450) / 97) as u32 + 25 ;"
      },
      {
        "id": 15,
