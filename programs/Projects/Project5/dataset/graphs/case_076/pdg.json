        "id": 35,
        "label": "q . tag ^= r . rotate_left ((i % 23) as u32) ;"
      },
      {
        "id": 36,
        "label": "let sum = a . load + b . load ;"
      },
      {
        "id": 37,
        "label": "if sum > d . maxq { d . state = Freeze ; a . dock ^= 1 ; b . dock = b . dock . saturating_add (1) ; q . bay = q . bay . saturating_add (1) ; msg ! (\"freeze: bay++ & dock tweaks\") ; } else { d . state = Service ; a . load = a . load . saturating_add (9) ; b . load = b . load / 2 + 11 ; q . tag ^= 0x00FF_F0F0 ; msg ! (\"service: adjust load & tag flip\") ; }"
      },
