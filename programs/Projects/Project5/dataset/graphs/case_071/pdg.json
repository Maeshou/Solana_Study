      },
      {
        "id": 24,
        "label": "for i in 0 .. steps { let tri = ((i % 10) as i32 - 5) . abs () as u32 ; a . heat = a . heat . checked_add (tri + 1) . unwrap_or (u32 :: MAX) ; b . heat = b . heat . saturating_add ((tri / 2) + 1) ; t . mix ^= ((a . heat ^ b . heat) as u64) << (i % 8) ; }"
      },
      {
        "id": 25,
        "label": "let tri = ((i % 10) as i32 - 5) . abs () as u32 ;"
      },
