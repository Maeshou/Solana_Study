        "label": "b . heat = b . heat . saturating_add ((tri / 2) + 1) ;"
      },
      {
        "id": 28,
        "label": "t . mix ^= ((a . heat ^ b . heat) as u64) << (i % 8) ;"
      },
      {
        "id": 29,
        "label": "let avg = ((a . heat + b . heat) / 2) as u32 ;"
      },
      {
        "id": 30,
        "label": "if avg > (k . flag & 0x0FFF) { k . state = Close ; a . station ^= 0x1 ; b . station = b . station . saturating_add (1) ; t . queue = t . queue . saturating_add ((avg as u64) & 63) ; msg ! (\"close: station flip & queue+\") ; } else { k . state = Serve ; k . flag ^= 0x00FF ; a . heat = a . heat . saturating_add (7) ; b . heat = b . heat / 2 + 9 ; msg ! (\"serve: heat adjust & flag flip\") ; }"
      },
