  {
    "name": "mix",
    "inputs": [
      "ctx: Context < Mix >",
      "steps: u32"
    ],
    "nodes": [
      {
        "id": 12,
        "label": "Entry"
      },
      {
        "id": 13,
        "label": "let s = & mut ctx . accounts ;"
      },
      {
        "id": 14,
        "label": "for i in 0 .. steps { s . panel_a . hue = s . panel_a . hue . checked_add (3 + (i % 5)) . unwrap_or (u32 :: MAX) ; s . panel_b . hue = s . panel_b . hue . checked_add (5 + (i % 7)) . unwrap_or (u32 :: MAX) ; let delta = ((s . panel_a . hue as u64 + s . panel_b . hue as u64) / 16) as u32 ; s . log . strokes = s . log . strokes . saturating_add (delta) ; }"
      },
      {
        "id": 15,
        "label": "s . panel_a . hue = s . panel_a . hue . checked_add (3 + (i % 5)) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 16,
        "label": "s . panel_b . hue = s . panel_b . hue . checked_add (5 + (i % 7)) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 17,
        "label": "let delta = ((s . panel_a . hue as u64 + s . panel_b . hue as u64) / 16) as u32 ;"
      },
      {
        "id": 18,
        "label": "s . log . strokes = s . log . strokes . saturating_add (delta) ;"
      },
      {
        "id": 19,
        "label": "let total = s . panel_a . hue as u64 + s . panel_b . hue as u64 ;"
      },
      {
        "id": 20,
        "label": "if total > s . studio . limit as u64 { s . studio . mode = Seal ; s . log . flags = s . log . flags . saturating_add (2) ; s . panel_a . hue = s . panel_a . hue / 2 + 7 ; s . panel_b . hue = s . panel_b . hue / 2 + 9 ; msg ! (\"sealed: flags+2, both panels damped\") ; } else { s . studio . mode = Blend ; s . log . strokes = s . log . strokes . saturating_add (11) ; s . panel_a . hue ^= 0x00FF_00FF ; s . panel_b . hue = s . panel_b . hue . saturating_add (13) ; msg ! (\"blend: strokes+11, hue xor/boost\") ; }"
      },
      {
        "id": 21,
        "label": "s . studio . mode = Seal ;"
      },
      {
        "id": 22,
        "label": "s . log . flags = s . log . flags . saturating_add (2) ;"
      },
      {
        "id": 23,
        "label": "s . panel_a . hue = s . panel_a . hue / 2 + 7 ;"
      },
      {
        "id": 24,
        "label": "s . panel_b . hue = s . panel_b . hue / 2 + 9 ;"
      },
      {
        "id": 25,
        "label": "msg ! (\"sealed: flags+2, both panels damped\") ;"
      },
      {
        "id": 26,
        "label": "s . studio . mode = Blend ;"
      },
      {
        "id": 27,
        "label": "s . log . strokes = s . log . strokes . saturating_add (11) ;"
      },
      {
        "id": 28,
        "label": "s . panel_a . hue ^= 0x00FF_00FF ;"
      },
      {
        "id": 29,
        "label": "s . panel_b . hue = s . panel_b . hue . saturating_add (13) ;"
      },
      {
        "id": 30,
        "label": "msg ! (\"blend: strokes+11, hue xor/boost\") ;"
      },
      {
        "id": 31,
        "label": "Ok (())"
      }
