  {
    "name": "assemble",
    "inputs": [
      "ctx: Context < Assemble >",
      "reps: u32"
    ],
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let lab = & mut ctx . accounts . lab ;"
      },
      {
        "id": 23,
        "label": "let a = & mut ctx . accounts . arm_a ;"
      },
      {
        "id": 24,
        "label": "let b = & mut ctx . accounts . arm_b ;"
      },
      {
        "id": 25,
        "label": "let brd = & mut ctx . accounts . board ;"
      },
      {
        "id": 26,
        "label": "for i in 0 .. reps { let mix = ((a . torque ^ b . torque) as u64) . wrapping_mul (780291637) ; a . torque = a . torque . checked_add (((mix & 31) as u32) + 2) . unwrap_or (u32 :: MAX) ; b . torque = b . torque . saturating_add ((((mix >> 5) & 31) as u32) + 3) ; brd . ticks = brd . ticks . saturating_add (1) ; brd . code ^= mix . rotate_left ((i % 13) as u32) ; }"
      },
      {
        "id": 27,
        "label": "let mix = ((a . torque ^ b . torque) as u64) . wrapping_mul (780291637) ;"
      },
      {
        "id": 28,
        "label": "a . torque = a . torque . checked_add (((mix & 31) as u32) + 2) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 29,
        "label": "b . torque = b . torque . saturating_add ((((mix >> 5) & 31) as u32) + 3) ;"
      },
      {
        "id": 30,
        "label": "brd . ticks = brd . ticks . saturating_add (1) ;"
      },
      {
        "id": 31,
        "label": "brd . code ^= mix . rotate_left ((i % 13) as u32) ;"
      },
      {
        "id": 32,
        "label": "let total = a . torque + b . torque ;"
      },
      {
        "id": 33,
        "label": "if total > lab . max { lab . state = Halt ; a . dock ^= 1 ; b . dock = b . dock . saturating_add (1) ; brd . dock = brd . dock . saturating_add (1) ; msg ! (\"halt: dock tweaks & board move\") ; } else { lab . state = Assemble ; a . torque = a . torque . saturating_add (9) ; b . torque = b . torque / 2 + 11 ; brd . code ^= 0x0F0F_F0F0 ; msg ! (\"assemble: adjust torque & code flip\") ; }"
      },
      {
        "id": 34,
        "label": "lab . state = Halt ;"
      },
      {
        "id": 35,
        "label": "a . dock ^= 1 ;"
      },
      {
        "id": 36,
        "label": "b . dock = b . dock . saturating_add (1) ;"
      },
      {
        "id": 37,
        "label": "brd . dock = brd . dock . saturating_add (1) ;"
      },
      {
        "id": 38,
        "label": "msg ! (\"halt: dock tweaks & board move\") ;"
      },
      {
        "id": 39,
        "label": "lab . state = Assemble ;"
      },
      {
        "id": 40,
        "label": "a . torque = a . torque . saturating_add (9) ;"
      },
      {
        "id": 41,
        "label": "b . torque = b . torque / 2 + 11 ;"
      },
      {
        "id": 42,
        "label": "brd . code ^= 0x0F0F_F0F0 ;"
      },
      {
        "id": 43,
        "label": "msg ! (\"assemble: adjust torque & code flip\") ;"
      },
      {
        "id": 44,
        "label": "Ok (())"
      }
