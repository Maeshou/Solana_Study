      {
        "id": 1,
        "label": "let lab = & mut ctx . accounts . lab ;"
      },
      {
        "id": 2,
        "label": "lab . owner = ctx . accounts . chief . key () ;"
      },
      {
        "id": 3,
        "label": "lab . max = (a11 + a22) * 8 + 200 ;"
      },
