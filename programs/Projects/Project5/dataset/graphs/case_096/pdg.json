        "id": 37,
        "label": "let lg = & mut ctx . accounts . log ;"
      },
      {
        "id": 38,
        "label": "for i in 0 .. n { let z = ((a . mass ^ b . mass) as u64) . wrapping_mul (1469598103934665603) ; a . mass = a . mass . checked_add (((z & 63) as u32) + 3) . unwrap_or (u32 :: MAX) ; b . mass = b . mass . saturating_add ((((z >> 6) & 63) as u32) + 5) ; lg . steps = lg . steps . saturating_add (1) ; lg . sig ^= z . rotate_left ((i % 19) as u32) ; }"
      },
      {
        "id": 39,
        "label": "let z = ((a . mass ^ b . mass) as u64) . wrapping_mul (1469598103934665603) ;"
      },
      {
        "id": 40,
        "label": "a . mass = a . mass . checked_add (((z & 63) as u32) + 3) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 41,
        "label": "b . mass = b . mass . saturating_add ((((z >> 6) & 63) as u32) + 5) ;"
      },
      {
        "id": 42,
        "label": "lg . steps = lg . steps . saturating_add (1) ;"
      },
      {
        "id": 43,
        "label": "lg . sig ^= z . rotate_left ((i % 19) as u32) ;"
      },
      {
        "id": 44,
        "label": "let sum = a . mass + b . mass ;"
      },
      {
        "id": 45,
        "label": "if sum > s . limit { s . phase = Retreat ; a . band ^= 1 ; b . band = b . band . saturating_add (1) ; lg . band = lg . band . saturating_add (1) ; msg ! (\"retreat: band tweaks & log move\") ; } else { s . phase = Sample ; a . mass = a . mass . saturating_add (9) ; b . mass = b . mass / 2 + 11 ; lg . sig ^= 0x0F0F_F0F0 ; msg ! (\"sample: mass adjust & sig flip\") ; }"
      },
      {
        "id": 46,
