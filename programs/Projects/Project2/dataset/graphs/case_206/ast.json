    "body": [
      "let h = & mut ctx . accounts . health ;",
      "h . max_hp = max_hp ;",
      "h . current_hp = max_hp ;",
      "h . regen_rate = 5 ;",
      "Ok (())"
    ]
  },
  {
    "name": "apply_damage",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyHealth >",
      "dmg: u64"
    ],
    "attributes": [],
    "body": [
      "let h = & mut ctx . accounts . health ;",
      "h . current_hp = h . current_hp . saturating_sub (dmg) ;",
      "Ok (())"
    ]
  },
  {
    "name": "apply_heal",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyHealth >",
      "heal: u64"
    ],
    "attributes": [],
    "body": [
      "let h = & mut ctx . accounts . health ;",
      "h . current_hp = (h . current_hp + heal) . min (h . max_hp) ;",
      "Ok (())"
    ]
  },
