[
  {
    "name": "init_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitProfile >",
      "name: String",
      "bio: String"
    ],
    "attributes": [
      "# [doc = \" プロファイルの作成\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . profile ;",
      "p . creator = ctx . accounts . user . key () ;",
      "p . name = name ;",
      "p . bio = bio ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_bio",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateBio >",
      "new_bio: String"
    ],
    "attributes": [
      "# [doc = \" 本人のみバイオを上書き\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . profile ;",
      "require_keys_eq ! (p . creator , ctx . accounts . user . key () , ProfileError :: NoAuth) ;",
      "p . bio = new_bio ;",
      "Ok (())"
    ]
  },
