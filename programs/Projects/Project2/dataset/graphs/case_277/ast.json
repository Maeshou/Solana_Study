[
  {
    "name": "load_codes",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < LoadCampaign >",
      "codes: Vec < (String , u32) >"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . data ;",
      "for (c , n) in codes",
      "{",
      "d . remaining . insert (c , n) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "use_campaign",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UseCampaign >",
      "code: String"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . data ;",
      "if let Some (r) = d . remaining . get_mut (& code)",
      "{",
      "* r = r . saturating_sub (1) ;",
      "d . used . push (code . clone ()) ;",
      "}",
      "Ok (())"
    ]
  },
