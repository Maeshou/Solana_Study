    "inputs": [
      "ctx: Context < InitQueue >"
    ],
    "attributes": [],
    "body": [
      "let q = & mut ctx . accounts . queue ;",
      "q . head = None ;",
      "Ok (())"
    ]
  },
  {
    "name": "enqueue",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyQueue >",
      "player: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let q = & mut ctx . accounts . queue ;",
      "let node = Node { key : player , next : q . head } ;",
      "q . head = Some (Box :: into_inner (Box :: new (node))) ;",
      "Ok (())"
    ]
  },
  {
    "name": "dequeue",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyQueue >"
    ],
    "attributes": [],
    "body": [
      "let q = & mut ctx . accounts . queue ;",
      "if let Some (mut first) = q . head . take ()",
      "{",
      "q . head = first . next . take () . map (Box :: into_inner) ;",
      "return Ok (Some (first . key)) ;",
      "}",
      "Ok (None)"
    ]
  },
