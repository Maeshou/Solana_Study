[
  {
    "name": "apply",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyHealth >",
      "dmg: u64"
    ],
    "attributes": [],
    "body": [
      "let h = & mut ctx . accounts . health ;",
      "if h . current_hp > dmg",
      "{",
      "h . current_hp -= dmg ;",
      "h . damage_taken = h . damage_taken . saturating_add (dmg) ;",
      "} else",
      "{",
      "h . current_hp = h . max_hp / 2 ;",
      "if h . res_tokens > 0",
      "{",
      "h . res_tokens -= 1 ;",
      "h . revived = true ;",
      "} else",
      "{",
      "h . dead = true ;",
      "}",
      "}",
      "Ok (())"
    ]
  },
