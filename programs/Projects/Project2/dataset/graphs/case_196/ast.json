  {
    "name": "PomodoroTracker",
    "node_type": "struct",
    "fields": [
      {
        "name": "",
        "attribute": null,
        "field_type": "u8"
      },
      {
        "name": "",
        "attribute": null,
        "field_type": "Vec < (i64 , u64) >"
      }
    ],
    "inputs": null,
    "attributes": null,
    "body": null
  },
  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Initialize >"
    ],
    "attributes": [
      "# [doc = \" 初期化：内部 Vec は空、bump のみ設定\"]"
    ],
    "body": [
      "let b = * ctx . bumps . get (\"tracker\") . unwrap () ;",
      "ctx . accounts . tracker . 0 = b ;",
      "Ok (())"
    ]
  },
  {
    "name": "start_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >"
    ],
    "attributes": [
      "# [doc = \" Pomodoro 開始：件数制限チェック＋開始時刻のみ追加（duration は 0 で初期化）\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . tracker . 1 ;",
      "if list . len () >= MAX_SESSIONS",
      "{",
      "return err ! (ErrorCode :: MaxSessionsReached) ;",
      "}",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "list . push ((now , 0)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "complete_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "duration_secs: u64"
    ],
    "attributes": [
      "# [doc = \" Pomodoro 完了：最後のセッションの duration を設定\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . tracker . 1 ;",
      "if let Some (last) = list . last_mut ()",
      "{",
      "last . 1 = duration_secs ;",
      "Ok (()) } else",
      "{",
      "err ! (ErrorCode :: NoActiveSession)",
      "}"
    ]
  },
  {
    "name": "purge_old",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "cutoff: i64"
    ],
    "attributes": [
      "# [doc = \" 古いセッション削除：start_ts が cutoff より前のものを除去\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . tracker . 1 ;",
      "list . retain (| & (start , _) | { if start < cutoff { false } else { true } }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "count_completed",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >"
    ],
    "attributes": [
      "# [doc = \" 完了済みセッション数をログ出力\"]"
    ],
    "body": [
      "let list = & ctx . accounts . tracker . 1 ;",
      "let mut cnt = 0u64 ;",
      "for & (_ , duration) in list . iter ()",
      "{",
      "if duration > 0",
      "{",
      "cnt = cnt . wrapping_add (1) ;",
      "}",
      "}",
      "msg ! (\"Completed sessions: {}\" , cnt) ;",
      "Ok (())"
    ]
  },
