    "name": "discover",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Discover >",
      "index: u8"
    ],
    "attributes": [],
    "body": [
      "let th = & mut ctx . accounts . hunt ;",
      "if (index as usize) < th . found . len ()",
      "{",
      "th . found [index as usize] = true ;",
      "th . total_found = th . total_found . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
