[
  {
    "name": "init",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitDur >",
      "max: u8"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . dur ;",
      "d . max = max ;",
      "d . current = max ;",
      "d . repairs_required = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "use_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UseDur >",
      "amount: u8"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . dur ;",
      "if d . current >= amount",
      "{",
      "d . current = d . current . saturating_sub (amount) ;",
      "d . times_used = d . times_used . saturating_add (1) ;",
      "} else",
      "{",
      "d . broken = true ;",
      "d . repairs_required = d . repairs_required . saturating_add (1) ;",
      "d . over_damage = d . over_damage . saturating_add (amount - d . current) ;",
      "d . current = 0 ;",
      "}",
      "Ok (())"
    ]
  },
