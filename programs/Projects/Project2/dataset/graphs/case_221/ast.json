[
  {
    "name": "check",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CheckWin >",
      "slot: u64"
    ],
    "attributes": [],
    "body": [
      "let w = & mut ctx . accounts . win ;",
      "if slot >= w . start && slot <= w . end",
      "{",
      "w . access_count = w . access_count . saturating_add (1) ;",
      "Ok (true) } else",
      "{",
      "w . denial_count = w . denial_count . saturating_add (1) ;",
      "w . last_denied = slot ;",
      "Ok (false)",
      "}"
    ]
  },
