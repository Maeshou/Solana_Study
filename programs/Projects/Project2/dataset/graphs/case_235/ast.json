[
  {
    "name": "init_chain",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitChain >",
      "ids: [u64 ; 5]"
    ],
    "attributes": [],
    "body": [
      "let qc = & mut ctx . accounts . chain ;",
      "qc . chain = ids ;",
      "qc . index = 0 ;",
      "qc . completions = [0u8 ; 5] ;",
      "Ok (())"
    ]
  },
  {
    "name": "complete_current",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyChain >"
    ],
    "attributes": [],
    "body": [
      "let qc = & mut ctx . accounts . chain ;",
      "let idx = qc . index as usize ;",
      "qc . completions [idx] = 1 ;",
      "qc . index = (qc . index + 1) . min (4) ;",
      "Ok (())"
    ]
  },
