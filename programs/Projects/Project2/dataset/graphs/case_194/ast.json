  {
    "name": "StudySessionManager",
    "node_type": "struct",
    "fields": [
      {
        "name": "",
        "attribute": null,
        "field_type": "u8"
      },
      {
        "name": "",
        "attribute": null,
        "field_type": "Vec < (u8 , u64) >"
      }
    ],
    "inputs": null,
    "attributes": null,
    "body": null
  },
  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Initialize >"
    ],
    "attributes": [
      "# [doc = \" 初期化：内部 Vec は空、bump のみ設定\"]"
    ],
    "body": [
      "let b = * ctx . bumps . get (\"manager\") . unwrap () ;",
      "ctx . accounts . manager . 0 = b ;",
      "Ok (())"
    ]
  },
  {
    "name": "start_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "subject_id: u8"
    ],
    "attributes": [
      "# [doc = \" セッション開始：件数制限チェック＋初期 0 で追加\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "if list . len () >= MAX_SESSIONS",
      "{",
      "return err ! (ErrorCode :: MaxSessionsReached) ;",
      "}",
      "list . push ((subject_id , 0)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_time",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "subject_id: u8",
      "secs: u64"
    ],
    "attributes": [
      "# [doc = \" 時間更新：既存セッション検索＋加算\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "let mut found = false ;",
      "for entry in list . iter_mut ()",
      "{",
      "if entry . 0 == subject_id",
      "{",
      "entry . 1 = entry . 1 . wrapping_add (secs) ;",
      "found = true ;",
      "}",
      "}",
      "if found == false",
      "{",
      "return err ! (ErrorCode :: SessionNotFound) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "end_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "subject_id: u8"
    ],
    "attributes": [
      "# [doc = \" セッション削除：該当 subject_id を一括除去\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "list . retain (| & (sid , _) | { if sid == subject_id { false } else { true } }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "count_sessions",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >"
    ],
    "attributes": [
      "# [doc = \" 現在のセッション数をログ出力\"]"
    ],
    "body": [
      "let cnt = ctx . accounts . manager . 1 . len () as u64 ;",
      "msg ! (\"Active study sessions: {}\" , cnt) ;",
      "Ok (())"
    ]
  },
