[
  {
    "name": "insert_component",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyComp >",
      "idx: u8",
      "comp_id: u64"
    ],
    "attributes": [],
    "body": [
      "let uc = & mut ctx . accounts . upgrade ;",
      "if (idx as usize) < uc . components . len () && uc . components [idx as usize] . is_none ()",
      "{",
      "uc . components [idx as usize] = Some (comp_id) ;",
      "uc . insert_count = uc . insert_count . saturating_add (1) ;",
      "} else",
      "{",
      "uc . replace_count = uc . replace_count . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "clear_components",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyComp >"
    ],
    "attributes": [],
    "body": [
      "let uc = & mut ctx . accounts . upgrade ;",
      "for slot in uc . components . iter_mut ()",
      "{",
      "* slot = None ;",
      "}",
      "uc . insert_count = 0 ;",
      "uc . replace_count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "ModifyComp",
    "node_type": "struct",
    "fields": [
      {
        "name": "upgrade",
        "attribute": "# [account (mut)]",
        "field_type": "Account < 'info , UpgradeData >"
      }
    ],
