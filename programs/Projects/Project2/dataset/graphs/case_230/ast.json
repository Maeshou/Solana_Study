[
  {
    "name": "unlock",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Unlock >",
      "skill_id: u8"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . skills ;",
      "if s . prereq_met . get (& skill_id) == Some (& true)",
      "{",
      "s . unlocked . insert (skill_id , true) ;",
      "s . unlock_count = s . unlock_count . saturating_add (1) ;",
      "} else",
      "{",
      "s . attempts = s . attempts . saturating_add (1) ;",
      "s . locked_attempts . entry (skill_id) . and_modify (| c | * c += 1) . or_insert (1) ;",
      "}",
      "Ok (())"
    ]
  },
