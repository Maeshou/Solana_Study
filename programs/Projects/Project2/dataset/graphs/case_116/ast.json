[
  {
    "name": "create_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateSession >",
      "timestamp: i64",
      "duration_minutes: u16",
      "note: String"
    ],
    "attributes": [
      "# [doc = \" 新規セッション記録\"]"
    ],
    "body": [
      "if duration_minutes == 0",
      "{",
      "return Err (ErrorCode :: InvalidDuration . into ()) ;",
      "}",
      "if note . len () > 128",
      "{",
      "return Err (ErrorCode :: NoteTooLong . into ()) ;",
      "}",
      "let sess = & mut ctx . accounts . session ;",
      "sess . owner = ctx . accounts . user . key () ;",
      "sess . timestamp = timestamp ;",
      "sess . duration_minutes = duration_minutes ;",
      "sess . note = note ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateSession >",
      "new_timestamp: i64",
      "new_duration_minutes: u16",
      "new_note: String"
    ],
    "attributes": [
      "# [doc = \" 既存セッションを更新\"]"
    ],
    "body": [
      "if new_duration_minutes == 0",
      "{",
      "return Err (ErrorCode :: InvalidDuration . into ()) ;",
      "}",
      "if new_note . len () > 128",
      "{",
      "return Err (ErrorCode :: NoteTooLong . into ()) ;",
      "}",
      "let sess = & mut ctx . accounts . session ;",
      "if sess . owner != ctx . accounts . user . key ()",
      "{",
      "return Err (ErrorCode :: Unauthorized . into ()) ;",
      "}",
      "sess . timestamp = new_timestamp ;",
      "sess . duration_minutes = new_duration_minutes ;",
      "sess . note = new_note ;",
      "Ok (())"
    ]
  },
  {
    "name": "CreateSession",
    "node_type": "struct",
    "fields": [
      {
        "name": "session",
        "attribute": "# [doc = \" init 制約で同一アカウント再初期化防止（Reinit Attack）\"] # [account (init , payer = user , space = 8 + 32 + 8 + 2 + 4 + 128)]",
        "field_type": "Account < 'info , Session >"
      },
      {
        "name": "user",
