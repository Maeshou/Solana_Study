[
  {
    "name": "enter",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < EnterDungeon >",
      "health: u64",
      "threshold: u64"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . dungeon ;",
      "if health >= threshold",
      "{",
      "d . clear_count = d . clear_count . saturating_add (1) ;",
      "let loot = threshold / 10 ;",
      "d . loot_map . insert (ctx . accounts . player . key () , loot) ;",
      "} else",
      "{",
      "d . fail_count = d . fail_count . saturating_add (1) ;",
      "d . locked = true ;",
      "d . penalty_map . entry (ctx . accounts . player . key ()) . and_modify (| c | * c += threshold - health) . or_insert (threshold - health) ;",
      "}",
      "Ok (())"
    ]
  },
