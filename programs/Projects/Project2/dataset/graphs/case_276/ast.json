[
  {
    "name": "claim",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClaimVest >",
      "epoch: u8"
    ],
    "attributes": [],
    "body": [
      "let v = & mut ctx . accounts . vesting . load_mut () ? ;",
      "if epoch < v . epochs . len () as u8 && ! v . claimed [epoch as usize]",
      "{",
      "v . claimed [epoch as usize] = true ;",
      "v . claim_count = v . claim_count . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
