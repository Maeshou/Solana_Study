  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Initialize >"
    ],
    "attributes": [
      "# [doc = \" 初期化：内部 Vec は空、bump のみ設定\"]"
    ],
    "body": [
      "let b = * ctx . bumps . get (\"manager\") . unwrap () ;",
      "ctx . accounts . manager . 0 = b ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_key",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "api_key: Vec < u8 >"
    ],
    "attributes": [
      "# [doc = \" API キー追加：件数制限チェック＋初期利用 0 で追加\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "if list . len () >= MAX_KEYS",
      "{",
      "return err ! (ErrorCode :: MaxKeysReached) ;",
      "}",
      "list . push ((api_key , 0)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "record_usage",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "api_key: Vec < u8 >"
    ],
    "attributes": [
      "# [doc = \" 利用記録：該当キーを探索し、カウントを加算\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "let mut found = false ;",
      "for entry in list . iter_mut ()",
      "{",
      "if entry . 0 == api_key",
      "{",
      "entry . 1 = entry . 1 . wrapping_add (1) ;",
      "found = true ;",
      "}",
      "}",
      "if found == false",
      "{",
      "return err ! (ErrorCode :: KeyNotFound) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "remove_key",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >",
      "api_key: Vec < u8 >"
    ],
    "attributes": [
      "# [doc = \" API キー削除：該当キーを一括除去\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "list . retain (| (k , _) | { if * k == api_key { false } else { true } }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "purge_unused",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >"
    ],
    "attributes": [
      "# [doc = \" 利用回数が 0 のキーを一括削除\"]"
    ],
    "body": [
      "let list = & mut ctx . accounts . manager . 1 ;",
      "list . retain (| & (_ , count) | { if count > 0 { true } else { false } }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "total_usage",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Modify >"
    ],
    "attributes": [
      "# [doc = \" 総利用回数をログ出力\"]"
    ],
    "body": [
      "let list = & ctx . accounts . manager . 1 ;",
      "let mut sum = 0u64 ;",
      "for & (_ , count) in list . iter ()",
      "{",
      "sum = sum . wrapping_add (count) ;",
      "}",
      "msg ! (\"Total API usage: {}\" , sum) ;",
      "Ok (())"
    ]
  },
