    "body": [
      "let factor = 100u64 . saturating_sub ((level as u64) . saturating_mul (2)) ;",
      "let price = base . saturating_mul (factor) . checked_div (100) . unwrap_or (base) ;",
      "Ok (Discounted { price })"
    ]
