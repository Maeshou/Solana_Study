[
  {
    "name": "add_tag",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyTag >",
      "tag: String"
    ],
    "attributes": [
      "# [doc = \" タグを追加\"]"
    ],
    "body": [
      "let ts = & mut ctx . accounts . tags ;",
      "ts . list . push (tag) ;",
      "Ok (())"
    ]
  },
  {
    "name": "remove_tag",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyTag >",
      "tag: String"
    ],
    "attributes": [
      "# [doc = \" 指定タグを削除\"]"
    ],
    "body": [
      "let ts = & mut ctx . accounts . tags ;",
      "ts . list . retain (| t | t != & tag) ;",
      "Ok (())"
    ]
  },
