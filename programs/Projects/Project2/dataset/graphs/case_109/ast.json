[
  {
    "name": "initialize_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeProfile >",
      "display_name: String"
    ],
    "attributes": [
      "# [doc = \" プロファイル作成：`owner` を署名者に固定\"]"
    ],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "profile . owner = ctx . accounts . user . key () ;",
      "profile . display_name = display_name ;",
      "profile . created_at = ctx . accounts . clock . unix_timestamp ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_name",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateName >",
      "new_name: String"
    ],
    "attributes": [
      "# [doc = \" 表示名更新：`has_one = user`＋`Signer` でオーナーのみ許可\"]"
    ],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "profile . display_name = new_name ;",
      "profile . updated_at = ctx . accounts . clock . unix_timestamp ;",
      "Ok (())"
    ]
  },
