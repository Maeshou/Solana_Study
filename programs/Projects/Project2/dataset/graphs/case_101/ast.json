  {
    "name": "record_temperature",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyLog >",
      "temp_x100: u64"
    ],
    "attributes": [],
    "body": [
      "let list = & mut ctx . accounts . log . 1 ;",
      "if list . len () >= MAX_ENTRIES",
      "{",
      "return err ! (ErrorCode :: MaxEntriesReached) ;",
      "}",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "list . push ((now , temp_x100)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "purge_old",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyLog >",
      "age_limit_secs: i64"
    ],
    "attributes": [],
    "body": [
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "ctx . accounts . log . 1 . retain (| & (ts , _) | now - ts <= age_limit_secs) ;",
      "Ok (())"
    ]
  },
