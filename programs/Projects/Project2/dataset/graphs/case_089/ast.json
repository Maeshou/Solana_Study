  {
    "name": "update_config",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateCfg >",
      "new_limit: u32"
    ],
    "attributes": [
      "# [doc = \" 設定の反映：ロード後に更新\"]"
    ],
    "body": [
      "let mut cfg = ctx . accounts . loader . load_mut () ? ;",
      "cfg . limit = new_limit ;",
      "emit ! (ConfigUpdated { new_limit }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "view_config",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ViewCfg >"
    ],
    "attributes": [
      "# [doc = \" 設定の参照：読み取り専用\"]"
    ],
    "body": [
      "let cfg = ctx . accounts . loader . load () ? ;",
      "Ok (CfgView { limit : cfg . limit })"
    ]
  },
