[
  {
    "name": "claim",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClaimDaily >",
      "slot: u64"
    ],
    "attributes": [
      "# [doc = \" ログイン報酬受取\"]"
    ],
    "body": [
      "let d = & mut ctx . accounts . daily ;",
      "if slot > d . last_slot",
      "{",
      "d . last_slot = slot ;",
      "let idx = (d . count % d . rewards . len () as u64) as usize ;",
      "d . count = d . count . saturating_add (1) ;",
      "Ok (d . rewards [idx]) } else",
      "{",
      "Ok (0)",
      "}"
    ]
  },
  {
    "name": "ClaimDaily",
    "node_type": "struct",
    "fields": [
      {
        "name": "daily",
