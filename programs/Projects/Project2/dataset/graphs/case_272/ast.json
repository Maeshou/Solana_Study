  {
    "name": "spin",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyWheel >",
      "reward: u64"
    ],
    "attributes": [],
    "body": [
      "let w = & mut ctx . accounts . wheel ;",
      "w . history . push (reward) ;",
      "w . spin_count = w . spin_count . saturating_add (1) ;",
      "if w . history . len () as u8 > 5",
      "{",
      "w . history . remove (0) ;",
      "}",
      "Ok (())"
    ]
  },
