[
  {
    "name": "feed",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Feed >",
      "food: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pet ;",
      "p . hunger = p . hunger . saturating_sub (food) ;",
      "if p . hunger == 0",
      "{",
      "p . full_meals = p . full_meals . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "starve",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Feed >",
      "amount: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pet ;",
      "p . hunger = (p . hunger + amount) . min (p . max_hunger) ;",
      "p . starve_events = p . starve_events . saturating_add (1) ;",
      "Ok (())"
    ]
  },
