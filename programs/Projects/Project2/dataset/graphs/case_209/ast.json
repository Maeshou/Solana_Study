[
  {
    "name": "init",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCraft >",
      "limit: u32"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . craft ;",
      "c . used = 0 ;",
      "c . limit = limit ;",
      "Ok (())"
    ]
  },
  {
    "name": "craft",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyCraft >"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . craft ;",
      "if c . used < c . limit",
      "{",
      "c . used = c . used . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "reset",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyCraft >"
    ],
    "attributes": [],
    "body": [
      "ctx . accounts . craft . used = 0 ;",
      "Ok (())"
    ]
  },
