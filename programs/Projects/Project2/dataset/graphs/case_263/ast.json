[
  {
    "name": "mark_participation",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyFlags >",
      "event_id: u8"
    ],
    "attributes": [
      "# [doc = \" イベント参加フラグを設定\"]"
    ],
    "body": [
      "let f = & mut ctx . accounts . flags ;",
      "let mask = 1u32 . checked_shl (event_id as u32) . unwrap_or (0) ;",
      "f . participation |= mask ;",
      "f . counts [event_id as usize] = f . counts [event_id as usize] . saturating_add (1) ;",
      "Ok (())"
    ]
  },
  {
    "name": "reset_flags",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyFlags >"
    ],
    "attributes": [
      "# [doc = \" フラグリセット\"]"
    ],
    "body": [
      "let f = & mut ctx . accounts . flags ;",
      "f . participation = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "ModifyFlags",
    "node_type": "struct",
    "fields": [
      {
        "name": "flags",
