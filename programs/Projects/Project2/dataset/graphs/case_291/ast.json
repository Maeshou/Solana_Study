    "body": [
      "let e = & mut ctx . accounts . data ;",
      "e . last_winner = winner ;",
      "e . total_wins = e . total_wins . saturating_add (1) ;",
      "e . history [e . index as usize] = winner ;",
      "e . index = (e . index + 1) % (e . history . len () as u8) ;",
      "Ok (())"
