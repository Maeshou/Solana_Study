[
  {
    "name": "create_bookmark",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateBookmark >",
      "url: String",
      "title: String"
    ],
    "attributes": [
      "# [doc = \" ブックマーク作成\"]"
    ],
    "body": [
      "require ! (url . len () <= 128 , ErrorCode :: UrlTooLong) ;",
      "require ! (title . len () <= 64 , ErrorCode :: TitleTooLong) ;",
      "let bm = & mut ctx . accounts . bookmark ;",
      "bm . owner = ctx . accounts . user . key () ;",
      "bm . url = url ;",
      "bm . title = title ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_bookmark",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateBookmark >",
      "new_url: String",
      "new_title: String"
    ],
    "attributes": [
      "# [doc = \" ブックマーク更新\"]"
    ],
    "body": [
      "let bm = & mut ctx . accounts . bookmark ;",
      "require ! (bm . owner == ctx . accounts . user . key () , ErrorCode :: Unauthorized) ;",
      "require ! (new_url . len () <= 128 , ErrorCode :: UrlTooLong) ;",
      "require ! (new_title . len () <= 64 , ErrorCode :: TitleTooLong) ;",
      "bm . url = new_url ;",
      "bm . title = new_title ;",
      "Ok (())"
    ]
  },
  {
    "name": "CreateBookmark",
    "node_type": "struct",
    "fields": [
      {
        "name": "bookmark",
        "attribute": "# [doc = \" init 制約で同一アカウント再初期化防止（Reinit Attack）\"] # [account (init , payer = user , space = 8 + 32 + 4 + 128 + 4 + 64)]",
        "field_type": "Account < 'info , Bookmark >"
      },
