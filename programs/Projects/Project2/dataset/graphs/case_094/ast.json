    "fields": null,
    "inputs": [
      "ctx: Context < ModifyCounter >"
    ],
    "attributes": [
      "# [doc = \" 呼び出し者の回数をインクリメント\"]"
    ],
    "body": [
      "let ctr = & mut ctx . accounts . counter ;",
      "let key = ctx . accounts . user . key () ;",
      "let entry = ctr . map . entry (key) . or_insert (0) ;",
      "* entry = entry . saturating_add (1) ;",
      "Ok (())"
    ]
  },
  {
    "name": "get_counts",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ViewCounter >"
    ],
    "attributes": [
      "# [doc = \" 現在のマップを返す\"]"
    ],
    "body": [
      "let ctr = & ctx . accounts . counter ;",
      "Ok (CountView { map : ctr . map . clone () })"
    ]
  },
