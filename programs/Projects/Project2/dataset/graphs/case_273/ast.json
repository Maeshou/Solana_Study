[
  {
    "name": "update_rep",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyRep >",
      "faction: String",
      "delta: i64"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . rep ;",
      "let entry = r . map . entry (faction . clone ()) . or_insert (0) ;",
      "* entry = (* entry) . saturating_add (delta) ;",
      "r . history . push ((faction , * entry)) ;",
      "if r . history . len () > 100",
      "{",
      "r . history . remove (0) ;",
      "}",
      "Ok (())"
    ]
  },
