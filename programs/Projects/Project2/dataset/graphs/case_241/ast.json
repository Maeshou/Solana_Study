    "name": "update_score",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateScore >",
      "score: u64"
    ],
    "attributes": [],
    "body": [
      "let lb = & mut ctx . accounts . board ;",
      "lb . scores . push ((ctx . accounts . user . key () , score)) ;",
      "lb . scores . sort_by (| a , b | b . 1 . cmp (& a . 1)) ;",
      "if lb . scores . len () > 10",
      "{",
      "lb . scores . pop () ;",
      "}",
      "Ok (())"
    ]
  },
