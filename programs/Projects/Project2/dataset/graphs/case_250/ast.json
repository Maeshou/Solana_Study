[
  {
    "name": "bid",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Bid >",
      "offer: u64",
      "slot: u64"
    ],
    "attributes": [],
    "body": [
      "let a = & mut ctx . accounts . auction ;",
      "a . bids . insert (ctx . accounts . user . key () , offer) ;",
      "a . bid_count = a . bids . len () as u64 ;",
      "if slot < a . start_slot || slot > a . end_slot",
      "{",
      "a . out_of_window = a . out_of_window . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
