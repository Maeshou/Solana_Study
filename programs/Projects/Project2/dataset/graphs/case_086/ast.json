    "name": "record_access",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Record >",
      "info: String"
    ],
    "attributes": [
      "# [doc = \" ログ記録：呼出者と時刻を履歴に追加\"]"
    ],
    "body": [
      "let rec = & mut ctx . accounts . log ;",
      "let clock = Clock :: get () ? ;",
      "rec . entries . push (AccessEntry { user : ctx . accounts . user . key () , timestamp : clock . unix_timestamp , note : info . clone () , }) ;",
      "emit ! (AccessRecorded { user : ctx . accounts . user . key () , timestamp : clock . unix_timestamp , note : info , }) ;",
      "Ok (())"
    ]
  },
