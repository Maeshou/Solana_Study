[
  {
    "name": "initialize_event",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeEvent >",
      "event_id: u64",
      "name: String",
      "total_seats: u64",
      "duration_secs: i64"
    ],
    "attributes": [
      "# [doc = \" イベント作成：名前・総座席数・継続時間を受け取り、主要フィールドだけセット\"]"
    ],
    "body": [
      "let ev = & mut ctx . accounts . event ;",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "* ev = EventReservation { owner : ctx . accounts . organizer . key () , bump : * ctx . bumps . get (\"event\") . unwrap () , event_id , name , total_seats , end_ts : now + duration_secs , last_action_ts : now , .. Default :: default () } ;",
      "Ok (())"
    ]
  },
  {
    "name": "reserve_seat",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyEvent >"
    ],
    "attributes": [
      "# [doc = \" 座席予約：期限切れ判定＋空き席があれば reservation を反映\"]"
    ],
    "body": [
      "let ev = & mut ctx . accounts . event ;",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "if now >= ev . end_ts",
      "{",
      "ev . closed = true ;",
      "}",
      "if ! ev . closed && ev . available_seats > 0",
      "{",
      "ev . available_seats = ev . available_seats . wrapping_sub (1) ;",
      "ev . reserved_count = ev . reserved_count . wrapping_add (1) ;",
      "}",
      "ev . last_action_ts = now ;",
      "Ok (())"
    ]
  },
  {
    "name": "cancel_reservation",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyEvent >"
    ],
    "attributes": [
      "# [doc = \" 予約キャンセル：既存予約があれば取消し\"]"
    ],
    "body": [
      "let ev = & mut ctx . accounts . event ;",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "if ev . reserved_count > 0",
      "{",
      "ev . reserved_count = ev . reserved_count . wrapping_sub (1) ;",
      "ev . available_seats = ev . available_seats . wrapping_add (1) ;",
      "}",
      "ev . last_action_ts = now ;",
      "Ok (())"
    ]
  },
  {
    "name": "finalize_event",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < FinalizeEvent >"
    ],
    "attributes": [
      "# [doc = \" イベント終了処理：期限到来で閉鎖\"]"
    ],
    "body": [
      "let ev = & mut ctx . accounts . event ;",
      "let now = ctx . accounts . clock . unix_timestamp ;",
      "if ! ev . closed && now >= ev . end_ts",
      "{",
      "ev . closed = true ;",
      "ev . last_action_ts = now ;",
      "}",
      "Ok (())"
    ]
  },
