[
  {
    "name": "claim",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClaimLand >",
      "x: u8",
      "y: u8"
    ],
    "attributes": [],
    "body": [
      "let grid = & mut ctx . accounts . grid ;",
      "if (x as usize) < 10 && (y as usize) < 10",
      "{",
      "grid . cells [(y as usize)] [(x as usize)] = Some (ctx . accounts . user . key ()) ;",
      "grid . claims = grid . claims . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "reset",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClaimLand >"
    ],
    "attributes": [],
    "body": [
      "let grid = & mut ctx . accounts . grid ;",
      "for row in grid . cells . iter_mut ()",
      "{",
      "for cell in row . iter_mut ()",
      "{",
      "* cell = None ;",
      "}",
      "}",
      "grid . claims = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "ClaimLand",
    "node_type": "struct",
    "fields": [
      {
        "name": "grid",
        "attribute": "# [account (mut)]",
        "field_type": "Account < 'info , LandGrid >"
      },
      {
        "name": "user",
        "attribute": null,
        "field_type": "Signer < 'info >"
      }
    ],
    "inputs": null,
