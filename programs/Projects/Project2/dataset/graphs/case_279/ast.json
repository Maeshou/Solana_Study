[
  {
    "name": "submit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SubmitBid >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . board ;",
      "b . bids . push ((ctx . accounts . user . key () , amount)) ;",
      "b . bids . sort_by (| a , b | b . 1 . cmp (& a . 1)) ;",
      "if b . bids . len () > b . capacity as usize",
      "{",
      "b . bids . pop () ;",
      "}",
      "Ok (())"
    ]
  },
