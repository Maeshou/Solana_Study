[
  {
    "name": "try_quest",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < TryQuest >",
      "day_index: u64"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . daily ;",
      "let cnt = d . attempts . entry (day_index) . or_insert (0) ;",
      "if * cnt < d . max_per_day",
      "{",
      "* cnt = cnt . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
