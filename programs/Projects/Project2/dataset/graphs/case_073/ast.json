[
  {
    "name": "init_pda",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPda >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let account = & mut ctx . accounts . pda_data ;",
      "account . count = amount ;",
      "account . bump = * ctx . bumps . get (\"pda_data\") . unwrap () ;",
      "emit ! (PdaInitialized { creator : ctx . accounts . user . key () , amount }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "increment_pda",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyPda >"
    ],
    "attributes": [],
    "body": [
      "let account = & mut ctx . accounts . pda_data ;",
      "account . count = account . count . checked_add (1) . unwrap () ;",
      "emit ! (PdaIncremented { new_count : account . count }) ;",
      "Ok (())"
    ]
  },
