[
  {
    "name": "attempt",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Attempt >",
      "success: bool"
    ],
    "attributes": [],
    "body": [
      "let q = & mut ctx . accounts . quest ;",
      "if success",
      "{",
      "q . completed = true ;",
      "q . reward_given = true ;",
      "q . success_count = q . success_count . saturating_add (1) ;",
      "} else",
      "{",
      "q . retries = q . retries . saturating_add (1) ;",
      "if q . retries > 3",
      "{",
      "q . locked = true ;",
      "} q . last_error = 1 ;",
      "}",
      "Ok (())"
    ]
  },
