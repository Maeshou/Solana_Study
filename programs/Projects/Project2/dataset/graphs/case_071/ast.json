[
  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Initialize >",
      "data: u64"
    ],
    "attributes": [],
    "body": [
      "let rec = & mut ctx . accounts . rec ;",
      "rec . value = data ;",
      "rec . creator = ctx . accounts . user . key () ;",
      "rec . count = 0 ;",
      "emit ! (DataInitialized { initializer : ctx . accounts . user . key () , data }) ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_data",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateData >",
      "new_data: u64"
    ],
    "attributes": [],
    "body": [
      "let rec = & mut ctx . accounts . rec ;",
      "require_keys_eq ! (rec . creator , ctx . accounts . user . key ()) ;",
      "rec . value = new_data ;",
      "rec . count = rec . count . checked_add (1) . unwrap () ;",
      "emit ! (DataUpdated { updater : ctx . accounts . user . key () , new_data , times : rec . count }) ;",
      "Ok (())"
    ]
  },
