[
  {
    "name": "distribute",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Distribute >"
    ],
    "attributes": [
      "# [doc = \" 分配計算\"]"
    ],
    "body": [
      "let t = & mut ctx . accounts . treasury ;",
      "for (& member , & contrib) in t . contributions . iter ()",
      "{",
      "let share = t . total_funds * contrib / t . total_contrib ;",
      "t . shares . insert (member , share) ;",
      "}",
      "t . distributed = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "Distribute",
    "node_type": "struct",
    "fields": [
