  {
    "name": "fuse",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Fuse >",
      "parent_a: u64",
      "parent_b: u64",
      "result: u64"
    ],
    "attributes": [],
    "body": [
      "let data = & mut ctx . accounts . history ;",
      "let key = (parent_a . min (parent_b) , parent_a . max (parent_b)) ;",
      "data . successes . insert (key , result) ;",
      "* data . fusion_count . entry (key) . or_insert (0) += 1 ;",
      "Ok (())"
    ]
  },
