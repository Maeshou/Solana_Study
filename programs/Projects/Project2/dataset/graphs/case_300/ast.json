[
  {
    "name": "set_member",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetMember >",
      "idx: u8",
      "member: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let team = & mut ctx . accounts . team ;",
      "if (idx as usize) < team . members . len ()",
      "{",
      "team . members [idx as usize] = Some (member) ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "clear_team",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetMember >"
    ],
    "attributes": [],
    "body": [
      "let team = & mut ctx . accounts . team ;",
      "for slot in team . members . iter_mut ()",
      "{",
      "* slot = None ;",
      "}",
      "Ok (())"
    ]
  },
