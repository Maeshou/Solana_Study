    "name": "upgrade",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Upgrade >",
      "comp: u8"
    ],
    "attributes": [],
    "body": [
      "let u = & mut ctx . accounts . upgrade ;",
      "if (comp as usize) < u . levels . len ()",
      "{",
      "u . levels [comp as usize] = u . levels [comp as usize] . saturating_add (1) ;",
      "u . count = u . count . saturating_add (1) ;",
      "} else",
      "{",
      "u . errors = u . errors . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
