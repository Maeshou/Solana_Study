[
  {
    "name": "init_box",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitBox >",
      "prizes: [u64 ; 5]"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . mbox ;",
      "b . prizes = prizes ;",
      "b . next_index = 0 ;",
      "b . opens = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "open_box",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OpenBox >"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . mbox ;",
      "if (b . next_index as usize) < b . prizes . len ()",
      "{",
      "let prize = b . prizes [b . next_index as usize] ;",
      "b . next_index += 1 ;",
      "b . opens = b . opens . saturating_add (1) ;",
      "Ok (prize) } else",
      "{",
      "b . empty_opens = b . empty_opens . saturating_add (1) ;",
      "Ok (0)",
      "}"
    ]
  },
