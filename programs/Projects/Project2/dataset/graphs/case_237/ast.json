  {
    "name": "count_combo",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CountCombo >",
      "hits: [bool ; 10]"
    ],
    "attributes": [],
    "body": [
      "let mut combo = 0u64 ;",
      "for & h in hits . iter ()",
      "{",
      "combo = if h",
      "{",
      "combo + 1 } else { combo } ;",
      "}",
      "Ok (ComboResult { combo })"
    ]
  },
