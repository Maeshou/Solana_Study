[
  {
    "name": "init_price",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPrice >",
      "floor: u64",
      "ceiling: u64"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . price ;",
      "p . floor = floor ;",
      "p . ceiling = ceiling ;",
      "p . current = (floor + ceiling) / 2 ;",
      "Ok (())"
    ]
  },
  {
    "name": "adjust",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyPrice >",
      "delta: i64"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . price ;",
      "let new = (p . current as i64) . saturating_add (delta) ;",
      "p . current = new . clamp (p . floor as i64 , p . ceiling as i64) as u64 ;",
      "Ok (())"
    ]
  },
