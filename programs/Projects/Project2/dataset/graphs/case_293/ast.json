    "body": [
      "let tracker = & mut ctx . accounts . tracker ;",
      "tracker . achievements . entry (nft_id) . and_modify (| v | if ! v . contains (& name) { v . push (name . clone ()) }) . or_insert_with (| | vec ! [name . clone ()]) ;",
      "Ok (())"
    ]
  },
