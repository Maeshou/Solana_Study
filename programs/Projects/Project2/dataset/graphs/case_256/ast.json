[
  {
    "name": "record_trade",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RecordTrade >",
      "sale: u64"
    ],
    "attributes": [],
    "body": [
      "let m = & mut ctx . accounts . market ;",
      "let fee = sale / 100 * m . fee_bps as u64 ;",
      "m . accumulated = m . accumulated . saturating_add (fee) ;",
      "m . trade_count = m . trade_count . saturating_add (1) ;",
      "Ok (())"
    ]
  },
  {
    "name": "adjust_fee",
    "node_type": "function",
    "fields": null,
