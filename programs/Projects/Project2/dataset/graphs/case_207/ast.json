  {
    "name": "init",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitMap >"
    ],
    "attributes": [],
    "body": [
      "let inv = & mut ctx . accounts . inv ;",
      "inv . items = BTreeMap :: new () ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyMap >",
      "token_id: u64",
      "qty: u32"
    ],
    "attributes": [],
    "body": [
      "let inv = & mut ctx . accounts . inv ;",
      "let e = inv . items . entry (token_id) . or_insert (0) ;",
      "* e = e . saturating_add (qty) ;",
      "Ok (())"
    ]
  },
  {
    "name": "use_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyMap >",
      "token_id: u64",
      "qty: u32"
    ],
    "attributes": [],
    "body": [
      "let inv = & mut ctx . accounts . inv ;",
      "if let Some (e) = inv . items . get_mut (& token_id)",
      "{",
      "* e = e . saturating_sub (qty) ;",
      "if * e == 0",
      "{",
      "inv . items . remove (& token_id) ;",
      "}",
      "}",
      "Ok (())"
    ]
  },
