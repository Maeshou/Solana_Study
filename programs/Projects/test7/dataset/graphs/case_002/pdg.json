        "label": "let dup_acc2 = & ctx . accounts . dup_account2 ;"
      },
      {
        "id": 4,
        "label": "let original = * * deposit_acc . try_borrow_lamports () ? ;"
      },
      {
        "id": 5,
        "label": "let new_balance = original + deposit_amount ;"
      },
      {
        "id": 6,
        "label": "* * deposit_acc . try_borrow_mut_lamports () ? = new_balance ;"
      },
      {
        "id": 7,
        "label": "let raw1 = dup_acc1 . try_borrow_data () ? ;"
      },
      {
        "id": 8,
        "label": "let raw2 = dup_acc2 . try_borrow_data () ? ;"
      },
      {
        "id": 9,
        "label": "let v1 = raw1 [0] ;"
      },
      {
        "id": 10,
        "label": "let v2 = raw2 [0] ;"
      },
      {
        "id": 11,
        "label": "let sum = v1 . wrapping_mul (2) . wrapping_add (v2) ;"
      },
      {
        "id": 12,
        "label": "let mut raw1_mut = dup_acc1 . try_borrow_mut_data () ? ;"
      },
      {
        "id": 13,
        "label": "raw1_mut [0] = sum ;"
      },
      {
        "id": 14,
        "label": "let mut raw2_mut = dup_acc2 . try_borrow_mut_data () ? ;"
      },
      {
        "id": 15,
        "label": "raw2_mut [0] = sum . wrapping_add (2) ;"
      },
      {
        "id": 16,
        "label": "Ok (())"
      }
