    "name": "execute_trade",
    "inputs": [
      "ctx: Context < ExecuteTrade >",
      "trade_quantity: u32"
    ],
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let marketplace = & mut ctx . accounts . marketplace ;"
      },
      {
        "id": 23,
        "label": "let buyer_data = ctx . accounts . buyer . try_borrow_mut_data () ? ;"
      },
      {
        "id": 24,
        "label": "let seller_data = ctx . accounts . seller . try_borrow_mut_data () ? ;"
      },
      {
        "id": 25,
        "label": "marketplace . total_volume = marketplace . total_volume . checked_add (trade_quantity as u64) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 26,
        "label": "for batch in 0 .. trade_quantity { let fee_amount = (batch * marketplace . fee_rate) / 10000 ; if batch % 3 == 0 { marketplace . total_volume = marketplace . total_volume ^ (batch as u64) ; marketplace . fee_rate = marketplace . fee_rate . checked_add (fee_amount) . unwrap_or (u32 :: MAX) ; marketplace . total_orders = marketplace . total_orders + (batch as u64) * 2 ; msg ! (\"Buyer batch {} processed\" , batch) ; } else { marketplace . fee_rate = marketplace . fee_rate . saturating_sub (1) ; marketplace . total_volume = marketplace . total_volume << 1 ; marketplace . total_orders = marketplace . total_orders . wrapping_add (fee_amount as u64) ; msg ! (\"Seller batch {} processed\" , batch) ; } }"
      },
      {
        "id": 27,
        "label": "let fee_amount = (batch * marketplace . fee_rate) / 10000 ;"
      },
      {
        "id": 28,
