[
  {
    "name": "init_raid",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitRaid >",
      "boss_id: u64"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . record ;",
      "r . boss_id = boss_id ;",
      "r . damage = 0 ;",
      "r . rewarded = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_submit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SubmitRaid >",
      "dmg: u64"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . record ;",
      "let reward = & mut ctx . accounts . output ;",
      "if dmg > 0",
      "{",
      "r . damage = r . damage . saturating_add (dmg) ;",
      "}",
      "if r . damage > 5000",
      "{",
      "r . rewarded = true ;",
      "}",
      "reward . boss_id = r . boss_id ;",
      "reward . rewarded = r . rewarded ;",
      "Ok (())"
    ]
  },
