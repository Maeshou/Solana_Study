  {
    "name": "process_loan",
    "inputs": [
      "ctx: Context < ProcessLoan >",
      "loan_amount: u64",
      "collateral_value: u64",
      "duration_days: u32"
    ],
    "nodes": [
      {
        "id": 10,
        "label": "Entry"
      },
      {
        "id": 11,
        "label": "let pool = & mut ctx . accounts . lending_pool ;"
      },
      {
        "id": 12,
        "label": "let borrower_account = & mut ctx . accounts . borrower_account ;"
      },
      {
        "id": 13,
        "label": "let processor = & ctx . accounts . processor ;"
      },
      {
        "id": 14,
        "label": "let ltv_ratio = (loan_amount * 100) / collateral_value ;"
      },
      {
        "id": 15,
        "label": "if ltv_ratio <= pool . max_loan_to_value as u64 { pool . total_borrowed += loan_amount ; pool . active_loans += 1 ; borrower_account . total_borrowed += loan_amount ; borrower_account . collateral_locked += collateral_value ; borrower_account . loan_start_time = Clock :: get () ? . unix_timestamp ; borrower_account . loan_duration = duration_days ; let daily_rate = pool . interest_rate as u64 / 365 ; let mut compound_interest = loan_amount ; for day in 0 .. duration_days . min (365) { compound_interest += (compound_interest * daily_rate) / 10000 ; borrower_account . daily_interest [day as usize % 30] = compound_interest - loan_amount ; } borrower_account . total_interest_owed = compound_interest - loan_amount ; borrower_account . is_active = true ; }"
      },
      {
        "id": 16,
        "label": "pool . total_borrowed += loan_amount ;"
      },
      {
        "id": 17,
        "label": "pool . active_loans += 1 ;"
      },
      {
        "id": 18,
        "label": "borrower_account . total_borrowed += loan_amount ;"
      },
      {
        "id": 19,
        "label": "borrower_account . collateral_locked += collateral_value ;"
      },
      {
        "id": 20,
        "label": "borrower_account . loan_start_time = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 21,
        "label": "borrower_account . loan_duration = duration_days ;"
      },
      {
        "id": 22,
        "label": "let daily_rate = pool . interest_rate as u64 / 365 ;"
      },
      {
        "id": 23,
        "label": "let mut compound_interest = loan_amount ;"
      },
      {
        "id": 24,
        "label": "for day in 0 .. duration_days . min (365) { compound_interest += (compound_interest * daily_rate) / 10000 ; borrower_account . daily_interest [day as usize % 30] = compound_interest - loan_amount ; }"
      },
      {
        "id": 25,
        "label": "compound_interest += (compound_interest * daily_rate) / 10000 ;"
      },
      {
        "id": 26,
        "label": "borrower_account . daily_interest [day as usize % 30] = compound_interest - loan_amount ;"
      },
      {
        "id": 27,
        "label": "borrower_account . total_interest_owed = compound_interest - loan_amount ;"
      },
      {
