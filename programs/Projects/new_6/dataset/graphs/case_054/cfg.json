    "name": "process_loan",
    "nodes": [
      {
        "id": 10,
        "label": "Entry"
      },
      {
        "id": 11,
        "label": "let pool = & mut ctx . accounts . lending_pool ;"
      },
      {
        "id": 12,
        "label": "let borrower_account = & mut ctx . accounts . borrower_account ;"
      },
      {
        "id": 13,
        "label": "let processor = & ctx . accounts . processor ;"
      },
      {
        "id": 14,
        "label": "let ltv_ratio = (loan_amount * 100) / collateral_value ;"
      },
      {
        "id": 15,
        "label": "if statement"
      },
      {
        "id": 16,
        "label": "predicate"
      },
      {
        "id": 17,
        "label": "ltv_ratio <= pool . max_loan_to_value as u64"
      },
      {
        "id": 18,
        "label": "True body"
      },
      {
        "id": 19,
        "label": "pool . total_borrowed += loan_amount ;"
      },
      {
        "id": 20,
        "label": "pool . active_loans += 1 ;"
      },
      {
        "id": 21,
        "label": "borrower_account . total_borrowed += loan_amount ;"
      },
      {
        "id": 22,
        "label": "borrower_account . collateral_locked += collateral_value ;"
      },
      {
        "id": 23,
        "label": "borrower_account . loan_start_time = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 24,
        "label": "borrower_account . loan_duration = duration_days ;"
      },
      {
        "id": 25,
        "label": "let daily_rate = pool . interest_rate as u64 / 365 ;"
      },
      {
        "id": 26,
        "label": "let mut compound_interest = loan_amount ;"
      },
      {
        "id": 27,
        "label": "Loop Start"
      },
      {
        "id": 28,
        "label": "compound_interest += (compound_interest * daily_rate) / 10000 ;"
      },
      {
        "id": 29,
        "label": "borrower_account . daily_interest [day as usize % 30] = compound_interest - loan_amount ;"
      },
      {
        "id": 30,
        "label": "Loop End"
      },
      {
        "id": 31,
        "label": "borrower_account . total_interest_owed = compound_interest - loan_amount ;"
      },
      {
        "id": 32,
        "label": "borrower_account . is_active = true ;"
