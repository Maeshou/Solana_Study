[
  {
    "name": "init_equipment",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitEq >",
      "kind: u8"
    ],
    "attributes": [],
    "body": [
      "let eq = & mut ctx . accounts . equipment ;",
      "eq . kind = kind ;",
      "eq . level = 1 ;",
      "eq . active = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_upgrade",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Upgrade >",
      "effort: u32"
    ],
    "attributes": [],
    "body": [
      "let eq = & mut ctx . accounts . equipment ;",
      "let log = & mut ctx . accounts . record ;",
      "for _ in 0 .. effort",
      "{",
      "if eq . level < 50",
      "{",
      "eq . level += 1 ;",
      "}",
      "}",
      "if eq . level % 10 == 0",
      "{",
      "eq . active = ! eq . active ;",
      "}",
      "log . level = eq . level ;",
      "Ok (())"
    ]
  },
