      },
      {
        "id": 28,
        "label": "let a_deposited = u64 :: from_le_bytes ([pos_a_data [40] , pos_a_data [41] , pos_a_data [42] , pos_a_data [43] , pos_a_data [44] , pos_a_data [45] , pos_a_data [46] , pos_a_data [47]]) ;"
