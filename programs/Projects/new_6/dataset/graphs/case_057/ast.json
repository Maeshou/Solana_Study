      "key: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let fighter = FighterCard { fighter : key } ;",
      "ctx . accounts . cfg . data . borrow_mut () [.. 32] . copy_from_slice (& fighter . try_to_vec () ?) ;",
      "Ok (())"
    ]
  },
