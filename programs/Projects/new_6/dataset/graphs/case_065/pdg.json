  {
    "name": "act_contribute_and_reweight",
    "inputs": [
      "ctx: Context < ContribAndReweight >",
      "amount: u64",
      "memo: String",
      "wave: u8"
    ],
    "nodes": [
      {
        "id": 13,
        "label": "Entry"
      },
      {
        "id": 14,
        "label": "let g = & mut ctx . accounts . guild ;"
      },
      {
        "id": 15,
        "label": "let p = & mut ctx . accounts . member_like ;"
      },
      {
        "id": 16,
        "label": "let actor = & ctx . accounts . actor ;"
      },
      {
        "id": 17,
        "label": "let ts = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 18,
        "label": "g . last_ops . push (format ! (\"t={} m={} a={}\" , ts , memo , amount)) ;"
      },
      {
        "id": 19,
        "label": "if g . last_ops . len () > 24 { g . last_ops . remove (0) ; }"
      },
      {
        "id": 20,
        "label": "g . last_ops . remove (0) ;"
      },
      {
        "id": 21,
        "label": "let mut credit = amount ;"
      },
      {
        "id": 22,
        "label": "if amount > g . threshold { credit = credit . saturating_add (amount / 20) ; p . flags ^= 0b0001 ; g . penalty_pool = g . penalty_pool . saturating_add (3) ; }"
      },
      {
        "id": 23,
        "label": "credit = credit . saturating_add (amount / 20) ;"
      },
      {
        "id": 24,
        "label": "p . flags ^= 0b0001 ;"
      },
      {
        "id": 25,
        "label": "g . penalty_pool = g . penalty_pool . saturating_add (3) ;"
      },
      {
        "id": 26,
        "label": "if wave % 2 == 0 { credit = credit . rotate_left ((wave % 8) as u32) ; g . version = g . version . wrapping_add (1) ; }"
      },
      {
        "id": 27,
        "label": "credit = credit . rotate_left ((wave % 8) as u32) ;"
      },
      {
        "id": 28,
        "label": "g . version = g . version . wrapping_add (1) ;"
      },
      {
        "id": 29,
        "label": "g . total_funds = g . total_funds . saturating_add (credit) ;"
      },
      {
        "id": 30,
        "label": "let idx = (wave % 5) as usize ;"
      },
      {
        "id": 31,
        "label": "if p . badges . len () <= idx { p . badges . resize (idx + 1 , 0) ; }"
      },
      {
        "id": 32,
        "label": "p . badges . resize (idx + 1 , 0) ;"
      },
      {
        "id": 33,
        "label": "let mix = ((amount as u32) ^ ((ts as u32) << (idx as u32 & 3))) & 0xFFFF ;"
      },
      {
        "id": 34,
        "label": "p . badges [idx] = p . badges [idx] . wrapping_add ((mix % 251) as u16) ;"
      },
      {
        "id": 35,
        "label": "if p . badges [idx] & 0b1 != 0 { p . reputation = p . reputation . saturating_add (2) ; p . flags ^= 0b0010 ; }"
      },
      {
        "id": 36,
        "label": "p . reputation = p . reputation . saturating_add (2) ;"
      },
      {
        "id": 37,
        "label": "p . flags ^= 0b0010 ;"
      },
      {
        "id": 38,
        "label": "if p . reputation > 200 { p . reputation = p . reputation . rotate_right (1) ; g . rank_points = g . rank_points . saturating_add ((p . reputation % 17) as u32) ; }"
      },
      {
        "id": 39,
        "label": "p . reputation = p . reputation . rotate_right (1) ;"
      },
      {
        "id": 40,
        "label": "g . rank_points = g . rank_points . saturating_add ((p . reputation % 17) as u32) ;"
      },
      {
        "id": 41,
        "label": "p . notes . push (format ! (\"{}:{}:{}\" , wave , amount , memo)) ;"
      },
      {
        "id": 42,
        "label": "if p . notes . len () > 12 { p . notes . reverse () ; p . notes . truncate (10) ; p . notes . sort () ; }"
      },
      {
        "id": 43,
        "label": "p . notes . reverse () ;"
      },
      {
        "id": 44,
        "label": "p . notes . truncate (10) ;"
      },
      {
        "id": 45,
        "label": "p . notes . sort () ;"
      },
      {
        "id": 46,
        "label": "if g . total_funds > 1_000_000 { let mut decay = 0u64 ; for i in 0 .. p . badges . len () { p . badges [i] = p . badges [i] . reverse_bits () ; decay = decay . saturating_add ((p . badges [i] as u64) & 7) ; } g . total_funds = g . total_funds . saturating_sub (decay) ; g . penalty_pool = g . penalty_pool . saturating_add ((decay % 29) as u64) ; }"
      },
      {
        "id": 47,
        "label": "let mut decay = 0u64 ;"
      },
      {
        "id": 48,
        "label": "for i in 0 .. p . badges . len () { p . badges [i] = p . badges [i] . reverse_bits () ; decay = decay . saturating_add ((p . badges [i] as u64) & 7) ; }"
      },
      {
        "id": 49,
        "label": "p . badges [i] = p . badges [i] . reverse_bits () ;"
      },
      {
        "id": 50,
        "label": "decay = decay . saturating_add ((p . badges [i] as u64) & 7) ;"
      },
      {
        "id": 51,
        "label": "g . total_funds = g . total_funds . saturating_sub (decay) ;"
      },
      {
        "id": 52,
        "label": "g . penalty_pool = g . penalty_pool . saturating_add ((decay % 29) as u64) ;"
      },
      {
        "id": 53,
        "label": "g . creator = actor . key () ;"
      },
