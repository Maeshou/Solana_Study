  {
    "name": "enhance_equipment",
    "inputs": [
      "ctx: Context < EnhanceEquipment >",
      "target_level: u8"
    ],
    "nodes": [
      {
        "id": 19,
        "label": "Entry"
      },
      {
        "id": 20,
        "label": "let workshop = & mut ctx . accounts . workshop ;"
      },
      {
        "id": 21,
        "label": "let equipment_data = ctx . accounts . equipment . try_borrow_mut_data () ? ;"
      },
      {
        "id": 22,
        "label": "let material_data = ctx . accounts . material . try_borrow_data () ? ;"
      },
      {
        "id": 23,
        "label": "workshop . enhancement_count = workshop . enhancement_count . checked_add (1) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 24,
        "label": "for level in 0 .. target_level { let enhancement_cost = (level as u32) * 50 ; if level % 2 == 0 { workshop . success_rate = workshop . success_rate . checked_add (enhancement_cost / 100) . unwrap_or (100) ; workshop . enhancement_count = workshop . enhancement_count ^ (level as u32) ; workshop . enhancement_count = workshop . enhancement_count >> 1 ; msg ! (\"Enhancement step {} succeeded\" , level) ; } else { workshop . success_rate = workshop . success_rate . saturating_sub (5) ; workshop . enhancement_count = workshop . enhancement_count . wrapping_add (enhancement_cost) ; workshop . creation_time = workshop . creation_time + (level as i64) ; msg ! (\"Enhancement step {} failed\" , level) ; } }"
      },
      {
        "id": 25,
        "label": "let enhancement_cost = (level as u32) * 50 ;"
      },
      {
