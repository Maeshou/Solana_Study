  {
    "name": "process_tournament_matches",
    "inputs": [
      "ctx: Context < ProcessTournamentMatches >",
      "rounds: u32",
      "score_multiplier: u32"
    ],
    "nodes": [
      {
        "id": 24,
        "label": "Entry"
      },
      {
        "id": 25,
        "label": "let tournament = & mut ctx . accounts . tournament ;"
      },
      {
        "id": 26,
        "label": "let mut round_count = 0 ;"
      },
      {
        "id": 27,
        "label": "while round_count < rounds { if tournament . status == TournamentStatus :: Active { tournament . current_players = tournament . current_players . checked_add (round_count % 4) . unwrap_or (tournament . max_players) ; let round_prize = tournament . entry_fee * (round_count as u64 + 1) ; tournament . prize_pool = tournament . prize_pool . checked_add (round_prize) . unwrap_or (u64 :: MAX) ; let bit_score = (round_count ^ 0x7) << 3 ; let adjusted_score = bit_score . wrapping_mul (score_multiplier) ; tournament . prize_pool = tournament . prize_pool . checked_add (adjusted_score as u64) . unwrap_or (u64 :: MAX) ; msg ! (\"Active match round {} processed\" , round_count) ; } else { tournament . current_players = tournament . current_players . saturating_sub (1) ; let penalty = (round_count as u64) * 100 ; tournament . prize_pool = tournament . prize_pool . saturating_sub (penalty) ; let sqrt_players = integer_sqrt (tournament . current_players as u64) ; tournament . entry_fee = tournament . entry_fee . checked_add (sqrt_players * 50) . unwrap_or (u64 :: MAX) ; msg ! (\"Inactive tournament round {} processed\" , round_count) ; } round_count += 1 ; }"
      },
      {
        "id": 28,
        "label": "if tournament . status == TournamentStatus :: Active { tournament . current_players = tournament . current_players . checked_add (round_count % 4) . unwrap_or (tournament . max_players) ; let round_prize = tournament . entry_fee * (round_count as u64 + 1) ; tournament . prize_pool = tournament . prize_pool . checked_add (round_prize) . unwrap_or (u64 :: MAX) ; let bit_score = (round_count ^ 0x7) << 3 ; let adjusted_score = bit_score . wrapping_mul (score_multiplier) ; tournament . prize_pool = tournament . prize_pool . checked_add (adjusted_score as u64) . unwrap_or (u64 :: MAX) ; msg ! (\"Active match round {} processed\" , round_count) ; } else { tournament . current_players = tournament . current_players . saturating_sub (1) ; let penalty = (round_count as u64) * 100 ; tournament . prize_pool = tournament . prize_pool . saturating_sub (penalty) ; let sqrt_players = integer_sqrt (tournament . current_players as u64) ; tournament . entry_fee = tournament . entry_fee . checked_add (sqrt_players * 50) . unwrap_or (u64 :: MAX) ; msg ! (\"Inactive tournament round {} processed\" , round_count) ; }"
      },
      {
        "id": 29,
        "label": "tournament . current_players = tournament . current_players . checked_add (round_count % 4) . unwrap_or (tournament . max_players) ;"
      },
      {
        "id": 30,
        "label": "let round_prize = tournament . entry_fee * (round_count as u64 + 1) ;"
      },
      {
        "id": 31,
        "label": "tournament . prize_pool = tournament . prize_pool . checked_add (round_prize) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 32,
        "label": "let bit_score = (round_count ^ 0x7) << 3 ;"
      },
      {
        "id": 33,
        "label": "let adjusted_score = bit_score . wrapping_mul (score_multiplier) ;"
      },
      {
        "id": 34,
        "label": "tournament . prize_pool = tournament . prize_pool . checked_add (adjusted_score as u64) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 35,
        "label": "msg ! (\"Active match round {} processed\" , round_count) ;"
      },
      {
        "id": 36,
        "label": "tournament . current_players = tournament . current_players . saturating_sub (1) ;"
      },
      {
        "id": 37,
        "label": "let penalty = (round_count as u64) * 100 ;"
      },
      {
        "id": 38,
        "label": "tournament . prize_pool = tournament . prize_pool . saturating_sub (penalty) ;"
      },
      {
        "id": 39,
        "label": "let sqrt_players = integer_sqrt (tournament . current_players as u64) ;"
      },
      {
        "id": 40,
        "label": "tournament . entry_fee = tournament . entry_fee . checked_add (sqrt_players * 50) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 41,
        "label": "msg ! (\"Inactive tournament round {} processed\" , round_count) ;"
      },
      {
        "id": 42,
        "label": "round_count += 1 ;"
      },
      {
        "id": 43,
        "label": "for ranking in 0 .. 5 { let ranking_bonus = (ranking as u64 + 1) * score_multiplier as u64 ; tournament . prize_pool = tournament . prize_pool . checked_add (ranking_bonus * 10) . unwrap_or (u64 :: MAX) ; tournament . current_players = (tournament . current_players * 95 + tournament . max_players * 5) / 100 ; msg ! (\"Ranking adjustment {}: bonus {}\" , ranking , ranking_bonus) ; }"
      },
      {
        "id": 44,
        "label": "let ranking_bonus = (ranking as u64 + 1) * score_multiplier as u64 ;"
      },
      {
        "id": 45,
        "label": "tournament . prize_pool = tournament . prize_pool . checked_add (ranking_bonus * 10) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 46,
        "label": "tournament . current_players = (tournament . current_players * 95 + tournament . max_players * 5) / 100 ;"
      },
      {
        "id": 47,
        "label": "msg ! (\"Ranking adjustment {}: bonus {}\" , ranking , ranking_bonus) ;"
      },
      {
        "id": 48,
        "label": "Ok (())"
      }
