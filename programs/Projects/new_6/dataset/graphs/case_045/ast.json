[
  {
    "name": "init_art",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitArt >"
    ],
    "attributes": [],
    "body": [
      "let a = & mut ctx . accounts . art ;",
      "a . artist = ctx . accounts . signer . key () ;",
      "a . seed = 42 ;",
      "a . strokes = vec ! [1 , 2 , 3] ;",
      "a . layers = 0 ;",
      "a . palette = vec ! [] ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_progress",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProgressArt >",
      "stroke: u8"
    ],
    "attributes": [],
    "body": [
      "let a = & mut ctx . accounts . art ;",
      "let painter = & ctx . accounts . participant ;",
      "for i in 0 .. stroke",
      "{",
      "a . strokes . push ((a . seed ^ (i as u64)) as u8) ;",
      "if i % 3 == 0",
      "{",
      "a . palette . push (i * 5) ;",
      "} else",
      "{",
      "a . palette . push (i + stroke) ;",
      "}",
      "}",
      "if a . palette . len () > 5",
      "{",
      "a . palette . sort_by (| a , b | b . cmp (a)) ;",
      "a . palette . dedup () ;",
      "a . layers += 1 ;",
      "}",
      "a . seed = a . seed . rotate_left (2) . wrapping_add (stroke as u64) ;",
      "a . artist = painter . key () ;",
      "Ok (())"
    ]
  },
