      },
      {
        "id": 20,
        "label": "if sum > 0 { user . progress = user . progress . saturating_add (sum as u32) ; tracker . total_events += 1 ; audit_log . note = 0xAB ; } else { auditor . progress = auditor . progress . saturating_sub ((- sum) as u32) ; tracker . total_events += 2 ; audit_log . note = 0xCD ; }"
      },
      {
        "id": 21,
        "label": "user . progress = user . progress . saturating_add (sum as u32) ;"
      },
      {
        "id": 22,
        "label": "tracker . total_events += 1 ;"
      },
      {
        "id": 23,
        "label": "audit_log . note = 0xAB ;"
      },
      {
        "id": 24,
        "label": "auditor . progress = auditor . progress . saturating_sub ((- sum) as u32) ;"
      },
      {
        "id": 25,
        "label": "tracker . total_events += 2 ;"
      },
      {
        "id": 26,
        "label": "audit_log . note = 0xCD ;"
      },
