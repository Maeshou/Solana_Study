[
  {
    "name": "init_palette",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPalette >",
      "base: u32",
      "alpha: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . palette ;",
      "p . creator = ctx . accounts . artist . key () ;",
      "p . color_base = base ;",
      "p . alpha = alpha ;",
      "p . versions = 1 ;",
      "p . saturated = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_mutate_color",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < MutateColor >",
      "bump: u32"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . palette ;",
      "let color_mgr = & ctx . accounts . manager ;",
      "for i in 0 .. 5",
      "{",
      "p . color_base = p . color_base . wrapping_add (bump . wrapping_mul (i)) ;",
      "}",
      "if p . alpha < 200",
      "{",
      "p . alpha += 20 ;",
      "} else",
      "{",
      "p . alpha -= 30 ;",
      "}",
      "if p . versions > 10",
      "{",
      "p . saturated = true ;",
      "} else",
      "{",
      "p . versions += 1 ;",
      "}",
      "msg ! (\"Color modified by: {}\" , color_mgr . key ()) ;",
      "Ok (())"
    ]
  },
