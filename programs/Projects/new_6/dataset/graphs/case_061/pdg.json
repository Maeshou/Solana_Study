  {
    "name": "harvest_rewards",
    "inputs": [
      "ctx: Context < HarvestRewards >",
      "user_address: Pubkey",
      "force_compound: bool"
    ],
    "nodes": [
      {
        "id": 12,
        "label": "Entry"
      },
      {
        "id": 13,
        "label": "let farm = & mut ctx . accounts . yield_farm ;"
      },
      {
        "id": 14,
        "label": "let user_info = & mut ctx . accounts . user_farming_info ;"
      },
      {
        "id": 15,
        "label": "let harvester = & ctx . accounts . harvester ;"
      },
      {
        "id": 16,
        "label": "let current_block = Clock :: get () ? . slot ;"
      },
      {
        "id": 17,
        "label": "let blocks_elapsed = current_block - farm . last_reward_block ;"
      },
      {
        "id": 18,
        "label": "if blocks_elapsed > 0 && farm . total_staked > 0 { let reward = blocks_elapsed * farm . reward_per_block ; farm . accumulated_reward_per_share += (reward * 1e12 as u64) / farm . total_staked ; farm . last_reward_block = current_block ; }"
      },
      {
        "id": 19,
        "label": "let reward = blocks_elapsed * farm . reward_per_block ;"
      },
      {
        "id": 20,
        "label": "farm . accumulated_reward_per_share += (reward * 1e12 as u64) / farm . total_staked ;"
      },
      {
        "id": 21,
        "label": "farm . last_reward_block = current_block ;"
      },
      {
        "id": 22,
        "label": "let user_accumulated_reward = (user_info . staked_amount * farm . accumulated_reward_per_share) / 1e12 as u64 ;"
      },
      {
        "id": 23,
        "label": "let pending_reward = user_accumulated_reward - user_info . reward_debt ;"
      },
      {
        "id": 24,
        "label": "if pending_reward > 0 { user_info . total_harvested += pending_reward ; farm . total_rewards_distributed += pending_reward ; if force_compound { user_info . staked_amount += pending_reward ; farm . total_staked += pending_reward ; user_info . compound_count += 1 ; let compound_bonus = (pending_reward * farm . bonus_multiplier as u64) / 10000 ; user_info . bonus_rewards += compound_bonus ; farm . total_bonus_distributed += compound_bonus ; } let farming_duration = current_block - user_info . first_stake_block ; if farming_duration > 100000 { user_info . loyalty_multiplier = (user_info . loyalty_multiplier + 5) . min (200) ; } for week in 0 .. 4 { if farming_duration > (week + 1) as u64 * 10080 { user_info . weekly_multipliers [week] = 110 + (week as u16 * 10) ; } } user_info . reward_debt = (user_info . staked_amount * farm . accumulated_reward_per_share) / 1e12 as u64 ; }"
      },
      {
        "id": 25,
        "label": "user_info . total_harvested += pending_reward ;"
      },
      {
        "id": 26,
        "label": "farm . total_rewards_distributed += pending_reward ;"
      },
      {
        "id": 27,
        "label": "if force_compound { user_info . staked_amount += pending_reward ; farm . total_staked += pending_reward ; user_info . compound_count += 1 ; let compound_bonus = (pending_reward * farm . bonus_multiplier as u64) / 10000 ; user_info . bonus_rewards += compound_bonus ; farm . total_bonus_distributed += compound_bonus ; }"
      },
      {
        "id": 28,
        "label": "user_info . staked_amount += pending_reward ;"
      },
      {
        "id": 29,
        "label": "farm . total_staked += pending_reward ;"
      },
      {
        "id": 30,
        "label": "user_info . compound_count += 1 ;"
      },
      {
        "id": 31,
        "label": "let compound_bonus = (pending_reward * farm . bonus_multiplier as u64) / 10000 ;"
      },
      {
        "id": 32,
        "label": "user_info . bonus_rewards += compound_bonus ;"
      },
      {
        "id": 33,
        "label": "farm . total_bonus_distributed += compound_bonus ;"
      },
      {
        "id": 34,
        "label": "let farming_duration = current_block - user_info . first_stake_block ;"
      },
      {
        "id": 35,
        "label": "if farming_duration > 100000 { user_info . loyalty_multiplier = (user_info . loyalty_multiplier + 5) . min (200) ; }"
      },
      {
        "id": 36,
        "label": "user_info . loyalty_multiplier = (user_info . loyalty_multiplier + 5) . min (200) ;"
      },
      {
        "id": 37,
        "label": "for week in 0 .. 4 { if farming_duration > (week + 1) as u64 * 10080 { user_info . weekly_multipliers [week] = 110 + (week as u16 * 10) ; } }"
      },
      {
        "id": 38,
        "label": "if farming_duration > (week + 1) as u64 * 10080 { user_info . weekly_multipliers [week] = 110 + (week as u16 * 10) ; }"
      },
      {
        "id": 39,
        "label": "user_info . weekly_multipliers [week] = 110 + (week as u16 * 10) ;"
      },
      {
        "id": 40,
        "label": "user_info . reward_debt = (user_info . staked_amount * farm . accumulated_reward_per_share) / 1e12 as u64 ;"
      },
