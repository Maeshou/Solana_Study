[
  {
    "name": "init_guild",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitGuild >",
      "name: String"
    ],
    "attributes": [],
    "body": [
      "let guild = & mut ctx . accounts . guild ;",
      "guild . name = name ;",
      "guild . leader = ctx . accounts . user . key () ;",
      "guild . audit_enabled = false ;",
      "guild . level = 1 ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_toggle_audit_and_level",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ToggleAudit >"
    ],
    "attributes": [],
    "body": [
      "let g = & mut ctx . accounts . guild ;",
      "let log = & mut ctx . accounts . audit_log ;",
      "for i in 0 .. 5",
      "{",
      "if i % 2 == 0",
      "{",
      "g . audit_enabled = ! g . audit_enabled ;",
      "} else",
      "{",
      "g . level = g . level . wrapping_add (1) ;",
      "}",
      "}",
      "log . last_action = g . leader ;",
      "Ok (())"
    ]
  },
