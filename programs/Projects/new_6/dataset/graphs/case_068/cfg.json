        "label": "let r = & mut ctx . accounts . relic ;"
      },
      {
        "id": 11,
        "label": "let appraiser = & ctx . accounts . appraiser ;"
      },
      {
        "id": 12,
        "label": "r . audit_notes . push (format ! (\"{}#{}\" , label , times)) ;"
      },
      {
        "id": 13,
        "label": "if statement"
      },
      {
        "id": 14,
        "label": "predicate"
      },
      {
        "id": 15,
        "label": "r . audit_notes . len () > 10"
      },
      {
        "id": 16,
        "label": "True body"
      },
      {
        "id": 17,
        "label": "r . audit_notes . remove (0) ;"
      },
      {
        "id": 18,
        "label": "False body"
