        "label": "merge"
      },
      {
        "id": 21,
        "label": "let mut t = 0 ;"
      },
      {
        "id": 22,
        "label": "Loop Start"
      },
      {
        "id": 23,
        "label": "let slot = ((gem as usize) + t as usize) % r . sockets . len () ;"
      },
      {
        "id": 24,
        "label": "let delta = ((gem as u16) << (t % 3)) ^ (r . core_power . rotate_left ((t % 7) as u32)) ;"
      },
      {
        "id": 25,
        "label": "r . sockets [slot] = r . sockets [slot] . wrapping_add ((delta & 0xFF) as u8) ;"
      },
      {
        "id": 26,
        "label": "if statement"
      },
      {
        "id": 27,
        "label": "predicate"
      },
      {
        "id": 28,
        "label": "r . sockets [slot] & 1 == 1"
      },
      {
        "id": 29,
        "label": "True body"
      },
      {
        "id": 30,
        "label": "r . affinity [(slot % 3)] = r . affinity [(slot % 3)] . wrapping_add ((gem ^ (t as u8)) & 0x1F) ;"
      },
      {
        "id": 31,
        "label": "r . mode = r . mode . rotate_left (1) ;"
      },
      {
        "id": 32,
        "label": "False body"
      },
      {
        "id": 33,
        "label": "No-op"
      },
      {
        "id": 34,
        "label": "merge"
      },
      {
        "id": 35,
        "label": "if statement"
      },
      {
        "id": 36,
        "label": "predicate"
      },
      {
        "id": 37,
        "label": "r . affinity [(slot % 3)] > 100"
      },
      {
        "id": 38,
        "label": "True body"
      },
      {
        "id": 39,
        "label": "r . affinity [(slot % 3)] = r . affinity [(slot % 3)] % 67 ;"
      },
      {
        "id": 40,
        "label": "r . core_power = r . core_power . rotate_right (1) ;"
      },
      {
        "id": 41,
        "label": "False body"
      },
      {
        "id": 42,
        "label": "No-op"
      },
      {
        "id": 43,
        "label": "merge"
      },
      {
        "id": 44,
        "label": "t += 1 ;"
      },
      {
        "id": 45,
        "label": "Loop End"
      },
      {
        "id": 46,
        "label": "let sum = r . sockets . iter () . fold (0u32 , | a , b | a + * b as u32) ;"
      },
      {
        "id": 47,
        "label": "if statement"
      },
      {
        "id": 48,
        "label": "predicate"
      },
      {
        "id": 49,
        "label": "sum % 9 == 0"
      },
      {
        "id": 50,
        "label": "True body"
      },
      {
        "id": 51,
        "label": "r . core_power = r . core_power . wrapping_add ((sum % 257) as u16) ;"
      },
      {
        "id": 52,
        "label": "r . sockets . rotate_left (1) ;"
      },
      {
        "id": 53,
        "label": "False body"
      },
      {
        "id": 54,
        "label": "No-op"
      },
      {
        "id": 55,
