  {
    "name": "transfer_energy",
    "inputs": [
      "ctx: Context < TransferEnergy >",
      "amount: u32"
    ],
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let hub = & mut ctx . accounts . hub ;"
      },
      {
        "id": 23,
        "label": "let source_data = ctx . accounts . source_energy . try_borrow_mut_data () ? ;"
      },
      {
        "id": 24,
        "label": "let target_data = ctx . accounts . target_energy . try_borrow_mut_data () ? ;"
      },
      {
        "id": 25,
        "label": "hub . total_energy = hub . total_energy . checked_add (amount as u64) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 26,
        "label": "for transfer_step in 0 .. amount { let step_amount = transfer_step + 1 ; if transfer_step % 2 == 0 { hub . regeneration_rate = hub . regeneration_rate . checked_add (step_amount / 10) . unwrap_or (u32 :: MAX) ; hub . total_energy = hub . total_energy ^ (transfer_step as u64) ; hub . active_users = hub . active_users << 1 ; msg ! (\"Source energy step {} processed\" , transfer_step) ; } else { hub . regeneration_rate = hub . regeneration_rate . saturating_sub (1) ; hub . total_energy = hub . total_energy + (step_amount as u64 * 5) ; hub . active_users = hub . active_users . wrapping_add (step_amount) ; msg ! (\"Target energy step {} processed\" , transfer_step) ; } }"
      },
      {
        "id": 27,
        "label": "let step_amount = transfer_step + 1 ;"
      },
      {
        "id": 28,
        "label": "if transfer_step % 2 == 0 { hub . regeneration_rate = hub . regeneration_rate . checked_add (step_amount / 10) . unwrap_or (u32 :: MAX) ; hub . total_energy = hub . total_energy ^ (transfer_step as u64) ; hub . active_users = hub . active_users << 1 ; msg ! (\"Source energy step {} processed\" , transfer_step) ; } else { hub . regeneration_rate = hub . regeneration_rate . saturating_sub (1) ; hub . total_energy = hub . total_energy + (step_amount as u64 * 5) ; hub . active_users = hub . active_users . wrapping_add (step_amount) ; msg ! (\"Target energy step {} processed\" , transfer_step) ; }"
      },
      {
        "id": 29,
        "label": "hub . regeneration_rate = hub . regeneration_rate . checked_add (step_amount / 10) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 30,
        "label": "hub . total_energy = hub . total_energy ^ (transfer_step as u64) ;"
      },
      {
        "id": 31,
        "label": "hub . active_users = hub . active_users << 1 ;"
      },
      {
        "id": 32,
        "label": "msg ! (\"Source energy step {} processed\" , transfer_step) ;"
      },
      {
        "id": 33,
        "label": "hub . regeneration_rate = hub . regeneration_rate . saturating_sub (1) ;"
      },
      {
        "id": 34,
