        "id": 36,
        "label": "let mut quality_iter = 0u8 ;"
      },
      {
        "id": 37,
        "label": "while quality_iter < 3 { let quality_boost = quality_iter * 10 ; workshop . crafting_fee += quality_boost as u64 ; quality_iter += 1 ; }"
      },
      {
        "id": 38,
        "label": "let quality_boost = quality_iter * 10 ;"
      },
