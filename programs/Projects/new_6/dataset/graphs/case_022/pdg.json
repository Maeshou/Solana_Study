  {
    "name": "execute_crafting_process",
    "inputs": [
      "ctx: Context < ExecuteCraftingProcess >",
      "iterations: u8",
      "base_power: u16"
    ],
    "nodes": [
      {
        "id": 18,
        "label": "Entry"
      },
      {
        "id": 19,
        "label": "let workshop = & mut ctx . accounts . workshop ;"
      },
      {
        "id": 20,
        "label": "let mut round = 0u8 ;"
      },
      {
        "id": 21,
        "label": "loop { if round >= iterations { break ; } if workshop . is_open { workshop . total_crafts += 1 ; let craft_bonus = (round as u32) * 50 ; workshop . crafting_fee += craft_bonus as u64 ; let power_mod = base_power ^ (round as u16) ; let final_power = power_mod & 0xFF ; msg ! (\"Craft round {}: power {}\" , round , final_power) ; } else { if workshop . total_crafts > 0 { workshop . total_crafts -= 1 ; } workshop . crafting_fee = workshop . crafting_fee . saturating_sub (100) ; msg ! (\"Closed workshop adjustment: round {}\" , round) ; } round += 1 ; }"
      },
      {
        "id": 22,
        "label": "if round >= iterations { break ; }"
      },
      {
        "id": 23,
        "label": "break ;"
      },
      {
        "id": 24,
        "label": "if workshop . is_open { workshop . total_crafts += 1 ; let craft_bonus = (round as u32) * 50 ; workshop . crafting_fee += craft_bonus as u64 ; let power_mod = base_power ^ (round as u16) ; let final_power = power_mod & 0xFF ; msg ! (\"Craft round {}: power {}\" , round , final_power) ; } else { if workshop . total_crafts > 0 { workshop . total_crafts -= 1 ; } workshop . crafting_fee = workshop . crafting_fee . saturating_sub (100) ; msg ! (\"Closed workshop adjustment: round {}\" , round) ; }"
      },
