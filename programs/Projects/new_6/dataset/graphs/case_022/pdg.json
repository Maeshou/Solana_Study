      },
      {
        "id": 28,
        "label": "let power_mod = base_power ^ (round as u16) ;"
      },
      {
        "id": 29,
        "label": "let final_power = power_mod & 0xFF ;"
      },
      {
        "id": 30,
        "label": "msg ! (\"Craft round {}: power {}\" , round , final_power) ;"
      },
      {
        "id": 31,
        "label": "if workshop . total_crafts > 0 { workshop . total_crafts -= 1 ; }"
      },
      {
        "id": 32,
        "label": "workshop . total_crafts -= 1 ;"
      },
      {
        "id": 33,
        "label": "workshop . crafting_fee = workshop . crafting_fee . saturating_sub (100) ;"
