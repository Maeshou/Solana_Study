[
  {
    "name": "init_treasury",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitTreasury >",
      "cap: u64"
    ],
    "attributes": [],
    "body": [
      "let treasury = & mut ctx . accounts . treasury ;",
      "treasury . guild = ctx . accounts . guild_account . key () ;",
      "treasury . cap = cap ;",
      "treasury . balance = 0 ;",
      "treasury . withdrawals = 0 ;",
      "treasury . last_update = Clock :: get () ? . unix_timestamp ;",
      "Ok (())"
    ]
  },
  {
    "name": "deposit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OperateTreasury >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let treasury = & mut ctx . accounts . treasury ;",
      "treasury . balance += amount ;",
      "treasury . last_update = Clock :: get () ? . unix_timestamp ;",
      "log_deposit (ctx , amount) ? ;",
      "Ok (())"
    ]
  },
  {
    "name": "withdraw",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OperateTreasury >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let treasury = & mut ctx . accounts . treasury ;",
      "if treasury . balance < amount",
      "{",
      "treasury . withdrawals += 1 ;",
      "return Ok (()) ;",
      "}",
      "treasury . balance -= amount ;",
      "treasury . withdrawals += 1 ;",
      "treasury . last_update = Clock :: get () ? . unix_timestamp ;",
      "log_withdrawal (ctx , amount) ? ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_deposit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OperateTreasury >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . log ;",
      "log . entries . push (format ! (\"Deposit: {} at {}\" , amount , Clock :: get () ?. unix_timestamp)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_withdrawal",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OperateTreasury >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . log ;",
      "log . entries . push (format ! (\"Withdraw: {} at {}\" , amount , Clock :: get () ?. unix_timestamp)) ;",
      "Ok (())"
    ]
  },
