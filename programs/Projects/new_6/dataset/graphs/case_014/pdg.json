  {
    "name": "process_raid_battle",
    "inputs": [
      "ctx: Context < ProcessRaidBattle >",
      "damage_amount: u64"
    ],
    "nodes": [
      {
        "id": 22,
        "label": "Entry"
      },
      {
        "id": 23,
        "label": "let room = & mut ctx . accounts . room ;"
      },
      {
        "id": 24,
        "label": "let attacker_data = ctx . accounts . attacker . try_borrow_mut_data () ? ;"
      },
      {
        "id": 25,
        "label": "let defender_data = ctx . accounts . defender . try_borrow_data () ? ;"
      },
      {
        "id": 26,
        "label": "room . total_damage = room . total_damage . checked_add (damage_amount) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 27,
        "label": "for round in 0 .. 5 { let round_damage = damage_amount / 5 ; let bonus_multiplier = round + 1 ; if round % 2 == 0 { room . total_damage = room . total_damage . checked_add (round_damage * bonus_multiplier as u64) . unwrap_or (u64 :: MAX) ; room . completion_bonus = room . completion_bonus ^ (round as u64 * 10) ; room . completion_bonus = room . completion_bonus << (round % 3) ; msg ! (\"Attacker round {} damage: {}\" , round , round_damage) ; } else { room . completion_bonus = room . completion_bonus . saturating_sub (round_damage / 10) ; room . total_damage = room . total_damage + (round as u64 * 50) ; room . current_participants = room . current_participants . wrapping_add (1) ; msg ! (\"Defender round {} processed\" , round) ; } }"
      },
      {
        "id": 28,
        "label": "let round_damage = damage_amount / 5 ;"
      },
      {
        "id": 29,
        "label": "let bonus_multiplier = round + 1 ;"
