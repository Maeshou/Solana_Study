      },
      {
        "id": 31,
        "label": "room . total_damage = room . total_damage . checked_add (round_damage * bonus_multiplier as u64) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 32,
        "label": "room . completion_bonus = room . completion_bonus ^ (round as u64 * 10) ;"
      },
