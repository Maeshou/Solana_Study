[
  {
    "name": "init_order",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitOrder >",
      "price: u64"
    ],
    "attributes": [],
    "body": [
      "let o = & mut ctx . accounts . order ;",
      "o . seller = ctx . accounts . user . key () ;",
      "o . price = price ;",
      "o . filled = false ;",
      "o . retry_count = 0 ;",
      "o . priority = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "act_fill",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < FillOrder >",
      "paid: u64",
      "boost: bool"
    ],
    "attributes": [],
    "body": [
      "let o = & mut ctx . accounts . order ;",
      "let payer = & ctx . accounts . payer ;",
      "if paid >= o . price",
      "{",
      "o . filled = true ;",
      "o . retry_count = 0 ;",
      "o . priority = o . priority . saturating_add (2) ;",
      "o . seller = payer . key () ;",
      "}",
      "if boost",
      "{",
      "o . priority = o . priority . saturating_add (10) ;",
      "o . price = o . price . saturating_mul (2) ;",
      "o . retry_count += 1 ;",
      "}",
      "if o . retry_count > 3",
      "{",
      "o . priority = 0 ;",
      "o . price = 0 ;",
      "}",
      "Ok (())"
    ]
  },
