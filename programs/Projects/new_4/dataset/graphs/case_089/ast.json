    "attributes": [],
    "body": [
      "let f = & mut ctx . accounts . feed ;",
      "f . samples = Vec :: new () ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_feed",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateFeed >",
      "price_points: Vec < u64 >"
    ],
    "attributes": [],
    "body": [
      "let f = & mut ctx . accounts . feed ;",
      "let mut sum = 0u128 ;",
      "for & p in price_points . iter ()",
      "{",
      "sum += p as u128 ;",
      "}",
      "let avg = if price_points . is_empty () { 0 } else { (sum / price_points . len () as u128) as u64 } ;",
      "f . last_price = avg ;",
      "Ok (())"
    ]
  },
