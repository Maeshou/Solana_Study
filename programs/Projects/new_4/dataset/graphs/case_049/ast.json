  {
    "name": "init_moderation",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitMod >",
      "flag_words: Vec < String >"
    ],
    "attributes": [],
    "body": [
      "require ! (flag_words . len () <= 10 , ErrorCode4 :: ManyFlags) ;",
      "let m = & mut ctx . accounts . mod_config ;",
      "m . flags = flag_words ;",
      "Ok (())"
    ]
  },
