[
  {
    "name": "init_report",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitReport >",
      "title: String",
      "summary: String"
    ],
    "attributes": [],
    "body": [
      "let rpt = & mut ctx . accounts . report ;",
      "require ! (summary . len () <= 256 , ErrorCode2 :: SummaryTooLong) ;",
      "rpt . title = title ;",
      "rpt . pages = 0 ;",
      "rpt . summary = summary ;",
      "rpt . finalized = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_page",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitReport >"
    ],
    "attributes": [],
    "body": [
      "let rpt = & mut ctx . accounts . report ;",
      "rpt . pages = rpt . pages . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "finalize_report",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitReport >"
    ],
    "attributes": [],
    "body": [
      "let rpt = & mut ctx . accounts . report ;",
      "require ! (! rpt . finalized , ErrorCode2 :: AlreadyFinalized) ;",
      "rpt . finalized = true ;",
      "let hist = & mut ctx . accounts . history ;",
      "if hist . notes . len () >= 10",
      "{",
      "hist . notes . remove (0) ;",
      "}",
      "hist . notes . push (rpt . title . clone ()) ;",
      "Ok (())"
    ]
  },
