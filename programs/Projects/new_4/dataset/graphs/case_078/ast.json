  {
    "name": "add_stock",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddStock >",
      "amount: u32"
    ],
    "attributes": [],
    "body": [
      "let stock = & mut ctx . accounts . stock ;",
      "stock . level += amount ;",
      "let record = & mut ctx . accounts . withdrawal ;",
      "if amount < stock . level",
      "{",
      "record . amount = stock . level - amount ;",
      "}",
      "Ok (())"
    ]
  },
