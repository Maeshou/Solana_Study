  {
    "name": "init_list",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitList >"
    ],
    "attributes": [],
    "body": [
      "let list = & mut ctx . accounts . task_list ;",
      "list . count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_tasks",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddTasks >",
      "titles: Vec < String >"
    ],
    "attributes": [],
    "body": [
      "let list = & mut ctx . accounts . task_list ;",
      "let entry = & mut ctx . accounts . entry ;",
      "for t in titles . iter ()",
      "{",
      "if t . len () > 0",
      "{",
      "list . count += 1 ;",
      "entry . title = t . clone () ;",
      "}",
      "}",
      "Ok (())"
    ]
  },
