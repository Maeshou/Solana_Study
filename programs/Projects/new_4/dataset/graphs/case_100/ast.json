[
  {
    "name": "init_course",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCourse >",
      "capacity: u8"
    ],
    "attributes": [],
    "body": [
      "let course = & mut ctx . accounts . course ;",
      "course . capacity = capacity ;",
      "course . enrolled = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "setup_roster",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetupRoster >",
      "students: Vec < Pubkey >"
    ],
    "attributes": [],
    "body": [
      "let course = & mut ctx . accounts . course ;",
      "let roster = & mut ctx . accounts . roster ;",
      "roster . students = Vec :: new () ;",
      "for pk in students . iter () . take (course . capacity as usize)",
      "{",
      "roster . students . push (* pk) ;",
      "course . enrolled += 1 ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "record_stats",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RecordStats >"
    ],
    "attributes": [],
    "body": [
      "let course = & ctx . accounts . course ;",
      "let stats = & mut ctx . accounts . stats ;",
      "stats . enrolled = course . enrolled as u32 ;",
      "stats . started_at = Clock :: get () ? . unix_timestamp ;",
      "Ok (())"
    ]
  },
