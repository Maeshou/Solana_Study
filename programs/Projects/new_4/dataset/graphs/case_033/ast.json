  {
    "name": "log_cache",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetupAnalysis >",
      "info: String"
    ],
    "attributes": [],
    "body": [
      "let ch = & mut ctx . accounts . cache_log ;",
      "if ch . logs . len () >= 20",
      "{",
      "ch . logs . remove (0) ;",
      "}",
      "ch . logs . push ((info , Clock :: get () ? . unix_timestamp)) ;",
      "Ok (())"
    ]
  },
