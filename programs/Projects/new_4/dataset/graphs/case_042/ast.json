[
  {
    "name": "init_cycle",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCycle >",
      "period: u32"
    ],
    "attributes": [],
    "body": [
      "let bc = & mut ctx . accounts . billing_cycle ;",
      "bc . period = period ;",
      "bc . counter = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "issue_invoice",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCycle >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let inv = & mut ctx . accounts . invoice ;",
      "inv . amount = amount ;",
      "let hist = & mut ctx . accounts . invoice_history ;",
      "hist . entries . push (amount) ;",
      "bc . counter = bc . counter . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
