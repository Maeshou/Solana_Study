[
  {
    "name": "init_tournament",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitTournament >",
      "capacity: u16"
    ],
    "attributes": [],
    "body": [
      "let t = & mut ctx . accounts . tournament ;",
      "t . capacity = capacity ;",
      "t . registered = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "register_player",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitTournament >"
    ],
    "attributes": [],
    "body": [
      "let t = & mut ctx . accounts . tournament ;",
      "require ! (t . registered < t . capacity , ErrorCode2 :: Full) ;",
      "t . registered = t . registered . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_registration",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitTournament >",
      "player: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . reg_log ;",
      "log . players . push (player) ;",
      "Ok (())"
    ]
  },
