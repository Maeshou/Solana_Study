[
  {
    "name": "init_lottery",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitLottery >",
      "max_ticket: u32"
    ],
    "attributes": [],
    "body": [
      "let l = & mut ctx . accounts . lottery ;",
      "l . max_ticket = max_ticket ;",
      "l . drawn = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "draw",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Draw >",
      "rand_numbers: Vec < u32 >"
    ],
    "attributes": [],
    "body": [
      "let l = & mut ctx . accounts . lottery ;",
      "if ! l . drawn",
      "{",
      "let mut winner = 0u32 ;",
      "for & r in rand_numbers . iter ()",
      "{",
      "if r < l . max_ticket { winner = r ;",
      "break ;",
      "} } l . winner = winner ;",
      "l . drawn = true ;",
      "}",
      "Ok (())"
    ]
  },
