[
  {
    "name": "init_pool",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPool >",
      "fee_bps: u16"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pool ;",
      "p . token_a_reserve = 0 ;",
      "p . token_b_reserve = 0 ;",
      "p . fee_bps = fee_bps ;",
      "Ok (())"
    ]
  },
  {
    "name": "swap",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Swap >",
      "amount_in: u64"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pool ;",
      "let amount_after_fee = amount_in - (amount_in * p . fee_bps as u64 / 10_000) ;",
      "if p . token_a_reserve > 0",
      "{",
      "let amount_out = amount_after_fee * p . token_b_reserve / p . token_a_reserve ;",
      "p . token_a_reserve += amount_after_fee ;",
      "p . token_b_reserve -= amount_out ;",
      "}",
      "Ok (())"
    ]
  },
