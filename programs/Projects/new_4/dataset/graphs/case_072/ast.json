    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ChangePlan >",
      "level: u8"
    ],
    "attributes": [],
    "body": [
      "let new_plan = match level { 1 => 100 , 2 => 200 , _ => 0 , } ;",
      "if new_plan == 0",
      "{",
      "return Ok (()) ;",
      "}",
      "let hist = & mut ctx . accounts . history ;",
      "hist . plan = new_plan ;",
      "Ok (())"
    ]
  },
