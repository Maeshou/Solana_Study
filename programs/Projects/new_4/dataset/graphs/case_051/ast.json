    "attributes": [],
    "body": [
      "let f = & mut ctx . accounts . file ;",
      "require ! (uri . starts_with (\"http\") , ErrorCode6 :: InvalidUri) ;",
      "f . uri = uri ;",
      "f . size = 0 ;",
      "Ok (())"
    ]
