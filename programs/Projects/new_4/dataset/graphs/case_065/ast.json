  {
    "name": "create_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateProfile >",
      "nickname: String"
    ],
    "attributes": [],
    "body": [
      "let prof = & mut ctx . accounts . profile ;",
      "prof . owner = ctx . accounts . user . key () ;",
      "prof . name = nickname ;",
      "Ok (())"
    ]
  },
