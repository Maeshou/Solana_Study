[
  {
    "name": "init_plan",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPlan >",
      "tier: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . plan ;",
      "p . tier = tier ;",
      "p . active = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "upgrade_plan",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPlan >",
      "new_tier: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . plan ;",
      "require ! (new_tier > p . tier , ErrorCode9 :: InvalidTier) ;",
      "p . tier = new_tier ;",
      "Ok (())"
    ]
  },
  {
    "name": "record_usage",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPlan >",
      "user: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . usage_log ;",
      "log . users . push (user) ;",
      "Ok (())"
    ]
  },
