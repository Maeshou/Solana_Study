  },
  {
    "name": "toggle_feature",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitFeat >"
    ],
    "attributes": [],
    "body": [
      "let f = & mut ctx . accounts . feature ;",
      "f . enabled = ! f . enabled ;",
      "Ok (())"
    ]
  },
  {
