  {
    "name": "init_document",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitDocument >",
      "cid: String"
    ],
    "attributes": [],
    "body": [
      "let doc = & mut ctx . accounts . document ;",
      "doc . cid = cid . clone () ;",
      "doc . owner = ctx . accounts . author . key () ;",
      "let sigs = & mut ctx . accounts . signatures ;",
      "sigs . signers = Vec :: new () ;",
      "let log = & mut ctx . accounts . verification_log ;",
      "log . entries = Vec :: new () ;",
      "log . entries . push (format ! (\"Document {} created\" , cid)) ;",
      "Ok (())"
    ]
  },
