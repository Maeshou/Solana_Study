[
  {
    "name": "init_schedule",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSchedule >",
      "cron: String"
    ],
    "attributes": [],
    "body": [
      "let ts = & mut ctx . accounts . schedule ;",
      "ts . cron = cron ;",
      "ts . run_count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "execute_task",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSchedule >"
    ],
    "attributes": [],
    "body": [
      "let ts = & mut ctx . accounts . schedule ;",
      "ts . run_count = ts . run_count . checked_add (1) . unwrap () ;",
      "let log = & mut ctx . accounts . execution_log ;",
      "log . counts . push (ts . run_count) ;",
      "Ok (())"
    ]
  },
