  {
    "name": "register_doc",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterDoc >",
      "cid: String"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . document ;",
      "d . cid = cid ;",
      "d . signatures = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "sign_doc",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SignDoc >",
      "pubkeys: Vec < Pubkey >"
    ],
    "attributes": [],
    "body": [
      "let d = & mut ctx . accounts . document ;",
      "for pk in pubkeys . iter ()",
      "{",
      "if * pk != Pubkey :: default ()",
      "{",
      "d . signatures += 1 ;",
      "}",
      "}",
      "Ok (())"
    ]
  },
