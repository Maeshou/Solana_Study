    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitRoom >",
      "msg: String"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . message_log ;",
      "log . messages . push (msg) ;",
      "Ok (())"
    ]
  },
