  {
    "name": "init_room",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitRoom >",
      "topic: String"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . room ;",
      "r . topic = topic ;",
      "r . participants = Vec :: new () ;",
      "Ok (())"
    ]
  },
  {
    "name": "join_room",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitRoom >",
      "user: Pubkey"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . room ;",
      "r . participants . push (user) ;",
      "Ok (())"
    ]
  },
  {
