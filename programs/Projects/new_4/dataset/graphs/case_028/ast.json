[
  {
    "name": "init_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >",
      "token: String",
      "ttl: i64"
    ],
    "attributes": [],
    "body": [
      "let ss = & mut ctx . accounts . session ;",
      "ss . token = token ;",
      "ss . active = true ;",
      "ss . expires_at = Clock :: get () ? . unix_timestamp + ttl ;",
      "Ok (())"
    ]
  },
  {
    "name": "refresh_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >"
    ],
    "attributes": [],
    "body": [
      "let ss = & mut ctx . accounts . session ;",
      "require ! (ss . active , ErrorCode :: SessionInactive) ;",
      "ss . expires_at = Clock :: get () ? . unix_timestamp + (ss . expires_at - ss . created_at) ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_timestamp",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >",
      "ts: i64"
    ],
    "attributes": [],
    "body": [
      "let tl = & mut ctx . accounts . time_log ;",
      "tl . timestamps . push (ts) ;",
      "if tl . timestamps . len () > 100",
      "{",
      "tl . timestamps . remove (0) ;",
      "}",
      "Ok (())"
    ]
  },
  {
