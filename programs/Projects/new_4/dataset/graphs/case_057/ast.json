[
  {
    "name": "start_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartSession >",
      "session_id: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . session ;",
      "s . session_id = session_id ;",
      "s . active = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "end_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartSession >"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . session ;",
      "s . active = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "record_score",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartSession >",
      "player: Pubkey",
      "score: u32"
    ],
    "attributes": [],
    "body": [
      "require ! (score <= 1_000_000 , ErrorCode2 :: ExcessiveScore) ;",
      "let hist = & mut ctx . accounts . score_history ;",
      "hist . entries . push ((player , score)) ;",
      "Ok (())"
    ]
  },
