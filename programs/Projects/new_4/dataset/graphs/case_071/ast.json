[
  {
    "name": "start_auction",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartAuction >",
      "item_id: u64"
    ],
    "attributes": [],
    "body": [
      "let a = & mut ctx . accounts . auction ;",
      "a . item = item_id ;",
      "a . highest_bid = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "process_bids",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessBids >",
      "bids: Vec < u64 >"
    ],
    "attributes": [],
    "body": [
      "let mut max = ctx . accounts . auction . highest_bid ;",
      "for & b in bids . iter ()",
      "{",
      "if b > max",
      "{",
      "max = b ;",
      "}",
      "}",
      "let rec = & mut ctx . accounts . auction_record ;",
      "rec . bid = max ;",
      "Ok (())"
    ]
  },
