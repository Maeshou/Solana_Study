[
  {
    "name": "create_proposal",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateProposal >",
      "description: String"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . proposal ;",
      "p . proposer = ctx . accounts . proposer . key () ;",
      "p . description = description ;",
      "p . for_votes = 0 ;",
      "p . against_votes = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "tally",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Tally >",
      "votes: Vec < bool >"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . proposal ;",
      "let mut for_count = 0u32 ;",
      "let mut against_count = 0u32 ;",
      "for & v in votes . iter ()",
      "{",
      "if v",
      "{",
      "for_count += 1 ;",
      "} else",
      "{",
      "against_count += 1 ;",
      "}",
      "}",
      "p . for_votes = for_count ;",
      "p . against_votes = against_count ;",
      "Ok (())"
    ]
  },
