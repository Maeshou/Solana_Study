[
  {
    "name": "init_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >",
      "token: String"
    ],
    "attributes": [],
    "body": [
      "let ss = & mut ctx . accounts . session ;",
      "ss . token = token ;",
      "ss . active = true ;",
      "ss . uses = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "use_session",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >"
    ],
    "attributes": [],
    "body": [
      "let ss = & mut ctx . accounts . session ;",
      "require ! (ss . active , ErrorCode3 :: Inactive) ;",
      "ss . uses = ss . uses . checked_add (1) . unwrap () ;",
      "if ss . uses >= 5",
      "{",
      "ss . active = false ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "log_try",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSession >",
      "info: String"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . time_log ;",
      "if log . tries . len () >= 20",
      "{",
      "log . tries . remove (0) ;",
      "}",
      "log . tries . push (info) ;",
      "Ok (())"
    ]
  },
