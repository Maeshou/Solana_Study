[
  {
    "name": "initialize_counter",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeCounter >"
    ],
    "attributes": [],
    "body": [
      "let counter = & mut ctx . accounts . counter ;",
      "counter . value = 0 ;",
      "let cp = & mut ctx . accounts . checkpoint ;",
      "cp . last_value = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "increment",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < IncrementCounter >"
    ],
    "attributes": [],
    "body": [
      "let counter = & mut ctx . accounts . counter ;",
      "let old = counter . value ;",
      "counter . value = old + 1 ;",
      "let cp = & mut ctx . accounts . checkpoint ;",
      "cp . last_value = old ;",
      "Ok (())"
    ]
  },
