[
  {
    "name": "init_vault",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitVault >",
      "bump: u8"
    ],
    "attributes": [],
    "body": [
      "let v = & mut ctx . accounts . vault ;",
      "v . authority = ctx . accounts . authority . key () ;",
      "v . bump = bump ;",
      "v . total_deposits = 0 ;",
      "v . last_update = Clock :: get () ? . unix_timestamp ;",
      "Ok (())"
    ]
  },
  {
    "name": "deposit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Deposit >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let v = & mut ctx . accounts . vault ;",
      "v . total_deposits += amount ;",
      "v . last_update = Clock :: get () ? . unix_timestamp ;",
      "let dr = & mut ctx . accounts . deposit_record ;",
      "dr . user = ctx . accounts . user . key () ;",
      "dr . amount = amount ;",
      "Ok (())"
    ]
  },
