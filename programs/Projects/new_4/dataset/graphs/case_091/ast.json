[
  {
    "name": "book_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < BookItem >",
      "item_id: u64",
      "until: i64"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . booking ;",
      "b . item_id = item_id ;",
      "b . user = ctx . accounts . user . key () ;",
      "b . expires = until ;",
      "b . reviewed = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_review",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddReview >",
      "rating: u8",
      "comment: String"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . booking ;",
      "if ! b . reviewed && rating > 0",
      "{",
      "b . rating = rating ;",
      "b . comment = comment ;",
      "b . reviewed = true ;",
      "b . reviewed_at = Clock :: get () ? . unix_timestamp ;",
      "}",
      "Ok (())"
    ]
  },
