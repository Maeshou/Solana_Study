[
  {
    "name": "create_post",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreatePost >",
      "content: String"
    ],
    "attributes": [],
    "body": [
      "require ! (content . len () <= 280 , ErrorCode3 :: TooLong) ;",
      "let post = & mut ctx . accounts . post ;",
      "post . content = content ;",
      "post . likes = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "like_post",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreatePost >"
    ],
    "attributes": [],
    "body": [
      "let post = & mut ctx . accounts . post ;",
      "post . likes = post . likes . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "comment_post",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreatePost >",
      "comment: String"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . comment_log ;",
      "log . comments . push (comment) ;",
      "Ok (())"
    ]
  },
