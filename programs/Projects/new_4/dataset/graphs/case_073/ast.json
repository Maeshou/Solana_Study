[
  {
    "name": "start_project",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartProject >",
      "goal: u64"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . project ;",
      "p . goal = goal ;",
      "p . raised = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_support",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddSupport >",
      "amounts: Vec < u64 >"
    ],
    "attributes": [],
    "body": [
      "let mut total = ctx . accounts . project . raised ;",
      "let mut i = 0 ;",
      "while i < amounts . len ()",
      "{",
      "if amounts [i] > 0",
      "{",
      "total += amounts [i] ;",
      "} i += 1 ;",
      "}",
      "let sup = & mut ctx . accounts . supporters ;",
      "sup . total = total ;",
      "Ok (())"
    ]
  },
