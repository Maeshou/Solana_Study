[
  {
    "name": "process_readings",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessReadings >",
      "readings: Vec < u32 >"
    ],
    "attributes": [],
    "body": [
      "let mut sum = 0u32 ;",
      "for & r in readings . iter ()",
      "{",
      "if r > 0",
      "{",
      "sum += r ;",
      "}",
      "}",
      "let sensor = & mut ctx . accounts . sensor ;",
      "sensor . last_sum = sum ;",
      "let log = & mut ctx . accounts . reading_log ;",
      "log . count = readings . len () as u32 ;",
      "Ok (())"
    ]
  },
