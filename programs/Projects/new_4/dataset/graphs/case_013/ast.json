[
  {
    "name": "create_record",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateRecord >",
      "id: u64",
      "description: String"
    ],
    "attributes": [],
    "body": [
      "let record = & mut ctx . accounts . record ;",
      "record . id = id ;",
      "record . description = description ;",
      "record . processed = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "complete_record",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyRecord >"
    ],
    "attributes": [],
    "body": [
      "let record = & mut ctx . accounts . record ;",
      "record . processed = true ;",
      "let log = & mut ctx . accounts . log_account ;",
      "log . data . push_str (\"completed;\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "reset_record",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyRecord >"
    ],
    "attributes": [],
    "body": [
      "let record = & mut ctx . accounts . record ;",
      "record . description . clear () ;",
      "record . processed = false ;",
      "Ok (())"
    ]
  },
