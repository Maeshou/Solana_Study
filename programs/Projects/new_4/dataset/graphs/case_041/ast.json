[
  {
    "name": "setup_analysis",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetupAnalysis >",
      "metric: f64"
    ],
    "attributes": [],
    "body": [
      "let an = & mut ctx . accounts . analytics ;",
      "an . metric = metric ;",
      "an . version = 1 ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_metric",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetupAnalysis >",
      "new_metric: f64"
    ],
    "attributes": [],
    "body": [
      "let an = & mut ctx . accounts . analytics ;",
      "an . metric = new_metric ;",
      "an . version = an . version . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_cache",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SetupAnalysis >",
      "label: String"
    ],
    "attributes": [],
    "body": [
      "let ch = & mut ctx . accounts . cache_log ;",
      "if ch . labels . len () >= 5",
      "{",
      "ch . labels . remove (0) ;",
      "}",
      "ch . labels . push (label) ;",
      "Ok (())"
    ]
  },
