[
  {
    "name": "init_chat",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitChat >",
      "topic: String"
    ],
    "attributes": [],
    "body": [
      "let cr = & mut ctx . accounts . chat_room ;",
      "cr . topic = topic ;",
      "cr . msg_count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "send_messages",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SendMessages >",
      "msgs: Vec < String >"
    ],
    "attributes": [],
    "body": [
      "let cr = & mut ctx . accounts . chat_room ;",
      "let pc = & mut ctx . accounts . parsed_cmds ;",
      "pc . count = 0 ;",
      "for m in msgs . iter ()",
      "{",
      "if m . len () > 0",
      "{",
      "cr . msg_count += 1 ;",
      "if m . starts_with (\"/\")",
      "{",
      "pc . count += 1 ;",
      "} }",
      "}",
      "Ok (())"
    ]
  },
