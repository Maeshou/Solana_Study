[
  {
    "name": "issue_coupon",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < IssueCoupon >",
      "code: String",
      "discount: u8"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . coupon ;",
      "c . code = code ;",
      "c . discount = discount ;",
      "c . used = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "redeem_coupon",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RedeemCoupon >"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . coupon ;",
      "if ! c . used",
      "{",
      "c . used = true ;",
      "c . redeemed_at = Clock :: get () ? . unix_timestamp ;",
      "}",
      "Ok (())"
    ]
  },
