[
  {
    "name": "register_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyRegistry >",
      "key: String",
      "value: u64"
    ],
    "attributes": [],
    "body": [
      "let reg = & mut ctx . accounts . registry ;",
      "reg . key = key ;",
      "reg . value = value ;",
      "reg . count = reg . count + 1 ;",
      "Ok (())"
    ]
  },
  {
    "name": "unregister_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyRegistry >"
    ],
    "attributes": [],
    "body": [
      "let reg = & mut ctx . accounts . registry ;",
      "reg . key . clear () ;",
      "reg . value = 0 ;",
      "Ok (())"
    ]
  },
