[
  {
    "name": "init_device",
    "inputs": [
      "ctx: Context < InitDevice >",
      "device_id: String",
      "thresholds: Vec < u32 >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let dev = & mut ctx . accounts . device ;"
      },
      {
        "id": 2,
        "label": "dev . id = device_id . clone () ;"
      },
      {
        "id": 3,
        "label": "dev . registered_at = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 4,
        "label": "let telem = & mut ctx . accounts . telemetry ;"
      },
      {
        "id": 5,
        "label": "telem . values = Vec :: new () ;"
      },
      {
        "id": 6,
        "label": "let alert = & mut ctx . accounts . alert_cfg ;"
      },
      {
        "id": 7,
        "label": "alert . levels = thresholds . clone () ;"
      },
      {
        "id": 8,
        "label": "alert . triggered = false ;"
      },
      {
        "id": 9,
        "label": "for & t in thresholds . iter () { telem . values . push (t) ; }"
      },
      {
        "id": 10,
        "label": "telem . values . push (t) ;"
      },
      {
        "id": 11,
        "label": "if let Some (& first) = telem . values . first () { if first > thresholds [0] { alert . triggered = true ; } }"
      },
      {
        "id": 12,
        "label": "if first > thresholds [0] { alert . triggered = true ; }"
      },
      {
        "id": 13,
        "label": "alert . triggered = true ;"
      },
      {
        "id": 14,
        "label": "Ok (())"
      }
    ],
