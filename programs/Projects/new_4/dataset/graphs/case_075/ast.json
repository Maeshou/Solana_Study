[
  {
    "name": "create_list",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateList >"
    ],
    "attributes": [],
    "body": [
      "let tl = & mut ctx . accounts . task_list ;",
      "tl . total = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "complete_tasks",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CompleteTasks >",
      "statuses: Vec < bool >"
    ],
    "attributes": [],
    "body": [
      "let tl = & mut ctx . accounts . task_list ;",
      "let fc = & mut ctx . accounts . fail_count ;",
      "fc . count = 0 ;",
      "for & s in statuses . iter ()",
      "{",
      "if s",
      "{",
      "tl . total += 1 ;",
      "} else",
      "{",
      "fc . count += 1 ;",
      "}",
      "}",
      "Ok (())"
    ]
  },
