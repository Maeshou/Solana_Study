  {
    "name": "init_all",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitAll >",
      "mode: u8"
    ],
    "attributes": [
      "# [doc = \" 全アカウントを初期化するつもりが、settings_account の init が漏れている例\"]"
    ],
    "body": [
      "let cfg = & mut ctx . accounts . config_account ;",
      "cfg . mode = mode ;",
      "cfg . version = 1 ;",
      "let settings = & mut ctx . accounts . settings_account ;",
      "settings . theme = \"default\" . to_string () ;",
      "settings . notifications = true ;",
      "Ok (())"
    ]
  },
