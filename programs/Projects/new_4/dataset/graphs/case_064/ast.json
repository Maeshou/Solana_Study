[
  {
    "name": "init_craft",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCraft >",
      "recipe_id: u32"
    ],
    "attributes": [],
    "body": [
      "let craft = & mut ctx . accounts . craft_account ;",
      "craft . recipe_id = recipe_id ;",
      "craft . status = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "execute_craft",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCraft >",
      "outcome: u8"
    ],
    "attributes": [],
    "body": [
      "let craft = & mut ctx . accounts . craft_account ;",
      "craft . status = outcome ;",
      "Ok (())"
    ]
  },
  {
    "name": "log_craft",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCraft >",
      "used: Vec < u32 >"
    ],
    "attributes": [],
    "body": [
      "let log = & mut ctx . accounts . craft_log ;",
      "log . materials . insert (0 , used) ;",
      "if log . materials . len () > 10",
      "{",
      "log . materials . pop () ;",
      "}",
      "Ok (())"
    ]
  },
