  {
    "name": "create_room",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateRoom >",
      "topic: String"
    ],
    "attributes": [],
    "body": [
      "let room = & mut ctx . accounts . room ;",
      "room . topic = topic ;",
      "room . count = 0 ;",
      "Ok (())"
    ]
  },
  {
