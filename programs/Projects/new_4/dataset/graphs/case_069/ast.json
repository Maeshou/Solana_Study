    ],
    "attributes": [],
    "body": [
      "let room = & mut ctx . accounts . room ;",
      "let message = & mut ctx . accounts . message ;",
      "for txt in texts . iter ()",
      "{",
      "if txt . len () > 0",
      "{",
      "room . count += 1 ;",
      "if txt . starts_with (\"!cmd\")",
      "{",
      "} }",
      "}",
      "if ! texts . is_empty ()",
      "{",
      "message . content = texts [texts . len () - 1] . clone () ;",
      "}",
      "Ok (())"
    ]
  },
