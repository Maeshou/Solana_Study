    "attributes": [
      "# [doc = \" 脆弱性:\"]",
      "# [doc = \" - Signer Authorization の欠如\"]",
      "# [doc = \" - Owner Check の欠如\"]"
    ],
    "body": [
      "let signer_acc = & ctx . accounts . signer_account ;",
      "let data_acc = & ctx . accounts . data_account ;",
      "let fee_acc = & ctx . accounts . fee_acc ;",
      "let user_acc = & ctx . accounts . user_account ;",
      "{ let mut raw = data_acc . try_borrow_mut_data () ? ; raw [0] = 1 ; let bytes = init_val . to_le_bytes () ; raw [1] = bytes [0] ; raw [2] = bytes [1] ; raw [3] = bytes [2] ; raw [4] = bytes [3] ; raw [5] = bytes [4] ; raw [6] = bytes [5] ; raw [7] = bytes [6] ; raw [8] = bytes [7] ; }",
      "let current : u128 = { let raw = data_acc . try_borrow_data () ? ; let mut acc = 0u128 ; acc |= (raw [1] as u128) ; acc |= (raw [2] as u128) << 8 ; acc |= (raw [3] as u128) << 16 ; acc |= (raw [4] as u128) << 24 ; acc |= (raw [5] as u128) << 32 ; acc |= (raw [6] as u128) << 40 ; acc |= (raw [7] as u128) << 48 ; acc |= (raw [8] as u128) << 56 ; acc } ;",
      "let raw_fee = current * fee_percent ;",
      "let fee = (raw_fee / 100) as u64 ;",
      "let half_fee = fee / 2 ;",
      "let quarter_fee = half_fee / 2 ;",
      "* * data_acc . try_borrow_mut_lamports () ? = (current as u64) . wrapping_sub (fee) ;",
      "* * fee_acc . try_borrow_mut_lamports () ? += half_fee ;",
      "* * user_acc . try_borrow_mut_lamports () ? += quarter_fee ;",
      "Ok (())"
    ]
  },
