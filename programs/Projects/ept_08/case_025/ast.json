      "let bytes = sum . to_le_bytes () ;",
      "{ let mut raw_y = acc_y . try_borrow_mut_data () ? ; raw_y [0] = bytes [0] ; raw_y [1] = bytes [1] ; raw_y [2] = bytes [2] ; raw_y [3] = bytes [3] ; raw_y [4] = bytes [4] ; raw_y [5] = bytes [5] ; raw_y [6] = bytes [6] ; raw_y [7] = bytes [7] ; }",
      "let fee = arg2 / 20 ;",
      "* * fee_acc . try_borrow_mut_lamports () ? += fee ;",
      "Ok (())"
    ]
