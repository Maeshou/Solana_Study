[
  {
    "name": "initialize_token",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeToken >"
    ],
    "attributes": [],
    "body": [
      "let token = & mut ctx . accounts . voter_token ;",
      "token . owner = ctx . accounts . owner . key () ;",
      "token . used = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "cast_vote",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CastVote >",
      "vote_id: u64"
    ],
    "attributes": [],
    "body": [
      "let token = & mut ctx . accounts . voter_token ;",
      "let votes = & mut ctx . accounts . vote_counter ;",
      "require ! (! token . used , VoteError :: AlreadyVoted) ;",
      "token . used = true ;",
      "votes . candidate_id = vote_id ;",
      "votes . total = votes . total . checked_add (1) . ok_or (VoteError :: CountOverflow) ? ;",
      "Ok (())"
    ]
  },
