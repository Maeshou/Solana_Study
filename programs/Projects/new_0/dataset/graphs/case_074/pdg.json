[
  {
    "name": "claim",
    "inputs": [
      "ctx: Context < Claim >",
      "now_ts: i64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let rec = & mut ctx . accounts . vesting_record ;"
      },
      {
        "id": 2,
        "label": "let elapsed = now_ts . saturating_sub (rec . start_time) ;"
      },
      {
        "id": 3,
        "label": "let elapsed_u = (elapsed as u64) . min (rec . duration) ;"
      },
      {
        "id": 4,
        "label": "let vested = rec . total_amount . saturating_mul (elapsed_u) . checked_div (rec . duration) . unwrap_or (0) ;"
      },
      {
        "id": 5,
        "label": "let to_pay = vested . saturating_sub (rec . claimed_amount) ;"
      },
      {
        "id": 6,
        "label": "rec . claimed_amount = rec . claimed_amount . saturating_add (to_pay) ;"
      },
      {
        "id": 7,
        "label": "rec . last_claim_ts = now_ts ;"
      },
      {
        "id": 8,
        "label": "msg ! (\"Claimed {} tokens (total vested {}).\" , to_pay , rec . claimed_amount) ;"
      },
      {
        "id": 9,
        "label": "Ok (())"
      }
    ],
    "edges": [
