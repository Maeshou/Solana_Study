[
  {
    "name": "initialize_registry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeRegistry >",
      "bump: u8"
    ],
    "attributes": [
      "# [doc = \" ドキュメントレジストリアカウントを初期化\"]"
    ],
    "body": [
      "let registry = & mut ctx . accounts . registry ;",
      "registry . owner = * ctx . accounts . user . key ;",
      "registry . bump = bump ;",
      "registry . entries = Vec :: new () ;",
      "Ok (())"
    ]
  },
  {
    "name": "register_doc",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterDoc >",
      "doc_id: [u8 ; 32]"
    ],
    "attributes": [
      "# [doc = \" ドキュメントIDとタイムスタンプを登録\"]"
    ],
    "body": [
      "let registry = & mut ctx . accounts . registry ;",
      "let timestamp = Clock :: get () ? . unix_timestamp ;",
      "registry . entries . push ((doc_id , timestamp)) ;",
      "Ok (())"
    ]
  },
  {
    "name": "clear_registry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClearRegistry >"
    ],
    "attributes": [
      "# [doc = \" 全エントリをクリア\"]"
    ],
    "body": [
      "let registry = & mut ctx . accounts . registry ;",
      "registry . entries . clear () ;",
      "Ok (())"
    ]
  },
