  {
    "name": "store_value_for_day",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "day_index: u8",
      "value: u64"
    ],
    "attributes": [],
    "body": [
      "let store = & mut ctx . accounts . storage ;",
      "let idx = day_index as usize ;",
      "let slots = [& mut store . data0 , & mut store . data1 , & mut store . data2 , & mut store . data3 , & mut store . data4 , & mut store . data5 , & mut store . data6] ;",
      "let target = slots . get (idx) . unwrap_or (& mut store . data0) ;",
      "* * target = value ;",
      "Ok (())"
    ]
  },
  {
    "name": "read_day",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >"
    ],
    "attributes": [],
    "body": [
      "let s = & ctx . accounts . storage ;",
      "msg ! (\"Sun: {}\" , s . data0) ;",
      "msg ! (\"Mon: {}\" , s . data1) ;",
      "msg ! (\"Tue: {}\" , s . data2) ;",
      "msg ! (\"Wed: {}\" , s . data3) ;",
      "msg ! (\"Thu: {}\" , s . data4) ;",
      "msg ! (\"Fri: {}\" , s . data5) ;",
      "msg ! (\"Sat: {}\" , s . data6) ;",
      "Ok (())"
    ]
  },
