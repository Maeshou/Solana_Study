[
  {
    "name": "create_bookmark",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateBookmark >",
      "bump: u8",
      "bookmark_id: u64",
      "title: String",
      "url: String"
    ],
    "attributes": [
      "# [doc = \" ブックマーク作成：URL とタイトルを受け取り、PDA で一意のアカウントを生成\"]"
    ],
    "body": [
      "let bm = & mut ctx . accounts . bookmark ;",
      "bm . owner = ctx . accounts . user . key () ;",
      "bm . bump = bump ;",
      "bm . bookmark_id = bookmark_id ;",
      "bm . title = title ;",
      "bm . url = url ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_bookmark",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateBookmark >",
