[
  {
    "name": "init_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitProfile >",
      "username: String"
    ],
    "attributes": [
      "# [doc = \" プロファイル設定アカウントの作成\"]"
    ],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "require ! (username . len () <= 32 , ErrorCode :: UsernameTooLong) ;",
      "profile . owner = ctx . accounts . user . key () ;",
      "profile . username = username ;",
      "profile . dark_mode = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateProfile >",
      "new_username: String",
      "dark_mode: bool"
    ],
    "attributes": [
      "# [doc = \" プロフィールを更新\"]"
    ],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "require ! (profile . owner == ctx . accounts . user . key () , ErrorCode :: AccessDenied) ;",
      "require ! (new_username . len () <= 32 , ErrorCode :: UsernameTooLong) ;",
      "profile . username = new_username ;",
      "profile . dark_mode = dark_mode ;",
      "Ok (())"
    ]
  },
