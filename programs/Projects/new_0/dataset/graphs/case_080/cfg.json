    "name": "claim_monthly",
    "nodes": [
      {
        "id": 6,
        "label": "Entry"
      },
      {
        "id": 7,
        "label": "let acct = & mut ctx . accounts . subscription ;"
      },
      {
        "id": 8,
        "label": "let months = ((current_ts - acct . start_timestamp) / 2_592_000) as u64 ;"
      },
      {
        "id": 9,
        "label": "let earned = months . saturating_mul (100) ;"
      },
      {
        "id": 10,
        "label": "acct . accumulated = acct . accumulated . saturating_add (earned) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"User {:?} claimed {} tokens ({} months), total = {}\" , acct . user , earned , months , acct . accumulated) ;"
