[
  {
    "name": "assign_title",
    "inputs": [
      "ctx: Context < RankContext >",
      "xp: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let tier = xp / 1000 ;"
      },
      {
        "id": 2,
        "label": "let rank = tier + 1 ;"
      },
      {
        "id": 3,
        "label": "let prefix = \"NoviceMasterEliteLegendRainbow\" ;"
      },
      {
        "id": 4,
        "label": "let len = prefix . len () ;"
      },
      {
        "id": 5,
        "label": "let section = (rank . min (5) as usize) * 6 ;"
      },
      {
        "id": 6,
        "label": "let name = & prefix [section . saturating_sub (6) .. section . min (len)] ;"
      },
      {
        "id": 7,
        "label": "let full_title = format ! (\"{} Lv{}\" , name , rank) ;"
      },
      {
        "id": 8,
        "label": "let reward = 500 + (rank * 120) ;"
      },
      {
        "id": 9,
        "label": "msg ! (\"Assigned Title: {}\" , full_title) ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Reward Points : {}\" , reward) ;"
      },
      {
        "id": 11,
        "label": "Ok (TitleBundle { rank , title : full_title , reward , })"
      }
    ],
