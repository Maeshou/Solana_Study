[
  {
    "name": "calculate_rank_title_reward",
    "inputs": [
      "ctx: Context < UserContext >",
      "activity_score: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let capped_score = core :: cmp :: min (activity_score , 9999) ;"
      },
      {
        "id": 2,
        "label": "let rank = (capped_score / 1000) + 1 ;"
      },
      {
        "id": 3,
        "label": "let base = \"Scholar\" ;"
      },
      {
        "id": 4,
        "label": "let title = format ! (\"{} Lv{}\" , base , rank) ;"
      },
      {
        "id": 5,
        "label": "let is_even = (rank % 2 == 0) as u64 ;"
      },
      {
        "id": 6,
        "label": "let bonus = 250 * is_even + 100 * (1 - is_even) ;"
      },
      {
        "id": 7,
        "label": "let reward_tokens = rank * 100 + bonus ;"
      },
      {
        "id": 8,
        "label": "msg ! (\"--- User Info ---\") ;"
      },
      {
        "id": 9,
        "label": "msg ! (\"Rank         : {}\" , rank) ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Title        : {}\" , title) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Token Reward : {}\" , reward_tokens) ;"
      },
      {
        "id": 12,
        "label": "Ok (OutputBundle { rank , title , reward_tokens , })"
      }
    ],
    "edges": [
