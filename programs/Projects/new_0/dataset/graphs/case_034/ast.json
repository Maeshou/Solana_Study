[
  {
    "name": "store_minimum",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "candidate: u64"
    ],
    "attributes": [],
    "body": [
      "let current = ctx . accounts . storage . value ;",
      "let smaller = (candidate < current) as u64 ;",
      "let larger = 1 - smaller ;",
      "let result = smaller * candidate + larger * current ;",
      "ctx . accounts . storage . value = result ;",
      "Ok (())"
    ]
  },
