[
  {
    "name": "create_contact",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateContact >",
      "name: String",
      "phone: String"
    ],
    "attributes": [],
    "body": [
      "require ! (name . len () <= 64 , ErrorCode :: NameTooLong) ;",
      "require ! (phone . len () <= 32 , ErrorCode :: PhoneTooLong) ;",
      "let contact = & mut ctx . accounts . contact ;",
      "contact . owner = ctx . accounts . user . key () ;",
      "contact . name = name ;",
      "contact . phone = phone ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_contact",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateContact >",
      "name: String",
      "phone: String"
    ],
    "attributes": [],
    "body": [
      "let contact = & mut ctx . accounts . contact ;",
      "require ! (contact . owner == ctx . accounts . user . key () , ErrorCode :: Unauthorized) ;",
      "require ! (name . len () <= 64 , ErrorCode :: NameTooLong) ;",
      "require ! (phone . len () <= 32 , ErrorCode :: PhoneTooLong) ;",
      "contact . name = name ;",
      "contact . phone = phone ;",
      "Ok (())"
    ]
  },
