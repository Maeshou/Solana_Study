[
  {
    "name": "record_study",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StudyCtx >",
      "minutes: u64"
    ],
    "attributes": [],
    "body": [
      "let user = & mut ctx . accounts . study_user ;",
      "user . total_minutes += minutes ;",
      "user . earned_token += minutes ;",
      "Ok (())"
    ]
  },
  {
    "name": "claim_reward",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClaimCtx >"
    ],
    "attributes": [],
    "body": [
      "let user = & mut ctx . accounts . study_user ;",
      "let amount = user . earned_token ;",
      "* * ctx . accounts . vault . to_account_info () . try_borrow_mut_lamports () ? -= amount ;",
      "* * ctx . accounts . user . to_account_info () . try_borrow_mut_lamports () ? += amount ;",
      "user . earned_token = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "show",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StudyCtx >"
    ],
    "attributes": [],
    "body": [
      "let u = & ctx . accounts . study_user ;",
      "msg ! (\"Total Study Minutes: {}\" , u . total_minutes) ;",
      "msg ! (\"Earned Token: {}\" , u . earned_token) ;",
      "Ok (())"
    ]
  },
