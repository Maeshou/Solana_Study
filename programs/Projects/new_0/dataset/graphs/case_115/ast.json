[
  {
    "name": "submit_feedback",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SubmitFeedback >",
      "feedback: String",
      "rating: u8"
    ],
    "attributes": [
      "# [doc = \" フィードバックを投稿\"]"
    ],
    "body": [
      "if feedback . len () > 200",
      "{",
      "return Err (ErrorCode :: FeedbackTooLong . into ()) ;",
      "}",
      "if rating < 1 || rating > 5",
      "{",
      "return Err (ErrorCode :: InvalidRating . into ()) ;",
      "}",
      "let fb = & mut ctx . accounts . feedback ;",
      "fb . owner = ctx . accounts . user . key () ;",
      "fb . feedback = feedback ;",
      "fb . rating = rating ;",
      "Ok (())"
    ]
  },
  {
    "name": "edit_feedback",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < EditFeedback >",
      "feedback: String",
      "rating: u8"
    ],
    "attributes": [
      "# [doc = \" 投稿済みフィードバックを編集\"]"
    ],
    "body": [
      "if feedback . len () > 200",
      "{",
      "return Err (ErrorCode :: FeedbackTooLong . into ()) ;",
      "}",
      "if rating < 1 || rating > 5",
      "{",
      "return Err (ErrorCode :: InvalidRating . into ()) ;",
      "}",
      "let fb = & mut ctx . accounts . feedback ;",
      "if fb . owner != ctx . accounts . user . key ()",
      "{",
      "return Err (ErrorCode :: Unauthorized . into ()) ;",
      "}",
      "fb . feedback = feedback ;",
      "fb . rating = rating ;",
      "Ok (())"
    ]
  },
