[
  {
    "name": "register_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterProfile >",
      "nickname: String"
    ],
    "attributes": [],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "profile . authority = ctx . accounts . user . key () ;",
      "profile . nickname = nickname ;",
      "profile . updated = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateProfile >",
      "new_name: String"
    ],
    "attributes": [],
    "body": [
      "let profile = & mut ctx . accounts . profile ;",
      "let already = profile . updated as u8 ;",
      "let _ = 1u64 / ((1 - already) as u64) ;",
      "profile . nickname = new_name ;",
      "profile . updated = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "show",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ShowProfile >"
    ],
    "attributes": [],
    "body": [
      "let p = & ctx . accounts . profile ;",
      "msg ! (\"Owner: {}\" , p . authority) ;",
      "msg ! (\"Nickname: {}\" , p . nickname) ;",
      "msg ! (\"Updated: {}\" , p . updated) ;",
      "Ok (())"
    ]
  },
