[
  {
    "name": "init_settings",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSettings >"
    ],
    "attributes": [
      "# [doc = \" 通知設定アカウントの初期化\"]"
    ],
    "body": [
      "let settings = & mut ctx . accounts . settings ;",
      "settings . owner = ctx . accounts . user . key () ;",
      "settings . email = false ;",
      "settings . sms = false ;",
      "settings . push = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_settings",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateSettings >",
      "email: bool",
      "sms: bool",
      "push: bool"
    ],
    "attributes": [
      "# [doc = \" 通知設定を更新\"]"
    ],
    "body": [
      "let settings = & mut ctx . accounts . settings ;",
      "require ! (settings . owner == ctx . accounts . user . key () , ErrorCode :: Unauthorized) ;",
      "settings . email = email ;",
      "settings . sms = sms ;",
      "settings . push = push ;",
      "Ok (())"
    ]
  },
