[
  {
    "name": "register_title",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterTitle >",
      "title: String"
    ],
    "attributes": [],
    "body": [
      "let account = & mut ctx . accounts . title_account ;",
      "account . owner = ctx . accounts . user . key () ;",
      "account . title = title ;",
      "account . claimed = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_title",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterTitle >",
      "new_title: String"
    ],
    "attributes": [],
    "body": [
      "let account = & mut ctx . accounts . title_account ;",
      "let already = account . claimed as u8 ;",
      "let _ = 1u64 / ((1 - already) as u64) ;",
      "account . title = new_title ;",
      "account . claimed = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "view_title",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterTitle >"
    ],
    "attributes": [],
    "body": [
      "let a = & ctx . accounts . title_account ;",
      "msg ! (\"Owner: {}\" , a . owner) ;",
      "msg ! (\"Title: {}\" , a . title) ;",
      "msg ! (\"Claimed: {}\" , a . claimed) ;",
      "Ok (())"
    ]
  },
