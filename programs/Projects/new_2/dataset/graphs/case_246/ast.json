[
  {
    "name": "draw",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx3 >"
    ],
    "attributes": [],
    "body": [
      "let seed = Clock :: get () ? . unix_timestamp as u64 ;",
      "ctx . accounts . result_buf . data . borrow_mut () [.. 8] . copy_from_slice (& seed . to_le_bytes ()) ;",
      "let st = & mut ctx . accounts . draw_state ;",
      "st . last_seed = seed ;",
      "st . counter = st . counter . saturating_add (1) ;",
      "Ok (())"
    ]
  },
