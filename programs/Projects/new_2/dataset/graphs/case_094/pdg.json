      },
      {
        "id": 3,
        "label": "let o = offered_nfts . len () . min (8) as u8 ;"
      },
      {
        "id": 4,
        "label": "let w = wanted_nfts . len () . min (8) as u8 ;"
      },
      {
        "id": 5,
        "label": "let mut record = Vec :: with_capacity (1 + 32 * (o as usize) + 1 + 32 * (w as usize) + 32) ;"
      },
      {
        "id": 6,
        "label": "record . push (o) ;"
      },
      {
        "id": 7,
        "label": "for & pk in offered_nfts . iter () . take (o as usize) { record . extend_from_slice (& pk . to_bytes ()) ; }"
      },
      {
        "id": 8,
        "label": "record . extend_from_slice (& pk . to_bytes ()) ;"
      },
      {
        "id": 9,
        "label": "record . push (w) ;"
      },
      {
        "id": 10,
        "label": "for & pk in wanted_nfts . iter () . take (w as usize) { record . extend_from_slice (& pk . to_bytes ()) ; }"
      },
      {
        "id": 11,
        "label": "record . extend_from_slice (& pk . to_bytes ()) ;"
      },
      {
        "id": 12,
        "label": "record . extend_from_slice (ctx . accounts . proposer . key . as_ref ()) ;"
      },
      {
        "id": 13,
        "label": "if buf . len () < record . len () { return err ! (ErrorCode :: DataTooShort) ; }"
      },
      {
        "id": 14,
        "label": "return err ! (ErrorCode :: DataTooShort) ;"
      },
      {
        "id": 15,
        "label": "buf . iter_mut () . zip (record . iter ()) . for_each (| (dst , & src) | * dst = src) ;"
      },
      {
        "id": 16,
        "label": "msg ! (\"Offer {} created by {}: offers {} items, wants {} items\" , acct . key () , ctx . accounts . proposer . key () , o , w) ;"
