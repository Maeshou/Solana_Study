  {
    "name": "toggle_feature",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx1 >"
    ],
    "attributes": [],
    "body": [
      "let mut buf = ctx . accounts . diagnostic_buf . data . borrow_mut () ;",
      "buf [0] ^= 1 ;",
      "let flag = & mut ctx . accounts . feature_flag ;",
      "flag . enabled = ! flag . enabled ;",
      "flag . toggle_count = flag . toggle_count . saturating_add (1) ;",
      "Ok (())"
    ]
