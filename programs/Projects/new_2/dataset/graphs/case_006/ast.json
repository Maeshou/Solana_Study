[
  {
    "name": "full_entry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < FullEntryCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 完全ガード：has_one + signer で所有権＋署名を検証\"]"
    ],
    "body": [
      "let p = ctx . accounts . protected_vault . to_account_info () ;",
      "* * p . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"full_entry 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "utility_entry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UtilityEntryCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 署名のみチェック：ヘルパー関数経由でラムポーツを更新（所有権チェックなし）\"]"
    ],
    "body": [
      "apply_delta (& ctx . accounts . blob_vault , amount) ? ;",
      "msg ! (\"utility_entry 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "unpack_entry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UnpackEntryCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" アンパック方式：Context 構造体を分解せず vault フィールドをそのまま操作（署名不要、所有権チェックあり）\"]"
    ],
    "body": [
      "let info = ctx . accounts . vault . to_account_info () ;",
      "* * info . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"unpack_entry 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "raw_entry",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RawEntryCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 無制限アクセス：raw AccountInfo に直接加算（誰でも実行可能）\"]"
    ],
    "body": [
      "{ let raw = & ctx . accounts . any_vault ; * * raw . try_borrow_mut_lamports () ? += amount ; }",
      "msg ! (\"raw_entry 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "apply_delta",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "account: & AccountInfo",
      "delta: u64"
    ],
    "attributes": [
      "# [doc = \" ラムポーツ更新用ヘルパー\"]"
    ],
    "body": [
      "* * account . try_borrow_mut_lamports () ? += delta ;",
      "Ok (())"
    ]
  },
