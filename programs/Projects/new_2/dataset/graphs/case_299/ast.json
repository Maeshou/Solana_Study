[
  {
    "name": "log_event",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < LogEvent >",
      "code: u8",
      "times: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . timeline . try_borrow_mut_data () ? ;",
      "let mut i = 0 ;",
      "while i < times as usize && i < buf . len ()",
      "{",
      "buf [i] = code ;",
      "i += 1 ;",
      "}",
      "msg ! (\"ロガー {} がイベント登録 (code={}, times={})\" , ctx . accounts . logger . key () , code , times) ;",
      "Ok (())"
    ]
  },
  {
    "name": "clear_timeline",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClearTimeline >"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . timeline . try_borrow_mut_data () ? ;",
      "for b in buf . iter_mut ()",
      "{",
      "* b = 0 ;",
      "}",
      "msg ! (\"ロガー {} がタイムラインクリア\" , ctx . accounts . logger . key ()) ;",
      "Ok (())"
    ]
  },
