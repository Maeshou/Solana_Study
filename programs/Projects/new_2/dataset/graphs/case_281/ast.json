    "body": [
      "let buf = & mut ctx . accounts . data . try_borrow_mut_data () ? ;",
      "if seed % 2 == 0",
      "{",
      "buf [0] = seed ;",
      "} else",
      "{",
      "buf [0] = seed . wrapping_mul (2) ;",
      "}",
      "msg ! (\"認可者 {} が初期化実行\" , ctx . accounts . auth . key ()) ;",
      "Ok (())"
