  {
    "name": "add_item",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddItem >",
      "item_id: u32",
      "category: String"
    ],
    "attributes": [],
    "body": [
      "let inv = & mut ctx . accounts . inventory ;",
      "inv . items . push (item_id) ;",
      "inv . categories . push (category . clone ()) ;",
      "inv . events . push (Event { actor : ctx . accounts . manager . key () , description : category , }) ;",
      "msg ! (\"Item {} added to inventory by {}\" , item_id , ctx . accounts . manager . key ()) ;",
      "Ok (())"
    ]
  },
