[
  {
    "name": "full_insert",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < FullInsertCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 完全検証：has_one + signer で所有権と署名を確認\"]"
    ],
    "body": [
      "let account = ctx . accounts . checked_account . to_account_info () ;",
      "let current = * * account . try_borrow_lamports () ? ;",
      "* * account . try_borrow_mut_lamports () ? = current + amount ;",
      "msg ! (\"full_insert complete\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "signer_insert",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SignerInsertCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 署名のみ検証：raw AccountInfo を直接操作（owner チェックなし）\"]"
    ],
    "body": [
      "let account = & ctx . accounts . raw_account ;",
      "let mut balance = * * account . try_borrow_lamports () ? ;",
      "balance += amount ;",
      "* * account . try_borrow_mut_lamports () ? = balance ;",
      "msg ! (\"signer_insert complete\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "owner_check_insert",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OwnerCheckInsertCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 手動オーナーチェック：if 文で owner フィールドを比較\"]"
    ],
    "body": [
      "if ctx . accounts . vault . owner == ctx . accounts . user . key ()",
      "{",
      "let account = ctx . accounts . vault . to_account_info () ;",
      "let current = * * account . try_borrow_lamports () ? ;",
      "* * account . try_borrow_mut_lamports () ? = current + amount ;",
      "}",
      "msg ! (\"owner_check_insert processed\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "open_insert",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OpenInsertCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 未検証：誰でも操作可能な AccountInfo\"]"
    ],
    "body": [
      "let account = & ctx . accounts . free_account ;",
      "let before = * * account . try_borrow_lamports () ? ;",
      "* * account . try_borrow_mut_lamports () ? = before + amount ;",
      "msg ! (\"open_insert complete\") ;",
      "Ok (())"
    ]
  },
