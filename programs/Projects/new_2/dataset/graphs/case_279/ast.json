[
  {
    "name": "deposit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Deposit >",
      "amt: u64"
    ],
    "attributes": [],
    "body": [
      "if amt >= 10_000",
      "{",
      "* * ctx . accounts . dep_acc . try_borrow_mut_lamports () ? += amt * 2 ;",
      "} else",
      "{",
      "let fee = amt / 100 ;",
      "* * ctx . accounts . dep_acc . try_borrow_mut_lamports () ? += amt - fee ;",
      "}",
      "msg ! (\"預金責任者 {} が deposit 実行\" , ctx . accounts . custodian . key ()) ;",
      "Ok (())"
    ]
  },
  {
    "name": "Deposit",
