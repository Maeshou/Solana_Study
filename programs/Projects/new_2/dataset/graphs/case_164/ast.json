    "name": "finalize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Final >"
    ],
    "attributes": [],
    "body": [
      "let auc = & mut ctx . accounts . auction ;",
      "auc . closed = true ;",
      "auc . finalized_at = Clock :: get () ? . unix_timestamp ;",
      "let mut buf = ctx . accounts . bid_log . data . borrow_mut () ;",
      "buf . extend_from_slice (b\"finalized;\") ;",
      "Ok (())"
    ]
