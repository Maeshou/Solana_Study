[
  {
    "name": "toggle_feature",
    "inputs": [
      "ctx: Context < ToggleFeature >",
      "feature_index: u8"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let acct = & mut ctx . accounts . feature_account . to_account_info () ;"
      },
      {
        "id": 2,
        "label": "let mut data = acct . data . borrow_mut () ;"
      },
      {
        "id": 3,
        "label": "if data . len () < 4 { return err ! (ErrorCode :: DataTooSmall) ; }"
      },
      {
        "id": 4,
        "label": "return err ! (ErrorCode :: DataTooSmall) ;"
      },
      {
        "id": 5,
        "label": "let mut mask_bytes = [0u8 ; 4] ;"
      },
      {
        "id": 6,
        "label": "mask_bytes . copy_from_slice (& data [0 .. 4]) ;"
      },
      {
        "id": 7,
        "label": "let mut mask = u32 :: from_le_bytes (mask_bytes) ;"
      },
      {
        "id": 8,
        "label": "let idx = (feature_index . min (31)) as u32 ;"
      },
      {
        "id": 9,
        "label": "mask ^= 1 << idx ;"
      },
      {
        "id": 10,
        "label": "data [0 .. 4] . copy_from_slice (& mask . to_le_bytes ()) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Toggled feature {}: new mask = {:#034b}\" , feature_index , mask) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
    ],
    "edges": [
      {
