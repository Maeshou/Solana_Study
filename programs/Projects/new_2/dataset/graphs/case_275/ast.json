    "inputs": [
      "ctx: Context < Release >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "if amount > 1_000_000",
      "{",
      "* * ctx . accounts . vault . try_borrow_mut_lamports () ? -= amount ;",
      "} else",
      "{",
      "* * ctx . accounts . vault . try_borrow_mut_lamports () ? -= (amount / 2) ;",
      "}",
      "* * ctx . accounts . receiver . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"ゲート {} で release を実行\" , ctx . accounts . gate . key ()) ;",
      "Ok (())"
    ]
  },
