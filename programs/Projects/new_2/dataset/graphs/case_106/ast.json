    "name": "distribute",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Dist >"
    ],
    "attributes": [],
    "body": [
      "let pool = & mut ctx . accounts . pool ;",
      "for (& user , & staked) in pool . stakes . iter ()",
      "{",
      "let reward = staked / 10 ;",
      "* * ctx . accounts . user_accounts [0] . lamports . borrow_mut () += reward ;",
      "}",
      "Ok (())"
    ]
  },
