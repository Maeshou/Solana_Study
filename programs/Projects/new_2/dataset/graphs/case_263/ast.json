  {
    "name": "count_occurrences",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx77 >",
      "target: u8"
    ],
    "attributes": [],
    "body": [
      "let data = ctx . accounts . data_acc . data . borrow () ;",
      "let count = data . iter () . filter (| & & b | b == target) . count () as u64 ;",
      "ctx . accounts . count_state . last_count = count ;",
      "ctx . accounts . count_state . update_count += 1 ;",
      "Ok (())"
    ]
