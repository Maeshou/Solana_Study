    "name": "calculate",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Calculate >",
      "txs: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . fee_pool . try_borrow_mut_data () ? ;",
      "for _ in 0 .. txs",
      "{",
      "if ! buf . is_empty ()",
      "{",
      "buf [0] = buf [0] . wrapping_add (2) ;",
      "}",
      "}",
      "msg ! (\"計算責任者 {} が calculate を実行\" , ctx . accounts . accountant . key ()) ;",
      "Ok (())"
    ]
  },
