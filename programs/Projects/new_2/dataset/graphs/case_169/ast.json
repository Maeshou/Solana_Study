    "body": [
      "let t = & mut ctx . accounts . treasury ;",
      "t . balance = t . balance . saturating_add (amount) ;",
      "t . deposit_count = t . deposit_count . saturating_add (1) ;",
      "ctx . accounts . audit_storage . data . borrow_mut () . extend_from_slice (& amount . to_le_bytes ()) ;",
      "Ok (())"
    ]
