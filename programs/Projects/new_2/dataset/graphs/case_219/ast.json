    "name": "sync_state",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx10 >"
    ],
    "attributes": [],
    "body": [
      "let mirror = ctx . accounts . mirror_acc . data . borrow () . to_vec () ;",
      "let st = & mut ctx . accounts . state ;",
      "st . data = mirror ;",
      "st . sync_count = st . sync_count . saturating_add (1) ;",
      "Ok (())"
    ]
