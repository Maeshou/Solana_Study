    "inputs": [
      "ctx: Context < Add >",
      "item_id: u32"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . inventory_info . try_borrow_mut_data () ? ;",
      "if item_id < 100",
      "{",
      "buf [.. 4] . copy_from_slice (& item_id . to_le_bytes ()) ;",
      "} else",
      "{",
      "let end = buf . len () ;",
      "buf [end - 4 .. end] . copy_from_slice (& item_id . to_le_bytes ()) ;",
      "}",
      "msg ! (\"在庫管理者 {} が追加実行\" , ctx . accounts . manager . key ()) ;",
      "Ok (())"
    ]
  },
