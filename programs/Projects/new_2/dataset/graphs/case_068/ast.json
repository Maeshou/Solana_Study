[
  {
    "name": "deposit_require",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RequireCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" パターン1：require! マクロでオーナーのみ検証して入金\"]"
    ],
    "body": [
      "let vault = & mut ctx . accounts . vault_account ;",
      "require ! (vault . owner == ctx . accounts . authority . key () , ErrorCode :: Unauthorized) ;",
      "* * vault . to_account_info () . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"deposit_require 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "deposit_warn",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < WarnCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" パターン2：WARN メッセージだけ出して、所有権チェックなしで入金\"]"
    ],
    "body": [
      "msg ! (\"Warning: owner check skipped\") ;",
      "* * ctx . accounts . raw_vault . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"deposit_warn 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "deposit_partition",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PartitionCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" パターン3：remaining_accounts の偶数番アカウントにだけ入金（owner 未検証）\"]"
    ],
    "body": [
      "for (idx , acct) in ctx . remaining_accounts . iter () . enumerate ()",
      "{",
      "if idx % 2 == 0",
      "{",
      "* * acct . try_borrow_mut_lamports () ? += amount ;",
      "}",
      "}",
      "msg ! (\"deposit_partition 完了\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "deposit_all",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AllCtx >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" パターン4：remaining_accounts 全件に一括入金（完全未検証）\"]"
    ],
    "body": [
      "for acct in & ctx . remaining_accounts",
      "{",
      "* * acct . try_borrow_mut_lamports () ? += amount ;",
      "}",
      "msg ! (\"deposit_all 完了\") ;",
      "Ok (())"
    ]
  },
