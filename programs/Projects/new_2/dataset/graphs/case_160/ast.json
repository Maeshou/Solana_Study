      },
      {
        "name": "admin",
        "attribute": null,
        "field_type": "Signer < 'info >"
      },
      {
        "name": "audit_log",
        "attribute": "# [doc = \" CHECK: 監査ログ、所有者検証なし\"] # [account (mut)]",
        "field_type": "AccountInfo < 'info >"
