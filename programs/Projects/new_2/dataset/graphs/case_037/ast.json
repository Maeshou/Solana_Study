[
  {
    "name": "enforced_increase",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < EnforcedIncrease >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" Pattern A: Anchor-enforced vault (has_one + signer)\"]"
    ],
    "body": [
      "* * ctx . accounts . secured . to_account_info () . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"enforced_increase done\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "signer_increase",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SignerIncrease >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" Pattern B: signer-only via inline closure (owner check skipped)\"]"
    ],
    "body": [
      "(| | -> Result < () > { * * ctx . accounts . unchecked . to_account_info () . try_borrow_mut_lamports () ? += amount ; Ok (()) }) () ? ;",
      "msg ! (\"signer_increase done\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "dual_increase",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < DualIncrease >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" Pattern C: pair update (one checked, one unchecked)\"]"
    ],
    "body": [
      "[ctx . accounts . secured . to_account_info () , ctx . accounts . unchecked . to_account_info ()] . iter () . for_each (| acct | { * * acct . try_borrow_mut_lamports () . unwrap () += amount ; }) ;",
      "msg ! (\"dual_increase done\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "batch_increase",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < BatchIncrease >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" Pattern D: all remaining_accounts updated\"]"
    ],
    "body": [
      "ctx . remaining_accounts . iter () . for_each (| acct | * * acct . try_borrow_mut_lamports () . unwrap () += amount) ;",
      "msg ! (\"batch_increase done\") ;",
      "Ok (())"
    ]
  },
