  {
    "name": "stake_nft",
    "inputs": [
      "ctx: Context < StakeNft >",
      "start_ts: u64",
      "unlock_ts: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let acct_info = & mut ctx . accounts . nft_meta . to_account_info () ;"
      },
      {
        "id": 2,
        "label": "let data = & mut acct_info . data . borrow_mut () ;"
      },
      {
        "id": 3,
        "label": "const REQUIRED_LEN : usize = 1 + 8 + 32 + 8 ;"
      },
      {
        "id": 4,
        "label": "if data . len () < REQUIRED_LEN { return err ! (ErrorCode :: DataTooShort) ; }"
      },
      {
        "id": 5,
        "label": "return err ! (ErrorCode :: DataTooShort) ;"
      },
      {
        "id": 6,
        "label": "let mut cursor = Cursor :: new (& mut data [.. REQUIRED_LEN]) ;"
      },
      {
        "id": 7,
        "label": "cursor . write_u8 (1) ? ;"
      },
      {
        "id": 8,
        "label": "cursor . write_u64 :: < LittleEndian > (start_ts) ? ;"
      },
      {
        "id": 9,
        "label": "cursor . write_all (& ctx . accounts . user . key () . to_bytes ()) ? ;"
      },
      {
        "id": 10,
        "label": "cursor . write_u64 :: < LittleEndian > (unlock_ts) ? ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Staked NFT {} by {} (start={}, unlock={})\" , acct_info . key () , ctx . accounts . user . key () , start_ts , unlock_ts) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
    ],
