  {
    "name": "edit_post",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Edit >",
      "new_body: String"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . post ;",
      "p . body = new_body ;",
      "p . edit_count = p . edit_count . saturating_add (1) ;",
      "p . last_edited = Clock :: get () ? . unix_timestamp ;",
      "ctx . accounts . temp_cache . data . borrow_mut () . fill (0) ;",
      "Ok (())"
    ]
