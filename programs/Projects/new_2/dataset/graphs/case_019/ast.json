[
  {
    "name": "reserved_credit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ReservedCredit >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" フルガード：has_one と signer でオーナーと署名を検証\"]"
    ],
    "body": [
      "* * ctx . accounts . vault_acc . to_account_info () . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"reserved_credit executed\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "signer_only_credit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SignerOnlyCredit >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 署名のみ検証：AccountInfo を直接操作、オーナー検証は省略\"]"
    ],
    "body": [
      "* * ctx . accounts . raw_acc . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"signer_only_credit executed\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "owner_only_credit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < OwnerOnlyCredit >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" オーナーのみ検証：has_one による検証後、to_account_info() で加算\"]"
    ],
    "body": [
      "let acct = ctx . accounts . vault . to_account_info () ;",
      "* * acct . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"owner_only_credit executed\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "unrestricted_credit",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UnrestrictedCredit >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 無制限アクセス：誰でも実行可能\"]"
    ],
    "body": [
      "* * ctx . accounts . any_acc . try_borrow_mut_lamports () ? += amount ;",
      "msg ! (\"unrestricted_credit executed\") ;",
      "Ok (())"
    ]
  },
