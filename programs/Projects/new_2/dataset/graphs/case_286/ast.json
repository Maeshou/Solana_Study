[
  {
    "name": "clean",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Clean >",
      "threshold: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . meta_data . try_borrow_mut_data () ? ;",
      "if buf . len () > threshold as usize",
      "{",
      "for b in buf . iter_mut ()",
      "{",
      "* b = 0 ;",
      "} } else",
      "{",
      "if ! buf . is_empty () { buf [0] = 0 ;",
      "}",
      "}",
      "msg ! (\"管理者 {} が clean を実行\" , ctx . accounts . admin . key ()) ;",
      "Ok (())"
    ]
  },
  {
    "name": "Clean",
    "node_type": "struct",
    "fields": [
