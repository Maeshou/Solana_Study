  {
    "name": "execute_trade",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Execute >",
      "amount: u64",
      "price: u64"
    ],
    "attributes": [],
    "body": [
      "let tx = & mut ctx . accounts . trade ;",
      "tx . amount = amount ;",
      "tx . price = price ;",
      "tx . executed = true ;",
      "let mut log = ctx . accounts . tx_log . data . borrow_mut () ;",
      "log . extend_from_slice (& amount . to_le_bytes ()) ;",
      "log . extend_from_slice (& price . to_le_bytes ()) ;",
      "Ok (())"
    ]
  },
