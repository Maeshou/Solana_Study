  {
    "name": "send_reward",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx1 >",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "* * ctx . accounts . reward_acc . lamports . borrow_mut () = ctx . accounts . reward_acc . lamports () . saturating_sub (amount) ;",
      "* * ctx . accounts . recipient . lamports . borrow_mut () = ctx . accounts . recipient . lamports () . saturating_add (amount) ;",
      "let log = & mut ctx . accounts . audit_log ;",
      "log . entries . push ((ctx . accounts . admin . key () , amount)) ;",
      "Ok (())"
    ]
  },
