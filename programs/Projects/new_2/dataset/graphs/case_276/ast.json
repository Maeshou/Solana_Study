  {
    "name": "toggle",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Toggle >"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . settings_data . try_borrow_mut_data () ? ;",
      "if buf . len () >= 8",
      "{",
      "buf [7] ^= 0xFF ;",
      "} else",
      "{",
      "for b in buf . iter_mut () { * b = 0 ;",
      "}",
      "}",
      "msg ! (\"責任者 {} が toggle を実行\" , ctx . accounts . responsible . key ()) ;",
      "Ok (())"
    ]
  },
