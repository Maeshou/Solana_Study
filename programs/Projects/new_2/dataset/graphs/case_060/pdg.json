  {
    "name": "claim_daily",
    "inputs": [
      "ctx: Context < ClaimDaily >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let acct_info = & mut ctx . accounts . claim_reward_account . to_account_info () ;"
      },
      {
        "id": 2,
        "label": "let mut data = acct_info . data . borrow_mut () ;"
      },
      {
        "id": 3,
        "label": "if data . len () < 16 { return err ! (ErrorCode :: DataTooShort) ; }"
      },
      {
        "id": 4,
        "label": "return err ! (ErrorCode :: DataTooShort) ;"
      },
      {
        "id": 5,
        "label": "let (ts_slice , rest) = data . split_at_mut (8) ;"
      },
      {
        "id": 6,
        "label": "let (amt_slice , _expand) = rest . split_at_mut (8) ;"
      },
      {
        "id": 7,
        "label": "let now = Clock :: get () ? . unix_timestamp as u64 ;"
      },
      {
        "id": 8,
        "label": "let last_ts = u64 :: from_le_bytes (ts_slice . try_into () . unwrap ()) ;"
      },
      {
        "id": 9,
        "label": "if now < last_ts . saturating_add (86400) { return err ! (ErrorCode :: TooSoon) ; }"
      },
      {
        "id": 10,
        "label": "return err ! (ErrorCode :: TooSoon) ;"
      },
      {
        "id": 11,
        "label": "ts_slice . copy_from_slice (& now . to_le_bytes ()) ;"
      },
      {
        "id": 12,
        "label": "let bonus = u64 :: from_le_bytes (amt_slice . try_into () . unwrap ()) ;"
      },
      {
        "id": 13,
        "label": "* * acct_info . lamports . borrow_mut () -= bonus ;"
      },
      {
        "id": 14,
        "label": "* * ctx . accounts . user . to_account_info () . lamports . borrow_mut () += bonus ;"
      },
      {
        "id": 15,
        "label": "msg ! (\"Daily bonus of {} lamports claimed by {} at {}\" , bonus , ctx . accounts . user . key () , now) ;"
      },
      {
        "id": 16,
        "label": "Ok (())"
      }
    ],
