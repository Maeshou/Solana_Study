[
  {
    "name": "stake_nft",
    "inputs": [
      "ctx: Context < StakeNft >",
      "lock_duration: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let acct_info = & mut ctx . accounts . nft_meta . to_account_info () ;"
      },
      {
        "id": 2,
        "label": "let data = & mut acct_info . data . borrow_mut () ;"
      },
      {
        "id": 3,
        "label": "const EXPECTED_LEN : usize = 1 + 8 + 32 + 8 ;"
      },
      {
        "id": 4,
        "label": "if data . len () < EXPECTED_LEN { return err ! (ErrorCode :: DataTooShort) ; }"
      },
      {
        "id": 5,
        "label": "return err ! (ErrorCode :: DataTooShort) ;"
      },
      {
        "id": 6,
        "label": "let now = Clock :: get () ? . unix_timestamp as u64 ;"
      },
      {
        "id": 7,
        "label": "let unlock = now . checked_add (lock_duration) . ok_or (ErrorCode :: TimestampOverflow) ? ;"
      },
      {
        "id": 8,
        "label": "let mut cursor = Cursor :: new (& mut data [.. EXPECTED_LEN]) ;"
      },
      {
        "id": 9,
        "label": "cursor . write_u8 (1) ? ;"
      },
      {
        "id": 10,
        "label": "cursor . write_u64 :: < LittleEndian > (now) ? ;"
      },
      {
        "id": 11,
        "label": "cursor . write_all (& ctx . accounts . user . key () . to_bytes ()) ? ;"
      },
      {
        "id": 12,
        "label": "cursor . write_u64 :: < LittleEndian > (unlock) ? ;"
      },
      {
        "id": 13,
        "label": "msg ! (\"Staked NFT {} by {} until {}\" , acct_info . key () , ctx . accounts . user . key () , unlock) ;"
      },
      {
        "id": 14,
        "label": "Ok (())"
      }
    ],
