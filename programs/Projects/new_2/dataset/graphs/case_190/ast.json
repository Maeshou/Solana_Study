    "name": "cancel",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Cancel >"
    ],
    "attributes": [],
    "body": [
      "let listing = & mut ctx . accounts . listing ;",
      "listing . active = false ;",
      "listing . cancelled_at = Clock :: get () ? . unix_timestamp ;",
      "ctx . accounts . op_log . data . borrow_mut () . extend_from_slice (b\"cancel;\") ;",
      "Ok (())"
    ]
  },
