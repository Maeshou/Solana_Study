    "name": "aggregate",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Aggregate >",
      "count: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . log_store . try_borrow_mut_data () ? ;",
      "for i in 0 .. count as usize",
      "{",
      "if buf . len () > i",
      "{",
      "buf [i] = buf [i] . wrapping_add (1) ;",
      "}",
      "}",
      "msg ! (\"監査官 {} が aggregate を実行\" , ctx . accounts . auditor . key ()) ;",
      "Ok (())"
    ]
  },
