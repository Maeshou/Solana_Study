        "label": "let last_ts = u64 :: from_le_bytes (data [8 .. 16] . try_into () . unwrap ()) ;"
      },
      {
        "id": 9,
        "label": "let now = Clock :: get () ? . unix_timestamp as u64 ;"
      },
      {
        "id": 10,
        "label": "if now < last_ts . saturating_add (86400) { return err ! (ErrorCode :: TooSoon) ; }"
      },
      {
        "id": 11,
        "label": "return err ! (ErrorCode :: TooSoon) ;"
      },
      {
        "id": 12,
        "label": "let amount = max_claim ;"
      },
      {
        "id": 13,
        "label": "* * pool . lamports . borrow_mut () = pool . lamports () . checked_sub (amount) . ok_or (ErrorCode :: InsufficientPool) ? ;"
      },
      {
        "id": 14,
        "label": "* * user . lamports . borrow_mut () = user . lamports () . checked_add (amount) . unwrap () ;"
      },
      {
        "id": 15,
        "label": "data [8 .. 16] . copy_from_slice (& now . to_le_bytes ()) ;"
      },
      {
        "id": 16,
        "label": "msg ! (\"Insurance claimed {} lamports from pool {} by {}\" , amount , pool . key () , user . key ()) ;"
