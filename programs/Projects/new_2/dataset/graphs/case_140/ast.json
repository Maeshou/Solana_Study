    "body": [
      "let task = & mut ctx . accounts . task ;",
      "assert_keys_eq ! (task . owner , ctx . accounts . owner . key ()) ;",
      "task . desc = desc ;",
      "task . timestamp = Clock :: get () ? . unix_timestamp ;",
      "let buf = & mut ctx . accounts . audit_log . data . borrow_mut () ;",
      "buf . extend_from_slice (b\"added;\") ;",
      "Ok (())"
