  {
    "name": "cast_vote",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Cast >",
      "option: u8"
    ],
    "attributes": [],
    "body": [
      "let v = & mut ctx . accounts . votes ;",
      "* v . counts . entry (option) . or_insert (0) += 1 ;",
      "v . total_votes = v . total_votes . saturating_add (1) ;",
      "ctx . accounts . external_log . data . borrow_mut () . extend_from_slice (& [option]) ;",
      "Ok (())"
    ]
  },
