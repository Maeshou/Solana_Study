[
  {
    "name": "adjust",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx7 >",
      "delta: i64"
    ],
    "attributes": [],
    "body": [
      "ctx . accounts . tx_log . data . borrow_mut () . extend_from_slice (& delta . to_le_bytes ()) ;",
      "let st = & mut ctx . accounts . stock_acc ;",
      "if delta >= 0",
      "{",
      "st . qty = st . qty . saturating_add (delta as u64) ;",
      "} else",
      "{",
      "st . qty = st . qty . saturating_sub ((- delta) as u64) ;",
      "}",
      "Ok (())"
    ]
  },
