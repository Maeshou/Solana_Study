  {
    "name": "batch_transfer",
    "inputs": [
      "ctx: Context < BatchTransfer >",
      "amounts: Vec < u64 >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let reserve = & mut ctx . accounts . reserve . to_account_info () ;"
      },
      {
        "id": 2,
        "label": "let recipients = & ctx . remaining_accounts ;"
      },
      {
        "id": 3,
        "label": "let total : u64 = amounts . iter () . copied () . sum () ;"
      },
      {
        "id": 4,
        "label": "if * * reserve . lamports . borrow () < total { return err ! (ErrorCode :: InsufficientFunds) ; }"
      },
      {
        "id": 5,
        "label": "return err ! (ErrorCode :: InsufficientFunds) ;"
      },
      {
        "id": 6,
        "label": "for (i , rcpt) in recipients . iter () . enumerate () { let amt = amounts . get (i) . copied () . ok_or (ErrorCode :: InvalidInput) ? ; * * reserve . lamports . borrow_mut () -= amt ; * * rcpt . lamports . borrow_mut () += amt ; }"
      },
      {
        "id": 7,
        "label": "let amt = amounts . get (i) . copied () . ok_or (ErrorCode :: InvalidInput) ? ;"
      },
      {
        "id": 8,
        "label": "* * reserve . lamports . borrow_mut () -= amt ;"
      },
      {
        "id": 9,
        "label": "* * rcpt . lamports . borrow_mut () += amt ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Batch transferred total {} lamports\" , total) ;"
      },
      {
        "id": 11,
        "label": "Ok (())"
      }
