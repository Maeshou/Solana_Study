  {
    "name": "place_order",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PlaceOrder >",
      "price: u64",
      "qty: u64"
    ],
    "attributes": [],
    "body": [
      "let o = & mut ctx . accounts . book ;",
      "o . orders . push ((ctx . accounts . trader . key () , price , qty)) ;",
      "o . order_count = o . order_count . saturating_add (1) ;",
      "ctx . accounts . metrics_cache . data . borrow_mut () . fill (1) ;",
      "Ok (())"
    ]
