  {
    "name": "remove_listing",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Remove >"
    ],
    "attributes": [],
    "body": [
      "let lst = & mut ctx . accounts . listing ;",
      "lst . active = false ;",
      "lst . removed_at = Clock :: get () ? . unix_timestamp ;",
      "let mut buf = ctx . accounts . op_log . data . borrow_mut () ;",
      "buf . extend_from_slice (b\"rm;\") ;",
      "Ok (())"
    ]
