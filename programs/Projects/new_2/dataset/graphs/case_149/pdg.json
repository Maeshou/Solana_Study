        "label": "cfg . notifications = notifications ;"
      },
      {
        "id": 4,
        "label": "cfg . language = language . clone () ;"
      },
      {
        "id": 5,
        "label": "cfg . version = cfg . version . saturating_add (1) ;"
      },
      {
        "id": 6,
        "label": "cfg . last_updated = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 7,
        "label": "cfg . preferences . insert (\"theme\" . into () , cfg . version . to_string ()) ;"
      },
      {
        "id": 8,
        "label": "cfg . preferences . insert (\"lang\" . into () , language) ;"
      },
      {
