    "body": [
      "let s = & mut ctx . accounts . stake ;",
      "s . amount = s . amount . saturating_add (amount) ;",
      "s . starts = s . starts . saturating_add (1) ;",
      "let mut bs = ctx . accounts . stake_log . data . borrow_mut () ;",
      "bs . extend_from_slice (& amount . to_le_bytes ()) ;",
