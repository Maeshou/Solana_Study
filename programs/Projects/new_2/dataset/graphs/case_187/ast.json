  {
    "name": "reset",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ResetBoard >"
    ],
    "attributes": [],
    "body": [
      "let lb = & mut ctx . accounts . board ;",
      "lb . scores . clear () ;",
      "lb . reset_count = lb . reset_count . saturating_add (1) ;",
      "ctx . accounts . reset_log . data . borrow_mut () . extend_from_slice (& lb . reset_count . to_le_bytes ()) ;",
      "Ok (())"
    ]
