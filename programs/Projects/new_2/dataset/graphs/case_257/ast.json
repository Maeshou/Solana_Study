[
  {
    "name": "compute_median",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx71 >"
    ],
    "attributes": [],
    "body": [
      "let data = ctx . accounts . numbers_buf . data . borrow () ;",
      "let mut nums : Vec < u64 > = data . chunks (8) . map (| c | u64 :: from_le_bytes (c . try_into () . unwrap ())) . collect () ;",
      "nums . sort_unstable () ;",
      "let median = nums [nums . len () / 2] ;",
      "ctx . accounts . stats . median = median ;",
      "ctx . accounts . stats . calc_count = ctx . accounts . stats . calc_count . saturating_add (1) ;",
      "Ok (())"
    ]
  },
