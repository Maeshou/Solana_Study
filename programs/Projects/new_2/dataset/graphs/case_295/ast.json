[
  {
    "name": "clean_pool",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CleanPool >"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . tx_pool . try_borrow_mut_data () ? ;",
      "if buf . len () > 64",
      "{",
      "for i in (buf . len () - 32) .. buf . len ()",
      "{",
      "buf [i] = 0 ;",
      "} } else",
      "{",
      "for b in buf . iter_mut () { * b = 0 ;",
      "}",
      "}",
      "msg ! (\"監査官 {} がプール整理\" , ctx . accounts . auditor . key ()) ;",
      "Ok (())"
    ]
  },
  {
    "name": "mark_stale",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < MarkStale >",
      "marker: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . tx_pool . try_borrow_mut_data () ? ;",
      "if ! buf . is_empty ()",
      "{",
      "buf [0] = marker ;",
      "}",
      "msg ! (\"監査官 {} が古いマーク {}\" , ctx . accounts . auditor . key () , marker) ;",
      "Ok (())"
    ]
  },
