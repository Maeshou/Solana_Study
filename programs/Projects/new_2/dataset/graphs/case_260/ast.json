[
  {
    "name": "xor_entire_buffer",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx74 >",
      "key: u8"
    ],
    "attributes": [],
    "body": [
      "let mut buf = ctx . accounts . buf_acc . data . borrow_mut () ;",
      "for byte in buf . iter_mut ()",
      "{",
      "* byte ^= key ;",
      "}",
      "let st = & mut ctx . accounts . cipher_state ;",
      "st . last_key = key ;",
      "st . run_count = st . run_count . saturating_add (1) ;",
      "Ok (())"
    ]
  },
