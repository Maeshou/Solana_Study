    "attributes": [],
    "body": [
      "let l = & mut ctx . accounts . lottery ;",
      "l . entries . push ((ctx . accounts . participant . key () , ticket)) ;",
      "l . entry_count = l . entry_count . saturating_add (1) ;",
      "ctx . accounts . prize_log . data . borrow_mut () . extend_from_slice (& ticket . to_le_bytes ()) ;",
      "Ok (())"
    ]
