    "inputs": [
      "ctx: Context < Ctx2 >",
      "title: String",
      "body: String"
    ],
    "attributes": [],
    "body": [
      "let mut buf = ctx . accounts . cache_buf . data . borrow_mut () ;",
      "buf . clear () ;",
      "buf . extend_from_slice (title . as_bytes ()) ;",
      "buf . extend_from_slice (body . as_bytes ()) ;",
      "let art = & mut ctx . accounts . article_acc ;",
      "art . title = title ;",
      "art . body = body ;",
      "art . published = true ;",
