    "name": "grant_points",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < GrantPoints >",
      "count: u8"
    ],
    "attributes": [],
    "body": [
      "let buf = & mut ctx . accounts . point_pool . try_borrow_mut_data () ? ;",
      "for i in 0 .. (count as usize)",
      "{",
      "if buf . len () > i",
      "{",
      "buf [i] = buf [i] . wrapping_add (10) ;",
      "}",
      "}",
      "msg ! (\"配布者 {} が {} 回ポイント付与\" , ctx . accounts . distributor . key () , count) ;",
      "Ok (())"
    ]
  },
