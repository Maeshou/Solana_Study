[
  {
    "name": "adjust_rate",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx72 >",
      "delta_bps: i16"
    ],
    "attributes": [],
    "body": [
      "let mut log = ctx . accounts . rate_log . data . borrow_mut () ;",
      "log . extend_from_slice (& delta_bps . to_le_bytes ()) ;",
      "let acct = & mut ctx . accounts . rate_account ;",
      "let old = acct . rate ;",
      "acct . rate = ((old as i64) * (10_000 + delta_bps as i64) / 10_000) as u64 ;",
      "Ok (())"
    ]
  },
