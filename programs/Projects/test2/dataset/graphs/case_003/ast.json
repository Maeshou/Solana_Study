    "body": [
      "let product = & mut ctx . accounts . product ;",
      "msg ! (\"Transferring product from {:?} to {:?}\" , product . owner , new_owner) ;",
      "product . owner = new_owner ;",
      "product . serial = new_serial ;",
      "product . logs . push (\"Ownership and serial updated\" . into ()) ;",
      "Ok (())"
