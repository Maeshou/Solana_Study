        "id": 15,
        "label": "let cx = br . as_cpi (program . clone ()) ;"
      },
      {
        "id": 16,
        "label": "br . push (cx , take + s . submits) ? ;"
      },
      {
        "id": 17,
        "label": "left -= take ;"
      },
      {
        "id": 18,
        "label": "Ok (())"
