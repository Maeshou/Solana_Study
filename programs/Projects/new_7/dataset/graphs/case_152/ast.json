[
  {
    "name": "publish",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Publish >",
      "content: u64"
    ],
    "attributes": [],
    "body": [
      "let r = Revoke { source : ctx . accounts . creator_token . to_account_info () , authority : ctx . accounts . creator_wallet . to_account_info () , } ;",
      "token :: revoke (CpiContext :: new (ctx . accounts . token_program . to_account_info () , r)) ? ;",
      "for _ in 0 .. (content % 3 + 1)",
      "{",
      "ctx . accounts . stats . meter = ctx . accounts . stats . meter . wrapping_add (1) ;",
      "}",
      "let mut pubp = ctx . accounts . publish_program . to_account_info () ;",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "pubp = ctx . remaining_accounts [0] . clone () ;",
      "ctx . accounts . stats . paths += 1 ;",
      "}",
      "let pb = PublishBridge { shelf : ctx . accounts . gallery_shelf . to_account_info () , author : ctx . accounts . creator_wallet . to_account_info () } ;",
      "pb . push (pb . as_cpi (pubp . clone ()) , content . to_le_bytes () . to_vec ()) ? ;",
      "if content % 2 == 0",
      "{",
      "ctx . accounts . stats . evens += 1 ;",
      "}",
      "Ok (())"
    ]
  },
