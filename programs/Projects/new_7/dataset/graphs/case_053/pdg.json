  {
    "name": "pay",
    "inputs": [
      "ctx: Context < Pay >",
      "x: u64",
      "note: String",
      "base: u64"
    ],
    "nodes": [
      {
        "id": 8,
        "label": "Entry"
      },
      {
        "id": 9,
        "label": "let s = & mut ctx . accounts . sys ;"
      },
      {
        "id": 10,
        "label": "require ! (s . owner == ctx . accounts . owner . key () , Errs :: Owner) ;"
      },
      {
        "id": 11,
        "label": "let mut val = 0u64 ;"
      },
      {
        "id": 12,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 13,
        "label": "while i < s . coeff . len () { val = val . saturating_mul (x) . saturating_add (s . coeff [i] as u64) ; i += 1 ; }"
      },
      {
        "id": 14,
        "label": "val = val . saturating_mul (x) . saturating_add (s . coeff [i] as u64) ;"
      },
      {
        "id": 15,
        "label": "i += 1 ;"
      },
      {
        "id": 16,
        "label": "let b = note . as_bytes () ;"
      },
      {
        "id": 17,
        "label": "let mut j = 0usize ;"
      },
      {
        "id": 18,
        "label": "while j < b . len () { let idx = (j % s . coeff . len ()) as usize ; if j % 2 == 0 { s . coeff [idx] = s . coeff [idx] . saturating_add ((b [j] % 7) as u32) ; } else { if s . coeff [idx] > 0 { s . coeff [idx] = s . coeff [idx] . saturating_sub (1) ; } } j += 1 ; }"
      },
      {
        "id": 19,
        "label": "let idx = (j % s . coeff . len ()) as usize ;"
      },
      {
        "id": 20,
        "label": "if j % 2 == 0 { s . coeff [idx] = s . coeff [idx] . saturating_add ((b [j] % 7) as u32) ; } else { if s . coeff [idx] > 0 { s . coeff [idx] = s . coeff [idx] . saturating_sub (1) ; } }"
      },
      {
        "id": 21,
        "label": "s . coeff [idx] = s . coeff [idx] . saturating_add ((b [j] % 7) as u32) ;"
      },
      {
        "id": 22,
        "label": "if s . coeff [idx] > 0 { s . coeff [idx] = s . coeff [idx] . saturating_sub (1) ; }"
      },
      {
        "id": 23,
        "label": "s . coeff [idx] = s . coeff [idx] . saturating_sub (1) ;"
      },
      {
        "id": 24,
        "label": "j += 1 ;"
      },
      {
        "id": 25,
        "label": "let mut id = s . id0 ;"
      },
      {
        "id": 26,
        "label": "if s . turn % 2 == 1 { id = s . id1 ; }"
      },
      {
        "id": 27,
        "label": "id = s . id1 ;"
      },
      {
        "id": 28,
        "label": "s . turn = s . turn . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "let mut extra = 0u64 ;"
      },
      {
        "id": 30,
        "label": "let mut k = 0u8 ;"
      },
      {
        "id": 31,
        "label": "while k < 5 { extra = extra . saturating_add (((s . turn % 11) as u64) + (k as u64)) ; k = k . saturating_add (1) ; }"
      },
      {
        "id": 32,
        "label": "extra = extra . saturating_add (((s . turn % 11) as u64) + (k as u64)) ;"
      },
      {
        "id": 33,
        "label": "k = k . saturating_add (1) ;"
      },
      {
        "id": 34,
        "label": "let amt = base . saturating_add (val) . saturating_add (extra) ;"
      },
      {
        "id": 35,
        "label": "let ix = spl_token :: instruction :: transfer (id , ctx . accounts . pool . key () , ctx . accounts . receiver_ata . key () , ctx . accounts . owner . key () , & [] , amt ,) ? ;"
      },
      {
        "id": 36,
        "label": "invoke (& ix , & [ctx . accounts . pool . to_account_info () , ctx . accounts . receiver_ata . to_account_info () , ctx . accounts . owner . to_account_info () ,]) ? ;"
      },
