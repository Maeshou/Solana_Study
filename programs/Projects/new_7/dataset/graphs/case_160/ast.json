    "body": [
      "let (cx , pack) = ctx . accounts . build_ctx (& ctx . remaining_accounts) ? ;",
      "let ix = Instruction { program_id : * cx . program . key , accounts : pack . metas () , data : payload . to_le_bytes () . to_vec () , } ;",
      "invoke (& ix , & pack . infos (& cx . program)) ? ;",
      "Ok (())"
