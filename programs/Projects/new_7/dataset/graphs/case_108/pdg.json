    "name": "funnel",
    "inputs": [
      "ctx: Context < Funnel >",
      "base: u64",
      "shards: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let st = & mut ctx . accounts . funnel_state ;"
      },
      {
        "id": 2,
        "label": "st . round += 1 ;"
      },
      {
        "id": 3,
        "label": "let mut program = ctx . accounts . pipe_prog . to_account_info () ;"
      },
      {
        "id": 4,
        "label": "if ctx . remaining_accounts . len () > 0 { st . path_a += base ; program = ctx . remaining_accounts [0] . clone () ; } else { st . path_b += base ; }"
      },
      {
        "id": 5,
        "label": "st . path_a += base ;"
      },
      {
        "id": 6,
        "label": "program = ctx . remaining_accounts [0] . clone () ;"
      },
      {
        "id": 7,
        "label": "st . path_b += base ;"
      },
      {
        "id": 8,
        "label": "let mut acc = 0u64 ;"
      },
      {
        "id": 9,
        "label": "let mut i = 0u64 ;"
      },
      {
        "id": 10,
        "label": "while i < shards { let bucket = base + (i * st . round) ; acc += bucket ; i += 1 ; }"
      },
      {
        "id": 11,
        "label": "let bucket = base + (i * st . round) ;"
      },
      {
        "id": 12,
        "label": "acc += bucket ;"
      },
      {
        "id": 13,
        "label": "i += 1 ;"
      },
      {
        "id": 14,
        "label": "let br = FunnelBridge { inq : ctx . accounts . in_q . to_account_info () , outq : ctx . accounts . out_q . to_account_info () } ;"
      },
      {
        "id": 15,
        "label": "let cx = br . as_cpi (program . clone ()) ;"
      },
      {
        "id": 16,
        "label": "br . send (cx , acc) ? ;"
      },
      {
        "id": 17,
        "label": "Ok (())"
      }
