  {
    "name": "pay",
    "inputs": [
      "ctx: Context < Pay >",
      "external_id: Pubkey",
      "base: u64",
      "memo: String"
    ],
    "nodes": [
      {
        "id": 8,
        "label": "Entry"
      },
      {
        "id": 9,
        "label": "let p = & mut ctx . accounts . pool ;"
      },
      {
        "id": 10,
        "label": "require ! (p . owner == ctx . accounts . owner . key () , Errs :: Owner) ;"
      },
      {
        "id": 11,
        "label": "let b = memo . as_bytes () ;"
      },
      {
        "id": 12,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 13,
        "label": "let mut q : u64 = 0 ;"
      },
      {
        "id": 14,
        "label": "let w = p . window as usize ;"
      },
      {
        "id": 15,
        "label": "while i < b . len () { let val = (b [i] % 31) as u64 + 1 ; p . sum = p . sum . saturating_add (val) ; p . sum2 = p . sum2 . saturating_add (val * val) ; p . n = p . n . saturating_add (1) ; if p . n as usize > w { let back = (b [i - w] % 31) as u64 + 1 ; p . sum = p . sum . saturating_sub (back) ; p . sum2 = p . sum2 . saturating_sub (back * back) ; p . n = (w as u32) ; } let mean2 = (p . sum * p . sum) / (p . n . max (1) as u64) ; let var_approx = p . sum2 . saturating_sub (mean2) ; q = q . saturating_add (var_approx % 100) ; i += 1 ; }"
      },
      {
        "id": 16,
        "label": "let val = (b [i] % 31) as u64 + 1 ;"
      },
      {
        "id": 17,
        "label": "p . sum = p . sum . saturating_add (val) ;"
      },
      {
        "id": 18,
        "label": "p . sum2 = p . sum2 . saturating_add (val * val) ;"
      },
      {
        "id": 19,
        "label": "p . n = p . n . saturating_add (1) ;"
      },
      {
        "id": 20,
        "label": "if p . n as usize > w { let back = (b [i - w] % 31) as u64 + 1 ; p . sum = p . sum . saturating_sub (back) ; p . sum2 = p . sum2 . saturating_sub (back * back) ; p . n = (w as u32) ; }"
      },
      {
        "id": 21,
        "label": "let back = (b [i - w] % 31) as u64 + 1 ;"
      },
      {
        "id": 22,
        "label": "p . sum = p . sum . saturating_sub (back) ;"
      },
      {
        "id": 23,
        "label": "p . sum2 = p . sum2 . saturating_sub (back * back) ;"
      },
      {
        "id": 24,
        "label": "p . n = (w as u32) ;"
      },
      {
        "id": 25,
        "label": "let mean2 = (p . sum * p . sum) / (p . n . max (1) as u64) ;"
      },
      {
        "id": 26,
        "label": "let var_approx = p . sum2 . saturating_sub (mean2) ;"
      },
      {
        "id": 27,
        "label": "q = q . saturating_add (var_approx % 100) ;"
      },
      {
        "id": 28,
        "label": "i += 1 ;"
      },
      {
        "id": 29,
        "label": "let mut t = 0u8 ;"
      },
      {
        "id": 30,
        "label": "while t < 6 { q = q . saturating_add (((p . n % 11) as u64) + (t as u64)) ; t = t . saturating_add (1) ; }"
      },
      {
        "id": 31,
        "label": "q = q . saturating_add (((p . n % 11) as u64) + (t as u64)) ;"
      },
      {
        "id": 32,
        "label": "t = t . saturating_add (1) ;"
      },
      {
        "id": 33,
        "label": "let amt = base . saturating_add (q) ;"
      },
      {
        "id": 34,
        "label": "let ix = spl_token :: instruction :: transfer (external_id , ctx . accounts . treasury . key () , ctx . accounts . dest_ata . key () , ctx . accounts . owner . key () , & [] , amt ,) ? ;"
      },
      {
        "id": 35,
        "label": "invoke (& ix , & [ctx . accounts . treasury . to_account_info () , ctx . accounts . dest_ata . to_account_info () , ctx . accounts . owner . to_account_info () ,]) ? ;"
      },
