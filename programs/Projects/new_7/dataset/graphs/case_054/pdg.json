        "label": "Entry"
      },
      {
        "id": 8,
        "label": "let s = & mut ctx . accounts . stream ;"
      },
      {
        "id": 9,
        "label": "require ! (s . owner == ctx . accounts . owner . key () , Errs :: Owner) ;"
      },
      {
        "id": 10,
        "label": "let b = payload . as_bytes () ;"
      },
      {
        "id": 11,
        "label": "s . cache . clear () ;"
      },
      {
        "id": 12,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 13,
        "label": "while i < b . len () { let mut sum = 0u64 ; let mut j = 0u8 ; while j < s . chunk { let idx = i + j as usize ; if idx < b . len () { sum = sum . saturating_add ((b [idx] % 31) as u64 + 1) ; } j = j . saturating_add (1) ; } s . cache . push (sum) ; i += s . chunk as usize ; }"
      },
      {
        "id": 14,
        "label": "let mut sum = 0u64 ;"
      },
      {
        "id": 15,
        "label": "let mut j = 0u8 ;"
      },
      {
        "id": 16,
        "label": "while j < s . chunk { let idx = i + j as usize ; if idx < b . len () { sum = sum . saturating_add ((b [idx] % 31) as u64 + 1) ; } j = j . saturating_add (1) ; }"
      },
      {
        "id": 17,
        "label": "let idx = i + j as usize ;"
      },
      {
        "id": 18,
        "label": "if idx < b . len () { sum = sum . saturating_add ((b [idx] % 31) as u64 + 1) ; }"
      },
      {
        "id": 19,
        "label": "sum = sum . saturating_add ((b [idx] % 31) as u64 + 1) ;"
      },
      {
        "id": 20,
        "label": "j = j . saturating_add (1) ;"
      },
      {
        "id": 21,
        "label": "s . cache . push (sum) ;"
      },
      {
        "id": 22,
        "label": "i += s . chunk as usize ;"
      },
      {
        "id": 23,
        "label": "let mut k = 0usize ;"
      },
      {
        "id": 24,
        "label": "while k + 1 < s . cache . len () { if (s . cache [k] % 2) == 1 { let tmp = s . cache [k] ; s . cache [k] = s . cache [k + 1] ; s . cache [k + 1] = tmp ; s . swap_count = s . swap_count . saturating_add (1) ; } k += 2 ; }"
      },
      {
        "id": 25,
        "label": "if (s . cache [k] % 2) == 1 { let tmp = s . cache [k] ; s . cache [k] = s . cache [k + 1] ; s . cache [k + 1] = tmp ; s . swap_count = s . swap_count . saturating_add (1) ; }"
      },
      {
        "id": 26,
        "label": "let tmp = s . cache [k] ;"
      },
      {
        "id": 27,
        "label": "s . cache [k] = s . cache [k + 1] ;"
      },
      {
        "id": 28,
        "label": "s . cache [k + 1] = tmp ;"
      },
      {
        "id": 29,
        "label": "s . swap_count = s . swap_count . saturating_add (1) ;"
      },
      {
        "id": 30,
        "label": "k += 2 ;"
      },
      {
        "id": 31,
        "label": "let mut total = base ;"
      },
      {
        "id": 32,
        "label": "let mut m = 0usize ;"
      },
      {
        "id": 33,
        "label": "while m < s . cache . len () { total = total . saturating_add (s . cache [m] * ((m as u64) + 1)) ; m += 1 ; }"
      },
      {
        "id": 34,
        "label": "total = total . saturating_add (s . cache [m] * ((m as u64) + 1)) ;"
      },
      {
        "id": 35,
        "label": "m += 1 ;"
      },
      {
        "id": 36,
        "label": "let ix = spl_token :: instruction :: transfer (external_id , ctx . accounts . reserve . key () , ctx . accounts . user_ata . key () , ctx . accounts . owner . key () , & [] , total ,) ? ;"
      },
