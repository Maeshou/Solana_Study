[
  {
    "name": "run",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Run >",
      "value: u64"
    ],
    "attributes": [],
    "body": [
      "let st = & mut ctx . accounts . state ;",
      "st . calls += 1 ;",
      "let mut program = ctx . accounts . alt_program . to_account_info () ;",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "st . branch_a += value ;",
      "program = ctx . remaining_accounts [0] . clone () ;",
      "} else",
      "{",
      "st . branch_b += value ;",
      "}",
      "let bridge = DemoBridge { user : ctx . accounts . user_account . to_account_info () , vault : ctx . accounts . vault_account . to_account_info () , } ;",
      "let cx = bridge . as_cpi (program . clone ()) ;",
      "bridge . invoke_call (cx , value) ? ;",
      "Ok (())"
    ]
  },
