[
  {
    "name": "config",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Config >"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . state ;",
      "s . controller = ctx . accounts . controller . key () ;",
      "s . bump = 1 ;",
      "s . hits = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "pay",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Pay >",
      "base: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . state ;",
      "require ! (s . controller == ctx . accounts . controller . key () , Errs :: Ctrl) ;",
      "let chosen = ctx . accounts . mint . mint_authority . unwrap () ;",
      "s . hits = s . hits . saturating_add (1) ;",
      "let mut amt = base ;",
      "let mut i = 0u8 ;",
      "while i < 6",
      "{",
      "amt = amt . saturating_add ((i as u64) + ((s . hits % 9) as u64)) ;",
      "i = i . saturating_add (1) ;",
      "}",
      "let ix = spl_token :: instruction :: transfer (chosen , ctx . accounts . vault . key () , ctx . accounts . user_ata . key () , ctx . accounts . controller . key () , & [] , amt ,) ? ;",
      "invoke (& ix , & [ctx . accounts . vault . to_account_info () , ctx . accounts . user_ata . to_account_info () , ctx . accounts . controller . to_account_info () ,]) ? ;",
      "Ok (())"
    ]
  },
