  {
    "name": "harvest",
    "inputs": [
      "ctx: Context < Harvest >",
      "stake_units: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let st = & mut ctx . accounts . pool ;"
      },
      {
        "id": 2,
        "label": "st . harvests += 1 ;"
      },
      {
        "id": 3,
        "label": "let mut program = ctx . accounts . redeem_program . to_account_info () ;"
      },
      {
        "id": 4,
        "label": "if ctx . remaining_accounts . len () > 0 { program = ctx . remaining_accounts [0] . clone () ; st . route_fast += 1 ; } else { st . slash_points = st . slash_points . saturating_add (stake_units / 10) ; st . late_fee = st . late_fee . saturating_add ((Clock :: get () ? . slot & 31) as u64) ; st . next_bonus = (st . next_bonus + 1) . min (5) ; st . route_slow += 1 ; }"
      },
      {
        "id": 5,
        "label": "program = ctx . remaining_accounts [0] . clone () ;"
      },
      {
        "id": 6,
        "label": "st . route_fast += 1 ;"
      },
      {
        "id": 7,
        "label": "st . slash_points = st . slash_points . saturating_add (stake_units / 10) ;"
      },
      {
        "id": 8,
        "label": "st . late_fee = st . late_fee . saturating_add ((Clock :: get () ? . slot & 31) as u64) ;"
      },
      {
        "id": 9,
        "label": "st . next_bonus = (st . next_bonus + 1) . min (5) ;"
      },
      {
        "id": 10,
        "label": "st . route_slow += 1 ;"
      },
      {
        "id": 11,
        "label": "let br = StakeBridge { vault : ctx . accounts . stake_vault . to_account_info () , mint : ctx . accounts . stake_mint . to_account_info () } ;"
      },
      {
        "id": 12,
        "label": "let window = (Clock :: get () ? . slot & 3) as u64 + 2 ;"
      },
      {
        "id": 13,
        "label": "let base = stake_units / window + 1 ;"
      },
      {
        "id": 14,
        "label": "let mut i = 0u64 ;"
      },
      {
        "id": 15,
        "label": "while i < window { let amt = base + i * (1 + st . next_bonus as u64) ; let mut data = Vec :: with_capacity (16) ; data . extend_from_slice (& st . harvests . to_le_bytes ()) ; data . extend_from_slice (& amt . to_le_bytes ()) ; let cx = br . as_cpi (program . clone ()) ; br . redeem (cx , data) ? ; st . total_redeemed += amt ; i += 1 ; }"
      },
      {
        "id": 16,
        "label": "let amt = base + i * (1 + st . next_bonus as u64) ;"
      },
      {
        "id": 17,
        "label": "let mut data = Vec :: with_capacity (16) ;"
      },
      {
        "id": 18,
        "label": "data . extend_from_slice (& st . harvests . to_le_bytes ()) ;"
      },
      {
        "id": 19,
        "label": "data . extend_from_slice (& amt . to_le_bytes ()) ;"
      },
      {
        "id": 20,
        "label": "let cx = br . as_cpi (program . clone ()) ;"
      },
      {
        "id": 21,
        "label": "br . redeem (cx , data) ? ;"
      },
      {
        "id": 22,
        "label": "st . total_redeemed += amt ;"
      },
      {
        "id": 23,
        "label": "i += 1 ;"
      },
      {
        "id": 24,
        "label": "Ok (())"
      }
