[
  {
    "name": "step",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Step >",
      "value: u64"
    ],
    "attributes": [],
    "body": [
      "let q = & mut ctx . accounts . progress ;",
      "q . counter += 1 ;",
      "let mut program = ctx . accounts . alt_prog . to_account_info () ;",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "q . path_a += value ;",
      "program = ctx . remaining_accounts [0] . clone () ;",
      "} else",
      "{",
      "q . path_b += value ;",
      "}",
      "let br = QuestBridge { src : ctx . accounts . progress_src . to_account_info () , dst : ctx . accounts . progress_dst . to_account_info () , } ;",
      "let cx = br . as_cpi (program . clone ()) ;",
      "br . apply (cx , value + q . counter) ? ;",
      "Ok (())"
    ]
  },
