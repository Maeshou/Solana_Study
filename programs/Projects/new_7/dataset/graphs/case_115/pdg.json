[
  {
    "name": "drive",
    "inputs": [
      "ctx: Context < Drive >",
      "load: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let s = & mut ctx . accounts . bus ;"
      },
      {
        "id": 2,
        "label": "s . turn += 1 ;"
      },
      {
        "id": 3,
        "label": "let mut program = ctx . accounts . route_prog . to_account_info () ;"
      },
      {
        "id": 4,
        "label": "if ctx . remaining_accounts . len () > 0 { s . path_a += load ; program = ctx . remaining_accounts [0] . clone () ; } else { s . path_b += load ; }"
      },
      {
        "id": 5,
        "label": "s . path_a += load ;"
      },
      {
        "id": 6,
        "label": "program = ctx . remaining_accounts [0] . clone () ;"
      },
      {
        "id": 7,
        "label": "s . path_b += load ;"
      },
      {
        "id": 8,
        "label": "let offset = (s . turn & 3) as u64 + 1 ;"
      },
      {
        "id": 9,
        "label": "let br = BusBridge { head : ctx . accounts . head_buf . to_account_info () , tail : ctx . accounts . tail_buf . to_account_info () } ;"
      },
      {
        "id": 10,
        "label": "let mut sent = 0u64 ;"
      },
      {
        "id": 11,
        "label": "while sent < load { let send = if load - sent > offset { offset } else { load - sent } ; let cx = br . as_cpi (program . clone ()) ; if (s . turn & 1) > 0 { br . push_ht (cx , send + s . turn) ? ; } else { br . push_th (cx , send + s . turn) ? ; } sent += send ; }"
      },
      {
        "id": 12,
        "label": "let send = if load - sent > offset { offset } else { load - sent } ;"
      },
      {
        "id": 13,
        "label": "offset"
      },
      {
        "id": 14,
        "label": "load - sent"
      },
      {
        "id": 15,
        "label": "let cx = br . as_cpi (program . clone ()) ;"
      },
      {
        "id": 16,
        "label": "if (s . turn & 1) > 0 { br . push_ht (cx , send + s . turn) ? ; } else { br . push_th (cx , send + s . turn) ? ; }"
      },
      {
        "id": 17,
        "label": "br . push_ht (cx , send + s . turn) ? ;"
      },
      {
        "id": 18,
        "label": "br . push_th (cx , send + s . turn) ? ;"
      },
      {
        "id": 19,
        "label": "sent += send ;"
      },
      {
        "id": 20,
        "label": "Ok (())"
      }
