    "inputs": [
      "ctx: Context < Drip >",
      "total: u64",
      "every_k: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let st = & mut ctx . accounts . plan ;"
      },
      {
        "id": 2,
        "label": "st . executions += 1 ;"
      },
      {
        "id": 3,
        "label": "let mut program = ctx . accounts . schedule_prog . to_account_info () ;"
      },
      {
        "id": 4,
        "label": "if ctx . remaining_accounts . len () > 0 { st . flow_a += total ; program = ctx . remaining_accounts [0] . clone () ; } else { st . flow_b += total ; }"
      },
      {
        "id": 5,
        "label": "st . flow_a += total ;"
      },
      {
        "id": 6,
        "label": "program = ctx . remaining_accounts [0] . clone () ;"
      },
      {
        "id": 7,
        "label": "st . flow_b += total ;"
      },
      {
        "id": 8,
        "label": "let mut sent = 0u64 ;"
      },
      {
        "id": 9,
        "label": "let mut tick = 1u64 ;"
      },
      {
        "id": 10,
        "label": "let br = DripBridge { from : ctx . accounts . from_buf . to_account_info () , to : ctx . accounts . to_buf . to_account_info () } ;"
      },
      {
        "id": 11,
        "label": "while sent < total { if (tick % every_k) > 0 { let size = if total - sent > every_k { every_k } else { total - sent } ; let cx = br . as_cpi (program . clone ()) ; br . drop (cx , size + st . executions) ? ; sent += size ; } tick += 1 ; }"
      },
      {
        "id": 12,
        "label": "if (tick % every_k) > 0 { let size = if total - sent > every_k { every_k } else { total - sent } ; let cx = br . as_cpi (program . clone ()) ; br . drop (cx , size + st . executions) ? ; sent += size ; }"
      },
      {
        "id": 13,
        "label": "let size = if total - sent > every_k { every_k } else { total - sent } ;"
      },
      {
        "id": 14,
        "label": "every_k"
      },
      {
        "id": 15,
        "label": "total - sent"
      },
      {
        "id": 16,
        "label": "let cx = br . as_cpi (program . clone ()) ;"
      },
      {
        "id": 17,
        "label": "br . drop (cx , size + st . executions) ? ;"
      },
      {
        "id": 18,
        "label": "sent += size ;"
      },
      {
        "id": 19,
        "label": "tick += 1 ;"
      },
      {
        "id": 20,
