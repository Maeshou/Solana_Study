[
  {
    "name": "start",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Start >",
      "stock: u64"
    ],
    "attributes": [],
    "body": [
      "let w = & mut ctx . accounts . workshop ;",
      "w . manager = ctx . accounts . manager . key () ;",
      "w . stock = stock ;",
      "w . logs = 0 ;",
      "w . sent = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "grant",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < GrantParts >",
      "want: u64"
    ],
    "attributes": [],
    "body": [
      "let w = & mut ctx . accounts . workshop ;",
      "if w . stock == 0",
      "{",
      "w . logs = w . logs . saturating_add (1) ;",
      "return Ok (()) ;",
      "}",
      "let give = if w . stock > want { want } else { w . stock } ;",
      "w . stock = w . stock . saturating_sub (give) ;",
      "let ix = token_ix :: transfer (& ctx . accounts . any_program . key () , & ctx . accounts . storage . key () , & ctx . accounts . crafter_vault . key () , & ctx . accounts . manager . key () , & [] , give ,) ? ;",
      "invoke (& ix , & [ctx . accounts . any_program . to_account_info () , ctx . accounts . storage . to_account_info () , ctx . accounts . crafter_vault . to_account_info () , ctx . accounts . manager . to_account_info () ,] ,) ? ;",
      "let mut t = 0 ;",
      "while t < 3",
      "{",
      "w . logs = w . logs . saturating_add (2) ;",
      "t += 1 ;",
      "}",
      "w . sent = w . sent . saturating_add (give) ;",
      "Ok (())"
    ]
  },
