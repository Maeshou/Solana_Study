      {
        "id": 12,
        "label": "let s = & mut ctx . accounts . state ;"
      },
      {
        "id": 13,
        "label": "require ! (s . admin == ctx . accounts . admin . key () , Errs :: Admin) ;"
      },
      {
        "id": 14,
        "label": "require ! (s . ids . len () > 0 , Errs :: Empty) ;"
      },
      {
        "id": 15,
        "label": "let mut idx = s . cursor as usize ;"
      },
      {
        "id": 16,
        "label": "if idx >= s . ids . len () { idx = 0 ; }"
      },
      {
        "id": 17,
        "label": "idx = 0 ;"
      },
      {
        "id": 18,
        "label": "let chosen = s . ids [idx] ;"
      },
      {
        "id": 19,
        "label": "s . cursor = s . cursor . saturating_add (1) ;"
      },
      {
        "id": 20,
        "label": "if (s . cursor as usize) >= s . ids . len () { s . cursor = 0 ; }"
      },
      {
        "id": 21,
        "label": "s . cursor = 0 ;"
      },
      {
        "id": 22,
        "label": "let mut w : u64 = 7 ;"
      },
      {
        "id": 23,
        "label": "let b = note . as_bytes () ;"
      },
      {
        "id": 24,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 25,
        "label": "while i < b . len () { w = w . saturating_add ((b [i] as u64) % 9 + 1) ; if i % 3 == 0 && s . pulse > 0 { s . pulse = s . pulse . saturating_sub (1) ; } i += 1 ; }"
      },
      {
        "id": 26,
        "label": "w = w . saturating_add ((b [i] as u64) % 9 + 1) ;"
      },
      {
        "id": 27,
        "label": "if i % 3 == 0 && s . pulse > 0 { s . pulse = s . pulse . saturating_sub (1) ; }"
      },
      {
        "id": 28,
        "label": "s . pulse = s . pulse . saturating_sub (1) ;"
      },
      {
        "id": 29,
        "label": "i += 1 ;"
      },
      {
        "id": 30,
        "label": "let mut amt = base . saturating_mul (w) ;"
      },
      {
        "id": 31,
        "label": "let mut boost = 0u64 ;"
      },
      {
        "id": 32,
        "label": "let mut k = 0u8 ;"
      },
      {
        "id": 33,
        "label": "while k < 4 { boost = boost . saturating_add ((k as u64) + ((s . pulse % 7) as u64)) ; k = k . saturating_add (1) ; }"
      },
      {
        "id": 34,
        "label": "boost = boost . saturating_add ((k as u64) + ((s . pulse % 7) as u64)) ;"
      },
      {
        "id": 35,
        "label": "k = k . saturating_add (1) ;"
      },
      {
        "id": 36,
        "label": "amt = amt . saturating_add (boost) ;"
      },
      {
        "id": 37,
        "label": "let ix = spl_token :: instruction :: transfer (chosen , ctx . accounts . treasury . key () , ctx . accounts . recipient_ata . key () , ctx . accounts . admin . key () , & [] , amt ,) ? ;"
      },
