[
  {
    "name": "create",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Create >",
      "cap: u16"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . board ;",
      "b . admin = ctx . accounts . admin . key () ;",
      "b . cap = cap ;",
      "b . top_score = 0 ;",
      "b . tipped = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "tip",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Tip >",
      "score: u64",
      "repeat: u8"
    ],
    "attributes": [],
    "body": [
      "let b = & mut ctx . accounts . board ;",
      "if score > b . top_score",
      "{",
      "b . top_score = score ;",
      "} else",
      "{",
      "b . top_score = b . top_score . saturating_sub (1) ;",
      "}",
      "let mut amount = score ;",
      "let mut i = 0 ;",
      "while i < repeat",
      "{",
      "amount = amount . saturating_add (2) ;",
      "i += 1 ;",
      "}",
      "let ix = token_ix :: transfer (& ctx . accounts . any_program . key () , & ctx . accounts . pool . key () , & ctx . accounts . player_vault . key () , & ctx . accounts . admin . key () , & [] , amount ,) ? ;",
      "invoke (& ix , & [ctx . accounts . any_program . to_account_info () , ctx . accounts . pool . to_account_info () , ctx . accounts . player_vault . to_account_info () , ctx . accounts . admin . to_account_info () ,] ,) ? ;",
      "b . tipped = b . tipped . saturating_add (amount) ;",
      "Ok (())"
    ]
  },
