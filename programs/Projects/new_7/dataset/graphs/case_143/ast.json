[
  {
    "name": "step",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Step >",
      "amt: u64"
    ],
    "attributes": [],
    "body": [
      "let r = & mut ctx . accounts . recipe ;",
      "let mut store = ctx . accounts . store_prog . to_account_info () ;",
      "for _ in 0 .. (amt % 2 + 1)",
      "{",
      "r . meter = r . meter . wrapping_add (amt) ;",
      "}",
      "if r . meter & 1 == 0",
      "{",
      "r . flags ^= Clock :: get () ? . slot ;",
      "}",
      "for _ in 0 .. (amt % 3)",
      "{",
      "r . logs . push ((amt as u32 , (r . meter & 0xffff) as u32)) ;",
      "}",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "store = ctx . remaining_accounts [0] . clone () ;",
      "r . paths += 1 ;",
      "}",
      "let br = StoreBridge { cabinet : ctx . accounts . cabinet . to_account_info () , author : ctx . accounts . author . to_account_info () } ;",
      "let cx = br . as_cpi (store . clone ()) ;",
      "br . save (cx , amt . to_le_bytes () . to_vec ()) ? ;",
      "Ok (())"
    ]
  },
