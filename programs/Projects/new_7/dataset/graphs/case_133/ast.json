[
  {
    "name": "boost",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Boost >",
      "seed: u64"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . crafter ;",
      "c . rolls += 1 ;",
      "let mut prg = ctx . accounts . helper_program . to_account_info () ;",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "prg = ctx . remaining_accounts [0] . clone () ;",
      "c . alt_used += 1 ;",
      "}",
      "let b = CraftBridge { workshop_vault : ctx . accounts . workshop_vault . to_account_info () , result_slot : ctx . accounts . result_slot . to_account_info () , } ;",
      "if seed % 3 == 0",
      "{",
      "let cx = b . as_cpi (prg . clone ()) ;",
      "b . call (cx , seed . to_le_bytes () . to_vec ()) ? ;",
      "}",
      "if c . rolls > 4",
      "{",
      "let cx2 = b . as_cpi (prg . clone ()) ;",
      "b . call (cx2 , (c . rolls as u64) . to_le_bytes () . to_vec ()) ? ;",
      "}",
      "Ok (())"
    ]
  },
