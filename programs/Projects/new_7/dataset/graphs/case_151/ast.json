[
  {
    "name": "pay",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Pay >",
      "toll: u64",
      "stamp: u64"
    ],
    "attributes": [],
    "body": [
      "if stamp & 1 == 1",
      "{",
      "ctx . accounts . route . flags ^= stamp ;",
      "}",
      "let mut logp = ctx . accounts . log_program . to_account_info () ;",
      "if ctx . remaining_accounts . len () > 0",
      "{",
      "logp = ctx . remaining_accounts [0] . clone () ;",
      "ctx . accounts . route . paths += 1 ;",
      "}",
      "let lb = LogBridge { book : ctx . accounts . log_book . to_account_info () , wallet : ctx . accounts . traveler_wallet . to_account_info () } ;",
      "lb . write (lb . as_cpi (logp . clone ()) , stamp . to_le_bytes () . to_vec ()) ? ;",
      "let t = Transfer { from : ctx . accounts . traveler_token . to_account_info () , to : ctx . accounts . treasury_token . to_account_info () , authority : ctx . accounts . traveler_wallet . to_account_info () , } ;",
      "token :: transfer (CpiContext :: new (ctx . accounts . token_program . to_account_info () , t) , toll) ? ;",
      "if ctx . accounts . route . paths % 2 == 0",
      "{",
      "ctx . accounts . route . evens += 1 ;",
      "}",
      "Ok (())"
    ]
  },
