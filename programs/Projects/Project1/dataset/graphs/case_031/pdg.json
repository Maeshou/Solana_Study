  {
    "name": "write_within_limit",
    "inputs": [
      "ctx: Context < Ctx003 >",
      "input: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let current_limit = ctx . accounts . storage . limit ;"
      },
      {
