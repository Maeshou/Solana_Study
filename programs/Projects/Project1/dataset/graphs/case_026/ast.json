[
  {
    "name": "set_flag_at",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "index: u8"
    ],
    "attributes": [],
    "body": [
      "let shift = 1u8 << (index % 8) ;",
      "let current = ctx . accounts . storage . flag_register ;",
      "ctx . accounts . storage . flag_register = current | shift ;",
      "Ok (())"
    ]
  },
  {
    "name": "clear_flag_at",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "index: u8"
    ],
    "attributes": [],
    "body": [
      "let mask = ! (1u8 << (index % 8)) ;",
      "let current = ctx . accounts . storage . flag_register ;",
      "ctx . accounts . storage . flag_register = current & mask ;",
      "Ok (())"
    ]
  },
  {
    "name": "display_flags",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >"
    ],
    "attributes": [],
    "body": [
      "let flags = ctx . accounts . storage . flag_register ;",
      "msg ! (\"Current Flags: {:#010b}\" , flags) ;",
      "Ok (())"
    ]
  },
