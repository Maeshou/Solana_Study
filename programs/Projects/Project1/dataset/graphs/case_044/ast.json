  {
    "name": "gather",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "resource_type: u64",
      "amount: u64"
    ],
    "attributes": [],
    "body": [
      "let store = & mut ctx . accounts . player_storage ;",
      "let r0 = (resource_type == 0) as u64 ;",
      "let r1 = (resource_type == 1) as u64 ;",
      "let r2 = (resource_type == 2) as u64 ;",
      "store . wood += r0 * amount ;",
      "store . stone += r1 * amount ;",
      "store . metal += r2 * amount ;",
      "Ok (())"
    ]
  },
