[
  {
    "name": "advance_state",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . storage ;",
      "let current = s . state ;",
      "let not_max = (current < 2) as u8 ;",
      "s . state = current + not_max ;",
      "Ok (())"
    ]
  },
  {
    "name": "display_state",
    "node_type": "function",
    "fields": null,
    "inputs": [
