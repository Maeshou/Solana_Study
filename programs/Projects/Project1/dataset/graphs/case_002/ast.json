    ],
    "attributes": [],
    "body": [
      "require ! (ctx . accounts . manager . is_signer , CustomError :: Unauthorized) ;",
      "ctx . accounts . record . access = Pubkey :: default () ;",
      "msg ! (\"Access has been revoked by {}\" , ctx . accounts . manager . key ()) ;",
      "Ok (())"
    ]
  },
