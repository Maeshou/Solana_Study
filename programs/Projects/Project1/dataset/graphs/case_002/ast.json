  {
    "name": "calibrate_access",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx002 >"
    ],
    "attributes": [],
    "body": [
      "let previous = ctx . accounts . record . access ;",
      "let new_key = ctx . accounts . new_access . key () ;",
      "ctx . accounts . record . access = new_key ;",
      "msg ! (\"Case 002: access updated from {} to {}\" , previous , new_key) ;",
      "Ok (())"
    ]
  },
  {
