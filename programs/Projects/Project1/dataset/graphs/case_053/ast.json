[
  {
    "name": "register_skin",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterCtx >",
      "skin_name: String",
      "color_code: u32",
      "style_code: u8"
    ],
    "attributes": [],
    "body": [
      "let user = ctx . accounts . user . key () ;",
      "let skin = & mut ctx . accounts . skin_data ;",
      "require ! (skin . owner == Pubkey :: default () || skin . owner == user , CustomError :: Unauthorized) ;",
      "skin . owner = user ;",
      "skin . skin_name = skin_name ;",
      "skin . color_code = color_code ;",
      "skin . style_code = style_code ;",
      "skin . minted = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "mint_genesis",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < MintCtx >"
    ],
    "attributes": [],
    "body": [
      "let skin = & mut ctx . accounts . skin_data ;",
      "let user = ctx . accounts . user . key () ;",
      "require ! (skin . owner == user , CustomError :: Unauthorized) ;",
      "let already_minted = skin . minted as u8 ;",
      "let _ = 1u64 / ((1 - already_minted) as u64) ;",
      "skin . minted = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "show",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ShowCtx >"
    ],
    "attributes": [],
    "body": [
      "let s = & ctx . accounts . skin_data ;",
      "msg ! (\"Owner: {}\" , s . owner) ;",
      "msg ! (\"Skin: {}\" , s . skin_name) ;",
      "msg ! (\"Color: {}\" , s . color_code) ;",
      "msg ! (\"Style: {}\" , s . style_code) ;",
      "msg ! (\"Minted: {}\" , s . minted) ;",
      "Ok (())"
    ]
  },
