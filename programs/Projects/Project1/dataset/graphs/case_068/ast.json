[
  {
    "name": "calculate_rank_title_reward",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UserContext >",
      "activity_score: u64"
    ],
    "attributes": [],
    "body": [
      "let capped_score = core :: cmp :: min (activity_score , 9999) ;",
      "let rank = (capped_score / 1000) + 1 ;",
      "let base = \"Scholar\" ;",
      "let title = format ! (\"{} Lv{}\" , base , rank) ;",
      "let is_even = (rank % 2 == 0) as u64 ;",
      "let bonus = 250 * is_even + 100 * (1 - is_even) ;",
      "let reward_tokens = rank * 100 + bonus ;",
      "msg ! (\"--- User Info ---\") ;",
      "msg ! (\"Rank         : {}\" , rank) ;",
      "msg ! (\"Title        : {}\" , title) ;",
      "msg ! (\"Token Reward : {}\" , reward_tokens) ;",
      "Ok (OutputBundle { rank , title , reward_tokens , })"
    ]
  },
