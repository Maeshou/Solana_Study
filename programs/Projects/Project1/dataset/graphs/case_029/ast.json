[
  {
    "name": "write_with_fee",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Ctx003 >",
      "val: u64"
    ],
    "attributes": [],
    "body": [
      "let fee = 500 ;",
      "let payer = ctx . accounts . payer . to_account_info () ;",
      "let receiver = ctx . accounts . fee_receiver . to_account_info () ;",
      "* * payer . try_borrow_mut_lamports () ? -= fee ;",
      "* * receiver . try_borrow_mut_lamports () ? += fee ;",
      "ctx . accounts . storage . data = val ;",
      "Ok (())"
    ]
  },
