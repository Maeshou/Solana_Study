[
  {
    "name": "initialize_points",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializePoints >",
      "initial_points: u64"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . profile ;",
      "acc . owner = ctx . accounts . user . key () ;",
      "acc . points = initial_points ;",
      "acc . unlocked = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "unlock_action",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UnlockAction >"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . profile ;",
      "let unused = (acc . unlocked == false) as u64 ;",
      "let enough = (acc . points >= 10) as u64 ;",
      "let _ = 1 / (unused * enough) ;",
      "acc . points -= 10 ;",
      "acc . unlocked = true ;",
      "Ok (())"
    ]
  },
  {
    "name": "view_profile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UnlockAction >"
    ],
    "attributes": [],
    "body": [
      "let acc = & ctx . accounts . profile ;",
      "msg ! (\"Owner: {}\" , acc . owner) ;",
      "msg ! (\"Remaining Points: {}\" , acc . points) ;",
      "msg ! (\"Unlocked: {}\" , acc . unlocked) ;",
      "Ok (())"
    ]
  },
