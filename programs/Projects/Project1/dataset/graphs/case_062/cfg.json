      },
      {
        "id": 7,
        "label": "let acc = & mut ctx . accounts . profile ;"
      },
      {
        "id": 8,
        "label": "let unused = (acc . unlocked == false) as u64 ;"
      },
      {
        "id": 9,
        "label": "let enough = (acc . points >= 10) as u64 ;"
      },
      {
        "id": 10,
        "label": "let _ = 1 / (unused * enough) ;"
      },
      {
        "id": 11,
        "label": "acc . points -= 10 ;"
      },
