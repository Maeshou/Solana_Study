      "require ! (ctx . accounts . authority . is_signer , CustomError :: Unauthorized) ;",
      "ctx . accounts . storage . data = ctx . accounts . storage . data . checked_add (1) . unwrap () ;",
      "msg ! (\"Action count incremented. Total: {}\" , ctx . accounts . storage . data) ;",
      "Ok (())"
    ]
  },
