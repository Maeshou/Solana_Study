[
  {
    "name": "generate_title",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < TitleContext >",
      "activity_score: u64"
    ],
    "attributes": [],
    "body": [
      "let tier = activity_score / 100 ;",
      "let base = \"Adventurer\" ;",
      "let level = (tier + 1) . to_string () ;",
      "let full_title = format ! (\"{} Lv{}\" , base , level) ;",
      "msg ! (\"Generated Title: {}\" , full_title) ;",
      "Ok (full_title)"
    ]
  },
