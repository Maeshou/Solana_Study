      {
        "id": 11,
        "label": "let mut delivered = 0u64 ;"
      },
      {
        "id": 12,
        "label": "let mut step = (tokens / 4) . max (1) ;"
      },
      {
        "id": 13,
        "label": "while delivered < tokens { let send = (tokens - delivered) . min (step) ; token :: transfer (cpi_ctx . clone () , send) ? ; delivered = delivered . saturating_add (send) ; step = step . rotate_left (1) . wrapping_add (1) ; if step > (tokens - delivered) && (tokens - delivered) > 5 { step = (tokens - delivered) - 2 ; } }"
      },
