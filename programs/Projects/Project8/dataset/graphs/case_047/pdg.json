[
  {
    "name": "start_raid_battle",
    "inputs": [
      "ctx: Context < StartRaidBattle >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let raid_boss = & ctx . accounts . raid_boss_info ;"
      },
      {
        "id": 2,
        "label": "let raid_ticket = & mut ctx . accounts . raid_ticket ;"
      },
      {
        "id": 3,
        "label": "let player = & ctx . accounts . player ;"
      },
      {
        "id": 4,
        "label": "let raid_instance = & mut ctx . accounts . raid_instance ;"
      },
      {
        "id": 5,
        "label": "require ! (raid_ticket . is_used == false , GameError :: RaidTicketAlreadyUsed) ;"
      },
      {
        "id": 6,
        "label": "require ! (raid_ticket . boss_id == raid_boss . boss_id , GameError :: TicketNotForThisBoss) ;"
      },
      {
        "id": 7,
        "label": "let clock = Clock :: get () ? ;"
      },
      {
        "id": 8,
        "label": "let current_timestamp = clock . unix_timestamp ;"
      },
      {
        "id": 9,
        "label": "require ! (current_timestamp >= raid_boss . start_timestamp , GameError :: RaidNotStarted) ;"
      },
      {
        "id": 10,
        "label": "require ! (current_timestamp <= raid_boss . end_timestamp , GameError :: RaidHasEnded) ;"
      },
      {
        "id": 11,
        "label": "raid_ticket . is_used = true ;"
      },
      {
        "id": 12,
        "label": "raid_instance . boss_id = raid_boss . boss_id ;"
      },
      {
        "id": 13,
        "label": "raid_instance . challenger = player . key () ;"
      },
      {
        "id": 14,
        "label": "raid_instance . remaining_hp = raid_boss . total_hp ;"
      },
      {
        "id": 15,
        "label": "raid_instance . battle_start_timestamp = current_timestamp ;"
      },
      {
        "id": 16,
        "label": "raid_instance . is_active = true ;"
      },
      {
        "id": 17,
        "label": "let mut initial_contributors = Vec :: new () ;"
      },
      {
        "id": 18,
        "label": "for _ in 0 .. 5 { initial_contributors . push (Contribution { player : Pubkey :: default () , damage : 0 }) ; }"
      },
      {
        "id": 19,
        "label": "initial_contributors . push (Contribution { player : Pubkey :: default () , damage : 0 }) ;"
      },
      {
        "id": 20,
        "label": "initial_contributors [0] = Contribution { player : player . key () , damage : 0 , } ;"
      },
      {
        "id": 21,
        "label": "raid_instance . contributors = initial_contributors ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"Raid battle against boss {} has started!\" , raid_boss . boss_id) ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
    ],
