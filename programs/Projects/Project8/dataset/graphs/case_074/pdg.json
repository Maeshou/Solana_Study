[
  {
    "name": "adjust",
    "inputs": [
      "ctx: Context < Adjust >",
      "base_rating: u64",
      "recent_scores: Vec < u32 >",
      "victories: Vec < bool >",
      "draws: Vec < bool >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let n = recent_scores . len () . min (20) ;"
      },
      {
        "id": 2,
        "label": "let mut sum = 0u64 ;"
      },
      {
        "id": 3,
        "label": "let mut win_count = 0u64 ;"
      },
      {
        "id": 4,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 5,
        "label": "while i < n { if victories . get (i) . copied () . unwrap_or (false) { win_count = win_count . saturating_add (1) ; sum = sum . saturating_add (recent_scores [i] as u64) ; } else { if draws . get (i) . copied () . unwrap_or (false) { sum = sum . saturating_add ((recent_scores [i] as u64 * 3) / 4) ; } else { sum = sum . saturating_add ((recent_scores [i] as u64) / 2) ; } } i = i . saturating_add (1) ; }"
      },
      {
        "id": 6,
        "label": "if victories . get (i) . copied () . unwrap_or (false) { win_count = win_count . saturating_add (1) ; sum = sum . saturating_add (recent_scores [i] as u64) ; } else { if draws . get (i) . copied () . unwrap_or (false) { sum = sum . saturating_add ((recent_scores [i] as u64 * 3) / 4) ; } else { sum = sum . saturating_add ((recent_scores [i] as u64) / 2) ; } }"
      },
      {
        "id": 7,
        "label": "win_count = win_count . saturating_add (1) ;"
      },
      {
        "id": 8,
        "label": "sum = sum . saturating_add (recent_scores [i] as u64) ;"
      },
      {
        "id": 9,
        "label": "if draws . get (i) . copied () . unwrap_or (false) { sum = sum . saturating_add ((recent_scores [i] as u64 * 3) / 4) ; } else { sum = sum . saturating_add ((recent_scores [i] as u64) / 2) ; }"
      },
      {
        "id": 10,
        "label": "sum = sum . saturating_add ((recent_scores [i] as u64 * 3) / 4) ;"
      },
      {
        "id": 11,
        "label": "sum = sum . saturating_add ((recent_scores [i] as u64) / 2) ;"
      },
      {
        "id": 12,
        "label": "i = i . saturating_add (1) ;"
      },
      {
        "id": 13,
        "label": "let denom = (n as u64) . max (1) ;"
      },
      {
        "id": 14,
        "label": "let weighted = (base_rating . saturating_mul (40) + (sum . saturating_mul (60) / denom)) / 100 ;"
      },
      {
        "id": 15,
        "label": "ctx . accounts . rating_state . adjusted_rating = weighted ;"
      },
      {
        "id": 16,
        "label": "let mut trend = PerformanceTrend :: Poor ;"
      },
      {
        "id": 17,
        "label": "let mut rate = 0u64 ;"
      },
      {
        "id": 18,
        "label": "if n > 0 { rate = win_count . saturating_mul (100) / (n as u64) ; }"
      },
      {
        "id": 19,
        "label": "rate = win_count . saturating_mul (100) / (n as u64) ;"
      },
      {
        "id": 20,
        "label": "if rate >= 80 { trend = PerformanceTrend :: Excellent ; }"
      },
      {
        "id": 21,
        "label": "trend = PerformanceTrend :: Excellent ;"
      },
      {
        "id": 22,
        "label": "if rate < 80 && rate >= 65 { trend = PerformanceTrend :: Strong ; }"
      },
      {
        "id": 23,
        "label": "trend = PerformanceTrend :: Strong ;"
      },
      {
        "id": 24,
        "label": "if rate < 65 && rate >= 50 { trend = PerformanceTrend :: Stable ; }"
      },
      {
        "id": 25,
        "label": "trend = PerformanceTrend :: Stable ;"
      },
      {
        "id": 26,
        "label": "if rate < 50 && rate >= 35 { trend = PerformanceTrend :: Declining ; }"
      },
      {
        "id": 27,
        "label": "trend = PerformanceTrend :: Declining ;"
      },
      {
        "id": 28,
        "label": "ctx . accounts . rating_state . trend = trend ;"
      },
