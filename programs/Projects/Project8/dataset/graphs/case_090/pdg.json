[
  {
    "name": "validate_and_fix_times",
    "inputs": [
      "ctx: Context < Validate >",
      "starting_price: u64",
      "duration_secs: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "require ! (starting_price >= 1_000 , GuardError :: StartingPriceTooLow) ;"
      },
      {
        "id": 2,
        "label": "require ! (duration_secs >= 3_600 , GuardError :: AuctionTooShort) ;"
      },
      {
        "id": 3,
        "label": "let mut valid_duration = duration_secs ;"
      },
      {
        "id": 4,
        "label": "if duration_secs > 604_800 { valid_duration = 604_800 ; }"
      },
      {
        "id": 5,
        "label": "valid_duration = 604_800 ;"
      },
      {
        "id": 6,
        "label": "let now = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 7,
        "label": "ctx . accounts . window . auction_start_time = now ;"
      },
      {
        "id": 8,
        "label": "ctx . accounts . window . auction_end_time = now + valid_duration as i64 ;"
      },
      {
        "id": 9,
        "label": "Ok (())"
      }
    ],
    "edges": [
      {
        "from": 3,
        "to": 3,
        "label": "def: valid_duration"
      },
      {
        "from": 3,
        "to": 4,
        "label": "data_dep: valid_duration"
      },
      {
        "from": 3,
        "to": 5,
        "label": "data_dep: valid_duration"
      },
      {
        "from": 6,
        "to": 6,
        "label": "def: now"
      },
      {
        "from": 6,
        "to": 7,
        "label": "data_dep: now"
      },
      {
        "from": 6,
        "to": 8,
        "label": "data_dep: now"
      },
      {
        "from": 3,
        "to": 8,
        "label": "data_dep: valid_duration"
      }
    ]
  }
]