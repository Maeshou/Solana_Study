[
  {
    "name": "init_pool",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPool >",
      "era: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . stage ;",
      "s . curator = ctx . accounts . curator . key () ;",
      "s . era = era . rotate_left (3) . wrapping_add (17) ;",
      "s . pitch = s . era . rotate_right (1) . wrapping_add (11) ;",
      "let mut i = 0u8 ;",
      "let mut tone = s . pitch ;",
      "while i < 6",
      "{",
      "tone = tone . rotate_left (1) . wrapping_add ((i as u64 * 7) + 5) ;",
      "s . pitch = s . pitch ^ tone ;",
      "i = i . saturating_add (1) ;",
      "}",
      "require ! (s . pitch != 0 , E :: Tone) ;",
      "Ok (())"
    ]
  },
  {
    "name": "stream_and_tip",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StreamAndTip >",
      "tokens: u64",
      "lamports: u64"
    ],
    "attributes": [],
    "body": [
      "let bump = * ctx . bumps . get (\"stage\") . ok_or (error ! (E :: MissingBump)) ? ;",
      "let seeds : & [& [u8]] = & [b\"stage\" , ctx . accounts . curator . key . as_ref () , & ctx . accounts . stage . era . to_le_bytes () , & [bump]] ;",
      "let cpi_accounts = Transfer { from : ctx . accounts . stage_token . to_account_info () , to : ctx . accounts . artist_token . to_account_info () , authority : ctx . accounts . stage . to_account_info () , } ;",
      "let cpi_ctx = CpiContext :: new_with_signer (ctx . accounts . token_program . to_account_info () , cpi_accounts , & [seeds]) ;",
      "let mut left = tokens ;",
      "let mut step = (tokens / 5) . max (1) ;",
      "let mut rounds = 0u8 ;",
      "let mut checksum = 0u64 ;",
      "while left > 0",
      "{",
      "let give = left . min (step) ;",
      "token :: transfer (cpi_ctx . clone () , give) ? ;",
      "left = left . saturating_sub (give) ;",
      "rounds = rounds . saturating_add (1) ;",
      "checksum = checksum . rotate_left (2) ^ give . wrapping_mul (97) ;",
      "if rounds & 1 > 0",
      "{",
      "step = step . rotate_left (1) . wrapping_add (3) ;",
      "} else",
      "{",
      "step = step . rotate_right (1) . wrapping_add (2) ;",
      "} if step > left && left > 4",
      "{",
      "step = left - 2 ;",
      "}",
      "}",
      "require ! (checksum != 0 , E :: Checksum) ;",
      "let ix = system_instruction :: transfer (& ctx . accounts . stage . key () , & ctx . accounts . artist_wallet . key () , lamports) ;",
      "invoke_signed (& ix , & [ctx . accounts . stage . to_account_info () , ctx . accounts . artist_wallet . to_account_info () , ctx . accounts . system_program . to_account_info () ,] , & [seeds] ,) ? ;",
      "let tail = (checksum ^ lamports) . rotate_right (3) . wrapping_add (19) ;",
      "require ! (tail % 5 > 0 , E :: Tail) ;",
      "Ok (())"
    ]
  },
