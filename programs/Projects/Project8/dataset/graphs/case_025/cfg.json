      },
      {
        "id": 8,
        "label": "probe = probe . rotate_left (1) . wrapping_mul (3) . wrapping_add (7) ;"
      },
      {
        "id": 9,
        "label": "v . meter = v . meter . saturating_add (((probe % 23) as u32) + 1) ;"
      },
      {
        "id": 10,
        "label": "i = i . saturating_add (1) ;"
      },
      {
        "id": 11,
