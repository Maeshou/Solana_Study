  {
    "name": "award",
    "inputs": [
      "ctx: Context < Award >",
      "points: u64"
    ],
    "nodes": [
      {
        "id": 8,
        "label": "Entry"
      },
      {
        "id": 9,
        "label": "let bump = * ctx . bumps . get (\"bank\") . ok_or (error ! (E :: MissingBump)) ? ;"
      },
      {
        "id": 10,
        "label": "let seeds : & [& [u8]] = & [b\"bank\" , ctx . accounts . owner . key . as_ref () , & ctx . accounts . bank . series . to_le_bytes () , & [bump]] ;"
      },
      {
        "id": 11,
        "label": "let cpi_accounts = Transfer { from : ctx . accounts . bank_token . to_account_info () , to : ctx . accounts . user_token . to_account_info () , authority : ctx . accounts . bank . to_account_info () , } ;"
      },
      {
        "id": 12,
        "label": "let cpi_ctx = CpiContext :: new_with_signer (ctx . accounts . token_program . to_account_info () , cpi_accounts , & [seeds]) ;"
      },
      {
        "id": 13,
        "label": "let mut rounds = 3u8 ;"
      },
      {
        "id": 14,
        "label": "let mut step = (points / 3) . max (1) ;"
      },
      {
        "id": 15,
        "label": "let mut left = points ;"
      },
      {
        "id": 16,
        "label": "while rounds > 0 { let give = left . min (step) ; token :: transfer (cpi_ctx . clone () , give) ? ; left = left . saturating_sub (give) ; step = step . rotate_left (1) . wrapping_add (1) ; if left > 0 && step > left { step = left ; } rounds = rounds . saturating_sub (1) ; }"
      },
      {
        "id": 17,
        "label": "let give = left . min (step) ;"
      },
      {
        "id": 18,
        "label": "token :: transfer (cpi_ctx . clone () , give) ? ;"
      },
      {
        "id": 19,
        "label": "left = left . saturating_sub (give) ;"
      },
      {
        "id": 20,
        "label": "step = step . rotate_left (1) . wrapping_add (1) ;"
      },
      {
        "id": 21,
        "label": "if left > 0 && step > left { step = left ; }"
      },
      {
        "id": 22,
        "label": "step = left ;"
      },
      {
        "id": 23,
        "label": "rounds = rounds . saturating_sub (1) ;"
      },
      {
        "id": 24,
        "label": "Ok (())"
      }
