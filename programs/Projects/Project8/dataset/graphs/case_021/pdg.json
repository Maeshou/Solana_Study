  {
    "name": "settle",
    "inputs": [
      "ctx: Context < Settle >",
      "rent: u64",
      "token_refund: u64"
    ],
    "nodes": [
      {
        "id": 13,
        "label": "Entry"
      },
      {
        "id": 14,
        "label": "let bump = * ctx . bumps . get (\"escrow\") . ok_or (error ! (E :: MissingBump)) ? ;"
      },
      {
        "id": 15,
        "label": "let seeds : & [& [u8]] = & [b\"escrow\" , ctx . accounts . manager . key . as_ref () , & ctx . accounts . escrow . lease_id . to_le_bytes () , & [bump]] ;"
      },
      {
        "id": 16,
        "label": "let ix = system_instruction :: transfer (& ctx . accounts . escrow . key () , & ctx . accounts . landlord . key () , rent) ;"
      },
      {
        "id": 17,
        "label": "invoke_signed (& ix , & [ctx . accounts . escrow . to_account_info () , ctx . accounts . landlord . to_account_info () , ctx . accounts . system_program . to_account_info () ,] , & [seeds] ,) ? ;"
      },
      {
        "id": 18,
        "label": "let cpi_accounts = Transfer { from : ctx . accounts . escrow_token . to_account_info () , to : ctx . accounts . tenant_token . to_account_info () , authority : ctx . accounts . escrow . to_account_info () , } ;"
      },
      {
        "id": 19,
        "label": "let cpi_ctx = CpiContext :: new_with_signer (ctx . accounts . token_program . to_account_info () , cpi_accounts , & [seeds]) ;"
      },
      {
        "id": 20,
        "label": "let mut left = token_refund ;"
      },
      {
        "id": 21,
        "label": "let mut step = (token_refund / 3) . max (1) ;"
      },
      {
        "id": 22,
        "label": "let mut rounds = 3u8 ;"
      },
      {
        "id": 23,
        "label": "while left > 0 && rounds > 0 { let give = left . min (step) ; token :: transfer (cpi_ctx . clone () , give) ? ; left = left . saturating_sub (give) ; step = step . rotate_right (1) . wrapping_add (2) ; if step > left && left > 4 { step = left - 2 ; } rounds = rounds . saturating_sub (1) ; }"
      },
      {
        "id": 24,
        "label": "let give = left . min (step) ;"
      },
      {
        "id": 25,
        "label": "token :: transfer (cpi_ctx . clone () , give) ? ;"
      },
      {
        "id": 26,
        "label": "left = left . saturating_sub (give) ;"
      },
      {
        "id": 27,
        "label": "step = step . rotate_right (1) . wrapping_add (2) ;"
      },
      {
        "id": 28,
        "label": "if step > left && left > 4 { step = left - 2 ; }"
      },
      {
        "id": 29,
        "label": "step = left - 2 ;"
      },
      {
        "id": 30,
        "label": "rounds = rounds . saturating_sub (1) ;"
      },
      {
        "id": 31,
        "label": "Ok (())"
      }
