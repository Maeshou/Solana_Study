  {
    "name": "build_structures",
    "inputs": [
      "count: u32",
      "seed: u64",
      "base: BaseStats",
      "now: i64"
    ],
    "nodes": [
      {
        "id": 30,
        "label": "Entry"
      },
      {
        "id": 31,
        "label": "let mut out = Vec :: new () ;"
      },
      {
        "id": 32,
        "label": "let start = (seed as usize) % (count as usize) ;"
      },
      {
        "id": 33,
        "label": "let mut i = 0usize ;"
      },
      {
        "id": 34,
        "label": "while i < count as usize { let idx = (start + i) % (count as usize) ; let kind = choose_kind (idx) ; let tweak = ((seed . rotate_left ((idx as u32) & 13)) as u32) % 37 ; let hp = base . hp . saturating_add (tweak * 10) ; let dp = base . dp . saturating_add ((tweak % 9) * 10) ; let up = base . upkeep . saturating_add (tweak % 7) ; out . push (DefenseStructure { structure_id : idx as u32 , structure_type : kind , health_points : hp , defense_power : dp , maintenance_cost : up , last_upgrade : now , }) ; i = i . saturating_add (1) ; }"
      },
