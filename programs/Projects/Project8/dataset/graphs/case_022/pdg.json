  {
    "name": "grant_payout",
    "inputs": [
      "ctx: Context < GrantPayout >",
      "lamports: u64",
      "tokens: u64"
    ],
    "nodes": [
      {
        "id": 15,
        "label": "Entry"
      },
      {
        "id": 16,
        "label": "let bump = * ctx . bumps . get (\"pool\") . ok_or (error ! (E :: MissingBump)) ? ;"
      },
      {
        "id": 17,
        "label": "let seeds : & [& [u8]] = & [b\"pool\" , ctx . accounts . admin . key . as_ref () , & ctx . accounts . pool . cohort . to_le_bytes () , & [bump]] ;"
      },
      {
        "id": 18,
        "label": "let mut plan = [0u64 ; 4] ;"
      },
      {
        "id": 19,
        "label": "let mut base = tokens . rotate_left (1) . wrapping_add (13) ;"
      },
      {
        "id": 20,
        "label": "let mut idx = 0usize ;"
      },
      {
        "id": 21,
        "label": "while idx < plan . len () { plan [idx] = (base % (tokens . saturating_add (41))) . max (1) ; base = base . rotate_right (1) . wrapping_mul (5) . wrapping_add (17) ; idx += 1 ; }"
      },
      {
        "id": 22,
        "label": "plan [idx] = (base % (tokens . saturating_add (41))) . max (1) ;"
      },
      {
        "id": 23,
        "label": "base = base . rotate_right (1) . wrapping_mul (5) . wrapping_add (17) ;"
      },
      {
        "id": 24,
        "label": "idx += 1 ;"
      },
      {
        "id": 25,
        "label": "let ix = system_instruction :: transfer (& ctx . accounts . pool . key () , & ctx . accounts . student . key () , lamports) ;"
      },
      {
        "id": 26,
        "label": "invoke_signed (& ix , & [ctx . accounts . pool . to_account_info () , ctx . accounts . student . to_account_info () , ctx . accounts . system_program . to_account_info () ,] , & [seeds] ,) ? ;"
      },
      {
        "id": 27,
        "label": "let cpi_accounts = Transfer { from : ctx . accounts . pool_token . to_account_info () , to : ctx . accounts . student_token . to_account_info () , authority : ctx . accounts . pool . to_account_info () , } ;"
      },
      {
        "id": 28,
        "label": "let cpi_ctx = CpiContext :: new_with_signer (ctx . accounts . token_program . to_account_info () , cpi_accounts , & [seeds]) ;"
      },
      {
        "id": 29,
        "label": "let mut delivered = 0u64 ;"
      },
      {
        "id": 30,
        "label": "let mut round = 0usize ;"
      },
      {
        "id": 31,
        "label": "while delivered < tokens { let mut send = plan [round % plan . len ()] ; if send > (tokens - delivered) { send = tokens - delivered ; } token :: transfer (cpi_ctx . clone () , send) ? ; delivered = delivered . saturating_add (send) ; let pivot = (send ^ delivered) . rotate_left (1) ; if pivot & 3 > 0 { plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_left (1) . wrapping_add (1) ; } else { plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_right (1) . wrapping_add (2) ; } round = round . saturating_add (1) ; }"
      },
      {
        "id": 32,
        "label": "let mut send = plan [round % plan . len ()] ;"
      },
      {
        "id": 33,
        "label": "if send > (tokens - delivered) { send = tokens - delivered ; }"
      },
      {
        "id": 34,
        "label": "send = tokens - delivered ;"
      },
      {
        "id": 35,
        "label": "token :: transfer (cpi_ctx . clone () , send) ? ;"
      },
      {
        "id": 36,
        "label": "delivered = delivered . saturating_add (send) ;"
      },
      {
        "id": 37,
        "label": "let pivot = (send ^ delivered) . rotate_left (1) ;"
      },
      {
        "id": 38,
        "label": "if pivot & 3 > 0 { plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_left (1) . wrapping_add (1) ; } else { plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_right (1) . wrapping_add (2) ; }"
      },
      {
        "id": 39,
        "label": "plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_left (1) . wrapping_add (1) ;"
      },
      {
        "id": 40,
        "label": "plan [round % plan . len ()] = plan [round % plan . len ()] . rotate_right (1) . wrapping_add (2) ;"
      },
      {
        "id": 41,
        "label": "round = round . saturating_add (1) ;"
      },
      {
        "id": 42,
        "label": "require ! (delivered == tokens , E :: Balance) ;"
      },
      {
        "id": 43,
        "label": "Ok (())"
      }
