        "id": 1,
        "label": "let character = & mut ctx . accounts . character_stats ;"
      },
      {
        "id": 2,
        "label": "character . experience += earned_exp ;"
      },
      {
        "id": 3,
        "label": "msg ! (\"Quest completed! '{}' earned {} EXP.\" , character . name , earned_exp) ;"
      },
      {
        "id": 4,
        "label": "msg ! (\"Total EXP: {}\" , character . experience) ;"
      },
      {
        "id": 5,
        "label": "let mut leveled_up = false ;"
      },
      {
        "id": 6,
        "label": "let mut required_exp = character . level as u64 * 100 ;"
      },
      {
        "id": 7,
        "label": "while character . experience >= required_exp { character . level += 1 ; character . experience -= required_exp ; character . hp += 20 ; character . attack += 5 ; leveled_up = true ; msg ! (\"Level up! '{}' is now level {}\" , character . name , character . level) ; required_exp = character . level as u64 * 100 ; }"
      },
