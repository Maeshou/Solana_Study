        "label": "let mut stone_found = false ;"
      },
      {
        "id": 6,
        "label": "let mut stone_index = 0 ;"
      },
      {
        "id": 7,
        "label": "for (index , item) in inventory . items . iter () . enumerate () { if item . item_id == evolution_stone_id { if item . quantity > 0 { stone_found = true ; stone_index = index ; } } }"
      },
      {
        "id": 8,
        "label": "if item . item_id == evolution_stone_id { if item . quantity > 0 { stone_found = true ; stone_index = index ; } }"
      },
