        "label": "let inventory = & mut ctx . accounts . player_inventory ;"
      },
      {
        "id": 3,
        "label": "require ! (monster . level >= 30 , GameError :: MonsterNotHighLevelEnough) ;"
      },
      {
        "id": 4,
        "label": "require ! (monster . is_evolvable , GameError :: MonsterCannotEvolve) ;"
      },
      {
