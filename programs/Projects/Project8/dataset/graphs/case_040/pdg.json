      },
      {
        "id": 10,
        "label": "stone_found = true ;"
      },
      {
        "id": 11,
        "label": "stone_index = index ;"
      },
      {
        "id": 12,
        "label": "require ! (stone_found , GameError :: EvolutionStoneNotFound) ;"
      },
      {
        "id": 13,
        "label": "inventory . items [stone_index] . quantity -= 1 ;"
      },
      {
        "id": 14,
        "label": "monster . monster_type += 1 ;"
      },
      {
        "id": 15,
        "label": "monster . attack_power = (monster . attack_power as f32 * 1.5) as u32 ;"
      },
      {
        "id": 16,
        "label": "monster . defense_power = (monster . defense_power as f32 * 1.4) as u32 ;"
      },
      {
        "id": 17,
        "label": "monster . level = 1 ;"
      },
      {
        "id": 18,
        "label": "monster . is_evolvable = false ;"
      },
      {
        "id": 19,
        "label": "msg ! (\"Monster has evolved to type {}!\" , monster . monster_type) ;"
