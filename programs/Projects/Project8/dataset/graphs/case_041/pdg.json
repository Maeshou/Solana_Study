      },
      {
        "id": 12,
        "label": "harvest_amount += 25 ;"
      },
      {
        "id": 13,
        "label": "if land . rarity == 3 { harvest_amount += 70 ; }"
      },
      {
        "id": 14,
        "label": "harvest_amount += 70 ;"
      },
      {
        "id": 15,
        "label": "let resource_id_to_add = land . resource_type ;"
      },
      {
        "id": 16,
        "label": "let mut resource_updated = false ;"
      },
      {
        "id": 17,
        "label": "for resource in player_resources . resources . iter_mut () { if resource . resource_id == resource_id_to_add { resource . amount += harvest_amount ; resource_updated = true ; } }"
      },
      {
        "id": 18,
        "label": "if resource . resource_id == resource_id_to_add { resource . amount += harvest_amount ; resource_updated = true ; }"
      },
      {
        "id": 19,
        "label": "resource . amount += harvest_amount ;"
      },
      {
        "id": 20,
        "label": "resource_updated = true ;"
      },
      {
        "id": 21,
        "label": "if ! resource_updated { player_resources . resources . push (Resource { resource_id : resource_id_to_add , amount : harvest_amount , }) ; }"
      },
      {
        "id": 22,
        "label": "player_resources . resources . push (Resource { resource_id : resource_id_to_add , amount : harvest_amount , }) ;"
      },
      {
        "id": 23,
        "label": "land . last_harvest_timestamp = current_timestamp ;"
      },
      {
        "id": 24,
