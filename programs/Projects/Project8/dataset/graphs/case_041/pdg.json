      "ctx: Context < HarvestFromLand >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let land = & mut ctx . accounts . land_plot ;"
      },
      {
        "id": 2,
        "label": "let player_resources = & mut ctx . accounts . player_resources ;"
      },
      {
        "id": 3,
        "label": "let clock = Clock :: get () ? ;"
      },
      {
        "id": 4,
        "label": "let current_timestamp = clock . unix_timestamp ;"
      },
      {
        "id": 5,
        "label": "let time_elapsed = current_timestamp . saturating_sub (land . last_harvest_timestamp) ;"
      },
      {
        "id": 6,
        "label": "require ! (time_elapsed > 3600 , GameError :: HarvestOnCooldown) ;"
      },
      {
        "id": 7,
        "label": "let mut harvest_amount : u64 = 0 ;"
      },
      {
        "id": 8,
        "label": "for _ in 0 .. (time_elapsed / 60) { if land . rarity == 1 { harvest_amount += 10 ; } if land . rarity == 2 { harvest_amount += 25 ; } if land . rarity == 3 { harvest_amount += 70 ; } }"
      },
      {
