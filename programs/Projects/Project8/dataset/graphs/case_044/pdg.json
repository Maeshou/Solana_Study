[
  {
    "name": "enchant_equipment",
    "inputs": [
      "ctx: Context < EnchantEquipment >",
      "scroll_id: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let equipment = & mut ctx . accounts . equipment_nft ;"
      },
      {
        "id": 2,
        "label": "let inventory = & mut ctx . accounts . player_inventory ;"
      },
      {
        "id": 3,
        "label": "require ! (equipment . enchant_level < 10 , GameError :: MaxEnchantLevelReached) ;"
      },
      {
        "id": 4,
        "label": "let mut scroll_found = false ;"
      },
      {
        "id": 5,
        "label": "let mut scroll_index = 0 ;"
      },
      {
        "id": 6,
        "label": "for (index , item) in inventory . items . iter () . enumerate () { if item . item_id == scroll_id { if item . quantity > 0 { scroll_found = true ; scroll_index = index ; } } }"
      },
      {
        "id": 7,
        "label": "if item . item_id == scroll_id { if item . quantity > 0 { scroll_found = true ; scroll_index = index ; } }"
      },
      {
        "id": 8,
        "label": "if item . quantity > 0 { scroll_found = true ; scroll_index = index ; }"
      },
      {
        "id": 9,
        "label": "scroll_found = true ;"
      },
      {
        "id": 10,
        "label": "scroll_index = index ;"
      },
      {
        "id": 11,
        "label": "require ! (scroll_found , GameError :: ScrollNotFound) ;"
      },
      {
        "id": 12,
        "label": "inventory . items [scroll_index] . quantity -= 1 ;"
      },
      {
        "id": 13,
        "label": "let success_chance = 100 - (equipment . enchant_level * 8) ;"
      },
      {
        "id": 14,
        "label": "let clock = Clock :: get () ? ;"
      },
      {
        "id": 15,
        "label": "let random_value = (clock . slot % 100) as u8 ;"
      },
      {
        "id": 16,
        "label": "if random_value < success_chance { equipment . enchant_level += 1 ; equipment . bonus_stats += 5 ; msg ! (\"Enchantment successful! New enchant level: +{}\" , equipment . enchant_level) ; } else { if equipment . enchant_level > 5 { equipment . enchant_level -= 1 ; equipment . bonus_stats -= 5 ; msg ! (\"Enchantment failed! Enchant level decreased to +{}.\" , equipment . enchant_level) ; } else { msg ! (\"Enchantment failed! No change in enchant level.\") ; } }"
      },
      {
        "id": 17,
        "label": "equipment . enchant_level += 1 ;"
      },
      {
        "id": 18,
        "label": "equipment . bonus_stats += 5 ;"
      },
      {
        "id": 19,
        "label": "msg ! (\"Enchantment successful! New enchant level: +{}\" , equipment . enchant_level) ;"
      },
      {
        "id": 20,
        "label": "if equipment . enchant_level > 5 { equipment . enchant_level -= 1 ; equipment . bonus_stats -= 5 ; msg ! (\"Enchantment failed! Enchant level decreased to +{}.\" , equipment . enchant_level) ; } else { msg ! (\"Enchantment failed! No change in enchant level.\") ; }"
      },
      {
        "id": 21,
        "label": "equipment . enchant_level -= 1 ;"
      },
      {
        "id": 22,
        "label": "equipment . bonus_stats -= 5 ;"
      },
      {
        "id": 23,
        "label": "msg ! (\"Enchantment failed! Enchant level decreased to +{}.\" , equipment . enchant_level) ;"
      },
      {
