[
  {
    "name": "compute_odds",
    "inputs": [
      "ctx: Context < OddsCtx >",
      "base_rate: u8",
      "total_mints: u64",
      "issuer_flag: u8"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let held = ctx . accounts . hold_acc . amount ;"
      },
      {
        "id": 2,
        "label": "let num = held + issuer_flag as u64 ;"
      },
      {
        "id": 3,
        "label": "let den = total_mints + 1 ;"
      },
      {
        "id": 4,
        "label": "let rate = (num . checked_mul (base_rate as u64) . unwrap_or (0) . checked_div (den) . unwrap_or (0)) . min (100) as u8 ;"
      },
      {
        "id": 5,
        "label": "let rnd = (ctx . accounts . clock . unix_timestamp as u64 % 100) as u8 ;"
      },
      {
        "id": 6,
        "label": "let success = rnd < rate ;"
      },
      {
        "id": 7,
        "label": "let o = & mut ctx . accounts . outcome ;"
      },
      {
        "id": 8,
        "label": "o . numerator = num ;"
      },
      {
        "id": 9,
        "label": "o . denominator = den ;"
      },
      {
        "id": 10,
        "label": "o . chance = rate ;"
      },
      {
        "id": 11,
        "label": "o . random = rnd ;"
      },
      {
        "id": 12,
        "label": "o . is_win = success ;"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: held"
      },
