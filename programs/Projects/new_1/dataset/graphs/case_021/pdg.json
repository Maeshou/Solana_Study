[
  {
    "name": "perform_draw",
    "inputs": [
      "ctx: Context < DrawContext >",
      "base_rate_bps: u16",
      "total_supply: u64",
      "issuer_flag: u8"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let owned = ctx . accounts . hold_acc . amount ;"
      },
      {
        "id": 2,
        "label": "let numerator = owned + issuer_flag as u64 ;"
      },
      {
        "id": 3,
        "label": "let denominator = total_supply + 1 ;"
      },
      {
        "id": 4,
        "label": "let rate_bps = (numerator . checked_mul (base_rate_bps as u64) . unwrap () / denominator) as u16 ;"
      },
      {
        "id": 5,
        "label": "let random_bps = (ctx . accounts . clock . unix_timestamp as u64 % 10_000) as u16 ;"
      },
      {
        "id": 6,
        "label": "let success = random_bps < rate_bps ;"
      },
      {
        "id": 7,
        "label": "emit ! (DrawOutcome { caller : * ctx . accounts . user . key , numerator , denominator , rate_bps , random_bps , success , }) ;"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: owned"
      },
      {
        "from": 2,
        "to": 2,
        "label": "def: numerator"
      },
      {
        "from": 1,
        "to": 2,
        "label": "data_dep: owned"
      },
      {
        "from": 3,
        "to": 3,
        "label": "def: denominator"
      },
      {
        "from": 4,
        "to": 4,
        "label": "def: rate_bps"
      },
      {
        "from": 2,
        "to": 4,
        "label": "data_dep: numerator"
      },
      {
        "from": 3,
        "to": 4,
        "label": "data_dep: denominator"
      },
      {
        "from": 5,
        "to": 5,
        "label": "def: random_bps"
      },
      {
        "from": 6,
        "to": 6,
        "label": "def: success"
      },
      {
        "from": 5,
        "to": 6,
        "label": "data_dep: random_bps"
      },
      {
        "from": 4,
        "to": 6,
        "label": "data_dep: rate_bps"
      }
    ]
  }
]