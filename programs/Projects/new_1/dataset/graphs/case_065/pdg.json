[
  {
    "name": "draw_lottery",
    "inputs": [
      "ctx: Context < DrawLottery >",
      "base_percent: u8"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let hold = ctx . accounts . hold_acc . amount ;"
      },
      {
        "id": 2,
        "label": "let raw = hold . checked_mul (base_percent as u64) . unwrap_or (0) ;"
      },
      {
        "id": 3,
        "label": "let chance = raw . min (100) . try_into () . unwrap () ;"
      },
      {
        "id": 4,
        "label": "let rand = (ctx . accounts . clock . unix_timestamp as u64 % 100) as u8 ;"
      },
      {
        "id": 5,
        "label": "let won = rand < chance ;"
      },
      {
        "id": 6,
        "label": "let s = & mut ctx . accounts . lottery_data ;"
      },
      {
        "id": 7,
        "label": "s . chance = chance ;"
      },
      {
        "id": 8,
        "label": "s . random = rand ;"
      },
      {
        "id": 9,
        "label": "s . is_win = won ;"
      },
      {
        "id": 10,
        "label": "Ok (())"
      }
    ],
