[
  {
    "name": "claim_airdrop",
    "inputs": [
      "ctx: Context < ClaimAirdrop >",
      "amount_per_nft: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let count = ctx . accounts . hold_acc . amount ;"
      },
      {
        "id": 2,
        "label": "let to_drop = count . checked_mul (amount_per_nft) . unwrap () ;"
      },
      {
        "id": 3,
        "label": "let data = & mut ctx . accounts . airdrop_data ;"
      },
      {
        "id": 4,
        "label": "data . total_airdropped = data . total_airdropped . checked_add (to_drop) . unwrap () ;"
      },
      {
        "id": 5,
        "label": "data . last_claim_time = ctx . accounts . clock . unix_timestamp ;"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: count"
      },
      {
