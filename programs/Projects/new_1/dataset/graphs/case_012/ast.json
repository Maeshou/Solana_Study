[
  {
    "name": "transfer",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < TransferContext >",
      "amount: u64"
    ],
    "attributes": [
      "# [doc = \" 任意のアカウントから資金を引き出せてしまう関数\"]"
    ],
    "body": [
      "let vault_info = ctx . accounts . vault . to_account_info () ;",
      "let recipient_info = ctx . accounts . recipient . clone () ;",
      "let _authority = & ctx . accounts . authority ;",
      "let _fallback = & ctx . accounts . fallback ;",
      "* * vault_info . try_borrow_mut_lamports () ? -= amount ;",
      "* * recipient_info . try_borrow_mut_lamports () ? += amount ;",
      "Ok (())"
    ]
  },
  {
    "name": "TransferContext",
    "node_type": "struct",
    "fields": [
      {
        "name": "vault",
        "attribute": "# [doc = \" 資金を保管するアカウント\"] # [account (mut)]",
        "field_type": "Account < 'info , Vault >"
      },
      {
        "name": "recipient",
        "attribute": "# [doc = \" 出金先アカウント（AccountInfo を使用）\"] # [account (mut)]",
        "field_type": "AccountInfo < 'info >"
      },
      {
        "name": "authority",
        "attribute": "# [doc = \" CHECK: 署名者チェックが省略されたままのアカウント\"]",
        "field_type": "AccountInfo < 'info >"
      },
      {
        "name": "fallback",
        "attribute": "# [doc = \" CHECK: UncheckedAccount も併用\"]",
        "field_type": "UncheckedAccount < 'info >"
      },
      {
        "name": "system_program",
        "attribute": null,
        "field_type": "Program < 'info , System >"
      }
    ],
    "inputs": null,
    "attributes": null,
    "body": null
  },
  {
    "name": "Vault",
    "node_type": "struct",
    "fields": [
      {
        "name": "authority",
        "attribute": "# [doc = \" 本来はここで authority を検証する\"]",
