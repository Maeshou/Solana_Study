[
  {
    "name": "stake_and_update",
    "inputs": [
      "ctx: Context < StakeUpdate >",
      "reward_per_nft: u64",
      "is_premium: u8"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let prev_count = ctx . accounts . stake_data . staked_count ;"
      },
      {
        "id": 2,
        "label": "let new_count = prev_count + 1 ;"
      },
      {
        "id": 3,
        "label": "let base_reward = reward_per_nft . checked_mul (new_count) . unwrap () ;"
      },
      {
        "id": 4,
        "label": "let bonus = is_premium as u64 ;"
      },
      {
        "id": 5,
        "label": "let reward = base_reward . checked_add (bonus) . unwrap () ;"
      },
      {
        "id": 6,
        "label": "let total_before = ctx . accounts . reward_data . total_reward ;"
      },
      {
        "id": 7,
        "label": "let total_after = total_before . checked_add (reward) . unwrap () ;"
      },
      {
        "id": 8,
        "label": "let now = ctx . accounts . clock . unix_timestamp ;"
      },
      {
        "id": 9,
        "label": "ctx . accounts . stake_data . staked_count = new_count ;"
      },
      {
        "id": 10,
        "label": "ctx . accounts . stake_data . last_stake_time = now ;"
      },
      {
        "id": 11,
        "label": "ctx . accounts . reward_data . total_reward = total_after ;"
      },
      {
        "id": 12,
        "label": "ctx . accounts . reward_data . last_reward_time = now ;"
      },
      {
        "id": 13,
        "label": "emit ! (StakeEvent { user : * ctx . accounts . user . key , staked : new_count , reward , total : total_after , }) ;"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: prev_count"
      },
      {
        "from": 2,
        "to": 2,
        "label": "def: new_count"
      },
      {
        "from": 1,
        "to": 2,
        "label": "data_dep: prev_count"
      },
      {
        "from": 3,
        "to": 3,
        "label": "def: base_reward"
      },
      {
        "from": 2,
        "to": 3,
        "label": "data_dep: new_count"
      },
      {
        "from": 4,
        "to": 4,
        "label": "def: bonus"
      },
      {
        "from": 5,
        "to": 5,
        "label": "def: reward"
      },
      {
        "from": 3,
        "to": 5,
        "label": "data_dep: base_reward"
      },
      {
        "from": 4,
        "to": 5,
        "label": "data_dep: bonus"
      },
      {
        "from": 6,
        "to": 6,
        "label": "def: total_before"
      },
      {
        "from": 7,
