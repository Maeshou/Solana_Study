[
  {
    "name": "convert_to_points",
    "inputs": [
      "ctx: Context < ConvertCtx >",
      "point_rate: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let count = ctx . accounts . hold_acc . amount ;"
      },
      {
        "id": 2,
        "label": "let minted = count . checked_mul (point_rate) . unwrap () ;"
      },
      {
        "id": 3,
        "label": "let data = & mut ctx . accounts . point_data ;"
      },
      {
        "id": 4,
        "label": "data . total = data . total . checked_add (minted) . unwrap () ;"
      },
      {
        "id": 5,
        "label": "data . last_earned = ctx . accounts . clock . unix_timestamp ;"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: count"
      },
      {
        "from": 2,
        "to": 2,
        "label": "def: minted"
      },
      {
        "from": 1,
        "to": 2,
        "label": "data_dep: count"
      },
      {
        "from": 3,
        "to": 3,
        "label": "def: data"
      },
      {
        "from": 3,
        "to": 4,
        "label": "data_dep: data"
      },
      {
        "from": 3,
        "to": 4,
        "label": "data_dep: data"
      },
      {
        "from": 2,
        "to": 4,
        "label": "data_dep: minted"
      },
      {
        "from": 3,
        "to": 5,
        "label": "data_dep: data"
      }
    ]
  }
