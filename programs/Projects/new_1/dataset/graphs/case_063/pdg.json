  {
    "name": "register_template",
    "inputs": [
      "ctx: Context < RegisterTemplate >",
      "name: String",
      "metadata_url: String",
      "image_hash: [u8 ; 32]"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let tmpl = & mut ctx . accounts . template ;"
      },
      {
        "id": 2,
        "label": "tmpl . author = * ctx . accounts . user . key ;"
      },
      {
        "id": 3,
        "label": "let mut hasher = hash (& [name . as_bytes () , & ctx . accounts . user . key . to_bytes ()] . concat ()) . to_bytes () ;"
      },
      {
        "id": 4,
        "label": "let mut id_arr = [0u8 ; 8] ;"
      },
      {
        "id": 5,
        "label": "id_arr . copy_from_slice (& hasher [.. 8]) ;"
      },
      {
        "id": 6,
        "label": "tmpl . template_id = u64 :: from_le_bytes (id_arr) ;"
      },
      {
        "id": 7,
        "label": "tmpl . name = name ;"
      },
      {
        "id": 8,
        "label": "tmpl . metadata = metadata_url ;"
      },
      {
