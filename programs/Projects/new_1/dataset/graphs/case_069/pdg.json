  {
    "name": "log_access",
    "inputs": [
      "ctx: Context < LogAccess >",
      "duration_secs: u64",
      "rate: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let nft_count = ctx . accounts . provider_hold . amount ;"
      },
      {
        "id": 2,
        "label": "let sum = duration_secs . checked_add (nft_count) . unwrap_or (0) ;"
      },
      {
        "id": 3,
        "label": "let to_award = sum . checked_mul (rate) . unwrap_or (0) ;"
      },
      {
        "id": 4,
        "label": "let rec = & mut ctx . accounts . provider_reward ;"
      },
      {
        "id": 5,
        "label": "rec . session_count = rec . session_count . saturating_add (1) ;"
      },
      {
        "id": 6,
        "label": "rec . combined_total = rec . combined_total . saturating_add (sum) ;"
      },
      {
        "id": 7,
        "label": "rec . total_awarded = rec . total_awarded . saturating_add (to_award) ;"
      }
    ],
