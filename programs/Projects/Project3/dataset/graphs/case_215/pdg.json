  {
    "name": "claim_tokens",
    "inputs": [
      "ctx: Context < ClaimTokens >"
    ],
    "nodes": [
      {
        "id": 11,
        "label": "Entry"
      },
      {
        "id": 12,
        "label": "let vest = & mut ctx . accounts . vesting ;"
      },
      {
        "id": 13,
        "label": "let now = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 14,
        "label": "require ! (now >= vest . release_at , ErrorCode :: TooEarly) ;"
      },
      {
        "id": 15,
        "label": "let to_release = vest . total_amount . checked_sub (vest . released) . ok_or (ErrorCode :: Overflow) ? ;"
      },
      {
        "id": 16,
        "label": "require ! (to_release > 0 , ErrorCode :: NothingToClaim) ;"
      },
      {
        "id": 17,
        "label": "let seeds = & [b\"vesting\" , vest . beneficiary . as_ref () , & [vest . bump]] ;"
      },
      {
        "id": 18,
        "label": "let signer = & [& seeds [..]] ;"
      },
      {
        "id": 19,
        "label": "let cpi_accounts = Transfer { from : ctx . accounts . pda_token_account . to_account_info () , to : ctx . accounts . beneficiary_account . to_account_info () , authority : ctx . accounts . vesting . to_account_info () , } ;"
      },
      {
        "id": 20,
        "label": "let cpi_ctx = CpiContext :: new_with_signer (ctx . accounts . token_program . to_account_info () , cpi_accounts , signer ,) ;"
      },
      {
        "id": 21,
        "label": "transfer (cpi_ctx , to_release) ? ;"
      },
      {
        "id": 22,
        "label": "vest . released = vest . released . checked_add (to_release) . ok_or (ErrorCode :: Overflow) ? ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
    ],
