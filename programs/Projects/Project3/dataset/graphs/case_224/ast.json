[
  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeLogger >",
      "bump: u8"
    ],
    "attributes": [
      "# [doc = \" ログアカウントの初期化\"]"
    ],
    "body": [
      "let logger = & mut ctx . accounts . logger ;",
      "logger . owner = * ctx . accounts . user . key ;",
      "logger . bump = bump ;",
      "logger . last_timestamp = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "log",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < LogTimestamp >"
    ],
    "attributes": [
      "# [doc = \" 現在の UNIX タイムスタンプを記録\"]"
    ],
    "body": [
      "let logger = & mut ctx . accounts . logger ;",
      "logger . last_timestamp = Clock :: get () ? . unix_timestamp ;",
      "Ok (())"
    ]
  },
