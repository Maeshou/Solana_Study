[
  {
    "name": "initialize_counter",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeCounter >",
      "bump: u8"
    ],
    "attributes": [
      "# [doc = \" カウンターアカウントの初期化\"]"
    ],
    "body": [
      "let ctr = & mut ctx . accounts . counter ;",
      "ctr . owner = * ctx . accounts . user . key ;",
      "ctr . bump = bump ;",
      "ctr . value = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "increment",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyCounter >"
    ],
    "attributes": [
      "# [doc = \" カウンターをインクリメント\"]"
    ],
    "body": [
      "let ctr = & mut ctx . accounts . counter ;",
      "ctr . value = ctr . value . checked_add (1) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "reset",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ModifyCounter >",
      "new_value: u64"
    ],
    "attributes": [
      "# [doc = \" カウンターをリセット\"]"
    ],
    "body": [
      "let ctr = & mut ctx . accounts . counter ;",
      "ctr . value = new_value ;",
      "Ok (())"
    ]
  },
