[
  {
    "name": "initialize",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializeTagManager >",
      "bump: u8"
    ],
    "attributes": [
      "# [doc = \" タグアカウントを初期化\"]"
    ],
    "body": [
      "let account = & mut ctx . accounts . tag_account ;",
      "account . owner = * ctx . accounts . user . key ;",
      "account . bump = bump ;",
      "account . tags = Default :: default () ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_tag",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddTag >",
      "tag: String"
    ],
    "attributes": [
      "# [doc = \" タグを追加\"]"
    ],
    "body": [
      "let account = & mut ctx . accounts . tag_account ;",
      "account . tags . push (tag) ;",
      "Ok (())"
    ]
  },
  {
    "name": "clear_tags",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ClearTags >"
    ],
    "attributes": [
      "# [doc = \" すべてのタグをクリア\"]"
    ],
    "body": [
      "let account = & mut ctx . accounts . tag_account ;",
      "account . tags . clear () ;",
      "Ok (())"
    ]
  },
