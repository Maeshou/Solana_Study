        "label": "let elapsed = current_time - stake_acc . staked_at ;"
      },
      {
        "id": 4,
        "label": "let reward_amount = (elapsed as u64) . checked_mul (ctx . accounts . config . reward_rate) . unwrap () ;"
      },
      {
        "id": 5,
        "label": "stake_acc . rewards = stake_acc . rewards . checked_add (reward_amount) . unwrap () ;"
      },
      {
