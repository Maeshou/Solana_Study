      {
        "id": 3,
        "label": "let user = & mut ctx . accounts . user ;"
      },
      {
        "id": 4,
        "label": "let pool = & mut ctx . accounts . penalty_pool . to_account_info () ;"
      },
      {
        "id": 5,
        "label": "let now = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 6,
        "label": "let overdue = (now - rental . end_time) . max (0) as u64 ;"
      },
      {
        "id": 7,
        "label": "let penalty = overdue . checked_mul (config . late_fee_per_second) . unwrap () ;"
      },
      {
        "id": 8,
        "label": "rental . total_late_fees = rental . total_late_fees . checked_add (penalty) . unwrap () ;"
      },
      {
        "id": 9,
        "label": "* * user . to_account_info () . lamports . borrow_mut () -= penalty ;"
      },
      {
        "id": 10,
        "label": "* * pool . lamports . borrow_mut () += penalty ;"
      },
