[
  {
    "name": "init_theta",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitTheta >",
      "f: u8",
      "g: u32",
      "h: u64"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . theta_account ;",
      "acc . f = f ;",
      "acc . g = g ;",
      "acc . h = h ;",
      "acc . bump = * ctx . bumps . get (\"theta_account\") . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_theta",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateTheta >"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . theta_account ;",
      "for i in 0 .. 3",
      "{",
      "if i == 1",
      "{",
      "msg ! (\"i is one\") ;",
      "acc . f = acc . f . checked_add (5) . unwrap () ;",
      "acc . g = acc . g . checked_add (acc . f as u32) . unwrap () ;",
      "msg ! (\"f+5 & g+f\") ;",
      "} else",
      "{",
      "msg ! (\"i != 1\") ;",
      "acc . h = acc . h . checked_mul ((i + 1) as u64) . unwrap () ;",
      "msg ! (\"h updated\") ;",
      "}",
      "}",
      "if acc . g % 2 == 0",
      "{",
      "msg ! (\"g even\") ;",
      "acc . h = acc . h . checked_sub (acc . g as u64) . unwrap () ;",
      "acc . f = acc . f . checked_add (2) . unwrap () ;",
      "} else",
      "{",
      "msg ! (\"g odd\") ;",
      "acc . g = acc . g . checked_mul (2) . unwrap () ;",
      "msg ! (\"g doubled\") ;",
      "}",
      "Ok (())"
    ]
  },
