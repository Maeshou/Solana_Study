[
  {
    "name": "create_course",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateCourse >",
      "capacity: u8"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . course ;",
      "c . capacity = capacity ;",
      "c . filled = 0 ;",
      "c . has_space = capacity > 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "enroll",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Enroll >",
      "want: u8"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let c = & mut ctx . accounts . course ;",
      "let mut i = 0u8 ;",
      "while i < want",
      "{",
      "if c . filled >= c . capacity",
      "{",
      "break ;",
      "} c . filled += 1 ;",
      "i += 1 ;",
      "}",
      "c . has_space = c . filled < c . capacity ;",
      "c . last_enroll = now ;",
      "Ok (())"
    ]
  },
