[
  {
    "name": "init_lottery",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitLottery >",
      "tickets: u32"
    ],
    "attributes": [],
    "body": [
      "let l = & mut ctx . accounts . lottery ;",
      "l . tickets = tickets ;",
      "l . winners = 0 ;",
      "l . draw_flag = false ;",
      "let mut w = 0u32 ;",
      "while (w + 1) * (w + 1) <= tickets",
      "{",
      "w += 1 ;",
      "}",
      "l . winners = w ;",
      "Ok (())"
    ]
  },
  {
    "name": "draw",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Draw >"
    ],
    "attributes": [],
    "body": [
      "let l = & mut ctx . accounts . lottery ;",
      "if ! l . draw_flag",
      "{",
      "l . draw_flag = true ;",
      "} else",
      "{",
      "l . draw_flag = false ;",
      "}",
      "Ok (())"
    ]
  },
