[
  {
    "name": "init_pipeline",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPipeline >",
      "stages: u8"
    ],
    "attributes": [
      "# [doc = \" パイプラインを初期化\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . pipeline ;",
      "p . stages = stages ;",
      "p . completed = 0 ;",
      "p . errors = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "process_stage",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessStage >",
      "success: bool"
    ],
    "attributes": [
      "# [doc = \" ステージを実行しエラーをカウント\"]"
    ],
    "body": [
      "let p = & mut ctx . accounts . pipeline ;",
      "if success",
      "{",
      "if p . completed < p . stages",
      "{",
      "p . completed += 1 ;",
      "} } else",
      "{",
      "p . errors += 1 ;",
      "}",
      "Ok (())"
    ]
  },
