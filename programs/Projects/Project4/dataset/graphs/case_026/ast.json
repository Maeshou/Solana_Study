[
  {
    "name": "create_event",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CreateEvent >",
      "start: i64",
      "end: i64"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let e = & mut ctx . accounts . event ;",
      "e . start = start ;",
      "e . end = end ;",
      "e . created_at = now ;",
      "if e . end < e . start",
      "{",
      "let tmp = e . start ;",
      "e . start = e . end ;",
      "e . end = tmp ;",
      "}",
      "e . duration = e . end - e . start ;",
      "Ok (())"
    ]
  },
  {
    "name": "cancel_event",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < CancelEvent >"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let ev = & mut ctx . accounts . event ;",
      "ev . canceled = true ;",
      "ev . canceled_at = now ;",
      "if now < ev . start",
      "{",
      "ev . early_cancel = true ;",
      "} else",
      "{",
      "ev . early_cancel = false ;",
      "}",
      "Ok (())"
    ]
  },
