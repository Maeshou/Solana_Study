[
  {
    "name": "register_sensor",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterSensor >",
      "readings: Vec < u32 >"
    ],
    "attributes": [],
    "body": [
      "let mut sum = 0 ;",
      "for & r in readings . iter ()",
      "{",
      "if r > 0",
      "{",
      "sum += r ;",
      "}",
      "}",
      "let sensor = & mut ctx . accounts . sensor ;",
      "sensor . total = sum ;",
      "Ok (())"
    ]
  },
  {
    "name": "reset_sensor",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ResetSensor >"
    ],
    "attributes": [],
    "body": [
      "let sensor = & mut ctx . accounts . sensor ;",
      "sensor . total = 0 ;",
      "Ok (())"
    ]
  },
