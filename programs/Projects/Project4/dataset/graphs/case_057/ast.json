[
  {
    "name": "init_auction",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitAuction >",
      "duration_secs: u32"
    ],
    "attributes": [],
    "body": [
      "let auc = & mut ctx . accounts . auction ;",
      "auc . duration = duration_secs ;",
      "auc . bids = 0 ;",
      "let bidcount = & mut ctx . accounts . bidcount ;",
      "bidcount . count = 0 ;",
      "let highflag = & mut ctx . accounts . highflag ;",
      "highflag . flag = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "place_bid",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PlaceBid >",
      "bid_value: u32"
    ],
    "attributes": [],
    "body": [
      "let auc = & mut ctx . accounts . auction ;",
      "auc . bids += 1 ;",
      "let bidcount = & mut ctx . accounts . bidcount ;",
      "bidcount . count += 1 ;",
      "let highflag = & mut ctx . accounts . highflag ;",
      "highflag . flag = bid_value > (auc . duration * 10) ;",
      "Ok (())"
    ]
  },
