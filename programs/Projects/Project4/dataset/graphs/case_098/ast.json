[
  {
    "name": "init_iota",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitIota >",
      "d1: u8",
      "d2: u32",
      "d3: u64"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . iota_account ;",
      "acc . d1 = d1 ;",
      "acc . d2 = d2 ;",
      "acc . d3 = d3 ;",
      "acc . bump = * ctx . bumps . get (\"iota_account\") . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_iota",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateIota >"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . iota_account ;",
      "for i in 0 .. (acc . d2 as usize + 1)",
      "{",
      "if i % 2 == 0",
      "{",
      "msg ! (\"even i: {}\" , i) ;",
      "acc . d2 = acc . d2 . checked_add (i as u32) . unwrap () ;",
      "acc . d1 = acc . d1 . checked_add (1) . unwrap () ;",
      "msg ! (\"d1 & d2 updated\") ;",
      "} else",
      "{",
      "msg ! (\"odd i: {}\" , i) ;",
      "acc . d3 = acc . d3 . checked_mul (2) . unwrap () ;",
      "msg ! (\"d3 doubled\") ;",
      "}",
      "}",
      "if acc . d3 > 1000",
      "{",
      "msg ! (\"d3 big\") ;",
      "acc . d1 = acc . d1 . checked_sub (1) . unwrap () ;",
      "acc . d2 = acc . d2 . checked_mul (3) . unwrap () ;",
      "} else",
      "{",
      "msg ! (\"d3 small\") ;",
      "acc . d3 = acc . d3 . checked_add (acc . d2 as u64) . unwrap () ;",
      "msg ! (\"d3 increased\") ;",
      "}",
      "Ok (())"
    ]
  },
