[
  {
    "name": "list",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < List >",
      "prices: Vec < u64 >"
    ],
    "attributes": [],
    "body": [
      "let mut min = u64 :: MAX ;",
      "for & p in prices . iter ()",
      "{",
      "if p < min",
      "{",
      "min = p ;",
      "}",
      "}",
      "let l = & mut ctx . accounts . listing ;",
      "l . lowest = min ;",
      "Ok (())"
    ]
  },
  {
    "name": "buy",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Buy >",
      "qty: u32"
    ],
    "attributes": [],
    "body": [
      "let _l = & ctx . accounts . listing ;",
      "let pur = & mut ctx . accounts . purchase_account ;",
      "pur . buyer = ctx . accounts . user . key () ;",
      "pur . qty = qty ;",
      "Ok (())"
    ]
  },
