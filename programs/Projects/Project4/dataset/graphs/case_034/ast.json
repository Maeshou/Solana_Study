[
  {
    "name": "register_vehicle",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegVeh >",
      "vin: String"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let v = & mut ctx . accounts . vehicle ;",
      "v . vin = vin ;",
      "v . maint_count = 0 ;",
      "v . registered_at = now ;",
      "v . next_service = now + 30 * 24 * 60 * 60 ;",
      "Ok (())"
    ]
  },
  {
    "name": "service_vehicle",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ServiceVeh >"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let v = & mut ctx . accounts . vehicle ;",
      "v . maint_count += 1 ;",
      "v . last_service = now ;",
      "v . next_service = now + 30 * 24 * 60 * 60 ;",
      "Ok (())"
    ]
  },
