[
  {
    "name": "init_encrypt",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitEncrypt >",
      "rounds: u8"
    ],
    "attributes": [],
    "body": [
      "let e = & mut ctx . accounts . encrypt ;",
      "e . rounds = rounds ;",
      "e . key_strength = 128 ;",
      "e . secure_flag = false ;",
      "if rounds >= 10",
      "{",
      "e . key_strength = 256 ;",
      "e . secure_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "process_round",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessRound >"
    ],
    "attributes": [],
    "body": [
      "let e = & mut ctx . accounts . encrypt ;",
      "let mut i = 0u8 ;",
      "while i < e . rounds",
      "{",
      "e . key_strength = e . key_strength . saturating_add (1) ;",
      "i += 1 ;",
      "}",
      "if e . key_strength > 512",
      "{",
      "e . secure_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
