[
  {
    "name": "init_pool",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitPool >",
      "threads: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pool ;",
      "p . threads = threads ;",
      "p . active_threads = 0 ;",
      "p . overload_flag = false ;",
      "if threads > 4",
      "{",
      "p . overload_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "spawn_thread",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < SpawnThread >"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pool ;",
      "if p . active_threads < p . threads",
      "{",
      "p . active_threads = p . active_threads . saturating_add (1) ;",
      "} else",
      "{",
      "p . overload_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
