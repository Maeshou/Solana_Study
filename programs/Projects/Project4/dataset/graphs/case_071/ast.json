[
  {
    "name": "init_rate_limiter",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitRateLimiter >",
      "limit: u32"
    ],
    "attributes": [],
    "body": [
      "let rl = & mut ctx . accounts . rate_limiter ;",
      "rl . limit = limit ;",
      "rl . used = 0 ;",
      "rl . reset_counter = 0 ;",
      "let mut cnt = 0u32 ;",
      "while cnt < limit",
      "{",
      "cnt += 1 ;",
      "}",
      "rl . reset_counter = cnt ;",
      "Ok (())"
    ]
  },
  {
    "name": "use_slot",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UseSlot >"
    ],
    "attributes": [],
    "body": [
      "let rl = & mut ctx . accounts . rate_limiter ;",
      "if rl . used < rl . limit",
      "{",
      "rl . used += 1 ;",
      "} else",
      "{",
      "rl . used = 0 ;",
      "rl . reset_counter = rl . limit ;",
      "}",
      "Ok (())"
    ]
  },
