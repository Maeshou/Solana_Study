      },
      {
        "id": 19,
        "label": "let stock = & mut ctx . accounts . stock ;"
      },
      {
        "id": 20,
        "label": "if change >= 0 { let half = (change as u32) / 2 ; stock . level = stock . level . saturating_add (half) ; stock . level = stock . level . saturating_add (change as u32 - half) ; } else { let dec = (- change) as u32 ; let half = dec / 2 ; stock . level = stock . level . saturating_sub (half) ; stock . level = stock . level . saturating_sub (dec - half) ; }"
      },
