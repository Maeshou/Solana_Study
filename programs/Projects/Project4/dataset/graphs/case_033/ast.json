[
  {
    "name": "start_project",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < StartProj >",
      "id: u64"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let p = & mut ctx . accounts . project ;",
      "p . id = id ;",
      "p . active = true ;",
      "p . created_at = now ;",
      "p . log_count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "add_log",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AddLog >",
      "msg: String"
    ],
    "attributes": [],
    "body": [
      "let pr = & mut ctx . accounts . project ;",
      "if pr . active",
      "{",
      "pr . log_count += 1 ;",
      "pr . last_log = msg . clone () ;",
      "pr . last_logged_at = Clock :: get () ? . unix_timestamp ;",
      "}",
      "Ok (())"
    ]
  },
