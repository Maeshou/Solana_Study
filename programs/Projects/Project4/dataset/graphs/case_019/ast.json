[
  {
    "name": "create",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Create >",
      "title: String"
    ],
    "attributes": [],
    "body": [
      "let a = & mut ctx . accounts . article ;",
      "a . title = title ;",
      "a . len = a . title . len () as u32 ;",
      "Ok (())"
    ]
  },
  {
    "name": "analyze",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Analyze >",
      "keywords: Vec < String >"
    ],
    "attributes": [],
    "body": [
      "let mut count = 0 ;",
      "for kw in keywords . iter ()",
      "{",
      "if ctx . accounts . article . title . contains (kw)",
      "{",
      "count += 1 ;",
      "}",
      "}",
      "let ana = & mut ctx . accounts . analysis_account ;",
      "ana . matches = count ;",
      "Ok (())"
    ]
  },
