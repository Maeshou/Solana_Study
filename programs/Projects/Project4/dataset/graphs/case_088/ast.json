[
  {
    "name": "init_cooldown",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCooldown >",
      "actions: u8"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . cooldown ;",
      "c . actions = actions ;",
      "c . cooldowns = 0 ;",
      "c . cooldown_flag = false ;",
      "let mut i = 0u8 ;",
      "while i < actions",
      "{",
      "c . cooldowns = c . cooldowns . saturating_add (1) ;",
      "i += 1 ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "trigger_action",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < TriggerAction >"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . cooldown ;",
      "if c . cooldowns < c . actions",
      "{",
      "c . cooldowns = c . cooldowns . saturating_add (1) ;",
      "c . cooldown_flag = false ;",
      "} else",
      "{",
      "c . cooldown_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
