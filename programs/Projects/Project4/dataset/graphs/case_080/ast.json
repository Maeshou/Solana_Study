[
  {
    "name": "init_uptime",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitUptime >",
      "cycles: u32"
    ],
    "attributes": [],
    "body": [
      "let u = & mut ctx . accounts . uptime ;",
      "u . start_count = cycles ;",
      "u . failures = 0 ;",
      "u . stable_flag = false ;",
      "if cycles > 1000",
      "{",
      "u . stable_flag = true ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "record_failure",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RecordFailure >"
    ],
    "attributes": [],
    "body": [
      "let u = & mut ctx . accounts . uptime ;",
      "u . failures = u . failures . saturating_add (1) ;",
      "if u . failures > u . start_count / 10",
      "{",
      "u . stable_flag = false ;",
      "}",
      "Ok (())"
    ]
  },
