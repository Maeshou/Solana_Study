[
  {
    "name": "init_session_pipe",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSessionPipe >",
      "steps: u8"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pipe ;",
      "p . steps = steps ;",
      "p . completed = 0 ;",
      "p . error_count = 0 ;",
      "let mut i = 0u8 ;",
      "while i < steps / 2",
      "{",
      "p . completed = p . completed . saturating_add (1) ;",
      "i += 1 ;",
      "}",
      "Ok (())"
    ]
  },
  {
    "name": "advance_step",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < AdvanceStep >",
      "success: bool"
    ],
    "attributes": [],
    "body": [
      "let p = & mut ctx . accounts . pipe ;",
      "if success",
      "{",
      "if p . completed < p . steps",
      "{",
      "p . completed = p . completed . saturating_add (1) ;",
      "} } else",
      "{",
      "p . error_count = p . error_count . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
