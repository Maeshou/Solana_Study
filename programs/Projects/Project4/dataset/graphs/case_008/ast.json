[
  {
    "name": "init_chat",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitChat >"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . chat ;",
      "c . count = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "broadcast",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Broadcast >",
      "messages: Vec < String >"
    ],
    "attributes": [],
    "body": [
      "let mut sent = ctx . accounts . chat . count ;",
      "for msg in messages . iter ()",
      "{",
      "if msg . len () > 0",
      "{",
      "sent += 1 ;",
      "}",
      "}",
      "let nc = & mut ctx . accounts . new_chat ;",
      "nc . sent = sent ;",
      "Ok (())"
    ]
  },
