[
  {
    "name": "init_eta",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitEta >",
      "u: u8",
      "v: u32",
      "w: u64"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . eta_account ;",
      "acc . u = u ;",
      "acc . v = v ;",
      "acc . w = w ;",
      "acc . bump = * ctx . bumps . get (\"eta_account\") . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "update_eta",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpdateEta >"
    ],
    "attributes": [],
    "body": [
      "let acc = & mut ctx . accounts . eta_account ;",
      "let mut i = 1 ;",
      "while i <= (acc . v as usize)",
      "{",
      "if i % 2 == 0",
      "{",
      "msg ! (\"even loop: {}\" , i) ;",
      "acc . v = acc . v . checked_add (4) . unwrap () ;",
      "acc . u = acc . u . checked_add (1) . unwrap () ;",
      "msg ! (\"u & v updated\") ;",
      "} else",
      "{",
      "msg ! (\"odd loop: {}\" , i) ;",
      "acc . w = acc . w . checked_mul (2) . unwrap () ;",
      "msg ! (\"w doubled\") ;",
      "} i += 1 ;",
      "}",
      "if acc . w < 100",
      "{",
      "msg ! (\"w low\") ;",
      "acc . u = acc . u . checked_add (2) . unwrap () ;",
      "acc . v = acc . v . checked_add (10) . unwrap () ;",
      "msg ! (\"u+2 & v+10\") ;",
      "} else",
      "{",
      "msg ! (\"w high\") ;",
      "acc . w = acc . w . checked_sub (50) . unwrap () ;",
      "msg ! (\"w-50\") ;",
      "}",
      "Ok (())"
    ]
  },
