[
  {
    "name": "register_user",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < RegisterUser >",
      "email: String"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let u = & mut ctx . accounts . user ;",
      "u . email = email . clone () ;",
      "u . plan = 0 ;",
      "u . last_updated = now ;",
      "u . valid_email = email . contains (\"@\") ;",
      "Ok (())"
    ]
  },
  {
    "name": "upgrade_plan",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < UpgradePlan >",
      "tier: u8"
    ],
    "attributes": [],
    "body": [
      "let now = Clock :: get () ? . unix_timestamp ;",
      "let u = & mut ctx . accounts . user ;",
      "if tier > 0",
      "{",
      "u . plan = tier as u32 ;",
      "u . upgrade_count += 1 ;",
      "u . last_updated = now ;",
      "}",
      "Ok (())"
    ]
  },
