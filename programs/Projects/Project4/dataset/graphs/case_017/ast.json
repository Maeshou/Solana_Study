[
  {
    "name": "init_course",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitCourse >",
      "slots: u8"
    ],
    "attributes": [],
    "body": [
      "let c = & mut ctx . accounts . course ;",
      "c . capacity = slots ;",
      "c . filled = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "batch_enroll",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < BatchEnroll >",
      "students: Vec < Pubkey >"
    ],
    "attributes": [],
    "body": [
      "let mut filled = ctx . accounts . course . filled ;",
      "if filled >= ctx . accounts . course . capacity",
      "{",
      "return Ok (()) ;",
      "}",
      "for _ in students . iter ()",
      "{",
      "if filled < ctx . accounts . course . capacity",
      "{",
      "filled += 1 ;",
      "}",
      "}",
      "let e = & mut ctx . accounts . enrollment_account ;",
      "e . count = filled ;",
      "Ok (())"
    ]
  },
