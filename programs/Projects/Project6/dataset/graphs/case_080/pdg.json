      },
      {
        "id": 8,
        "label": "for i in 1 ..= amount { let seed = clock . slot . saturating_add (i as u64) ; let random_index = seed % (loot_table . len () as u64) ; received_items . push (loot_table [random_index as usize]) ; }"
      },
