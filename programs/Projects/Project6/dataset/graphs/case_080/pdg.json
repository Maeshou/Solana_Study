      },
      {
        "id": 4,
        "label": "let cost_per_draw = 100 ;"
      },
      {
        "id": 5,
        "label": "player_resources . gems = player_resources . gems . saturating_sub (cost_per_draw * amount) ;"
      },
      {
        "id": 6,
        "label": "let mut received_items = Vec :: new () ;"
      },
