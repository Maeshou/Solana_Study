      },
      {
        "id": 26,
        "label": "energy_delta += d as i32 ;"
      },
      {
        "id": 27,
        "label": "mix = mix . rotate_left (2) ^ (d as u32 as u32) ;"
      },
      {
        "id": 28,
        "label": "Loop End"
      },
      {
        "id": 29,
        "label": "let adj = if energy_delta >= 0 { energy_delta as u32 } else { 0 } ;"
