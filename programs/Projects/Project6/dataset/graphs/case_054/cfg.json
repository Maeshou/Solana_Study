[
  {
    "name": "enchant_weapon",
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let weapon = & mut ctx . accounts . weapon_to_enchant ;"
      },
      {
        "id": 2,
        "label": "let scroll = & mut ctx . accounts . enchantment_scroll ;"
      },
      {
        "id": 3,
        "label": "let clock = Clock :: get () ? ;"
      },
      {
        "id": 4,
        "label": "let success_seed = (clock . slot % 100) as u32 ;"
      },
      {
        "id": 5,
        "label": "let success_chance = 75 - (weapon . enchant_level * 5) ;"
      },
      {
        "id": 6,
        "label": "let is_success = success_seed < success_chance ;"
      },
      {
        "id": 7,
        "label": "let success_increment = is_success as u32 ;"
      },
      {
        "id": 8,
        "label": "weapon . enchant_level = weapon . enchant_level . saturating_add (success_increment) ;"
      },
      {
        "id": 9,
        "label": "weapon . base_attack = weapon . base_attack . saturating_add (success_increment * 5) ;"
      },
      {
        "id": 10,
        "label": "scroll . quantity = scroll . quantity . saturating_sub (1) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Enchantment finished. Weapon level is now: {}\" , weapon . enchant_level) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
    ],
