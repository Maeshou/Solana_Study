[
  {
    "name": "consume_multiple_potions",
    "inputs": [
      "ctx: Context < ConsumePotion >",
      "quantity: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let character_stats = & mut ctx . accounts . player_character . stats ;"
      },
      {
        "id": 2,
        "label": "let inventory = & mut ctx . accounts . player_inventory ;"
      },
      {
        "id": 3,
        "label": "let health_per_potion = 50 ;"
      },
      {
        "id": 4,
        "label": "let potion_item_id = 1 ;"
      },
      {
        "id": 5,
        "label": "let potion_entry = inventory . items . iter_mut () . find (| item | item . item_id == potion_item_id) . unwrap () ;"
      },
      {
        "id": 6,
        "label": "potion_entry . quantity = potion_entry . quantity . saturating_sub (quantity) ;"
      },
      {
        "id": 7,
        "label": "for _ in 1 .. quantity { let potential_health = character_stats . current_health . saturating_add (health_per_potion) ; character_stats . current_health = potential_health . min (character_stats . max_health) ; }"
      },
      {
        "id": 8,
        "label": "let potential_health = character_stats . current_health . saturating_add (health_per_potion) ;"
      },
      {
        "id": 9,
        "label": "character_stats . current_health = potential_health . min (character_stats . max_health) ;"
      },
      {
        "id": 10,
        "label": "inventory . items . retain (| item | item . quantity > 0) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Consumed {} potions.\" , quantity) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
    ],
