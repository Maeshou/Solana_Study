  {
    "name": "match_orders",
    "inputs": [
      "ctx: Context < MatchOrders >"
    ],
    "nodes": [
      {
        "id": 22,
        "label": "Entry"
      },
      {
        "id": 23,
        "label": "let book = & mut ctx . accounts . order_book ;"
      },
      {
        "id": 24,
        "label": "let a = & mut ctx . accounts . order_a ;"
      },
      {
        "id": 25,
        "label": "let b = & mut ctx . accounts . order_b ;"
      },
      {
        "id": 26,
        "label": "let w = & mut ctx . accounts . warehouse ;"
      },
      {
        "id": 27,
        "label": "let cross = if a . side == Side :: Bid { a . price as i64 - b . price as i64 } else { b . price as i64 - a . price as i64 } ;"
      },
      {
        "id": 28,
        "label": "a . price as i64 - b . price as i64"
      },
      {
        "id": 29,
        "label": "b . price as i64 - a . price as i64"
      },
      {
        "id": 30,
        "label": "let can_match = cross >= 0 ;"
      },
      {
        "id": 31,
        "label": "let mut sum : u64 = 0 ;"
      },
      {
        "id": 32,
        "label": "for i in 0 .. book . hist . len () { let slot = (book . hist [i] as u64) . wrapping_add ((a . price as u64 ^ b . price as u64) & 0xFFFF) ; book . hist [i] = (slot & 0xFFFF) as u32 ; sum = sum . wrapping_add (book . hist [i] as u64) ; }"
      },
      {
        "id": 33,
        "label": "let slot = (book . hist [i] as u64) . wrapping_add ((a . price as u64 ^ b . price as u64) & 0xFFFF) ;"
      },
      {
        "id": 34,
        "label": "book . hist [i] = (slot & 0xFFFF) as u32 ;"
      },
      {
        "id": 35,
        "label": "sum = sum . wrapping_add (book . hist [i] as u64) ;"
      },
      {
        "id": 36,
        "label": "let avg = (sum / (book . hist . len () as u64)) . min (u32 :: MAX as u64) as u32 ;"
      },
      {
        "id": 37,
        "label": "book . mid = avg ;"
      },
      {
        "id": 38,
        "label": "if can_match { let fill = a . qty . min (b . qty) ; a . qty = a . qty . saturating_sub (fill) ; b . qty = b . qty . saturating_sub (fill) ; w . filled = w . filled . saturating_add (fill as u64) ; w . last = fill ; w . hash ^= (fill as u64) . rotate_left (((a . price ^ b . price) & 31) as u32) ; book . liq = book . liq . saturating_add (fill as u64) ; msg ! (\"matched: fill={}, book_mid={}, liq={}\" , fill , book . mid , book . liq) ; } else { let drift = cross . unsigned_abs () . min (10_000) as u32 ; book . hist [0] = book . hist [0] . saturating_add (drift) ; w . last = 0 ; msg ! (\"no match: drift={}, book_mid={}\" , drift , book . mid) ; }"
      },
      {
        "id": 39,
        "label": "let fill = a . qty . min (b . qty) ;"
      },
      {
        "id": 40,
        "label": "a . qty = a . qty . saturating_sub (fill) ;"
      },
      {
        "id": 41,
        "label": "b . qty = b . qty . saturating_sub (fill) ;"
      },
      {
        "id": 42,
        "label": "w . filled = w . filled . saturating_add (fill as u64) ;"
      },
      {
        "id": 43,
        "label": "w . last = fill ;"
      },
      {
        "id": 44,
        "label": "w . hash ^= (fill as u64) . rotate_left (((a . price ^ b . price) & 31) as u32) ;"
      },
      {
        "id": 45,
        "label": "book . liq = book . liq . saturating_add (fill as u64) ;"
      },
      {
        "id": 46,
        "label": "msg ! (\"matched: fill={}, book_mid={}, liq={}\" , fill , book . mid , book . liq) ;"
      },
      {
        "id": 47,
        "label": "let drift = cross . unsigned_abs () . min (10_000) as u32 ;"
      },
      {
        "id": 48,
        "label": "book . hist [0] = book . hist [0] . saturating_add (drift) ;"
      },
      {
        "id": 49,
        "label": "w . last = 0 ;"
      },
      {
        "id": 50,
        "label": "msg ! (\"no match: drift={}, book_mid={}\" , drift , book . mid) ;"
      },
      {
        "id": 51,
        "label": "Ok (())"
      }
