      },
      {
        "id": 26,
        "label": "let w = & mut ctx . accounts . warehouse ;"
      },
      {
        "id": 27,
        "label": "let cross = if a . side == Side :: Bid { a . price as i64 - b . price as i64 } else { b . price as i64 - a . price as i64 } ;"
      },
      {
        "id": 28,
        "label": "let can_match = cross >= 0 ;"
      },
      {
        "id": 29,
        "label": "let mut sum : u64 = 0 ;"
      },
      {
        "id": 30,
        "label": "Loop Start"
      },
      {
        "id": 31,
        "label": "let slot = (book . hist [i] as u64) . wrapping_add ((a . price as u64 ^ b . price as u64) & 0xFFFF) ;"
      },
      {
        "id": 32,
        "label": "book . hist [i] = (slot & 0xFFFF) as u32 ;"
      },
      {
        "id": 33,
        "label": "sum = sum . wrapping_add (book . hist [i] as u64) ;"
      },
      {
        "id": 34,
        "label": "Loop End"
      },
      {
        "id": 35,
        "label": "let avg = (sum / (book . hist . len () as u64)) . min (u32 :: MAX as u64) as u32 ;"
      },
      {
        "id": 36,
        "label": "book . mid = avg ;"
      },
      {
        "id": 37,
        "label": "if statement"
