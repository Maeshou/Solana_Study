[
  {
    "name": "convert_experience_to_levels",
    "inputs": [
      "ctx: Context < ConvertExperience >"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let character = & mut ctx . accounts . player_character ;"
      },
      {
        "id": 2,
        "label": "for _ in 1 .. 6 { let has_enough_exp = character . stats . experience >= character . experience_to_next_level ; let level_up_flag = has_enough_exp as u64 ; character . stats . experience = character . stats . experience . saturating_sub (character . experience_to_next_level * level_up_flag) ; character . stats . level = character . stats . level . saturating_add (level_up_flag as u32) ; character . stats . strength = character . stats . strength . saturating_add (3 * level_up_flag as u32) ; let next_level_exp_increase = (character . experience_to_next_level / 10) * level_up_flag ; character . experience_to_next_level = character . experience_to_next_level . saturating_add (next_level_exp_increase) ; }"
      },
      {
        "id": 3,
        "label": "let has_enough_exp = character . stats . experience >= character . experience_to_next_level ;"
      },
      {
        "id": 4,
        "label": "let level_up_flag = has_enough_exp as u64 ;"
      },
      {
        "id": 5,
        "label": "character . stats . experience = character . stats . experience . saturating_sub (character . experience_to_next_level * level_up_flag) ;"
      },
      {
        "id": 6,
        "label": "character . stats . level = character . stats . level . saturating_add (level_up_flag as u32) ;"
      },
      {
        "id": 7,
        "label": "character . stats . strength = character . stats . strength . saturating_add (3 * level_up_flag as u32) ;"
      },
      {
        "id": 8,
        "label": "let next_level_exp_increase = (character . experience_to_next_level / 10) * level_up_flag ;"
      },
      {
        "id": 9,
        "label": "character . experience_to_next_level = character . experience_to_next_level . saturating_add (next_level_exp_increase) ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Experience converted. Current level: {}\" , character . stats . level) ;"
      },
      {
        "id": 11,
        "label": "Ok (())"
      }
    ],
