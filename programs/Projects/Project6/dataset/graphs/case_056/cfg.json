      },
      {
        "id": 21,
        "label": "let mut sum : u128 = 0 ;"
      },
      {
        "id": 22,
        "label": "let mut mix : u64 = 0 ;"
      },
      {
        "id": 23,
        "label": "Loop Start"
      },
      {
        "id": 24,
        "label": "let clipped = c . min (100_000) ;"
      },
      {
        "id": 25,
        "label": "sum = sum . saturating_add (clipped as u128) ;"
      },
      {
        "id": 26,
        "label": "mix = (mix ^ clipped) . rotate_left (3) ;"
      },
      {
        "id": 27,
        "label": "Loop End"
      },
      {
        "id": 28,
        "label": "let avg = if contributions . is_empty () { 0 } else { (sum / contributions . len () as u128) as u64 } ;"
      },
      {
        "id": 29,
        "label": "if statement"
      },
      {
        "id": 30,
        "label": "predicate"
      },
      {
        "id": 31,
        "label": "actor . rank == Officer"
      },
      {
        "id": 32,
        "label": "True body"
      },
      {
        "id": 33,
        "label": "actor . points = actor . points . saturating_add (avg + (mix & 0x3FF)) ;"
      },
      {
        "id": 34,
        "label": "target . points = target . points . saturating_add (avg / 2) ;"
      },
      {
        "id": 35,
        "label": "g . activity_sum = g . activity_sum . saturating_add (actor . points) . saturating_add (target . points) ;"
      },
      {
        "id": 36,
        "label": "msg ! (\"Officer path: avg={}, mix={}, actor={}, target={}\" , avg , mix , actor . points , target . points) ;"
      },
      {
        "id": 37,
        "label": "False body"
      },
      {
        "id": 38,
        "label": "actor . points = actor . points . saturating_add (avg / 2) ;"
      },
      {
        "id": 39,
        "label": "target . points = target . points . saturating_add (avg + ((mix >> 2) & 0x1FF)) ;"
      },
      {
        "id": 40,
        "label": "g . activity_sum = g . activity_sum . saturating_add (actor . points) . saturating_add (target . points) ;"
      },
      {
        "id": 41,
        "label": "msg ! (\"Non-officer path: avg={}, mix={}, actor={}, target={}\" , avg , mix , actor . points , target . points) ;"
      },
      {
        "id": 42,
        "label": "merge"
      },
      {
        "id": 43,
        "label": "let mut x = (g . activity_sum as u128) . max (1) ;"
      },
      {
        "id": 44,
        "label": "let mut i = 0 ;"
      },
      {
        "id": 45,
        "label": "Loop Start"
      },
      {
        "id": 46,
        "label": "x = (x + (g . activity_sum as u128 / x)) . max (1) / 2 ;"
      },
      {
        "id": 47,
        "label": "i += 1 ;"
      },
      {
        "id": 48,
        "label": "Loop End"
      },
      {
        "id": 49,
        "label": "board . guild = g . key () ;"
      },
      {
        "id": 50,
        "label": "board . meter = (x as u64) . min (1_000_000) ;"
      },
