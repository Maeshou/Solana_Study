  {
    "name": "execute_raid_attack",
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let raid_boss = & mut ctx . accounts . raid_boss ;"
      },
      {
        "id": 2,
        "label": "let participants = & ctx . remaining_accounts ;"
      },
      {
        "id": 3,
        "label": "let mut total_damage : u64 = 1 ;"
      },
      {
        "id": 4,
        "label": "Loop Start"
      },
      {
        "id": 5,
        "label": "let mut participant = Account :: < PlayerCharacter > :: try_from (participant_account) ? ;"
      },
      {
        "id": 6,
        "label": "let damage = participant . stats . strength . saturating_add (participant . stats . dexterity / 2) ;"
      },
      {
        "id": 7,
        "label": "participant . stats . experience = participant . stats . experience . saturating_add (50) ;"
      },
      {
        "id": 8,
        "label": "total_damage = total_damage . saturating_add (damage as u64) ;"
      },
      {
        "id": 9,
        "label": "Loop End"
      },
      {
        "id": 10,
        "label": "raid_boss . current_health = raid_boss . current_health . saturating_sub (total_damage) ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Raid attack executed. Total damage: {}.\" , total_damage) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
