      },
      {
        "id": 25,
        "label": "let mut i = 0u16 ;"
      },
      {
        "id": 26,
        "label": "while i < cycles { qa . head = qa . head . wrapping_add (ra . ratio as u64 & 7) ; qb . tail = qb . tail . wrapping_add (rb . ratio as u64 & 11) ; let score = ((qa . head ^ qb . tail) as u32) & 0xFFFF ; if (score & 1) == 0 { w . good = w . good . saturating_add ((score / 3) as u64 + 1) ; ra . ratio = (ra . ratio + 1) . min (100) ; } else { w . bad = w . bad . saturating_add ((score / 2) as u64 + 1) ; rb . ratio = rb . ratio . saturating_sub (1) . max (1) ; } w . hash ^= ((ra . tag as u64) << 16) ^ ((rb . tag as u64) << 1) ^ (score as u64) ; i += 1 ; }"
      },
      {
        "id": 27,
        "label": "qa . head = qa . head . wrapping_add (ra . ratio as u64 & 7) ;"
      },
      {
        "id": 28,
        "label": "qb . tail = qb . tail . wrapping_add (rb . ratio as u64 & 11) ;"
      },
      {
        "id": 29,
        "label": "let score = ((qa . head ^ qb . tail) as u32) & 0xFFFF ;"
      },
      {
        "id": 30,
        "label": "if (score & 1) == 0 { w . good = w . good . saturating_add ((score / 3) as u64 + 1) ; ra . ratio = (ra . ratio + 1) . min (100) ; } else { w . bad = w . bad . saturating_add ((score / 2) as u64 + 1) ; rb . ratio = rb . ratio . saturating_sub (1) . max (1) ; }"
      },
      {
        "id": 31,
        "label": "w . good = w . good . saturating_add ((score / 3) as u64 + 1) ;"
      },
      {
        "id": 32,
        "label": "ra . ratio = (ra . ratio + 1) . min (100) ;"
      },
      {
        "id": 33,
        "label": "w . bad = w . bad . saturating_add ((score / 2) as u64 + 1) ;"
      },
      {
        "id": 34,
        "label": "rb . ratio = rb . ratio . saturating_sub (1) . max (1) ;"
      },
      {
        "id": 35,
        "label": "w . hash ^= ((ra . tag as u64) << 16) ^ ((rb . tag as u64) << 1) ^ (score as u64) ;"
      },
      {
        "id": 36,
