        "label": "let mut mix : u16 = 0 ;"
      },
      {
        "id": 26,
        "label": "for r in reps { s = s . saturating_add ((r & 0x7F) as u32) ; mix ^= (r as u16) . rotate_left (1) ; }"
      },
      {
        "id": 27,
        "label": "s = s . saturating_add ((r & 0x7F) as u32) ;"
      },
      {
        "id": 28,
        "label": "mix ^= (r as u16) . rotate_left (1) ;"
      },
      {
        "id": 29,
        "label": "let base = s + (mix & 0xFF) as u32 ;"
      },
      {
        "id": 30,
        "label": "if a . role == Mentor { a . apt = a . apt . saturating_add ((base / 2) as u16) ; b . apt = b . apt . saturating_add (((mix >> 2) & 0x3F) as u16) ; p . affection = p . affection . saturating_add ((base / 4) as u16) ; g . visits = g . visits . saturating_add (1) ; msg ! (\"Mentor: base={}, a.apt={}, b.apt={}, pet.aff={}\" , base , a . apt , b . apt , p . affection) ; } else { a . apt = a . apt . saturating_add ((base / 3) as u16) ; b . apt = b . apt . saturating_add (((mix >> 3) & 0x3F) as u16) ; p . affection = p . affection . saturating_add ((base / 5) as u16) ; g . visits = g . visits . saturating_add (1) ; msg ! (\"Assistant/Guest: base={}, a.apt={}, b.apt={}, pet.aff={}\" , base , a . apt , b . apt , p . affection) ; }"
      },
      {
        "id": 31,
        "label": "a . apt = a . apt . saturating_add ((base / 2) as u16) ;"
      },
      {
        "id": 32,
        "label": "b . apt = b . apt . saturating_add (((mix >> 2) & 0x3F) as u16) ;"
      },
      {
        "id": 33,
        "label": "p . affection = p . affection . saturating_add ((base / 4) as u16) ;"
      },
      {
        "id": 34,
        "label": "g . visits = g . visits . saturating_add (1) ;"
      },
      {
        "id": 35,
        "label": "msg ! (\"Mentor: base={}, a.apt={}, b.apt={}, pet.aff={}\" , base , a . apt , b . apt , p . affection) ;"
      },
      {
        "id": 36,
        "label": "a . apt = a . apt . saturating_add ((base / 3) as u16) ;"
      },
      {
        "id": 37,
        "label": "b . apt = b . apt . saturating_add (((mix >> 3) & 0x3F) as u16) ;"
      },
      {
        "id": 38,
        "label": "p . affection = p . affection . saturating_add ((base / 5) as u16) ;"
      },
      {
        "id": 39,
        "label": "g . visits = g . visits . saturating_add (1) ;"
      },
      {
        "id": 40,
        "label": "msg ! (\"Assistant/Guest: base={}, a.apt={}, b.apt={}, pet.aff={}\" , base , a . apt , b . apt , p . affection) ;"
      },
      {
        "id": 41,
        "label": "let mut x = (g . visits as u128) . max (1) ;"
      },
      {
        "id": 42,
        "label": "let mut i = 0 ;"
      },
      {
        "id": 43,
        "label": "while i < 3 { x = (x + (g . visits as u128 / x)) . max (1) / 2 ; i += 1 ; }"
      },
      {
        "id": 44,
        "label": "x = (x + (g . visits as u128 / x)) . max (1) / 2 ;"
      },
      {
        "id": 45,
        "label": "i += 1 ;"
      },
