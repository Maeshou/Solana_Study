      },
      {
        "id": 25,
        "label": "for st in steps { s = s . saturating_add ((st & 0x3FF) as u32) ; pattern ^= (st . rotate_left (4)) ^ 0xAAAA ; }"
      },
      {
        "id": 26,
        "label": "s = s . saturating_add ((st & 0x3FF) as u32) ;"
      },
