        "label": "a . finds = a . finds . saturating_add (base / 2) ;"
      },
      {
        "id": 31,
        "label": "b . finds = b . finds . saturating_add (((pattern >> 2) & 0x3F) as u32) ;"
      },
      {
        "id": 32,
        "label": "t . counter = t . counter . saturating_add ((base / 4) as u32) ;"
      },
      {
        "id": 33,
        "label": "m . tiles = m . tiles . saturating_add (1) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"Veteran: base={}, a={}, b={}, tile={}\" , base , a . finds , b . finds , t . counter) ;"
      },
      {
        "id": 35,
        "label": "a . finds = a . finds . saturating_add (base / 3) ;"
      },
      {
        "id": 36,
        "label": "b . finds = b . finds . saturating_add (((pattern >> 1) & 0x7F) as u32) ;"
      },
      {
        "id": 37,
        "label": "t . counter = t . counter . saturating_add ((base / 5) as u32) ;"
      },
      {
        "id": 38,
        "label": "m . tiles = m . tiles . saturating_add (1) ;"
      },
      {
        "id": 39,
        "label": "msg ! (\"Rookie/Scout: base={}, a={}, b={}, tile={}\" , base , a . finds , b . finds , t . counter) ;"
      },
      {
        "id": 40,
        "label": "let mut x = (m . tiles as u128) . max (1) ;"
      },
      {
        "id": 41,
        "label": "let mut i = 0 ;"
      },
      {
        "id": 42,
        "label": "while i < 3 { x = (x + (m . tiles as u128 / x)) . max (1) / 2 ; i += 1 ; }"
      },
      {
