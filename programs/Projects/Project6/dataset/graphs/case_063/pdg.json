[
  {
    "name": "unlock_skill_in_tree",
    "inputs": [
      "ctx: Context < UnlockSkill >",
      "skill_id_to_unlock: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let player_skills = & mut ctx . accounts . player_skills ;"
      },
      {
        "id": 2,
        "label": "let player_stats = & mut ctx . accounts . player_stats ;"
      },
      {
        "id": 3,
        "label": "let skill_cost = (skill_id_to_unlock / 10) . saturating_add (1) ;"
      },
      {
        "id": 4,
        "label": "player_stats . skill_points = player_stats . skill_points . saturating_sub (skill_cost) ;"
      },
      {
        "id": 5,
        "label": "for skill in player_skills . unlocked_skills . iter_mut () { let should_unlock = skill . skill_id == skill_id_to_unlock ; skill . is_unlocked |= should_unlock ; }"
      },
      {
        "id": 6,
        "label": "let should_unlock = skill . skill_id == skill_id_to_unlock ;"
      },
      {
        "id": 7,
        "label": "skill . is_unlocked |= should_unlock ;"
      },
      {
        "id": 8,
        "label": "msg ! (\"Skill {} unlocked!\" , skill_id_to_unlock) ;"
      },
      {
        "id": 9,
        "label": "Ok (())"
      }
    ],
