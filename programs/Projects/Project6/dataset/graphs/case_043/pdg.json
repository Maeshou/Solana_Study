  {
    "name": "process_raid_activities",
    "inputs": [
      "ctx: Context < ProcessRaidActivities >",
      "raid_difficulty: u8",
      "bonus_multiplier: u32"
    ],
    "nodes": [
      {
        "id": 22,
        "label": "Entry"
      },
      {
        "id": 23,
        "label": "let guild = & mut ctx . accounts . guild ;"
      },
      {
        "id": 24,
        "label": "let member_a = & mut ctx . accounts . member_a ;"
      },
      {
        "id": 25,
        "label": "let member_b = & mut ctx . accounts . member_b ;"
      },
      {
        "id": 26,
        "label": "let raid_log = & mut ctx . accounts . raid_log ;"
      },
      {
        "id": 27,
        "label": "for round in 0 .. raid_difficulty { if member_a . tier == MemberTier :: Leader { member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 150) . unwrap_or (u64 :: MAX) ; member_a . raids_completed = member_a . raids_completed . checked_add (2) . unwrap_or (u32 :: MAX) ; guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ; msg ! (\"Leader {} gained double experience\" , member_a . member_name) ; } else { member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 75) . unwrap_or (u64 :: MAX) ; member_a . raids_completed = member_a . raids_completed . checked_add (1) . unwrap_or (u32 :: MAX) ; guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ; msg ! (\"Member {} gained standard experience\" , member_a . member_name) ; } }"
      },
      {
        "id": 28,
        "label": "if member_a . tier == MemberTier :: Leader { member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 150) . unwrap_or (u64 :: MAX) ; member_a . raids_completed = member_a . raids_completed . checked_add (2) . unwrap_or (u32 :: MAX) ; guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ; msg ! (\"Leader {} gained double experience\" , member_a . member_name) ; } else { member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 75) . unwrap_or (u64 :: MAX) ; member_a . raids_completed = member_a . raids_completed . checked_add (1) . unwrap_or (u32 :: MAX) ; guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ; msg ! (\"Member {} gained standard experience\" , member_a . member_name) ; }"
      },
      {
        "id": 29,
        "label": "member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 150) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 30,
        "label": "member_a . raids_completed = member_a . raids_completed . checked_add (2) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 31,
        "label": "guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 32,
        "label": "msg ! (\"Leader {} gained double experience\" , member_a . member_name) ;"
      },
      {
        "id": 33,
        "label": "member_a . experience_points = member_a . experience_points . checked_add ((round as u64) * 75) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 34,
        "label": "member_a . raids_completed = member_a . raids_completed . checked_add (1) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 35,
        "label": "guild . total_raids = guild . total_raids . checked_add (1) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 36,
        "label": "msg ! (\"Member {} gained standard experience\" , member_a . member_name) ;"
      },
      {
        "id": 37,
        "label": "let mut b_multiplier = 1 ;"
      },
      {
        "id": 38,
        "label": "while b_multiplier <= bonus_multiplier { member_b . experience_points = member_b . experience_points . checked_add (b_multiplier as u64 * 25) . unwrap_or (u64 :: MAX) ; let bit_bonus = (b_multiplier ^ 0x3) << 2 ; member_b . raids_completed = member_b . raids_completed . checked_add (bit_bonus) . unwrap_or (u32 :: MAX) ; b_multiplier = b_multiplier . wrapping_mul (2) ; }"
      },
      {
        "id": 39,
        "label": "member_b . experience_points = member_b . experience_points . checked_add (b_multiplier as u64 * 25) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 40,
        "label": "let bit_bonus = (b_multiplier ^ 0x3) << 2 ;"
      },
      {
        "id": 41,
        "label": "member_b . raids_completed = member_b . raids_completed . checked_add (bit_bonus) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 42,
        "label": "b_multiplier = b_multiplier . wrapping_mul (2) ;"
      },
      {
        "id": 43,
        "label": "raid_log . raid_count = raid_log . raid_count . checked_add (1) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 44,
        "label": "raid_log . last_raid_timestamp = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 45,
        "label": "raid_log . difficulty_sum = raid_log . difficulty_sum . checked_add (raid_difficulty as u32) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 46,
        "label": "msg ! (\"Raid activities processed for {} rounds\" , raid_difficulty) ;"
      },
      {
        "id": 47,
        "label": "Ok (())"
      }
