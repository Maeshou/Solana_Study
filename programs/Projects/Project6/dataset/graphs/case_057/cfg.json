  {
    "name": "upgrade_gear",
    "nodes": [
      {
        "id": 15,
        "label": "Entry"
      },
      {
        "id": 16,
        "label": "let w = & mut ctx . accounts . workshop ;"
      },
      {
        "id": 17,
        "label": "let smith = & mut ctx . accounts . smith ;"
      },
      {
        "id": 18,
        "label": "let inspector = & mut ctx . accounts . inspector ;"
      },
      {
        "id": 19,
        "label": "let gear = & mut ctx . accounts . gear ;"
      },
      {
        "id": 20,
        "label": "let panel = & mut ctx . accounts . panel ;"
      },
      {
        "id": 21,
        "label": "let mut total : u32 = 0 ;"
      },
      {
        "id": 22,
        "label": "let mut mask : u16 = 0 ;"
      },
      {
        "id": 23,
        "label": "Loop Start"
      },
      {
        "id": 24,
        "label": "let capped = v . min (500) ;"
      },
      {
        "id": 25,
        "label": "total = total . saturating_add (capped as u32) ;"
      },
      {
        "id": 26,
        "label": "mask ^= ((capped as u16) << 1) | ((capped as u16) >> 1) ;"
      },
      {
        "id": 27,
        "label": "Loop End"
      },
      {
        "id": 28,
        "label": "let avg = if base_stats . is_empty () { 0 } else { (total / base_stats . len () as u32) as u16 } ;"
      },
      {
        "id": 29,
        "label": "if statement"
      },
      {
        "id": 30,
        "label": "predicate"
      },
      {
        "id": 31,
        "label": "smith . role == Master"
      },
      {
        "id": 32,
        "label": "True body"
      },
      {
        "id": 33,
        "label": "gear . power = gear . power . saturating_add (avg as u32 + (mask as u32 & 0x3FF)) ;"
      },
      {
        "id": 34,
        "label": "smith . reputation = smith . reputation . saturating_add (2) ;"
      },
      {
        "id": 35,
        "label": "inspector . reputation = inspector . reputation . saturating_add (1) ;"
      },
      {
        "id": 36,
        "label": "w . tool_count = w . tool_count . saturating_add (1) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"Master upgrade applied: avg={}, mask={}, power={}\" , avg , mask , gear . power) ;"
      },
      {
        "id": 38,
        "label": "False body"
      },
      {
        "id": 39,
        "label": "gear . power = gear . power . saturating_add (avg as u32 / 2 + ((mask as u32 >> 2) & 0x1FF)) ;"
      },
      {
        "id": 40,
        "label": "smith . reputation = smith . reputation . saturating_add (1) ;"
      },
      {
        "id": 41,
        "label": "inspector . reputation = inspector . reputation . saturating_add (2) ;"
      },
      {
        "id": 42,
        "label": "w . tool_count = w . tool_count . saturating_add (1) ;"
      },
      {
        "id": 43,
        "label": "msg ! (\"Apprentice/Journeyman upgrade: avg={}, mask={}, power={}\" , avg , mask , gear . power) ;"
      },
      {
        "id": 44,
        "label": "merge"
      },
      {
        "id": 45,
        "label": "let mut x = (gear . power as u128) . max (1) ;"
      },
      {
        "id": 46,
        "label": "let mut i = 0 ;"
      },
      {
        "id": 47,
        "label": "Loop Start"
      },
      {
        "id": 48,
        "label": "x = (x + (gear . power as u128 / x)) . max (1) / 2 ;"
      },
      {
        "id": 49,
        "label": "i += 1 ;"
      },
      {
        "id": 50,
        "label": "Loop End"
      },
      {
        "id": 51,
        "label": "panel . workshop = w . key () ;"
      },
      {
        "id": 52,
        "label": "panel . durability_meter = (x as u32) . min (1_000_000) ;"
      },
      {
        "id": 53,
        "label": "Ok (())"
      }
