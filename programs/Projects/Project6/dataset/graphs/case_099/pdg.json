      },
      {
        "id": 31,
        "label": "let l = & mut ctx . accounts . log ;"
      },
      {
        "id": 32,
        "label": "let mut acc : u32 = 0 ;"
      },
      {
        "id": 33,
        "label": "for i in 0 .. tr . wind . len () { let local = ((tr . wind [i] as u32) ^ (drift as u32) . rotate_left ((i as u32) & 15)) . wrapping_add ((factor as u32) * ((i as u32) + 1)) ; a . telemetry [i % a . telemetry . len ()] = (a . telemetry [i % a . telemetry . len ()] as u32 ^ (local & 0x7FFF)) as u16 ; acc = acc . wrapping_add (local & 0x3FFF) ; }"
      },
      {
        "id": 34,
        "label": "let local = ((tr . wind [i] as u32) ^ (drift as u32) . rotate_left ((i as u32) & 15)) . wrapping_add ((factor as u32) * ((i as u32) + 1)) ;"
      },
      {
        "id": 35,
        "label": "a . telemetry [i % a . telemetry . len ()] = (a . telemetry [i % a . telemetry . len ()] as u32 ^ (local & 0x7FFF)) as u16 ;"
      },
      {
        "id": 36,
        "label": "acc = acc . wrapping_add (local & 0x3FFF) ;"
      },
      {
        "id": 37,
        "label": "if a . kind == Falcon { a . energy = a . energy . saturating_add ((acc & 0x1FFF) + (factor as u32)) ; tr . mix = tr . mix . rotate_left (5) ^ ((a . lane as u32) << 7) ; l . rounds = l . rounds . saturating_add (1) ; l . hash = l . hash . wrapping_add ((acc as u64) . rotate_left (17)) ; msg ! (\"Falcon path: energy boosted and track mixed\") ; } else { r . energy = r . energy . saturating_add (((acc >> 2) & 0x0FFF) + (drift as u32)) ; tr . mix = tr . mix . rotate_right (3) ^ ((r . lane as u32) << 9) ; l . rounds = l . rounds . saturating_add (2) ; l . hash = l . hash ^ ((acc as u64) . rotate_right (11)) ; msg ! (\"Non-Falcon path: rival boosted and track mixed differently\") ; }"
      },
      {
        "id": 38,
        "label": "a . energy = a . energy . saturating_add ((acc & 0x1FFF) + (factor as u32)) ;"
      },
      {
        "id": 39,
        "label": "tr . mix = tr . mix . rotate_left (5) ^ ((a . lane as u32) << 7) ;"
      },
      {
        "id": 40,
        "label": "l . rounds = l . rounds . saturating_add (1) ;"
      },
      {
        "id": 41,
        "label": "l . hash = l . hash . wrapping_add ((acc as u64) . rotate_left (17)) ;"
      },
      {
        "id": 42,
        "label": "msg ! (\"Falcon path: energy boosted and track mixed\") ;"
      },
      {
        "id": 43,
        "label": "r . energy = r . energy . saturating_add (((acc >> 2) & 0x0FFF) + (drift as u32)) ;"
      },
      {
        "id": 44,
        "label": "tr . mix = tr . mix . rotate_right (3) ^ ((r . lane as u32) << 9) ;"
      },
      {
        "id": 45,
        "label": "l . rounds = l . rounds . saturating_add (2) ;"
      },
      {
        "id": 46,
        "label": "l . hash = l . hash ^ ((acc as u64) . rotate_right (11)) ;"
      },
      {
        "id": 47,
        "label": "msg ! (\"Non-Falcon path: rival boosted and track mixed differently\") ;"
      },
      {
        "id": 48,
        "label": "for i in 0 .. l . trace . len () { l . trace [i] = l . trace [i] . rotate_left ((i as u32) & 7) ^ (tr . mix & 0xFFFF) ; }"
      },
      {
        "id": 49,
        "label": "l . trace [i] = l . trace [i] . rotate_left ((i as u32) & 7) ^ (tr . mix & 0xFFFF) ;"
      },
      {
        "id": 50,
        "label": "Ok (())"
      }
