  {
    "name": "generate_passive_resources",
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let generator = & mut ctx . accounts . resource_generator ;"
      },
      {
        "id": 2,
        "label": "let player_resources = & mut ctx . accounts . player_resources ;"
      },
      {
        "id": 3,
        "label": "let clock = Clock :: get () ? ;"
      },
      {
        "id": 4,
        "label": "let time_elapsed = clock . unix_timestamp . saturating_sub (generator . last_claim_timestamp) ;"
      },
      {
        "id": 5,
        "label": "let gold_generated = (time_elapsed as u64) . saturating_mul (generator . gold_per_second) ;"
      },
      {
        "id": 6,
        "label": "player_resources . gold = player_resources . gold . saturating_add (gold_generated) ;"
      },
      {
        "id": 7,
        "label": "generator . last_claim_timestamp = clock . unix_timestamp ;"
      },
      {
        "id": 8,
        "label": "msg ! (\"Claimed {} gold.\" , gold_generated) ;"
      },
      {
        "id": 9,
        "label": "Ok (())"
      }
