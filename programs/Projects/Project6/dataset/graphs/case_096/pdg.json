      },
      {
        "id": 18,
        "label": "let l = & mut ctx . accounts . ledger ;"
      },
      {
        "id": 19,
        "label": "let mut h = salt ^ (f . batch as u64) ;"
      },
      {
        "id": 20,
        "label": "for i in 0 .. 8 { h = h . rotate_left (9) ^ 0x9E3779B97F4A7C15u64 ; s . hist [i] = s . hist [i] . saturating_add (((h >> (i * 5)) & 0xFF) as u32) ; f . mix = f . mix ^ ((h as u32) & 0xFFFF) ; }"
      },
      {
        "id": 21,
        "label": "h = h . rotate_left (9) ^ 0x9E3779B97F4A7C15u64 ;"
      },
      {
        "id": 22,
        "label": "s . hist [i] = s . hist [i] . saturating_add (((h >> (i * 5)) & 0xFF) as u32) ;"
      },
      {
        "id": 23,
        "label": "f . mix = f . mix ^ ((h as u32) & 0xFFFF) ;"
      },
      {
        "id": 24,
        "label": "if s . role == Smith { s . quality = s . quality . saturating_add (((h & 0xFFFF) as u32) + 7) ; f . batch = f . batch . saturating_add (1) ; l . lines = l . lines . saturating_add (1) ; l . last = l . last . wrapping_add (h . rotate_left (7)) ; msg ! (\"Smith path: forged new emblem\") ; } else { a . quality = a . quality . saturating_add ((((h >> 3) & 0x7FFF) as u32) + 5) ; f . mix = f . mix . rotate_right (3) ^ ((h as u32) & 0xFFFF) ; l . lines = l . lines . saturating_add (2) ; l . last = l . last ^ h . rotate_right (11) ; msg ! (\"Non-smith path: appraiser evaluated\") ; }"
      },
      {
        "id": 25,
        "label": "s . quality = s . quality . saturating_add (((h & 0xFFFF) as u32) + 7) ;"
      },
      {
        "id": 26,
        "label": "f . batch = f . batch . saturating_add (1) ;"
      },
      {
        "id": 27,
        "label": "l . lines = l . lines . saturating_add (1) ;"
      },
      {
        "id": 28,
        "label": "l . last = l . last . wrapping_add (h . rotate_left (7)) ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"Smith path: forged new emblem\") ;"
      },
      {
        "id": 30,
        "label": "a . quality = a . quality . saturating_add ((((h >> 3) & 0x7FFF) as u32) + 5) ;"
      },
      {
        "id": 31,
        "label": "f . mix = f . mix . rotate_right (3) ^ ((h as u32) & 0xFFFF) ;"
      },
      {
        "id": 32,
        "label": "l . lines = l . lines . saturating_add (2) ;"
      },
      {
        "id": 33,
        "label": "l . last = l . last ^ h . rotate_right (11) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"Non-smith path: appraiser evaluated\") ;"
      },
      {
        "id": 35,
        "label": "Ok (())"
      }
