        "id": 14,
        "label": "Entry"
      },
      {
        "id": 15,
        "label": "let f = & mut ctx . accounts . foundry ;"
      },
      {
        "id": 16,
        "label": "let s = & mut ctx . accounts . smith ;"
      },
      {
        "id": 17,
