  {
    "name": "contribute_to_guild_treasury",
    "inputs": [
      "ctx: Context < ContributeToGuild >",
      "gold_amount: u64",
      "wood_amount: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let guild = & mut ctx . accounts . guild ;"
      },
      {
        "id": 2,
        "label": "let member = & mut ctx . accounts . guild_member ;"
      },
      {
        "id": 3,
        "label": "member . gold_balance = member . gold_balance . saturating_sub (gold_amount) ;"
      },
      {
        "id": 4,
        "label": "member . wood_balance = member . wood_balance . saturating_sub (wood_amount) ;"
      },
      {
        "id": 5,
        "label": "guild . treasury_gold = guild . treasury_gold . saturating_add (gold_amount) ;"
      },
      {
        "id": 6,
        "label": "guild . treasury_wood = guild . treasury_wood . saturating_add (wood_amount) ;"
      },
      {
        "id": 7,
        "label": "let contribution_score = gold_amount . saturating_add (wood_amount * 2) ;"
      },
      {
        "id": 8,
        "label": "member . total_contribution = member . total_contribution . saturating_add (contribution_score) ;"
      },
      {
        "id": 9,
        "label": "msg ! (\"Contributed to guild.\") ;"
      },
