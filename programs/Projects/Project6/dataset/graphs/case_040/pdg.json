      },
      {
        "id": 1,
        "label": "let tournament = & mut ctx . accounts . tournament ;"
      },
      {
        "id": 2,
        "label": "let winner_profile = & mut ctx . accounts . winner_profile ;"
      },
      {
        "id": 3,
