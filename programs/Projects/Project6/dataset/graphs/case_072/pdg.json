      },
      {
        "id": 25,
        "label": "for n in notes { s = s . saturating_add ((n & 0x3F) as u32) ; mix ^= (n as u16) . rotate_left (2) ; }"
      },
      {
        "id": 26,
        "label": "s = s . saturating_add ((n & 0x3F) as u32) ;"
      },
