      },
      {
        "id": 19,
        "label": "let s = & mut ctx . accounts . stable ;"
      },
      {
        "id": 20,
        "label": "let a = & mut ctx . accounts . actor ;"
      },
      {
        "id": 21,
        "label": "let b = & mut ctx . accounts . partner ;"
      },
      {
        "id": 22,
