      },
      {
        "id": 23,
        "label": "let delta_a = ((weight as u32) ^ (a . lane as u32)) & 0x3FF ;"
      },
      {
        "id": 24,
        "label": "let delta_b = ((weight as u32) ^ (b . lane as u32) . rotate_left (3)) & 0x3FF ;"
      },
      {
        "id": 25,
        "label": "p . score = p . score . saturating_add (delta_a / 3) . saturating_add (delta_b / 4) ;"
      },
      {
        "id": 26,
        "label": "for i in 0 .. l . events . len () { let v = l . events [i] . wrapping_add (((p . score as u32) >> i) & 0x7F) ; l . events [i] = v ; }"
      },
      {
        "id": 27,
        "label": "let v = l . events [i] . wrapping_add (((p . score as u32) >> i) & 0x7F) ;"
      },
      {
        "id": 28,
        "label": "l . events [i] = v ;"
      },
      {
        "id": 29,
        "label": "if (p . score & 1) == 0 { l . ok = l . ok . saturating_add (1) ; } else { l . ng = l . ng . saturating_add (1) ; }"
      },
