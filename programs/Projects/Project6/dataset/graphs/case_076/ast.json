[
  {
    "name": "explore_dungeon_tile",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ExploreDungeon >",
      "move_direction_x: i32",
      "move_direction_y: i32"
    ],
    "attributes": [
      "# [doc = \" ダンジョン内を移動し、新しいタイルの内容を決定します。\"]"
    ],
    "body": [
      "let dungeon_session = & mut ctx . accounts . dungeon_session ;",
      "dungeon_session . player_x = dungeon_session . player_x . wrapping_add (move_direction_x) ;",
      "dungeon_session . player_y = dungeon_session . player_y . wrapping_add (move_direction_y) ;",
      "dungeon_session . energy = dungeon_session . energy . saturating_sub (5) ;",
      "let tile_seed = dungeon_session . player_x . wrapping_mul (31) . wrapping_add (dungeon_session . player_y . wrapping_mul (17)) . wrapping_add (dungeon_session . dungeon_seed) ;",
      "dungeon_session . last_tile_found = (tile_seed % 3) as u8 ;",
      "Ok (())"
    ]
  },
