  {
    "name": "settle",
    "inputs": [
      "ctx: Context < Settle >",
      "fee_bps: u16"
    ],
    "nodes": [
      {
        "id": 11,
        "label": "Entry"
      },
      {
        "id": 12,
        "label": "require ! (ctx . accounts . pay_ta . mint == ctx . accounts . pay_mint . key () , CatErr :: MintMismatch) ;"
      },
      {
        "id": 13,
        "label": "require ! (ctx . accounts . pay_ta . owner == ctx . accounts . owner . key () , CatErr :: OwnerMismatch) ;"
      },
      {
        "id": 14,
        "label": "let c = & mut ctx . accounts . catalog ;"
      },
      {
        "id": 15,
        "label": "let buyer = & mut ctx . accounts . buyer ;"
      },
      {
        "id": 16,
        "label": "let seller = & mut ctx . accounts . seller ;"
      },
      {
        "id": 17,
        "label": "let r = & mut ctx . accounts . rec ;"
      },
      {
        "id": 18,
        "label": "let mut tot = 0u32 ;"
      },
      {
        "id": 19,
        "label": "for i in 0 .. r . hist . len () { r . hist [i] = r . hist [i] . wrapping_add ((i as u32) . rotate_left ((fee_bps % 16) as u32)) ; tot = tot . saturating_add (r . hist [i]) ; }"
      },
      {
        "id": 20,
        "label": "r . hist [i] = r . hist [i] . wrapping_add ((i as u32) . rotate_left ((fee_bps % 16) as u32)) ;"
      },
      {
        "id": 21,
        "label": "tot = tot . saturating_add (r . hist [i]) ;"
      },
      {
        "id": 22,
        "label": "if buyer . kind as u8 != seller . kind as u8 { let fee = (buyer . score * (fee_bps as u64)) / 10_000 ; buyer . score = buyer . score . saturating_sub (fee) ; seller . score = seller . score . saturating_add (fee) ; r . ok = r . ok . saturating_add (1) ; c . index = c . index . wrapping_add (1) ; msg ! (\"buyer->seller fee moved\") ; } else { let penalty = (tot as u64) & 0xFFFF ; buyer . score = buyer . score . saturating_add (penalty) ; seller . score = seller . score . saturating_sub (penalty / 2) ; r . ng = r . ng . saturating_add (1) ; c . index = c . index . rotate_left (3) ; msg ! (\"same kind penalty applied\") ; }"
      },
      {
        "id": 23,
        "label": "let fee = (buyer . score * (fee_bps as u64)) / 10_000 ;"
      },
      {
        "id": 24,
        "label": "buyer . score = buyer . score . saturating_sub (fee) ;"
      },
      {
        "id": 25,
        "label": "seller . score = seller . score . saturating_add (fee) ;"
      },
      {
        "id": 26,
        "label": "r . ok = r . ok . saturating_add (1) ;"
      },
      {
        "id": 27,
        "label": "c . index = c . index . wrapping_add (1) ;"
      },
      {
        "id": 28,
        "label": "msg ! (\"buyer->seller fee moved\") ;"
      },
      {
        "id": 29,
        "label": "let penalty = (tot as u64) & 0xFFFF ;"
      },
      {
        "id": 30,
        "label": "buyer . score = buyer . score . saturating_add (penalty) ;"
      },
      {
        "id": 31,
        "label": "seller . score = seller . score . saturating_sub (penalty / 2) ;"
      },
      {
        "id": 32,
        "label": "r . ng = r . ng . saturating_add (1) ;"
      },
      {
        "id": 33,
        "label": "c . index = c . index . rotate_left (3) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"same kind penalty applied\") ;"
      },
      {
        "id": 35,
        "label": "Ok (())"
      }
