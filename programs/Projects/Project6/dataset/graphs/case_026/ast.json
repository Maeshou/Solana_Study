[
  {
    "name": "init_factory",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitFactory >"
    ],
    "attributes": [],
    "body": [
      "let f = & mut ctx . accounts . factory ;",
      "f . owner = ctx . accounts . owner . key () ;",
      "f . hash = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "init_slot",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitSlot >",
      "idx: u8"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . slot ;",
      "s . parent = ctx . accounts . factory . key () ;",
      "s . idx = idx ;",
      "s . used = false ;",
      "Ok (())"
    ]
  },
  {
    "name": "batch",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Batch >",
      "bias: u16"
    ],
    "attributes": [],
    "body": [
      "require ! (ctx . accounts . mat_ta . mint == ctx . accounts . mat_mint . key () , CFErr :: MintMismatch) ;",
      "require ! (ctx . accounts . mat_ta . owner == ctx . accounts . owner . key () , CFErr :: OwnerMismatch) ;",
      "let f = & mut ctx . accounts . factory ;",
      "let a = & mut ctx . accounts . slot_a ;",
      "let b = & mut ctx . accounts . slot_b ;",
      "let w = & mut ctx . accounts . warehouse ;",
      "let mut s = 0u32 ;",
      "for i in 0 .. w . blks . len ()",
      "{",
      "let v = ((bias as u32) + i as u32 * 9) & 0xFFF ;",
      "w . blks [i] = w . blks [i] . saturating_add (v) ;",
      "s = s . saturating_add (v) ;",
      "}",
      "if a . idx != b . idx",
      "{",
      "w . ok = w . ok . saturating_add ((s / 8) as u64) ;",
      "a . used = true ;",
      "f . hash ^= (s as u64) . rotate_left (11) ;",
      "msg ! (\"different slot: ok+\") ;",
      "} else",
      "{",
      "w . ng = w . ng . saturating_add ((s / 7) as u64) ;",
      "b . used = true ;",
      "f . hash = f . hash . rotate_right (9) ^ 0xA5A5u64 ;",
      "msg ! (\"same slot: ng+\") ;",
      "}",
      "Ok (())"
    ]
  },
