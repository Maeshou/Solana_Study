[
  {
    "name": "process_user_account",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessUserAccount >"
    ],
    "attributes": [],
    "body": [
      "let user_account = & mut ctx . accounts . user_account ;",
      "msg ! (\"Processing user account with balance: {}\" , user_account . balance) ;",
      "user_account . balance = user_account . balance . checked_add (100) . unwrap () ;",
      "Ok (())"
    ]
  },
  {
    "name": "process_admin_account",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ProcessAdminAccount >"
    ],
    "attributes": [],
    "body": [
      "let admin_account = & mut ctx . accounts . admin_account ;",
      "msg ! (\"Processing admin account with permissions: {}\" , admin_account . permissions) ;",
      "admin_account . permissions |= 0x01 ;",
      "Ok (())"
    ]
  },
