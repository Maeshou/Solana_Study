  {
    "name": "init_voter",
    "inputs": [
      "ctx: Context < InitVoter >",
      "voter_id: u64",
      "starting_votes: u32"
    ],
    "nodes": [
      {
        "id": 12,
        "label": "Entry"
      },
      {
        "id": 13,
        "label": "let voter = & mut ctx . accounts . voter_profile ;"
      },
      {
        "id": 14,
        "label": "voter . parent_tally = ctx . accounts . tally_core . key () ;"
      },
      {
        "id": 15,
        "label": "voter . voter_id = voter_id ^ 0xF0F0F0F0F0F0F0F0 ;"
      },
      {
        "id": 16,
        "label": "if let Some (sv) = starting_votes . checked_add (STARTING_VOTES_BONUS) { voter . votes_remaining = sv ; } else { voter . votes_remaining = u32 :: MAX ; msg ! (\"Warning: Starting votes overflowed for voter {}.\" , voter_id) ; }"
      },
      {
        "id": 17,
        "label": "voter . votes_remaining = sv ;"
      },
      {
        "id": 18,
        "label": "voter . votes_remaining = u32 :: MAX ;"
      },
      {
        "id": 19,
        "label": "msg ! (\"Warning: Starting votes overflowed for voter {}.\" , voter_id) ;"
      },
      {
        "id": 20,
        "label": "voter . is_eligible = starting_votes > MIN_ELIGIBILITY_VOTES ;"
      },
      {
        "id": 21,
        "label": "voter . total_votes_cast = 0 ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"New voter {} registered with {} votes.\" , voter . voter_id , voter . votes_remaining) ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
