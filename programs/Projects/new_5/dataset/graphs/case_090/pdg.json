  {
    "name": "init_tally",
    "inputs": [
      "ctx: Context < InitTally >",
      "founding_era: u64",
      "max_voters: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let tally = & mut ctx . accounts . tally_core ;"
      },
      {
        "id": 2,
        "label": "tally . founding_era = founding_era << 8 ;"
      },
      {
        "id": 3,
        "label": "if let Some (mv) = max_voters . checked_add (VOTING_CAPACITY_BONUS) { tally . max_voters = mv ; } else { tally . max_voters = u32 :: MAX ; msg ! (\"Warning: Max voters overflowed.\") ; }"
      },
      {
        "id": 4,
        "label": "tally . max_voters = mv ;"
      },
      {
        "id": 5,
        "label": "tally . max_voters = u32 :: MAX ;"
      },
      {
        "id": 6,
        "label": "msg ! (\"Warning: Max voters overflowed.\") ;"
      },
      {
        "id": 7,
        "label": "tally . total_votes = founding_era % 10 ;"
      },
      {
        "id": 8,
        "label": "tally . current_voters = 0 ;"
      },
      {
        "id": 9,
        "label": "tally . tally_status = TallyStatus :: Open ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Cosmic Tally established in era {} with capacity for {} voters.\" , tally . founding_era , tally . max_voters) ;"
      },
      {
        "id": 11,
        "label": "Ok (())"
      }
    ],
