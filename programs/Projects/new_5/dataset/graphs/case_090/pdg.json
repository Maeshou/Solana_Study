  {
    "name": "cast_votes",
    "inputs": [
      "ctx: Context < CastVotes >",
      "votes_to_cast_1: u32",
      "votes_to_cast_2: u32"
    ],
    "nodes": [
      {
        "id": 24,
        "label": "Entry"
      },
      {
        "id": 25,
        "label": "let tally = & mut ctx . accounts . tally_core ;"
      },
      {
        "id": 26,
        "label": "let primary_voter = & mut ctx . accounts . primary_voter ;"
      },
      {
        "id": 27,
        "label": "let secondary_voter = & mut ctx . accounts . secondary_voter ;"
      },
      {
        "id": 28,
        "label": "let votes_to_cast_v1 = votes_to_cast_1 . min (primary_voter . votes_remaining) ;"
      },
      {
        "id": 29,
        "label": "primary_voter . votes_remaining = primary_voter . votes_remaining . saturating_sub (votes_to_cast_v1) ;"
      },
      {
        "id": 30,
        "label": "primary_voter . total_votes_cast = primary_voter . total_votes_cast . saturating_add (votes_to_cast_v1) ;"
      },
      {
        "id": 31,
        "label": "tally . total_votes = tally . total_votes . saturating_add (votes_to_cast_v1 as u64) ;"
      },
      {
        "id": 32,
        "label": "primary_voter . is_eligible = primary_voter . votes_remaining > MIN_ELIGIBILITY_VOTES ;"
      },
      {
        "id": 33,
        "label": "let votes_to_cast_v2 = votes_to_cast_2 . min (secondary_voter . votes_remaining) ;"
      },
      {
        "id": 34,
        "label": "secondary_voter . votes_remaining = secondary_voter . votes_remaining . saturating_sub (votes_to_cast_v2) ;"
      },
      {
        "id": 35,
        "label": "secondary_voter . total_votes_cast = secondary_voter . total_votes_cast . saturating_add (votes_to_cast_v2) ;"
      },
      {
        "id": 36,
        "label": "tally . total_votes = tally . total_votes . saturating_add (votes_to_cast_v2 as u64) ;"
      },
      {
        "id": 37,
        "label": "secondary_voter . is_eligible = secondary_voter . votes_remaining > MIN_ELIGIBILITY_VOTES ;"
      },
      {
        "id": 38,
        "label": "tally . current_voters = tally . current_voters . saturating_add ((primary_voter . total_votes_cast > MIN_VOTES_TO_COUNT) as u32 + (secondary_voter . total_votes_cast > MIN_VOTES_TO_COUNT) as u32) ;"
      },
      {
        "id": 39,
        "label": "msg ! (\"Primary voter ({}) cast {} votes. Secondary voter ({}) cast {} votes.\" , primary_voter . voter_id , votes_to_cast_v1 , secondary_voter . voter_id , votes_to_cast_v2) ;"
      },
      {
        "id": 40,
        "label": "Ok (())"
