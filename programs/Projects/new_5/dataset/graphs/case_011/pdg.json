    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let stake_acc = & mut ctx . accounts . stake_acc ;"
      },
      {
        "id": 2,
        "label": "let reward_acc = & mut ctx . accounts . reward_acc ;"
      },
      {
        "id": 3,
        "label": "stake_acc . total_staked = stake_acc . total_staked . saturating_add (amount) ;"
      },
      {
        "id": 4,
        "label": "reward_acc . pending_rewards |= amount ;"
      },
      {
        "id": 5,
        "label": "reward_acc . multiplier = reward_acc . multiplier . saturating_mul (2) ;"
      },
      {
        "id": 6,
        "label": "stake_acc . last_stake = ctx . accounts . clock . unix_timestamp ;"
      },
      {
        "id": 7,
        "label": "reward_acc . last_update = ctx . accounts . clock . unix_timestamp ;"
      },
      {
        "id": 8,
        "label": "stake_acc . note . push_str (\"-staked\") ;"
      },
      {
        "id": 9,
        "label": "reward_acc . note . push_str (\"-rewarded\") ;"
      },
