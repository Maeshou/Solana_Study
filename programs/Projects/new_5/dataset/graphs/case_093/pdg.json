  {
    "name": "init_circle",
    "inputs": [
      "ctx: Context < InitCircle >",
      "circle_id: u64",
      "initial_aether: u64"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let circle = & mut ctx . accounts . magic_circle ;"
      },
      {
        "id": 2,
        "label": "circle . circle_id = circle_id + CIRCLE_ID_BONUS ;"
      },
      {
        "id": 3,
        "label": "circle . aether_pool = initial_aether * INITIAL_AETHER_MULTIPLIER ;"
      },
      {
        "id": 4,
        "label": "circle . active_casters = 0 ;"
      },
      {
        "id": 5,
        "label": "circle . is_active = circle . aether_pool > INITIAL_AETHER_THRESHOLD ;"
      },
      {
        "id": 6,
        "label": "msg ! (\"Magic Circle {} created with {} aether.\" , circle . circle_id , circle . aether_pool) ;"
      },
      {
        "id": 7,
        "label": "Ok (())"
      }
