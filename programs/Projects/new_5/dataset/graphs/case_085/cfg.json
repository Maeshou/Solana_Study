  {
    "name": "cast_votes",
    "nodes": [
      {
        "id": 34,
        "label": "Entry"
      },
      {
        "id": 35,
        "label": "let tally = & mut ctx . accounts . tally_core ;"
      },
      {
        "id": 36,
        "label": "let voter1 = & mut ctx . accounts . voter1_profile ;"
      },
      {
        "id": 37,
        "label": "let voter2 = & mut ctx . accounts . voter2_profile ;"
      },
      {
        "id": 38,
        "label": "let votes_to_cast_v1 = votes_to_cast_1 . min (voter1 . votes_remaining) ;"
      },
      {
        "id": 39,
        "label": "voter1 . votes_remaining = voter1 . votes_remaining . saturating_sub (votes_to_cast_v1) ;"
      },
      {
        "id": 40,
        "label": "voter1 . total_votes_cast = voter1 . total_votes_cast . saturating_add (votes_to_cast_v1) ;"
      },
      {
        "id": 41,
        "label": "tally . total_votes = tally . total_votes . saturating_add (votes_to_cast_v1 as u64) ;"
      },
      {
        "id": 42,
        "label": "voter1 . is_eligible = voter1 . votes_remaining > 0 ;"
      },
      {
        "id": 43,
        "label": "let votes_to_cast_v2 = votes_to_cast_2 . min (voter2 . votes_remaining) ;"
      },
      {
        "id": 44,
        "label": "voter2 . votes_remaining = voter2 . votes_remaining . saturating_sub (votes_to_cast_v2) ;"
      },
      {
        "id": 45,
        "label": "voter2 . total_votes_cast = voter2 . total_votes_cast . saturating_add (votes_to_cast_v2) ;"
      },
      {
        "id": 46,
        "label": "tally . total_votes = tally . total_votes . saturating_add (votes_to_cast_v2 as u64) ;"
      },
      {
        "id": 47,
        "label": "voter2 . is_eligible = voter2 . votes_remaining > 0 ;"
      },
      {
        "id": 48,
        "label": "tally . current_voters = tally . current_voters . saturating_add ((voter1 . total_votes_cast > 0) as u32 + (voter2 . total_votes_cast > 0) as u32) ;"
      },
      {
        "id": 49,
        "label": "msg ! (\"Voter 1 ({}) cast {} votes. Voter 2 ({}) cast {} votes.\" , voter1 . voter_id , votes_to_cast_v1 , voter2 . voter_id , votes_to_cast_v2) ;"
      },
