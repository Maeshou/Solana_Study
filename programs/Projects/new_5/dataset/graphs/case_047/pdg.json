        "label": "let ticket_to_demote = & mut ctx . accounts . ticket_to_demote ;"
      },
      {
        "id": 26,
        "label": "if ticket_to_promote . ticket_status == TicketStatus :: Closed || ticket_to_demote . ticket_status == TicketStatus :: Closed { return Err (error ! (TicketError :: TicketClosed)) ; }"
      },
      {
        "id": 27,
        "label": "return Err (error ! (TicketError :: TicketClosed)) ;"
      },
      {
        "id": 28,
        "label": "ticket_to_promote . priority = ticket_to_promote . priority . saturating_add (promote_value) . min (255) ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"Ticket to promote priority increased to {}.\" , ticket_to_promote . priority) ;"
      },
      {
        "id": 30,
        "label": "ticket_to_demote . priority = ticket_to_demote . priority . saturating_sub (demote_value) . max (0) ;"
      },
