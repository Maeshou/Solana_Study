  {
    "name": "modify_ticket_priority",
    "nodes": [
      {
        "id": 29,
        "label": "Entry"
      },
      {
        "id": 30,
        "label": "let ticket_to_promote = & mut ctx . accounts . ticket_to_promote ;"
      },
      {
        "id": 31,
        "label": "let ticket_to_demote = & mut ctx . accounts . ticket_to_demote ;"
      },
      {
        "id": 32,
        "label": "if statement"
      },
      {
        "id": 33,
        "label": "predicate"
      },
      {
        "id": 34,
        "label": "ticket_to_promote . ticket_status == TicketStatus :: Closed || ticket_to_demote . ticket_status == TicketStatus :: Closed"
      },
      {
        "id": 35,
        "label": "True body"
      },
      {
        "id": 36,
        "label": "return Err (error ! (TicketError :: TicketClosed)) ;"
      },
      {
        "id": 37,
        "label": "False body"
      },
      {
        "id": 38,
        "label": "No-op"
      },
      {
        "id": 39,
        "label": "merge"
      },
      {
        "id": 40,
        "label": "ticket_to_promote . priority = ticket_to_promote . priority . saturating_add (promote_value) . min (255) ;"
      },
      {
        "id": 41,
        "label": "msg ! (\"Ticket to promote priority increased to {}.\" , ticket_to_promote . priority) ;"
      },
      {
        "id": 42,
        "label": "ticket_to_demote . priority = ticket_to_demote . priority . saturating_sub (demote_value) . max (0) ;"
      },
      {
        "id": 43,
        "label": "msg ! (\"Ticket to demote priority decreased to {}.\" , ticket_to_demote . priority) ;"
      },
      {
        "id": 44,
        "label": "if statement"
      },
      {
        "id": 45,
        "label": "predicate"
      },
      {
        "id": 46,
        "label": "ticket_to_promote . priority > ticket_to_demote . priority"
      },
