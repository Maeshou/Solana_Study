  {
    "name": "conduct_training_session",
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let leader_profile = & mut ctx . accounts . leader_profile ;"
      },
      {
        "id": 23,
        "label": "let trainee_profile = & mut ctx . accounts . trainee_profile ;"
      },
      {
        "id": 24,
        "label": "let mut points_transferred = points_to_transfer ;"
      },
      {
        "id": 25,
        "label": "let mut loop_counter = 0 ;"
      },
      {
        "id": 26,
        "label": "Loop Start"
      },
      {
        "id": 27,
        "label": "if statement"
      },
      {
        "id": 28,
        "label": "predicate"
      },
      {
        "id": 29,
        "label": "leader_profile . contribution_points > trainee_profile . contribution_points"
      },
      {
        "id": 30,
        "label": "True body"
      },
      {
        "id": 31,
        "label": "leader_profile . contribution_points = leader_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 32,
        "label": "trainee_profile . contribution_points = trainee_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ;"
      },
      {
        "id": 33,
        "label": "msg ! (\"Leader has more points, adding transfer points to leader.\") ;"
      },
      {
        "id": 34,
        "label": "False body"
      },
      {
        "id": 35,
        "label": "leader_profile . contribution_points = leader_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ;"
      },
      {
        "id": 36,
        "label": "trainee_profile . contribution_points = trainee_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"Trainee has more or equal points, adding transfer points to trainee.\") ;"
      },
      {
        "id": 38,
        "label": "merge"
      },
      {
        "id": 39,
        "label": "loop_counter += 1 ;"
      },
      {
        "id": 40,
        "label": "Loop End"
      },
      {
        "id": 41,
        "label": "let new_prestige = (leader_profile . contribution_points . saturating_add (trainee_profile . contribution_points)) / 1000 ;"
      },
      {
        "id": 42,
        "label": "ctx . accounts . guild . prestige = ctx . accounts . guild . prestige . saturating_add (new_prestige as u64) ;"
      },
      {
        "id": 43,
        "label": "msg ! (\"Training session finished. Guild prestige increased by {}.\" , new_prestige) ;"
      },
      {
        "id": 44,
        "label": "Ok (())"
      }
