  {
    "name": "conduct_training_session",
    "inputs": [
      "ctx: Context < ConductTrainingSession >",
      "points_to_transfer: u32"
    ],
    "nodes": [
      {
        "id": 21,
        "label": "Entry"
      },
      {
        "id": 22,
        "label": "let leader_profile = & mut ctx . accounts . leader_profile ;"
      },
      {
        "id": 23,
        "label": "let trainee_profile = & mut ctx . accounts . trainee_profile ;"
      },
      {
        "id": 24,
        "label": "let mut points_transferred = points_to_transfer ;"
      },
      {
        "id": 25,
        "label": "let mut loop_counter = 0 ;"
      },
      {
        "id": 26,
        "label": "while loop_counter < 4 { if leader_profile . contribution_points > trainee_profile . contribution_points { leader_profile . contribution_points = leader_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ; trainee_profile . contribution_points = trainee_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ; msg ! (\"Leader has more points, adding transfer points to leader.\") ; } else { leader_profile . contribution_points = leader_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ; trainee_profile . contribution_points = trainee_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ; msg ! (\"Trainee has more or equal points, adding transfer points to trainee.\") ; } loop_counter += 1 ; }"
      },
      {
        "id": 27,
        "label": "if leader_profile . contribution_points > trainee_profile . contribution_points { leader_profile . contribution_points = leader_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ; trainee_profile . contribution_points = trainee_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ; msg ! (\"Leader has more points, adding transfer points to leader.\") ; } else { leader_profile . contribution_points = leader_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ; trainee_profile . contribution_points = trainee_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ; msg ! (\"Trainee has more or equal points, adding transfer points to trainee.\") ; }"
      },
      {
        "id": 28,
        "label": "leader_profile . contribution_points = leader_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 29,
        "label": "trainee_profile . contribution_points = trainee_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ;"
      },
      {
        "id": 30,
        "label": "msg ! (\"Leader has more points, adding transfer points to leader.\") ;"
      },
      {
        "id": 31,
        "label": "leader_profile . contribution_points = leader_profile . contribution_points . checked_sub (points_transferred) . unwrap_or (0) ;"
      },
      {
        "id": 32,
        "label": "trainee_profile . contribution_points = trainee_profile . contribution_points . checked_add (points_transferred) . unwrap_or (u32 :: MAX) ;"
      },
      {
        "id": 33,
        "label": "msg ! (\"Trainee has more or equal points, adding transfer points to trainee.\") ;"
      },
      {
        "id": 34,
        "label": "loop_counter += 1 ;"
      },
      {
        "id": 35,
        "label": "let new_prestige = (leader_profile . contribution_points . saturating_add (trainee_profile . contribution_points)) / 1000 ;"
      },
      {
        "id": 36,
        "label": "ctx . accounts . guild . prestige = ctx . accounts . guild . prestige . saturating_add (new_prestige as u64) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"Training session finished. Guild prestige increased by {}.\" , new_prestige) ;"
      },
      {
        "id": 38,
        "label": "Ok (())"
      }
