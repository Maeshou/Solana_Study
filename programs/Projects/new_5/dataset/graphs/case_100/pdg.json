  {
    "name": "synchronize_entries",
    "inputs": [
      "ctx: Context < SynchronizeEntries >",
      "new_content_hash: u64"
    ],
    "nodes": [
      {
        "id": 16,
        "label": "Entry"
      },
      {
        "id": 17,
        "label": "let journal = & mut ctx . accounts . journal_core ;"
      },
      {
        "id": 18,
        "label": "let main_entry = & mut ctx . accounts . main_entry ;"
      },
      {
        "id": 19,
        "label": "let backup_entry = & mut ctx . accounts . backup_entry ;"
      },
      {
        "id": 20,
        "label": "require ! (! main_entry . is_synced && ! backup_entry . is_synced , ChronoscribeError :: EntriesAlreadySynced) ;"
      },
      {
        "id": 21,
        "label": "if main_entry . content_hash != backup_entry . content_hash { backup_entry . content_hash = main_entry . content_hash ; msg ! (\"Content mismatch detected. Backup entry {} synchronized with main entry {}.\" , backup_entry . entry_id , main_entry . entry_id) ; }"
      },
      {
        "id": 22,
        "label": "backup_entry . content_hash = main_entry . content_hash ;"
      },
      {
        "id": 23,
        "label": "msg ! (\"Content mismatch detected. Backup entry {} synchronized with main entry {}.\" , backup_entry . entry_id , main_entry . entry_id) ;"
      },
      {
        "id": 24,
        "label": "main_entry . content_hash = new_content_hash ;"
      },
      {
        "id": 25,
        "label": "backup_entry . content_hash = new_content_hash ;"
      },
      {
        "id": 26,
        "label": "main_entry . is_synced = true ;"
      },
      {
        "id": 27,
        "label": "backup_entry . is_synced = true ;"
      },
      {
        "id": 28,
        "label": "journal . sync_count = journal . sync_count . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "journal . is_complete = journal . sync_count . checked_cmp (& SYNC_COMPLETION_THRESHOLD) != Some (std :: cmp :: Ordering :: Less) ;"
      },
      {
        "id": 30,
        "label": "msg ! (\"Entries {} and {} synchronized. New hash: {}.\" , main_entry . entry_id , backup_entry . entry_id , new_content_hash) ;"
      },
      {
        "id": 31,
        "label": "Ok (())"
