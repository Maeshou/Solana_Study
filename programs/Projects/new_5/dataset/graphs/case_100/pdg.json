      },
      {
        "id": 1,
        "label": "let journal = & mut ctx . accounts . journal_core ;"
      },
      {
        "id": 2,
        "label": "journal . journal_id = journal_id ^ 0xFEEDC0DE ;"
      },
      {
        "id": 3,
        "label": "journal . total_pages = initial_pages . saturating_add (INITIAL_PAGES_BONUS) ;"
