    ],
    "nodes": [
      {
        "id": 24,
        "label": "Entry"
      },
      {
        "id": 25,
        "label": "let order_one = & mut ctx . accounts . order_one ;"
      },
      {
        "id": 26,
        "label": "let order_two = & mut ctx . accounts . order_two ;"
      },
      {
        "id": 27,
        "label": "if order_one . order_status != OrderStatus :: Pending || order_two . order_status != OrderStatus :: Pending { return Err (error ! (MarketError :: OrderNotPending)) ; }"
      },
      {
        "id": 28,
        "label": "return Err (error ! (MarketError :: OrderNotPending)) ;"
      },
      {
        "id": 29,
        "label": "let mut loop_count = 0 ;"
      },
      {
        "id": 30,
        "label": "while loop_count < 2 { if order_one . price > order_two . price { order_one . price = order_one . price . saturating_sub (order_two . price / 2) ; order_two . price = order_two . price . saturating_add (order_one . price / 2) ; msg ! (\"Order One has higher price, adjusting.\") ; } else { order_one . price = order_one . price . saturating_add (order_two . price / 2) ; order_two . price = order_two . price . saturating_sub (order_one . price / 2) ; msg ! (\"Order Two has higher or equal price, adjusting.\") ; } loop_count += 1 ; }"
      },
      {
        "id": 31,
        "label": "if order_one . price > order_two . price { order_one . price = order_one . price . saturating_sub (order_two . price / 2) ; order_two . price = order_two . price . saturating_add (order_one . price / 2) ; msg ! (\"Order One has higher price, adjusting.\") ; } else { order_one . price = order_one . price . saturating_add (order_two . price / 2) ; order_two . price = order_two . price . saturating_sub (order_one . price / 2) ; msg ! (\"Order Two has higher or equal price, adjusting.\") ; }"
      },
      {
        "id": 32,
        "label": "order_one . price = order_one . price . saturating_sub (order_two . price / 2) ;"
      },
      {
        "id": 33,
        "label": "order_two . price = order_two . price . saturating_add (order_one . price / 2) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"Order One has higher price, adjusting.\") ;"
      },
      {
        "id": 35,
        "label": "order_one . price = order_one . price . saturating_add (order_two . price / 2) ;"
      },
      {
        "id": 36,
        "label": "order_two . price = order_two . price . saturating_sub (order_one . price / 2) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"Order Two has higher or equal price, adjusting.\") ;"
      },
      {
        "id": 38,
        "label": "loop_count += 1 ;"
      },
      {
        "id": 39,
        "label": "if order_one . price == order_two . price { order_one . order_status = OrderStatus :: Fulfilled ; order_two . order_status = OrderStatus :: Fulfilled ; msg ! (\"Orders matched and fulfilled!\") ; } else { msg ! (\"Orders did not match.\") ; }"
