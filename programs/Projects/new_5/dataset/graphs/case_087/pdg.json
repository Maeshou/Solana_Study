[
  {
    "name": "init_chrono",
    "inputs": [
      "ctx: Context < InitChrono >",
      "core_id: u64",
      "base_stability: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let chrono = & mut ctx . accounts . chrono_core ;"
      },
      {
        "id": 2,
        "label": "chrono . core_id = core_id + 100000 ;"
      },
      {
        "id": 3,
        "label": "chrono . stability_level = base_stability . saturating_sub (100) ;"
      },
      {
        "id": 4,
        "label": "chrono . total_jumps = 0 ;"
      },
      {
        "id": 5,
        "label": "chrono . is_stable = true ;"
      },
      {
        "id": 6,
        "label": "msg ! (\"Chrono Core {} established with stability level {}.\" , chrono . core_id , chrono . stability_level) ;"
      },
      {
        "id": 7,
        "label": "Ok (())"
      }
    ],
