  {
    "name": "perform_jump",
    "inputs": [
      "ctx: Context < PerformJump >",
      "jump_distance_1: i64",
      "jump_distance_2: i64"
    ],
    "nodes": [
      {
        "id": 17,
        "label": "Entry"
      },
      {
        "id": 18,
        "label": "let chrono = & mut ctx . accounts . chrono_core ;"
      },
      {
        "id": 19,
        "label": "let explorer_traveler = & mut ctx . accounts . explorer_traveler ;"
      },
      {
        "id": 20,
        "label": "let scout_traveler = & mut ctx . accounts . scout_traveler ;"
      },
      {
        "id": 21,
        "label": "let mut rng = rand :: thread_rng () ;"
      },
      {
        "id": 22,
        "label": "let stability_delta_1 = rng . gen_range (- 10 ..= 10) ;"
      },
      {
        "id": 23,
        "label": "explorer_traveler . stability_score = explorer_traveler . stability_score . saturating_add (stability_delta_1) . max (0) ;"
      },
      {
        "id": 24,
        "label": "explorer_traveler . is_stranded = explorer_traveler . stability_score < 20 ;"
      },
      {
        "id": 25,
        "label": "if ! explorer_traveler . is_stranded { explorer_traveler . current_coordinate = explorer_traveler . current_coordinate . saturating_add (jump_distance_1) ; chrono . stability_level = chrono . stability_level . saturating_sub (jump_distance_1 . abs () as u32) ; chrono . total_jumps = chrono . total_jumps . saturating_add (1) ; } else { msg ! (\"Explorer traveler is stranded! Jump aborted.\") ; }"
      },
      {
        "id": 26,
        "label": "explorer_traveler . current_coordinate = explorer_traveler . current_coordinate . saturating_add (jump_distance_1) ;"
      },
      {
        "id": 27,
        "label": "chrono . stability_level = chrono . stability_level . saturating_sub (jump_distance_1 . abs () as u32) ;"
      },
      {
        "id": 28,
        "label": "chrono . total_jumps = chrono . total_jumps . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"Explorer traveler is stranded! Jump aborted.\") ;"
      },
      {
        "id": 30,
        "label": "let stability_delta_2 = rng . gen_range (- 10 ..= 10) ;"
      },
      {
        "id": 31,
        "label": "scout_traveler . stability_score = scout_traveler . stability_score . saturating_add (stability_delta_2) . max (0) ;"
      },
      {
        "id": 32,
        "label": "scout_traveler . is_stranded = scout_traveler . stability_score < 20 ;"
      },
      {
        "id": 33,
        "label": "if ! scout_traveler . is_stranded { scout_traveler . current_coordinate = scout_traveler . current_coordinate . saturating_add (jump_distance_2) ; chrono . stability_level = chrono . stability_level . saturating_sub (jump_distance_2 . abs () as u32) ; chrono . total_jumps = chrono . total_jumps . saturating_add (1) ; } else { msg ! (\"Scout traveler is stranded! Jump aborted.\") ; }"
      },
      {
        "id": 34,
        "label": "scout_traveler . current_coordinate = scout_traveler . current_coordinate . saturating_add (jump_distance_2) ;"
      },
      {
        "id": 35,
        "label": "chrono . stability_level = chrono . stability_level . saturating_sub (jump_distance_2 . abs () as u32) ;"
      },
      {
        "id": 36,
        "label": "chrono . total_jumps = chrono . total_jumps . saturating_add (1) ;"
      },
      {
        "id": 37,
        "label": "msg ! (\"Scout traveler is stranded! Jump aborted.\") ;"
      },
      {
        "id": 38,
        "label": "chrono . is_stable = chrono . stability_level > 500 && ! explorer_traveler . is_stranded && ! scout_traveler . is_stranded ;"
      },
      {
        "id": 39,
        "label": "msg ! (\"Explorer traveler jumped to coordinate {}. Scout traveler jumped to coordinate {}.\" , explorer_traveler . current_coordinate , scout_traveler . current_coordinate) ;"
      },
      {
        "id": 40,
        "label": "Ok (())"
      }
