  {
    "name": "rotate_and_trade",
    "inputs": [
      "ctx: Context < RotateAndTrade >",
      "step: u32"
    ],
    "nodes": [
      {
        "id": 15,
        "label": "Entry"
      },
      {
        "id": 16,
        "label": "let m = & mut ctx . accounts . market ;"
      },
      {
        "id": 17,
        "label": "let x = & mut ctx . accounts . lot_x ;"
      },
      {
        "id": 18,
        "label": "let y = & mut ctx . accounts . lot_y ;"
      },
      {
        "id": 19,
        "label": "for i in 0 .. 5 { let wave = ((m . tick + i) % 7) as u32 ; let adj = (wave * step) % 97 ; m . turnover = m . turnover . saturating_add ((adj + 3) as u64) ; m . tick = m . tick . saturating_add (1) ; }"
      },
      {
        "id": 20,
        "label": "let wave = ((m . tick + i) % 7) as u32 ;"
      },
      {
        "id": 21,
        "label": "let adj = (wave * step) % 97 ;"
      },
      {
        "id": 22,
        "label": "m . turnover = m . turnover . saturating_add ((adj + 3) as u64) ;"
      },
      {
        "id": 23,
        "label": "m . tick = m . tick . saturating_add (1) ;"
      },
      {
        "id": 24,
        "label": "if (x . sku . rotate_left ((step % 31) as u32) & 3) == 0 { x . state = Listed ; x . price = x . price . saturating_add (step + (m . fee_bps as u32)) ; x . volume = x . volume . saturating_add (1) ; m . turnover = m . turnover . saturating_add (x . price as u64) ; msg ! (\"X listed; price={}, turn={}\" , x . price , m . turnover) ; } else { x . state = Delisted ; let cut = (x . price / 7) . max (1) ; x . price = x . price . saturating_sub (cut) ; m . tick = m . tick . saturating_add ((cut % 13) as u32) ; msg ! (\"X delisted; price={}, tick={}\" , x . price , m . tick) ; }"
      },
      {
        "id": 25,
        "label": "x . state = Listed ;"
      },
      {
        "id": 26,
        "label": "x . price = x . price . saturating_add (step + (m . fee_bps as u32)) ;"
      },
      {
        "id": 27,
        "label": "x . volume = x . volume . saturating_add (1) ;"
      },
      {
        "id": 28,
        "label": "m . turnover = m . turnover . saturating_add (x . price as u64) ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"X listed; price={}, turn={}\" , x . price , m . turnover) ;"
      },
      {
        "id": 30,
        "label": "x . state = Delisted ;"
      },
      {
        "id": 31,
        "label": "let cut = (x . price / 7) . max (1) ;"
      },
      {
        "id": 32,
        "label": "x . price = x . price . saturating_sub (cut) ;"
      },
      {
        "id": 33,
        "label": "m . tick = m . tick . saturating_add ((cut % 13) as u32) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"X delisted; price={}, tick={}\" , x . price , m . tick) ;"
      },
      {
        "id": 35,
        "label": "for _ in 0 .. 3 { if y . price & 1 == 1 { y . state = Listed ; y . volume = y . volume . saturating_add (step % 5 + 1) ; m . turnover = m . turnover . saturating_add ((y . volume as u64) * 2) ; msg ! (\"Y trade+; vol={}, turn={}\" , y . volume , m . turnover) ; } else { y . state = Pending ; y . price = y . price / 2 + (m . tick % 19) ; m . tick = m . tick . saturating_add (2) ; msg ! (\"Y cooled; price={}, tick={}\" , y . price , m . tick) ; } }"
      },
      {
        "id": 36,
        "label": "if y . price & 1 == 1 { y . state = Listed ; y . volume = y . volume . saturating_add (step % 5 + 1) ; m . turnover = m . turnover . saturating_add ((y . volume as u64) * 2) ; msg ! (\"Y trade+; vol={}, turn={}\" , y . volume , m . turnover) ; } else { y . state = Pending ; y . price = y . price / 2 + (m . tick % 19) ; m . tick = m . tick . saturating_add (2) ; msg ! (\"Y cooled; price={}, tick={}\" , y . price , m . tick) ; }"
      },
      {
        "id": 37,
        "label": "y . state = Listed ;"
      },
      {
        "id": 38,
        "label": "y . volume = y . volume . saturating_add (step % 5 + 1) ;"
      },
      {
        "id": 39,
        "label": "m . turnover = m . turnover . saturating_add ((y . volume as u64) * 2) ;"
      },
      {
        "id": 40,
        "label": "msg ! (\"Y trade+; vol={}, turn={}\" , y . volume , m . turnover) ;"
      },
      {
        "id": 41,
        "label": "y . state = Pending ;"
      },
      {
        "id": 42,
        "label": "y . price = y . price / 2 + (m . tick % 19) ;"
      },
      {
        "id": 43,
        "label": "m . tick = m . tick . saturating_add (2) ;"
      },
      {
        "id": 44,
        "label": "msg ! (\"Y cooled; price={}, tick={}\" , y . price , m . tick) ;"
      },
      {
        "id": 45,
        "label": "Ok (())"
      }
