        "label": "base . xp = base . xp . checked_add (catalyst . xp) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 7,
        "label": "base . rarity = cmp :: max (base . rarity , catalyst . rarity) ;"
