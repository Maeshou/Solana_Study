[
  {
    "name": "fuse_items",
    "inputs": [
      "ctx: Context < FuseItems >",
      "new_suffix: String"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let base = & mut ctx . accounts . base_nft ;"
      },
      {
        "id": 2,
        "label": "let catalyst = & mut ctx . accounts . catalyst_nft ;"
      },
      {
        "id": 3,
        "label": "let clock = & ctx . accounts . clock ;"
      },
      {
        "id": 4,
        "label": "let lvl = base . level . saturating_add (catalyst . level) ;"
      },
      {
        "id": 5,
        "label": "base . level = cmp :: min (lvl , 100) ;"
      },
      {
        "id": 6,
        "label": "base . xp = base . xp . checked_add (catalyst . xp) . unwrap_or (u64 :: MAX) ;"
      },
      {
        "id": 7,
        "label": "base . rarity = cmp :: max (base . rarity , catalyst . rarity) ;"
      },
      {
        "id": 8,
        "label": "for i in 0 .. base . attributes . len () . min (catalyst . attributes . len ()) { base . attributes [i] |= catalyst . attributes [i] ; }"
      },
      {
        "id": 9,
        "label": "base . attributes [i] |= catalyst . attributes [i] ;"
      },
      {
        "id": 10,
        "label": "for & abil in catalyst . abilities . iter () { base . abilities . push (abil) ; }"
      },
      {
        "id": 11,
        "label": "base . abilities . push (abil) ;"
      },
      {
        "id": 12,
        "label": "let ts = clock . unix_timestamp ;"
      },
      {
        "id": 13,
        "label": "base . history . push (format ! (\"Fused at {}\" , ts)) ;"
      },
      {
        "id": 14,
        "label": "base . title . push_str (\"-\") ;"
      },
      {
        "id": 15,
        "label": "base . title . push_str (& new_suffix) ;"
      },
      {
        "id": 16,
        "label": "catalyst . xp = 0 ;"
      },
      {
        "id": 17,
        "label": "catalyst . level = 0 ;"
      },
      {
        "id": 18,
        "label": "catalyst . rarity = 0 ;"
      },
      {
        "id": 19,
        "label": "catalyst . attributes . clear () ;"
      },
      {
        "id": 20,
        "label": "catalyst . abilities . clear () ;"
      },
      {
        "id": 21,
        "label": "catalyst . history . push (format ! (\"Used as catalyst at {}\" , ts)) ;"
      },
      {
        "id": 22,
        "label": "msg ! (\"Fusion complete: '{}' lvl={} xp={} rarity={}\" , base . title , base . level , base . xp , base . rarity) ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
    ],
