[
  {
    "name": "refine_artifact",
    "inputs": [
      "ctx: Context < RefineArtifact >",
      "magic_factor: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let art_src = & mut ctx . accounts . artifact_src ;"
      },
      {
        "id": 2,
        "label": "let art_dest = & mut ctx . accounts . artifact_dest ;"
      },
      {
        "id": 3,
        "label": "let now = ctx . accounts . clock . unix_timestamp ;"
      },
      {
        "id": 4,
        "label": "art_src . durability = art_src . durability + art_dest . durability ;"
      },
      {
        "id": 5,
        "label": "art_src . efficiency = art_src . efficiency * magic_factor / 100 ;"
      },
      {
        "id": 6,
        "label": "art_src . element_id = (art_src . element_id << 2) ^ (art_dest . element_id >> 1) ;"
      },
      {
        "id": 7,
        "label": "art_src . description = art_src . description . clone () + \" âž” \" + & art_dest . description ;"
      },
      {
        "id": 8,
        "label": "art_src . tag = format ! (\"{}|{}\" , art_src . tag , art_dest . tag) ;"
      },
      {
        "id": 9,
        "label": "art_src . history = vec ! [format ! (\"Refined at {} by {}\" , now , ctx . accounts . user . key ())] ;"
      },
      {
        "id": 10,
        "label": "art_dest . history = vec ! [format ! (\"Consumed at {} in refinement\" , now)] ;"
      },
      {
        "id": 11,
        "label": "msg ! (\"Refinement complete: {} durability={}, efficiency={}, element={}\" , art_src . tag , art_src . durability , art_src . efficiency , art_src . element_id) ;"
      },
      {
        "id": 12,
        "label": "Ok (())"
      }
