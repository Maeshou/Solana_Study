  {
    "name": "execute_duel_round",
    "inputs": [
      "ctx: Context < ExecuteDuelRound >",
      "attacker_move: u8",
      "defender_move: u8"
    ],
    "nodes": [
      {
        "id": 25,
        "label": "Entry"
      },
      {
        "id": 26,
        "label": "let attacker_stats = & mut ctx . accounts . attacker_stats ;"
      },
      {
        "id": 27,
        "label": "let defender_stats = & mut ctx . accounts . defender_stats ;"
      },
      {
        "id": 28,
        "label": "if attacker_stats . combat_status != CombatantStatus :: Ready || defender_stats . combat_status != CombatantStatus :: Ready { return Err (error ! (BattleError :: CombatantNotReady)) ; }"
      },
      {
        "id": 29,
        "label": "return Err (error ! (BattleError :: CombatantNotReady)) ;"
      },
      {
        "id": 30,
        "label": "let damage = (attacker_move as u16) . saturating_sub (defender_move as u16) ;"
      },
      {
        "id": 31,
        "label": "defender_stats . health = defender_stats . health . checked_sub (damage) . unwrap_or (0) ;"
      },
      {
        "id": 32,
        "label": "if defender_stats . health == 0 { msg ! (\"Defender defeated!\") ; defender_stats . combat_status = CombatantStatus :: Defeated ; ctx . accounts . battle_zone . active_battles = ctx . accounts . battle_zone . active_battles . checked_sub (1) . unwrap_or (0) ; } else { msg ! (\"Defender takes {} damage.\" , damage) ; }"
      },
      {
        "id": 33,
        "label": "msg ! (\"Defender defeated!\") ;"
      },
      {
        "id": 34,
        "label": "defender_stats . combat_status = CombatantStatus :: Defeated ;"
      },
      {
        "id": 35,
        "label": "ctx . accounts . battle_zone . active_battles = ctx . accounts . battle_zone . active_battles . checked_sub (1) . unwrap_or (0) ;"
      },
      {
        "id": 36,
        "label": "msg ! (\"Defender takes {} damage.\" , damage) ;"
