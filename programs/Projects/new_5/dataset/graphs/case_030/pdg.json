      {
        "id": 19,
        "label": "let mut tone : u32 = a . seed ^ intensity ;"
      },
      {
        "id": 20,
        "label": "for _ in 0 .. 6 { tone = ((tone << 3) ^ (tone >> 2)) & 1023 ; a . strokes = a . strokes . saturating_add ((tone & 7) as u64) ; a . curve = a . curve . saturating_add ((tone % 19) as u32) ; }"
      },
      {
        "id": 21,
        "label": "tone = ((tone << 3) ^ (tone >> 2)) & 1023 ;"
      },
      {
        "id": 22,
        "label": "a . strokes = a . strokes . saturating_add ((tone & 7) as u64) ;"
      },
      {
        "id": 23,
        "label": "a . curve = a . curve . saturating_add ((tone % 19) as u32) ;"
      },
      {
        "id": 24,
        "label": "if p1 . base + p1 . range < 255 { p1 . mode = Linear ; p1 . base = (p1 . base + (intensity % 11) as u16) . min (255) ; p1 . range = (p1 . range + (a . curve % 9) as u16) . min (180) ; a . curve = a . curve / 2 + 3 ; msg ! (\"P1 linear; base={}, range={}, curve={}\" , p1 . base , p1 . range , a . curve) ; } else { p1 . mode = Curve ; p1 . base = p1 . base . saturating_sub ((intensity % 13) as u16) ; p1 . range = (p1 . range / 2) . max (8) ; a . curve = a . curve . saturating_sub (5) ; msg ! (\"P1 curve; base={}, range={}, curve={}\" , p1 . base , p1 . range , a . curve) ; }"
      },
      {
