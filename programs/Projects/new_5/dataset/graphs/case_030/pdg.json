      },
      {
        "id": 33,
        "label": "a . curve = a . curve . saturating_sub (5) ;"
      },
      {
        "id": 34,
        "label": "msg ! (\"P1 curve; base={}, range={}, curve={}\" , p1 . base , p1 . range , a . curve) ;"
      },
      {
        "id": 35,
        "label": "for _ in 0 .. 3 { if p2 . tag & 1 == 1 { p2 . mode = Curve ; p2 . base = p2 . base . saturating_add (((a . seed % 7) + 1) as u16) ; p2 . range = p2 . range . saturating_add (((a . curve % 5) + 2) as u16) ; msg ! (\"P2 curve+; base={}, range={}\" , p2 . base , p2 . range) ; } else { p2 . mode = Linear ; p2 . base = p2 . base / 2 + 12 ; p2 . range = p2 . range . saturating_sub (3) ; msg ! (\"P2 linear-; base={}, range={}\" , p2 . base , p2 . range) ; } }"
      },
