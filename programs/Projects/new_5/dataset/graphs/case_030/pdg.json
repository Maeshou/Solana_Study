      },
      {
        "id": 28,
        "label": "a . curve = a . curve / 2 + 3 ;"
      },
      {
        "id": 29,
        "label": "msg ! (\"P1 linear; base={}, range={}, curve={}\" , p1 . base , p1 . range , a . curve) ;"
      },
      {
