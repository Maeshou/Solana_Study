        "label": "data_dep: forge"
      },
      {
        "from": 1,
        "to": 5,
        "label": "data_dep: forge"
      }
    ]
  },
  {
    "name": "init_artifact",
    "inputs": [
      "ctx: Context < InitArtifact >",
      "artifact_id: u64",
      "artifact_type: ArtifactType"
    ],
    "nodes": [
      {
        "id": 8,
        "label": "Entry"
      },
      {
        "id": 9,
        "label": "let artifact = & mut ctx . accounts . artifact ;"
      },
      {
        "id": 10,
        "label": "artifact . parent_forge = ctx . accounts . ember_forge . key () ;"
      },
      {
        "id": 11,
        "label": "artifact . artifact_id = artifact_id ^ 0xA5A5A5A5A5A5A5A5 ;"
      },
      {
        "id": 12,
        "label": "artifact . artifact_type = artifact_type ;"
      },
      {
        "id": 13,
        "label": "artifact . power_level = 0 ;"
      },
      {
        "id": 14,
        "label": "artifact . is_charged = false ;"
      },
      {
        "id": 15,
        "label": "msg ! (\"New artifact {} of type {:?} created.\" , artifact . artifact_id , artifact . artifact_type) ;"
      },
      {
        "id": 16,
        "label": "Ok (())"
      }
    ],
