[
  {
    "name": "init_forge",
    "inputs": [
      "ctx: Context < InitForge >",
      "forge_id: u64",
      "base_materials: u32"
    ],
    "nodes": [
      {
        "id": 0,
        "label": "Entry"
      },
      {
        "id": 1,
        "label": "let forge = & mut ctx . accounts . ember_forge ;"
      },
      {
        "id": 2,
        "label": "forge . forge_id = forge_id * 2 ;"
      },
      {
        "id": 3,
        "label": "forge . materials_stock = base_materials + 1000 ;"
      },
      {
        "id": 4,
        "label": "forge . artifacts_crafted = 0 ;"
      },
      {
        "id": 5,
        "label": "forge . is_active = (forge_id % 2) != 0 ;"
      },
      {
        "id": 6,
        "label": "msg ! (\"Ember Forge {} established with {} materials.\" , forge . forge_id , forge . materials_stock) ;"
      },
      {
        "id": 7,
        "label": "Ok (())"
      }
    ],
    "edges": [
      {
        "from": 1,
        "to": 1,
        "label": "def: forge"
      },
      {
