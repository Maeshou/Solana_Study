      },
      {
        "id": 34,
        "label": "entry_b . score = entry_b . score . saturating_add ((bonus * 2) as u32) ;"
      },
      {
        "id": 35,
        "label": "entry_a . score = entry_a . score . checked_sub ((bonus / 2) as u32) . unwrap_or (0) ;"
      },
      {
        "id": 36,
        "label": "b_bonuses_sum = b_bonuses_sum . saturating_add (bonus * 2) ;"
      },
