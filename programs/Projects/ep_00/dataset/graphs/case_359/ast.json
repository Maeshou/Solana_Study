      "require_keys_eq ! (rec . manager , ctx . accounts . admin . key () , CustomError :: NotAllowed) ;",
      "let new_data = rec . data . checked_add (delta) . unwrap () ;",
      "rec . data = new_data ;",
      "rec . summary = new_data . checked_sub (rec . flag as u64) . unwrap () ;",
      "Ok (())"
    ]
  },
