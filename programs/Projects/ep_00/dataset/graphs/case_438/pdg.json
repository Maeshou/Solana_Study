        "label": "let now = Clock :: get () ? . unix_timestamp ;"
      },
      {
        "id": 7,
        "label": "let sum = bal_a . checked_add (bal_b) . unwrap () ;"
      },
      {
        "id": 8,
        "label": "let avg = sum . checked_div (2) . unwrap_or (0) ;"
      },
      {
        "id": 9,
        "label": "let scaled = avg . checked_div (4) . unwrap_or (0) ;"
      },
      {
        "id": 10,
        "label": "msg ! (\"Time {}: half-sum {}\" , now , scaled) ;"
      },
      {
        "id": 11,
        "label": "Ok (())"
