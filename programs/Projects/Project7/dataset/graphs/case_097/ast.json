[
  {
    "name": "initialize_pass_state",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitializePassState >",
      "max_charges: u32",
      "charge_token_amount: u64"
    ],
    "attributes": [],
    "body": [
      "let pass_state = & mut ctx . accounts . pass_state ;",
      "pass_state . owner = ctx . accounts . owner . key () ;",
      "pass_state . charges_left = max_charges ;",
      "pass_state . max_charges = max_charges ;",
      "pass_state . charge_token_amount = charge_token_amount ;",
      "pass_state . charge_token_mint = ctx . accounts . charge_token_mint . key () ;",
      "Ok (())"
    ]
  },
  {
    "name": "charge_pass",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < ChargePass >"
    ],
    "attributes": [],
    "body": [
      "let pass_state = & mut ctx . accounts . pass_state ;",
      "if pass_state . charges_left == 0",
      "{",
      "return Err (ErrorCode :: PassFullyCharged . into ()) ;",
      "}",
      "let mut charges_added = 0 ;",
      "let mut loop_counter = 0 ;",
      "while loop_counter < 5",
      "{",
      "if pass_state . charges_left > 0",
      "{",
      "let charge_ctx = CpiContext :: new (ctx . accounts . token_program . to_account_info () , Transfer { from : ctx . accounts . owner_token_account . to_account_info () , to : ctx . accounts . pass_token_account . to_account_info () , authority : ctx . accounts . owner . to_account_info () , } ,) ;",
      "token :: transfer (charge_ctx , pass_state . charge_token_amount) ? ;",
      "pass_state . charges_left -= 1 ;",
      "charges_added += 1 ;",
      "} else",
      "{",
      "break ;",
      "} loop_counter += 1 ;",
      "}",
      "if charges_added == 0",
      "{",
      "return Err (ErrorCode :: PassFullyCharged . into ()) ;",
      "}",
      "Ok (())"
    ]
  },
