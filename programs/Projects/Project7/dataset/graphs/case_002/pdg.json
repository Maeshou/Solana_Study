      },
      {
        "id": 9,
        "label": "let b = & mut ctx . accounts . boss ;"
      },
      {
        "id": 10,
        "label": "require ! (participants > 0 , ErrorCode :: NoParticipants) ;"
      },
      {
        "id": 11,
        "label": "let mut bonus = 0u64 ;"
      },
      {
        "id": 12,
        "label": "for _ in 0 .. participants { bonus = bonus . saturating_add ((b . bonus_step as u64) * 10) ; }"
      },
      {
        "id": 13,
        "label": "bonus = bonus . saturating_add ((b . bonus_step as u64) * 10) ;"
      },
      {
        "id": 14,
        "label": "let mut reward = b . base_drop . saturating_add (bonus) ;"
      },
      {
        "id": 15,
        "label": "if combo >= 50 { b . hard_mode = true ; reward = reward . saturating_mul (2) ; } else { b . hard_mode = false ; }"
      },
      {
        "id": 16,
        "label": "b . hard_mode = true ;"
      },
      {
        "id": 17,
        "label": "reward = reward . saturating_mul (2) ;"
      },
      {
        "id": 18,
        "label": "b . hard_mode = false ;"
      },
      {
        "id": 19,
        "label": "let cpi_ctx = ctx . accounts . transfer_ctx () ;"
