  {
    "name": "act_release",
    "inputs": [
      "ctx: Context < ActRelease >",
      "rent_days: u8",
      "disputed: bool"
    ],
    "nodes": [
      {
        "id": 10,
        "label": "Entry"
      },
      {
        "id": 11,
        "label": "let es = & mut ctx . accounts . escrow ;"
      },
      {
        "id": 12,
        "label": "let mut total_rent = es . deposit_required ;"
      },
      {
        "id": 13,
        "label": "let mut d : u8 = 1 ;"
      },
      {
        "id": 14,
        "label": "while d < rent_days { let mut per = es . deposit_required / 3 ; if d >= 5 { per = per + per / 5 ; } total_rent = total_rent + per ; d = d + 1 ; }"
      },
      {
        "id": 15,
        "label": "let mut per = es . deposit_required / 3 ;"
      },
      {
        "id": 16,
        "label": "if d >= 5 { per = per + per / 5 ; }"
      },
      {
        "id": 17,
        "label": "per = per + per / 5 ;"
      },
      {
        "id": 18,
        "label": "total_rent = total_rent + per ;"
      },
      {
        "id": 19,
        "label": "d = d + 1 ;"
      },
      {
        "id": 20,
        "label": "let fee = total_rent * es . fee_bps as u64 / 10_000 ;"
      },
      {
        "id": 21,
        "label": "if disputed { let refund = total_rent / 2 ; token :: transfer (ctx . accounts . escrow_to_renter () , refund) ? ; token :: transfer (ctx . accounts . escrow_to_fee () , fee) ? ; es . total_released = es . total_released + refund ; }"
      },
      {
        "id": 22,
        "label": "let refund = total_rent / 2 ;"
      },
      {
        "id": 23,
        "label": "token :: transfer (ctx . accounts . escrow_to_renter () , refund) ? ;"
      },
      {
        "id": 24,
        "label": "token :: transfer (ctx . accounts . escrow_to_fee () , fee) ? ;"
      },
      {
        "id": 25,
        "label": "es . total_released = es . total_released + refund ;"
      },
      {
        "id": 26,
        "label": "if ! disputed { let payout = total_rent - fee ; token :: transfer (ctx . accounts . escrow_to_landlord () , payout) ? ; token :: transfer (ctx . accounts . escrow_to_fee () , fee) ? ; es . total_released = es . total_released + payout ; }"
      },
      {
        "id": 27,
        "label": "let payout = total_rent - fee ;"
      },
      {
        "id": 28,
        "label": "token :: transfer (ctx . accounts . escrow_to_landlord () , payout) ? ;"
      },
      {
        "id": 29,
        "label": "token :: transfer (ctx . accounts . escrow_to_fee () , fee) ? ;"
      },
      {
        "id": 30,
        "label": "es . total_released = es . total_released + payout ;"
      },
      {
        "id": 31,
        "label": "es . days_recorded = es . days_recorded + rent_days as u64 ;"
      },
