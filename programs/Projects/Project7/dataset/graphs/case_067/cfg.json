      {
        "id": 37,
        "label": "state . curve == IncentiveCurve :: Cautious"
      },
      {
        "id": 38,
        "label": "True body"
      },
      {
        "id": 39,
        "label": "effective_bps = effective_bps - (effective_bps / 12) ;"
      },
      {
        "id": 40,
        "label": "False body"
      },
      {
        "id": 41,
        "label": "No-op"
      },
      {
        "id": 42,
        "label": "merge"
      },
      {
        "id": 43,
        "label": "let mut reward_units : u64 = (uptime_bps_reported as u64) * effective_bps / 10_000 ;"
      },
      {
        "id": 44,
        "label": "let upper_limit : u64 = (state . base_rate_bps as u64) * 4 / 10 ;"
      },
      {
        "id": 45,
        "label": "if statement"
      },
      {
        "id": 46,
        "label": "predicate"
      },
      {
        "id": 47,
        "label": "reward_units > upper_limit"
      },
      {
        "id": 48,
        "label": "True body"
      },
      {
        "id": 49,
        "label": "reward_units = upper_limit ;"
      },
      {
        "id": 50,
        "label": "False body"
      },
      {
        "id": 51,
        "label": "No-op"
      },
      {
        "id": 52,
        "label": "merge"
      },
      {
        "id": 53,
        "label": "if statement"
      },
      {
        "id": 54,
        "label": "predicate"
      },
      {
        "id": 55,
        "label": "reward_units < 1"
      },
      {
        "id": 56,
        "label": "True body"
      },
      {
        "id": 57,
        "label": "reward_units = 1 ;"
      },
      {
        "id": 58,
        "label": "False body"
      },
      {
        "id": 59,
        "label": "No-op"
      },
      {
        "id": 60,
        "label": "merge"
