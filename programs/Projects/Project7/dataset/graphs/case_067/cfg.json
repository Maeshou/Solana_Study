      {
        "id": 27,
        "label": "if statement"
      },
      {
        "id": 28,
        "label": "predicate"
      },
      {
        "id": 29,
        "label": "state . curve == IncentiveCurve :: Aggressive"
      },
      {
        "id": 30,
        "label": "True body"
      },
      {
        "id": 31,
        "label": "effective_bps = effective_bps + (effective_bps / 8) ;"
      },
      {
        "id": 32,
        "label": "False body"
      },
      {
        "id": 33,
        "label": "No-op"
      },
      {
        "id": 34,
        "label": "merge"
      },
      {
        "id": 35,
        "label": "if statement"
      },
      {
        "id": 36,
        "label": "predicate"
