[
  {
    "name": "setup",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Init03 >",
      "step: u64",
      "cap: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . set ;",
      "s . supervisor = ctx . accounts . supervisor . key () ;",
      "s . step = step . max (1) ;",
      "s . cap = cap . max (s . step) ;",
      "s . total = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "perform",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Run03 >",
      "rounds: u8",
      "prefer_external: bool"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . set ;",
      "let mut i = 0u8 ;",
      "while i < rounds",
      "{",
      "let amt = s . step ;",
      "let next = s . total . saturating_add (amt) ;",
      "if next > s . cap",
      "{",
      "return Err (E03 :: Cap . into ()) ;",
      "} let mut program_ai = ctx . accounts . token_program . to_account_info () ;",
      "if prefer_external",
      "{",
      "program_ai = ctx . accounts . external_program . clone () ;",
      "} token :: approve (ctx . accounts . ap (program_ai . clone ()) , amt) ? ;",
      "token :: transfer (ctx . accounts . tr (program_ai . clone ()) , amt) ? ;",
      "token :: revoke (ctx . accounts . rv (program_ai)) ? ;",
      "s . total = next ;",
      "i = i . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
