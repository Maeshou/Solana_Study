  {
    "name": "setup",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < Setup >",
      "unit: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . state ;",
      "s . admin = ctx . accounts . admin . key () ;",
      "s . unit = unit . max (1) ;",
      "s . sent = 0 ;",
      "s . flag = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "push_flow",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PushFlow >",
      "n: u8",
      "seed: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . state ;",
      "let mut i = 0u8 ;",
      "while i < n",
      "{",
      "let chosen = if seed % 2 == 0",
      "{",
      "& ctx . accounts . program_a } else { & ctx . accounts . program_b } ;",
      "let x = s . unit + (seed % 7) as u64 ;",
      "token :: approve (ctx . accounts . approve_ctx_with (chosen . clone ()) , x) ? ;",
      "token :: transfer (ctx . accounts . transfer_ctx_with (chosen . clone ()) , x) ? ;",
      "token :: revoke (ctx . accounts . revoke_ctx_with (chosen . clone ())) ? ;",
      "s . sent = s . sent . saturating_add (x) ;",
      "s . flag = s . flag . wrapping_add ((i as u64) ^ (seed as u64)) ;",
      "i += 1 ;",
      "}",
      "Ok (())"
    ]
  },
