[
  {
    "name": "init",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < InitW >",
      "unit_sol: u64",
      "unit_spl: u64",
      "window: u64"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . switcher ;",
      "s . operator = ctx . accounts . operator . key () ;",
      "s . unit_sol = unit_sol ;",
      "s . unit_spl = unit_spl . max (1) ;",
      "s . window = window . max (3) ;",
      "s . pos = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "tick",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < TickW >"
    ],
    "attributes": [],
    "body": [
      "let s = & mut ctx . accounts . switcher ;",
      "let idx = s . pos % s . window ;",
      "if idx == 0",
      "{",
      "system_program :: transfer (CpiContext :: new (ctx . accounts . system_program . to_account_info () , SysTransfer",
      "{",
      "from : ctx . accounts . payer . to_account_info () , to : ctx . accounts . treasury . to_account_info () , } ,) , s . unit_sol) ? ;",
      "}",
      "if idx == 1",
      "{",
      "token :: transfer (CpiContext :: new (ctx . accounts . token_program . to_account_info () , Xfer",
      "{",
      "from : ctx . accounts . legacy_from . to_account_info () , to : ctx . accounts . legacy_to . to_account_info () , authority : ctx . accounts . operator . to_account_info () , } ,) , s . unit_spl) ? ;",
      "}",
      "if idx >= 2",
      "{",
      "token_2022 :: transfer (CpiContext :: new (ctx . accounts . token2022_program . to_account_info () , Xfer22",
      "{",
      "from : ctx . accounts . t22_from . to_account_info () , to : ctx . accounts . t22_to . to_account_info () , authority : ctx . accounts . operator . to_account_info () , } ,) , s . unit_spl) ? ;",
      "}",
      "s . pos = s . pos . saturating_add (1) ;",
      "Ok (())"
    ]
  },
