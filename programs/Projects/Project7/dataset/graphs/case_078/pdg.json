        "label": "token :: transfer (ctx . accounts . pool_to_label () , label_cut) ? ;"
      },
      {
        "id": 26,
        "label": "token :: transfer (ctx . accounts . pool_to_producer () , producer_cut) ? ;"
      },
      {
        "id": 27,
        "label": "s . epoch = s . epoch + 1 ;"
      },
