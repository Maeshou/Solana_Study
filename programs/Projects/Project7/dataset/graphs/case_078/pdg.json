      {
        "id": 12,
        "label": "let mut factor = 100u64 ;"
      },
      {
        "id": 13,
        "label": "let mut count = plays ;"
      },
      {
        "id": 14,
        "label": "while count >= 10_000 { factor = factor + 3 ; count = count - 10_000 ; }"
      },
      {
        "id": 15,
        "label": "factor = factor + 3 ;"
      },
      {
        "id": 16,
        "label": "count = count - 10_000 ;"
      },
      {
        "id": 17,
        "label": "let gross = (plays as u128 * unit_revenue as u128 / 1000u128) as u64 ;"
      },
      {
        "id": 18,
        "label": "let scaled = (gross as u128 * factor as u128 / 100u128) as u64 ;"
      },
      {
        "id": 19,
        "label": "let artist_cut = (scaled as u128 * s . artist_bps as u128 / 10_000u128) as u64 ;"
      },
      {
        "id": 20,
        "label": "let label_cut = (scaled as u128 * s . label_bps as u128 / 10_000u128) as u64 ;"
      },
      {
        "id": 21,
        "label": "let producer_cut = if scaled > artist_cut + label_cut { scaled - artist_cut - label_cut } else { s . min_payout } ;"
      },
      {
        "id": 22,
        "label": "scaled - artist_cut - label_cut"
      },
      {
        "id": 23,
        "label": "s . min_payout"
