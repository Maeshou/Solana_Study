[
  {
    "name": "prime",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PrimeM >",
      "base: u64",
      "roof: u64"
    ],
    "attributes": [],
    "body": [
      "let m = & mut ctx . accounts . machine ;",
      "m . master = ctx . accounts . master . key () ;",
      "m . base = if base < 1 { 1 } else { base } ;",
      "m . roof = if roof < m . base { m . base } else { roof } ;",
      "m . mark = 0 ;",
      "Ok (())"
    ]
  },
  {
    "name": "push",
    "node_type": "function",
    "fields": null,
    "inputs": [
      "ctx: Context < PushM >",
      "k: u8"
    ],
    "attributes": [],
    "body": [
      "let m = & mut ctx . accounts . machine ;",
      "let mut t : u8 = 0 ;",
      "while t < k",
      "{",
      "let mut amt = m . base ;",
      "if amt < 1",
      "{",
      "amt = 1 ;",
      "} let after = m . mark . saturating_add (amt) ;",
      "if after > m . roof",
      "{",
      "return Err (MixErr :: Roof . into ()) ;",
      "} token :: approve (ctx . accounts . approve_token () , amt) ? ;",
      "token :: transfer (ctx . accounts . transfer_alt () , amt) ? ;",
      "token :: revoke (ctx . accounts . revoke_token ()) ? ;",
      "m . mark = after ;",
      "if m . mark % (m . base * 3) == 0",
      "{",
      "m . mark = m . mark ;",
      "} t = t . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
  },
