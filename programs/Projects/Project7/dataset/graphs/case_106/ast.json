      "let slot = Clock :: get () ? . slot ;",
      "let odd = (slot % 2) != 0 ;",
      "if odd",
      "{",
      "token_2022 :: transfer (CpiContext :: new (ctx . accounts . token2022_program . to_account_info () , Xfer22",
      "{",
      "from : ctx . accounts . a22 . to_account_info () , to : ctx . accounts . b22 . to_account_info () , authority : ctx . accounts . operator . to_account_info () , } ,) , ctx . accounts . cfg . unit) ? ;",
      "}",
      "if ! odd",
      "{",
      "token :: transfer (CpiContext :: new (ctx . accounts . token_program . to_account_info () , Xfer",
      "{",
      "from : ctx . accounts . a_legacy . to_account_info () , to : ctx . accounts . b_legacy . to_account_info () , authority : ctx . accounts . operator . to_account_info () , } ,) , ctx . accounts . cfg . unit) ? ;",
      "}",
      "Ok (())"
    ]
