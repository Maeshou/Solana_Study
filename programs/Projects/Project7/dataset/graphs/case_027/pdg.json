  {
    "name": "act_distribute",
    "inputs": [
      "ctx: Context < ActDistribute >",
      "party_count: u8",
      "combo_points: u32"
    ],
    "nodes": [
      {
        "id": 9,
        "label": "Entry"
      },
      {
        "id": 10,
        "label": "require ! (party_count > 0 , ErrRaid :: NoParticipants) ;"
      },
      {
        "id": 11,
        "label": "let boss_state = & mut ctx . accounts . boss_state ;"
      },
      {
        "id": 12,
        "label": "let mut total_increment = 0u64 ;"
      },
      {
        "id": 13,
        "label": "let mut member_index = 1u8 ;"
      },
      {
        "id": 14,
        "label": "while member_index <= party_count { let add = (boss_state . increment_step as u64) . saturating_div (member_index as u64) ; total_increment = total_increment . saturating_add (add) ; member_index = member_index . saturating_add (1) ; }"
      },
      {
        "id": 15,
        "label": "let add = (boss_state . increment_step as u64) . saturating_div (member_index as u64) ;"
      },
      {
        "id": 16,
        "label": "total_increment = total_increment . saturating_add (add) ;"
      },
      {
        "id": 17,
        "label": "member_index = member_index . saturating_add (1) ;"
      },
      {
        "id": 18,
        "label": "if combo_points > boss_state . max_combo_record as u32 { boss_state . max_combo_record = combo_points as u64 ; }"
      },
      {
        "id": 19,
        "label": "boss_state . max_combo_record = combo_points as u64 ;"
      },
      {
        "id": 20,
        "label": "if combo_points >= 50 { boss_state . challenge_flag = true ; }"
      },
      {
        "id": 21,
        "label": "boss_state . challenge_flag = true ;"
      },
      {
        "id": 22,
        "label": "if combo_points < 30 { boss_state . challenge_flag = false ; }"
      },
      {
        "id": 23,
        "label": "boss_state . challenge_flag = false ;"
      },
      {
        "id": 24,
        "label": "let mut payout = boss_state . base_reward . saturating_add (total_increment) ;"
      },
      {
        "id": 25,
        "label": "if boss_state . challenge_flag { payout = payout . saturating_mul (2) ; }"
      },
      {
        "id": 26,
        "label": "payout = payout . saturating_mul (2) ;"
      },
      {
        "id": 27,
        "label": "token :: transfer (ctx . accounts . treasury_to_player () , payout) ? ;"
      },
      {
        "id": 28,
        "label": "boss_state . clear_count = boss_state . clear_count . saturating_add (1) ;"
      },
      {
        "id": 29,
        "label": "Ok (())"
      }
