    "name": "act_craft",
    "inputs": [
      "ctx: Context < ActCraft >",
      "provided_power: u32",
      "base_cost: u64",
      "step_count: u8"
    ],
    "nodes": [
      {
        "id": 8,
        "label": "Entry"
      },
      {
        "id": 9,
        "label": "let station = & mut ctx . accounts . station ;"
      },
      {
        "id": 10,
        "label": "let mut total_cost = base_cost . max (1) ;"
      },
      {
        "id": 11,
        "label": "let mut step_cursor = 0u8 ;"
      },
      {
        "id": 12,
        "label": "while step_cursor < step_count { total_cost = total_cost . saturating_add ((step_cursor as u64) . saturating_add (2)) ; step_cursor = step_cursor . saturating_add (1) ; }"
      },
      {
        "id": 13,
        "label": "total_cost = total_cost . saturating_add ((step_cursor as u64) . saturating_add (2)) ;"
      },
      {
        "id": 14,
        "label": "step_cursor = step_cursor . saturating_add (1) ;"
      },
      {
        "id": 15,
        "label": "if provided_power < station . required_power { station . phase = CraftPhase :: Cooling ; return Err (CraftErr :: LowPower . into ()) ; }"
      },
      {
        "id": 16,
        "label": "station . phase = CraftPhase :: Cooling ;"
      },
      {
        "id": 17,
        "label": "return Err (CraftErr :: LowPower . into ()) ;"
      },
      {
        "id": 18,
        "label": "station . phase = CraftPhase :: Heating ;"
      },
      {
        "id": 19,
        "label": "token :: burn (ctx . accounts . burn_ctx () , total_cost) ? ;"
      },
      {
        "id": 20,
        "label": "station . materials_burned = station . materials_burned . saturating_add (total_cost) ;"
      },
      {
        "id": 21,
        "label": "station . progress_points = station . progress_points . saturating_add (total_cost . saturating_div (4) . max (1)) ;"
      },
      {
        "id": 22,
        "label": "if station . progress_points > station . materials_burned . saturating_div (3) { station . phase = CraftPhase :: Assembling ; }"
      },
      {
        "id": 23,
        "label": "station . phase = CraftPhase :: Assembling ;"
      },
      {
        "id": 24,
        "label": "if station . progress_points > station . materials_burned . saturating_sub (station . materials_burned / 5) { station . phase = CraftPhase :: Complete ; }"
      },
      {
        "id": 25,
        "label": "station . phase = CraftPhase :: Complete ;"
      },
      {
        "id": 26,
        "label": "Ok (())"
      }
