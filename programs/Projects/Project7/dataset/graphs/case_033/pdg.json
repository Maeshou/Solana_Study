  {
    "name": "act_split",
    "inputs": [
      "ctx: Context < ActSplit >",
      "cycle_revenue: u64"
    ],
    "nodes": [
      {
        "id": 9,
        "label": "Entry"
      },
      {
        "id": 10,
        "label": "let deal = & mut ctx . accounts . deal ;"
      },
      {
        "id": 11,
        "label": "let mut extra_bps = 0u64 ;"
      },
      {
        "id": 12,
        "label": "let mut r = 1u64 ;"
      },
      {
        "id": 13,
        "label": "while r <= deal . round_counter { if r % 5 == 0 { extra_bps = extra_bps . saturating_add (100) ; } r = r . saturating_add (1) ; }"
      },
      {
        "id": 14,
        "label": "if r % 5 == 0 { extra_bps = extra_bps . saturating_add (100) ; }"
      },
      {
        "id": 15,
        "label": "extra_bps = extra_bps . saturating_add (100) ;"
      },
      {
        "id": 16,
        "label": "r = r . saturating_add (1) ;"
      },
      {
        "id": 17,
        "label": "let average = deal . revenue_accumulator . saturating_div (deal . round_counter . max (1)) ;"
      },
      {
        "id": 18,
        "label": "if cycle_revenue > average { deal . variable_mode = true ; }"
      },
      {
        "id": 19,
        "label": "deal . variable_mode = true ;"
      },
      {
        "id": 20,
        "label": "if cycle_revenue <= average { deal . variable_mode = false ; }"
      },
      {
        "id": 21,
        "label": "deal . variable_mode = false ;"
      },
      {
        "id": 22,
        "label": "let fund_cut = cycle_revenue . saturating_mul (deal . dev_fund_bps as u64) / 10_000 ;"
      },
      {
        "id": 23,
        "label": "let pool_after_fund = cycle_revenue . saturating_sub (fund_cut) ;"
      },
      {
        "id": 24,
        "label": "let scholar_cut = if deal . variable_mode { pool_after_fund . saturating_mul (55) / 100 . saturating_add (pool_after_fund . saturating_mul (extra_bps) / 10_000) } else { pool_after_fund . saturating_mul ((deal . scholar_share_bps as u64) . saturating_add (extra_bps)) / 10_000 } ;"
      },
      {
        "id": 25,
        "label": "pool_after_fund . saturating_mul (55) / 100 . saturating_add (pool_after_fund . saturating_mul (extra_bps) / 10_000)"
      },
      {
        "id": 26,
        "label": "pool_after_fund . saturating_mul ((deal . scholar_share_bps as u64) . saturating_add (extra_bps)) / 10_000"
      },
      {
        "id": 27,
        "label": "let manager_cut = pool_after_fund . saturating_sub (scholar_cut) ;"
      },
      {
        "id": 28,
        "label": "token :: transfer (ctx . accounts . income_to_fund () , fund_cut) ? ;"
      },
      {
        "id": 29,
        "label": "token :: transfer (ctx . accounts . income_to_scholar () , scholar_cut) ? ;"
      },
      {
        "id": 30,
        "label": "token :: transfer (ctx . accounts . income_to_manager () , manager_cut) ? ;"
      },
      {
        "id": 31,
        "label": "deal . revenue_accumulator = deal . revenue_accumulator . saturating_add (cycle_revenue) ;"
      },
      {
        "id": 32,
        "label": "deal . round_counter = deal . round_counter . saturating_add (1) ;"
      },
      {
        "id": 33,
        "label": "Ok (())"
      }
