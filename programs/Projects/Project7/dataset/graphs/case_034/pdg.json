      {
        "id": 9,
        "label": "let board = & mut ctx . accounts . board ;"
      },
      {
        "id": 10,
        "label": "token :: burn (ctx . accounts . burn_ctx () , proof_units . max (1)) ? ;"
      },
      {
        "id": 11,
        "label": "let mut grant_units = board . weekly_basis . saturating_div (25) . max (1) ;"
      },
      {
        "id": 12,
        "label": "let mut task_cursor = 0u8 ;"
      },
      {
        "id": 13,
        "label": "while task_cursor < completed_tasks { grant_units = grant_units . saturating_add ((task_cursor as u64 % 4) . saturating_add (1)) ; task_cursor = task_cursor . saturating_add (1) ; }"
      },
      {
        "id": 14,
        "label": "grant_units = grant_units . saturating_add ((task_cursor as u64 % 4) . saturating_add (1)) ;"
      },
      {
        "id": 15,
        "label": "task_cursor = task_cursor . saturating_add (1) ;"
      },
      {
        "id": 16,
        "label": "if board . current_streak % 7 == 0 { grant_units = grant_units . saturating_add (3) ; }"
      },
      {
        "id": 17,
        "label": "grant_units = grant_units . saturating_add (3) ;"
      },
      {
        "id": 18,
        "label": "let next_total = board . issued_today . saturating_add (grant_units) ;"
      },
      {
        "id": 19,
        "label": "if next_total > board . daily_max { let overflow = next_total . saturating_sub (board . daily_max) ; board . weekly_basis = board . weekly_basis . saturating_add (overflow) ; board . current_streak = 1 ; return Err (QuestErr :: DailyCap . into ()) ; }"
      },
      {
        "id": 20,
        "label": "let overflow = next_total . saturating_sub (board . daily_max) ;"
      },
      {
        "id": 21,
        "label": "board . weekly_basis = board . weekly_basis . saturating_add (overflow) ;"
      },
      {
        "id": 22,
        "label": "board . current_streak = 1 ;"
      },
      {
        "id": 23,
        "label": "return Err (QuestErr :: DailyCap . into ()) ;"
      },
      {
        "id": 24,
        "label": "token :: mint_to (ctx . accounts . mint_ctx () , grant_units) ? ;"
      },
      {
        "id": 25,
        "label": "board . issued_today = next_total ;"
      },
      {
        "id": 26,
        "label": "board . current_streak = board . current_streak . saturating_add (1) ;"
      },
