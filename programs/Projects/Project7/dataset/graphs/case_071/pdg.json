        "label": "let mut penalty_bps : u64 = guard . base_penalty_bps as u64 ;"
      },
      {
        "id": 10,
        "label": "let mut severity_cursor : u8 = 0 ;"
      },
      {
        "id": 11,
        "label": "while severity_cursor < defect_severity { penalty_bps = penalty_bps + 50 ; severity_cursor = severity_cursor + 1 ; }"
      },
      {
        "id": 12,
        "label": "penalty_bps = penalty_bps + 50 ;"
      },
      {
        "id": 13,
        "label": "severity_cursor = severity_cursor + 1 ;"
      },
      {
        "id": 14,
        "label": "if penalty_bps > 5000 { penalty_bps = 5000 ; }"
      },
      {
        "id": 15,
        "label": "penalty_bps = 5000 ;"
      },
      {
        "id": 16,
        "label": "let penalty_amount : u64 = (order_amount as u128 * penalty_bps as u128 / 10_000u128) as u64 ;"
      },
      {
        "id": 17,
        "label": "let buyer_refund : u64 = order_amount - penalty_amount ;"
      },
      {
        "id": 18,
        "label": "token :: transfer (ctx . accounts . seller_pool_to_buyer () , buyer_refund) ? ;"
      },
      {
