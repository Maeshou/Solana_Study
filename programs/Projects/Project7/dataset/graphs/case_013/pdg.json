    "name": "act_collect_or_refund",
    "inputs": [
      "ctx: Context < ActCollectOrRefund >",
      "months: u8",
      "do_refund: bool"
    ],
    "nodes": [
      {
        "id": 9,
        "label": "Entry"
      },
      {
        "id": 10,
        "label": "let guild = & mut ctx . accounts . guild ;"
      },
      {
        "id": 11,
        "label": "let mut aggregate = guild . due_units ;"
      },
      {
        "id": 12,
        "label": "let mut idx = 1u8 ;"
      },
      {
        "id": 13,
        "label": "while idx < months { aggregate = aggregate . saturating_add (guild . due_units) ; idx = idx . saturating_add (1) ; }"
      },
      {
        "id": 14,
        "label": "aggregate = aggregate . saturating_add (guild . due_units) ;"
      },
      {
        "id": 15,
        "label": "idx = idx . saturating_add (1) ;"
      },
      {
        "id": 16,
        "label": "if do_refund { let refund_amount = aggregate . saturating_mul (guild . refund_bps as u64) / 10_000 ; let cpi = ctx . accounts . pay_from_treasury_to_member () ; token :: transfer (cpi , refund_amount) ? ; guild . collected_units = guild . collected_units . saturating_sub (refund_amount) ; } else { if guild . strict { let cpi = ctx . accounts . pay_from_member_to_treasury () ; token :: transfer (cpi , aggregate) ? ; } else { let cpi = ctx . accounts . pay_from_member_to_treasury () ; token :: transfer (cpi , aggregate / 2) ? ; } guild . collected_units = guild . collected_units . saturating_add (aggregate) ; }"
      },
      {
        "id": 17,
        "label": "let refund_amount = aggregate . saturating_mul (guild . refund_bps as u64) / 10_000 ;"
      },
      {
        "id": 18,
        "label": "let cpi = ctx . accounts . pay_from_treasury_to_member () ;"
      },
      {
        "id": 19,
        "label": "token :: transfer (cpi , refund_amount) ? ;"
      },
      {
        "id": 20,
        "label": "guild . collected_units = guild . collected_units . saturating_sub (refund_amount) ;"
      },
      {
        "id": 21,
        "label": "if guild . strict { let cpi = ctx . accounts . pay_from_member_to_treasury () ; token :: transfer (cpi , aggregate) ? ; } else { let cpi = ctx . accounts . pay_from_member_to_treasury () ; token :: transfer (cpi , aggregate / 2) ? ; }"
      },
      {
        "id": 22,
        "label": "let cpi = ctx . accounts . pay_from_member_to_treasury () ;"
      },
      {
        "id": 23,
        "label": "token :: transfer (cpi , aggregate) ? ;"
      },
      {
        "id": 24,
        "label": "let cpi = ctx . accounts . pay_from_member_to_treasury () ;"
      },
      {
        "id": 25,
        "label": "token :: transfer (cpi , aggregate / 2) ? ;"
      },
      {
        "id": 26,
        "label": "guild . collected_units = guild . collected_units . saturating_add (aggregate) ;"
      },
      {
        "id": 27,
        "label": "guild . period_count = guild . period_count . saturating_add (1) ;"
      },
