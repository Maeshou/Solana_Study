      {
        "id": 23,
        "label": "p . current_level = p . current_level . saturating_add (1) ;"
      },
      {
        "id": 24,
        "label": "token :: mint_to (ctx . accounts . mint_ctx () , grant) ? ;"
      },
      {
        "id": 25,
        "label": "p . total_charged = p . total_charged . saturating_add (grant) ;"
      },
