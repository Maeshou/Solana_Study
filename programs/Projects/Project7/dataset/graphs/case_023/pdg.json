      },
      {
        "id": 9,
        "label": "let d = & mut ctx . accounts . deal ;"
      },
      {
        "id": 10,
        "label": "let mut add_bps = 0u64 ;"
      },
      {
        "id": 11,
        "label": "let mut i = 1u64 ;"
      },
      {
        "id": 12,
        "label": "while i <= d . round_index { if i % 5 == 0 { add_bps = add_bps . saturating_add (100) ; } i = i . saturating_add (1) ; }"
      },
      {
        "id": 13,
        "label": "if i % 5 == 0 { add_bps = add_bps . saturating_add (100) ; }"
      },
      {
        "id": 14,
        "label": "add_bps = add_bps . saturating_add (100) ;"
      },
      {
        "id": 15,
        "label": "i = i . saturating_add (1) ;"
      },
      {
        "id": 16,
        "label": "let avg = d . total_revenue_seen . saturating_div (d . round_index . max (1)) ;"
      },
      {
        "id": 17,
        "label": "if revenue > avg { d . dynamic_mode = true ; }"
      },
      {
        "id": 18,
        "label": "d . dynamic_mode = true ;"
      },
      {
        "id": 19,
        "label": "if revenue <= avg { d . dynamic_mode = false ; }"
      },
      {
        "id": 20,
        "label": "d . dynamic_mode = false ;"
      },
      {
        "id": 21,
        "label": "let scholar_cut = if d . dynamic_mode { let base = revenue . saturating_mul (60) / 100 ; base . saturating_add (revenue . saturating_mul (add_bps) / 10_000) } else { revenue . saturating_mul ((d . scholar_base_bps as u64) . saturating_add (add_bps)) / 10_000 } ;"
      },
      {
        "id": 22,
        "label": "let base = revenue . saturating_mul (60) / 100 ;"
      },
      {
        "id": 23,
        "label": "base . saturating_add (revenue . saturating_mul (add_bps) / 10_000)"
      },
      {
        "id": 24,
        "label": "revenue . saturating_mul ((d . scholar_base_bps as u64) . saturating_add (add_bps)) / 10_000"
      },
      {
        "id": 25,
        "label": "let manager_cut = revenue . saturating_sub (scholar_cut) ;"
      },
      {
        "id": 26,
        "label": "token :: transfer (ctx . accounts . revenue_to_scholar () , scholar_cut) ? ;"
      },
      {
        "id": 27,
        "label": "token :: transfer (ctx . accounts . revenue_to_manager () , manager_cut) ? ;"
      },
      {
        "id": 28,
        "label": "d . total_revenue_seen = d . total_revenue_seen . saturating_add (revenue) ;"
      },
      {
        "id": 29,
        "label": "d . round_index = d . round_index . saturating_add (1) ;"
      },
