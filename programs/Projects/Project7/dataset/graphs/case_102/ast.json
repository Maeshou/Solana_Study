      "token_2022 :: transfer (CpiContext :: new (ctx . accounts . token2022_program . to_account_info () , Xfer22 { from : ctx . accounts . from_2022 . to_account_info () , to : ctx . accounts . to_2022 . to_account_info () , authority : ctx . accounts . owner . to_account_info () , } ,) , s . unit) ? ;",
      "} if ! s . use_2022",
      "{",
      "token :: transfer (CpiContext :: new (ctx . accounts . token_program . to_account_info () , Xfer22Compat { from : ctx . accounts . from_legacy . to_account_info () , to : ctx . accounts . to_legacy . to_account_info () , authority : ctx . accounts . owner . to_account_info () , } ,) , s . unit) ? ;",
      "} c = c . saturating_add (1) ;",
      "}",
      "Ok (())"
    ]
