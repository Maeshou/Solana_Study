  {
    "name": "act_split",
    "inputs": [
      "ctx: Context < ActSplit >",
      "w1: u16",
      "w2: u16",
      "w3: u16"
    ],
    "nodes": [
      {
        "id": 9,
        "label": "Entry"
      },
      {
        "id": 10,
        "label": "let pool = & mut ctx . accounts . pool ;"
      },
      {
        "id": 11,
        "label": "let total_weight = w1 as u64 + w2 as u64 + w3 as u64 ;"
      },
      {
        "id": 12,
        "label": "let mut s1 = pool . units * w1 as u64 / total_weight ;"
      },
      {
        "id": 13,
        "label": "let mut s2 = pool . units * w2 as u64 / total_weight ;"
      },
      {
        "id": 14,
        "label": "let mut s3 = pool . units * w3 as u64 / total_weight ;"
      },
      {
        "id": 15,
        "label": "let now = s1 + s2 + s3 ;"
      },
      {
        "id": 16,
        "label": "if now < pool . units { s1 = s1 + (pool . units - now) ; }"
      },
      {
        "id": 17,
        "label": "s1 = s1 + (pool . units - now) ;"
      },
      {
        "id": 18,
        "label": "token :: transfer (ctx . accounts . pool_to_one () , s1) ? ;"
      },
      {
        "id": 19,
        "label": "token :: transfer (ctx . accounts . pool_to_two () , s2) ? ;"
      },
      {
        "id": 20,
        "label": "token :: transfer (ctx . accounts . pool_to_three () , s3) ? ;"
      },
      {
        "id": 21,
        "label": "pool . total_paid = pool . total_paid + s1 + s2 + s3 ;"
      },
      {
        "id": 22,
        "label": "pool . season = pool . season + 1 ;"
      },
      {
        "id": 23,
        "label": "Ok (())"
      }
